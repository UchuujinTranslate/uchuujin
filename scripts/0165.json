[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 28,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よっ、は……っ、ああっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2202,
            "text_len": 42,
            "text_offset": 2210,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……う―ん。\\n何か今日は調子が悪いなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2274,
            "text_len": 70,
            "text_offset": 2282,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "さっきからこれで五回目の失敗かぁ。\\nいつもと同じはずなんだけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2374,
            "text_len": 52,
            "text_offset": 2382,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ひょっとしてスランプ？\\nなんて、そんなわけないか。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2456,
            "text_len": 80,
            "text_offset": 2464,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "悩んでないで練習練習。今、戦力になるのは\\n部でオレだけっぽいし、頑張らないと。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2566,
            "text_len": 26,
            "text_offset": 2574,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よっ、っと……おっとと。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2622,
            "text_len": 36,
            "text_offset": 2630,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よし……今度はいい感じに弌弌あっ！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2758,
            "text_len": 34,
            "text_offset": 2762,
            "warnings": 1
        },
        "speaker": "",
        "text": "ボ―ルが飛んでくるぞ、回避急げ！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2820,
            "text_len": 74,
            "text_offset": 2824,
            "warnings": 1
        },
        "speaker": "",
        "text": "まあ慌てなさんな。こんなこともあろうかと、\\n覇道砲を準備しておいたのさ！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2920,
            "text_len": 62,
            "text_offset": 2924,
            "warnings": 3
        },
        "speaker": "",
        "text": "覇道砲、発射準備！エネルギ―充填１００！\\n……ぽちっとな\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3008,
            "text_len": 22,
            "text_offset": 3012,
            "warnings": 1
        },
        "speaker": "",
        "text": "おい、早くしろって！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3056,
            "text_len": 44,
            "text_offset": 3060,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あれ？　あれれっ！？　……あ、そうだ！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3160,
            "text_len": 62,
            "text_offset": 3164,
            "warnings": 2
        },
        "speaker": "",
        "text": "覇道砲と言えば１２０ですよね―！\\n……次は気をつけよっと。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3272,
            "text_len": 28,
            "text_offset": 3276,
            "warnings": 1
        },
        "speaker": "",
        "text": "次って、もうないわ――！！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3428,
            "text_len": 40,
            "text_offset": 3436,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわぁっ！？\\n人に当たっちゃった……？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3512,
            "text_len": 44,
            "text_offset": 3520,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "スッスススイマセン！\\n大丈夫ですかっ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3596,
            "text_len": 34,
            "text_offset": 3604,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あっ、あの人ってもしかして……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3662,
            "text_len": 48,
            "text_offset": 3670,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "やっぱり、モヒカンの人だ！！\\nどうしよう……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3744,
            "text_len": 36,
            "text_offset": 3752,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの人って色んな噂あるんだよなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3804,
            "text_len": 58,
            "text_offset": 3812,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あんな髪型なのに\\n先生にぜんぜん呼び出されないらしいし。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3886,
            "text_len": 72,
            "text_offset": 3894,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あと、横断歩道でゆっくり歩いてたお年寄りを\\n無理やり早く渡らせたとか。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3988,
            "text_len": 70,
            "text_offset": 3996,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "他にも花壇でジョウロ持って\\n何かしてたとか……うわっ、こっち来てる！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4098,
            "text_len": 70,
            "text_offset": 4106,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ボ―ル持ってるってことは、\\nオレのだってわかってるって事だよな……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4198,
            "text_len": 38,
            "text_offset": 4206,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ひょ、ひょっとして殴られるかも……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4306,
            "text_len": 28,
            "text_offset": 4314,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "お―い、これ君のだよな―？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4368,
            "text_len": 12,
            "text_offset": 4376,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4414,
            "text_len": 30,
            "text_offset": 4422,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（う、うわ、どうしよう……）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4478,
            "text_len": 64,
            "text_offset": 4486,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（わ、悪いのはオレなんだけど……\\n　でも、やっぱり怖いし……）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4572,
            "text_len": 10,
            "text_offset": 4580,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "お―い！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4616,
            "text_len": 16,
            "text_offset": 4624,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（弌弌よしっ）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4688,
            "text_len": 16,
            "text_offset": 4696,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（逃げろっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4744,
            "text_len": 14,
            "text_offset": 4752,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あっ、おい！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4886,
            "text_len": 32,
            "text_offset": 4894,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "なっ！！？　追いかけてきた！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4952,
            "text_len": 20,
            "text_offset": 4960,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うおおおおおお！！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5000,
            "text_len": 54,
            "text_offset": 5008,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわっ、足速っ！！\\nこ、これは捕まったらヤバい……！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5082,
            "text_len": 22,
            "text_offset": 5090,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "待てええぇぇぇ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5234,
            "text_len": 24,
            "text_offset": 5242,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はぁっ、はぁっ、は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5288,
            "text_len": 20,
            "text_offset": 5296,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ぜぇ……ぜぇ……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5346,
            "text_len": 14,
            "text_offset": 5354,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……ははっ。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5392,
            "text_len": 12,
            "text_offset": 5400,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "弌弌ふっ。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5520,
            "text_len": 14,
            "text_offset": 5528,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ナイスラン！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5562,
            "text_len": 16,
            "text_offset": 5570,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ナイスラン！！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5648,
            "text_len": 30,
            "text_offset": 5656,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（いや、素直に謝るべきだよ）\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5722,
            "text_len": 70,
            "text_offset": 5730,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（そりゃあ悪い噂はあるけど、\\n　そんなとんでもない人じゃないはず！）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5824,
            "text_len": 20,
            "text_offset": 5832,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "お―い、これ君の？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5876,
            "text_len": 36,
            "text_offset": 5884,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はい。すいません、大丈夫でしたか？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5942,
            "text_len": 26,
            "text_offset": 5950,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "平気だよ。\\nはい、これ。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6002,
            "text_len": 24,
            "text_offset": 6010,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ありがとうございます！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6064,
            "text_len": 58,
            "text_offset": 6072,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（ほら、いい人じゃないか。\\n　素直に謝って正解だったよ）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6162,
            "text_len": 58,
            "text_offset": 6170,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ところで君、サッカ―部？\\nこんな所で練習なんて珍しいね。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6250,
            "text_len": 38,
            "text_offset": 6258,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いえ、オレは囲碁サッカ―部なんです。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6318,
            "text_len": 58,
            "text_offset": 6326,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "い、囲碁サッカ―！？\\nそ、そっか……うん、練習頑張って。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6412,
            "text_len": 56,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ありがとうございます！\\nあの、本当にすいませんでした。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6498,
            "text_len": 36,
            "text_offset": 6506,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "いいっていいって。そ、それじゃあ。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6574,
            "text_len": 58,
            "text_offset": 6582,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "良かった……変な噂の多い人だし、\\nどうなるかと思ったよ。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6662,
            "text_len": 62,
            "text_offset": 6670,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "でも、やっぱり噂は噂。\\n噂とか見かけで判断しちゃダメだよな！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6854,
            "text_len": 40,
            "text_offset": 6862,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ふぅ、まさか囲碁サッカ―部だったとは。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7006,
            "text_len": 76,
            "text_offset": 7014,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "頭痛いけど、変な噂ばっかり聞く部だし、\\nこれ以上関わらない方がいいもんな。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7148,
            "text_len": 34,
            "text_offset": 7156,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……そうだ、知らないフリしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7224,
            "text_len": 26,
            "text_offset": 7232,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "お―い、これ君のボ―ル？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7286,
            "text_len": 26,
            "text_offset": 7294,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え……いえ、チガイマス。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7342,
            "text_len": 46,
            "text_offset": 7350,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "え、違うの？\\nこっちから飛んできたんだけど。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7422,
            "text_len": 78,
            "text_offset": 7430,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（おかしいな。すごくあせっていたし、\\n　方向も間違いないと思うんだけど……）\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7530,
            "text_len": 52,
            "text_offset": 7538,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そ、そういえば！\\nあっちに誰か走っていきましたよ！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7616,
            "text_len": 12,
            "text_offset": 7624,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7662,
            "text_len": 80,
            "text_offset": 7670,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（まさか僕を怖い不良だと思っている！？\\n　やはり、この髪型のせいだろうか……）\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7782,
            "text_len": 88,
            "text_offset": 7790,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（それはまずい、誤解はちゃんと解かないと。\\n　決して僕は不良なんかじゃないんだって！）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7910,
            "text_len": 12,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……よし。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7956,
            "text_len": 8,
            "text_offset": 7964,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はい？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7994,
            "text_len": 78,
            "text_offset": 8002,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "このボ―ル、ずいぶん汚れてるな―。\\nきれいに磨いてから返してあげようかな―。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8102,
            "text_len": 64,
            "text_offset": 8110,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え！？　あ、あのっ。\\nそこまでしなくてもいいんじゃないですか？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8200,
            "text_len": 42,
            "text_offset": 8208,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（……ちょっとわざとらしかっただろうか）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8286,
            "text_len": 76,
            "text_offset": 8294,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（でも、不良と思われてるなら、\\n　これぐらいしないと取り戻せないはずだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8392,
            "text_len": 70,
            "text_offset": 8400,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "いや―、何だか驚かせたみたいだし、\\nそれに汚れも気になったからさ―。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8496,
            "text_len": 64,
            "text_offset": 8504,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（よし、いけるぞ。\\n　このまま不良のイメ―ジを打ち消すんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8590,
            "text_len": 66,
            "text_offset": 8598,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "この辺りに置いとけば、\\nボ―ルの持ち主も気がついてくれるかな―。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8686,
            "text_len": 30,
            "text_offset": 8694,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "た、多分、気付くと思います。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8756,
            "text_len": 50,
            "text_offset": 8764,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……あれ？\\n　この人、実は不良じゃないのか？）\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8840,
            "text_len": 62,
            "text_offset": 8848,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（だとしたら、オレ、\\n　ものすごく悪いことしてるんじゃ……）\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8932,
            "text_len": 20,
            "text_offset": 8940,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………あ、あの。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8982,
            "text_len": 6,
            "text_offset": 8990,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9022,
            "text_len": 60,
            "text_offset": 9030,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "す、すいません。\\n弌弌実はオレなんです、ボ―ルぶつけたの。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9126,
            "text_len": 42,
            "text_offset": 9134,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（多分、正直に謝ればこの人も許して弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9198,
            "text_len": 64,
            "text_offset": 9206,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、うん、それは知っていたんだ。\\nでも気にしてないから大丈夫。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9292,
            "text_len": 8,
            "text_offset": 9300,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9334,
            "text_len": 60,
            "text_offset": 9342,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（よし！　これで\\n　僕が不良じゃないと証明されたはずだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9428,
            "text_len": 22,
            "text_offset": 9436,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……し、知っていた？\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9494,
            "text_len": 60,
            "text_offset": 9502,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（ま、まさか、\\n　オレが逃げられないよう自白させたっ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9602,
            "text_len": 84,
            "text_offset": 9610,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（マズイぞ。オレ、この人のこと誤解していた。\\n　不良どころじゃない……プロだっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9716,
            "text_len": 12,
            "text_offset": 9724,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あははは。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9758,
            "text_len": 18,
            "text_offset": 9766,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、あはははは。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9810,
            "text_len": 38,
            "text_offset": 9818,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（やれやれ、誤解がとけて助かったよ）\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 9960,
            "text_len": 44,
            "text_offset": 9980,
            "warnings": 2
        },
        "speaker": "ナレ―タ―お姉さん",
        "text": "その日、不良の誤解は解けたと言います。\\n",
        "type": "dialog"
    }
]