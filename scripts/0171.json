[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2144,
            "text_len": 64,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "さあいらっしゃい、白いよ―、丸いよ―。\\nその上黒くて甘いよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2242,
            "text_len": 18,
            "text_offset": 2250,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、やたいだ！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2294,
            "text_len": 54,
            "text_offset": 2306,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ははは、お嬢ちゃん。\\n屋台じゃなくて大福屋さんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2382,
            "text_len": 44,
            "text_offset": 2390,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、ホントだ！\\n大福がいっぱい売ってる！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2456,
            "text_len": 28,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ああ、大福屋さんだからね。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2522,
            "text_len": 70,
            "text_offset": 2530,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "すごいすごい！　大福屋さんだって！\\nこれはなのに教えてあげなきゃ！！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2622,
            "text_len": 44,
            "text_offset": 2634,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "お友達かな？\\nどんどん教えてあげておくれ。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2700,
            "text_len": 30,
            "text_offset": 2708,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん、帰ったら教えなくちゃ！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2760,
            "text_len": 28,
            "text_offset": 2772,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "お嬢ちゃんは大福好きかい？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2826,
            "text_len": 36,
            "text_offset": 2834,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。でもケ―キの方がすきだけど。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2902,
            "text_len": 6,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3048,
            "text_len": 20,
            "text_offset": 3060,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "大福をなめるな！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3100,
            "text_len": 6,
            "text_offset": 3108,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3134,
            "text_len": 30,
            "text_offset": 3142,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "阪本！　大福仞めちゃダメ！！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3192,
            "text_len": 18,
            "text_offset": 3202,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "えええ―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3240,
            "text_len": 40,
            "text_offset": 3252,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "何！？\\n大福を仞めたのか、この猫は！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3312,
            "text_len": 18,
            "text_offset": 3322,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "えええ―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3420,
            "text_len": 30,
            "text_offset": 3424,
            "warnings": 1
        },
        "speaker": "",
        "text": "とばっちりでアンテナがっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3500,
            "text_len": 24,
            "text_offset": 3504,
            "warnings": 1
        },
        "speaker": "",
        "text": "勘弁してくれ――っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3628,
            "text_len": 44,
            "text_offset": 3640,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（くっ、やっぱり子供はケ―キが好きなのか）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3710,
            "text_len": 74,
            "text_offset": 3722,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（やはりシックで落ち着いた外見の魅力は\\n　大人にしかわからないのか……）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3826,
            "text_len": 26,
            "text_offset": 3838,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（いや、違う、違うぞ！）\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3890,
            "text_len": 60,
            "text_offset": 3902,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（これは子供を大福に目覚めさせる\\n　チャンスじゃないか！）\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3998,
            "text_len": 62,
            "text_offset": 4010,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（そうだ、ここで諦めていては\\n　大福ブ―ムなど夢のまた夢！）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4094,
            "text_len": 64,
            "text_offset": 4106,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "お嬢ちゃん、大福もおいしいんだよ。\\nほら、１つ食べてみなさい。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4194,
            "text_len": 38,
            "text_offset": 4202,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、くれるの！？　いただきま―す！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4262,
            "text_len": 16,
            "text_offset": 4270,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あむあむ……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4312,
            "text_len": 44,
            "text_offset": 4320,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "おいしい！！\\nすごい、大福おいしいです！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4396,
            "text_len": 24,
            "text_offset": 4408,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだろうそうだろう！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4454,
            "text_len": 66,
            "text_offset": 4466,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "さ、気に入ったならどんどん食べなさい！\\n今お茶も亞れてあげよう！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4584,
            "text_len": 34,
            "text_offset": 4596,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ほらほら！　どんどん食べなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4652,
            "text_len": 40,
            "text_offset": 4660,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わわっ、凄い凄い！\\n甘い！　おいしい！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4722,
            "text_len": 40,
            "text_offset": 4734,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "まだまだおかわりならたくさんあるぞ―！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4796,
            "text_len": 20,
            "text_offset": 4804,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "すごい！　すごい！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4846,
            "text_len": 70,
            "text_offset": 4858,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "はははっ、本当にたくさんあるからな！\\nほら、もっともっと食べなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5052,
            "text_len": 56,
            "text_offset": 5064,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "おや、手が止まってるじゃないか。\\nまだたくさんあるぞ？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5148,
            "text_len": 58,
            "text_offset": 5156,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "え、えっと……ちょっと\\nお腹がいっぱいになってきたかも。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5236,
            "text_len": 62,
            "text_offset": 5248,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ははは、甘いものは別腹と言うじゃないか。\\nまだ沢山あるぞ―！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5434,
            "text_len": 70,
            "text_offset": 5446,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ほら、また手が止まっているじゃないか！\\nさあもっと大福を食べるんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5550,
            "text_len": 44,
            "text_offset": 5558,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "も、もうムリ―！\\nうわ―ん、大福怖い―！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5634,
            "text_len": 6,
            "text_offset": 5646,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5702,
            "text_len": 18,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "た、助けて―！！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5832,
            "text_len": 30,
            "text_offset": 5844,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "わ、私は何か間違いを……！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5932,
            "text_len": 30,
            "text_offset": 5940,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。ありがとう、おじさん！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5992,
            "text_len": 58,
            "text_offset": 6000,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あむあむ……んくっ、んくっ……ぷは―！\\nお茶もおいしい！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6080,
            "text_len": 38,
            "text_offset": 6092,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだろう？\\nさ、もっと食べなさい。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6152,
            "text_len": 52,
            "text_offset": 6160,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "は―い！\\nそうだ、おじさんも一緒に食べればいいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6234,
            "text_len": 38,
            "text_offset": 6246,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうか、それじゃ私も食べるとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6412,
            "text_len": 28,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ふぅ、ごちそうさまでした！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6472,
            "text_len": 20,
            "text_offset": 6484,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "おいしかったかい？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6526,
            "text_len": 52,
            "text_offset": 6534,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん、すっごくおいしかった！\\nケ―キより好きかも！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6608,
            "text_len": 34,
            "text_offset": 6620,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうかそうか！\\nそれは良かった！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6676,
            "text_len": 50,
            "text_offset": 6684,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、そろそろ行かなきゃ！\\nごちそうさまでした！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6766,
            "text_len": 66,
            "text_offset": 6778,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……自信を失いかけていた所を\\nあの子に助けられたかもしれないな。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6866,
            "text_len": 32,
            "text_offset": 6878,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "よし、はりきって大福を売るぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6946,
            "text_len": 12,
            "text_offset": 6958,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7002,
            "text_len": 22,
            "text_offset": 7014,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "いっ、一個も無い！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7154,
            "text_len": 28,
            "text_offset": 7166,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（全部食べてしまった……）\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7260,
            "text_len": 12,
            "text_offset": 7268,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7302,
            "text_len": 24,
            "text_offset": 7314,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ん？　どうしたんだい？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7360,
            "text_len": 44,
            "text_offset": 7368,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "……しらない人からおかしをもらっちゃった。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7438,
            "text_len": 30,
            "text_offset": 7446,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "どうしよう、誘拐されるかも！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7520,
            "text_len": 64,
            "text_offset": 7532,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "え、何を言ってるんだお嬢ちゃん、\\nそんな、誘拐なんてしないよ。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7626,
