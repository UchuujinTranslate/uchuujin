[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2096,
            "text_len": 16,
            "text_offset": 2102,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "一番乗りかな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2140,
            "text_len": 24,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……今日はどれ読もう。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2290,
            "text_len": 12,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2336,
            "text_len": 34,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ……。\\n昨日部長が来た時間だ。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2398,
            "text_len": 22,
            "text_offset": 2404,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "今日は来ないのかな？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2448,
            "text_len": 38,
            "text_offset": 2454,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……うん。\\nやっぱり、私は部長が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2528,
            "text_len": 6,
            "text_offset": 2534,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2570,
            "text_len": 52,
            "text_offset": 2576,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ！　関口おはよ―！！\\n相変わらず毎日早いんだな！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2650,
            "text_len": 22,
            "text_offset": 2656,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "おはようございます。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2700,
            "text_len": 60,
            "text_offset": 2706,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの、昨日もですけど、\\nこんなに早く来てどうしたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2788,
            "text_len": 48,
            "text_offset": 2794,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そうそう、それなんだよ！\\n聞いてくれよ、関口！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2864,
            "text_len": 8,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2900,
            "text_len": 78,
            "text_offset": 2906,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "昨日、帰りに寄り道したら、なんと、\\n囲碁サッカ―の最新ＣＵＣが出てたんだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3006,
            "text_len": 16,
            "text_offset": 3012,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ＣＵＣが……？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3050,
            "text_len": 88,
            "text_offset": 3056,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、それで店ごと買ったから一緒に見ようぜ！\\nえ―っと、プレイヤ―どこ片付けたっけな。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3180,
            "text_len": 12,
            "text_offset": 3186,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3228,
            "text_len": 42,
            "text_offset": 3234,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（部長、囲碁サッカ―にハマってきてる？）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3302,
            "text_len": 76,
            "text_offset": 3308,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（何かに熱中するのはいいことだし、\\n　囲碁サッカ―部だから当たり前だけど）\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3410,
            "text_len": 58,
            "text_offset": 3416,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（でも、何かちょっと、\\n　囲碁サッカ―っていうのが……）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3498,
            "text_len": 60,
            "text_offset": 3504,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お、あったあった！\\n関口―、ケ―スからＣＵＣ出してくれ―。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3586,
            "text_len": 16,
            "text_offset": 3592,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "わかりました。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3634,
            "text_len": 66,
            "text_offset": 3640,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（何とか違うものに興味をもってもらえるよう\\n　頑張らないと……）\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3810,
            "text_len": 12,
            "text_offset": 3816,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……はぁ。\\n",
        "type": "dialog"
    }
]