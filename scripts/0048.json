[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 14,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん――……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2188,
            "text_len": 14,
            "text_offset": 2196,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん――……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2236,
            "text_len": 12,
            "text_offset": 2246,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "おいガキ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2280,
            "text_len": 26,
            "text_offset": 2288,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん―？　どうしたの阪本？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2336,
            "text_len": 56,
            "text_offset": 2346,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "それはこっちのセリフだ。\\nさっきから何を个ってやがる。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2424,
            "text_len": 30,
            "text_offset": 2432,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "それはね―、なののことだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2484,
            "text_len": 8,
            "text_offset": 2494,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "娘の？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2524,
            "text_len": 56,
            "text_offset": 2532,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そう。今度はなのに\\nどんな改造してあげようかな―って。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2614,
            "text_len": 12,
            "text_offset": 2624,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2700,
            "text_len": 12,
            "text_offset": 2708,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2754,
            "text_len": 20,
            "text_offset": 2762,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ！？　犬だ！？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2810,
            "text_len": 6,
            "text_offset": 2820,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2854,
            "text_len": 32,
            "text_offset": 2862,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あわわわわ、かまれちゃう……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2920,
            "text_len": 12,
            "text_offset": 2928,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2964,
            "text_len": 34,
            "text_offset": 2974,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "なにか頭に見えるな……アンテナ？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3036,
            "text_len": 20,
            "text_offset": 3044,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "アンテナ―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3150,
            "text_len": 50,
            "text_offset": 3154,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわああああ！？\\nアンテナ引っこ抜かれた――！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3246,
            "text_len": 50,
            "text_offset": 3250,
            "warnings": 1
        },
        "speaker": "",
        "text": "怖がっていたのに、\\nなんでそんなことをするんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3316,
            "text_len": 24,
            "text_offset": 3320,
            "warnings": 1
        },
        "speaker": "",
        "text": "……怖すぎたんじゃね？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3436,
            "text_len": 46,
            "text_offset": 3446,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "改造って、\\nまた娘に変な機能つけるつもりか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3516,
            "text_len": 40,
            "text_offset": 3524,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "変じゃないよ？\\nカッコイイし可愛いよ？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3586,
            "text_len": 60,
            "text_offset": 3596,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "お前は娘をカッコよくしたいのか\\n可愛くしたいのかどっちだ。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3682,
            "text_len": 12,
            "text_offset": 3690,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "どっちも！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3724,
            "text_len": 42,
            "text_offset": 3734,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……だったらまず\\nあのダサいネジを取れ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3804,
            "text_len": 6,
            "text_offset": 3812,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3888,
            "text_len": 18,
            "text_offset": 3896,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ダサくないもん！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3946,
            "text_len": 10,
            "text_offset": 3956,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "ダサい。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3988,
            "text_len": 32,
            "text_offset": 3996,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ダサくないもん！\\n可愛いもん！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4050,
            "text_len": 24,
            "text_offset": 4060,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "ダサい。\\n可愛くない。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4106,
            "text_len": 32,
            "text_offset": 4114,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "それは阪本がダサいからだもん！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4172,
            "text_len": 18,
            "text_offset": 4182,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "えええ―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4232,
            "text_len": 56,
            "text_offset": 4240,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そうだよ、ダサい阪本には\\nネジの良さはわからないもん！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4318,
            "text_len": 62,
            "text_offset": 4328,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "なら良さのわかるやつは\\nお前以外にどこにいる？　いないだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4412,
            "text_len": 12,
            "text_offset": 4420,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "いるもん！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4454,
            "text_len": 10,
            "text_offset": 4464,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "いない。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4500,
            "text_len": 12,
            "text_offset": 4508,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "いるもん！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4542,
            "text_len": 10,
            "text_offset": 4552,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "いない。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4584,
            "text_len": 18,
            "text_offset": 4592,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うう〜〜〜……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4632,
            "text_len": 58,
            "text_offset": 4640,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあ捜してくるもん！\\nみんなネジは可愛いって言うもん！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4740,
            "text_len": 36,
            "text_offset": 4750,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "フン。いるわけないだろそんなやつ。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4812,
            "text_len": 14,
            "text_offset": 4822,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……………。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4940,
            "text_len": 44,
            "text_offset": 4950,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……あのガキ、一人で外に出て大丈夫だよな？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5046,
            "text_len": 18,
            "text_offset": 5054,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ダサくないもん！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5104,
            "text_len": 32,
            "text_offset": 5112,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "弌弌あ、レトロだからダサいの？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5176,
            "text_len": 58,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "そういうダサさじゃなくてだな。\\nネジ自体がダメだと……。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5266,
            "text_len": 28,
            "text_offset": 5274,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあどんなネジがいいの？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5324,
            "text_len": 38,
            "text_offset": 5334,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "まずはネジをつけることをやめないか？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5394,
            "text_len": 24,
            "text_offset": 5402,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあ羽とかがいいの？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5448,
            "text_len": 60,
            "text_offset": 5458,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "そうじゃなくてだな、\\n背中に何かをつけることをやめるんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5540,
            "text_len": 34,
            "text_offset": 5548,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあどこに何をつければいいの？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5604,
            "text_len": 58,
            "text_offset": 5614,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "だからな、余計なものを\\nつける必要は無いと言ってるんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5694,
            "text_len": 38,
            "text_offset": 5702,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ダメ！\\nそれじゃ何も可愛くないもん！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5762,
            "text_len": 60,
            "text_offset": 5772,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "だからってネジが\\n可愛いなんて言ってるのはガキくらいだぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5864,
            "text_len": 46,
            "text_offset": 5872,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そんなことないよ？\\nみんな可愛いって言うよ？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5940,
            "text_len": 62,
            "text_offset": 5950,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "じゃあ誰かに聞いてみろ。\\nそしたら自分の間違いに気付くだろ。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6034,
            "text_len": 58,
            "text_offset": 6042,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わかった！　じゃあ可愛いって言ったら\\nネジつけるからね！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6122,
            "text_len": 14,
            "text_offset": 6132,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "好きにしろ。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6192,
            "text_len": 12,
            "text_offset": 6202,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6318,
            "text_len": 36,
            "text_offset": 6328,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……どっちになっても俺に害はない。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6416,
            "text_len": 40,
            "text_offset": 6424,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ぷぷぷ。\\n可愛くない、だって。ぷぷぷ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6486,
            "text_len": 26,
            "text_offset": 6496,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "なんだ？　何がおかしい？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6544,
            "text_len": 42,
            "text_offset": 6552,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だって猫にそんなことわかるわけないもん。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6626,
            "text_len": 48,
            "text_offset": 6636,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "猫でもわかることを\\n言ってるつもりなんだがな。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6706,
            "text_len": 66,
            "text_offset": 6714,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあはかせが他の人にもネジが\\n可愛いかどうか聞いてきてあげる。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6802,
            "text_len": 58,
            "text_offset": 6810,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "それならいいでしょ？\\nきっとはかせが正しいってわかるよ。\\n",
        "type": "dialog"
    },
    {
