[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2138,
            "text_len": 10,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "う―む。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2186,
            "text_len": 68,
            "text_offset": 2198,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（最近、客の数が落ち込んでいる。\\n　大福フェアの効果も減り気味だ）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2292,
            "text_len": 62,
            "text_offset": 2304,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（不景気のせいかと思ったが、\\n　どうやらそうでもなさそうだ）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2388,
            "text_len": 26,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……くっ、大工カフェめ。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2448,
            "text_len": 68,
            "text_offset": 2460,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "あそこは相変わらず大盛況。\\nむしろ、うちの客を取られてる気がする。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2550,
            "text_len": 60,
            "text_offset": 2562,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "しかし、大福がコ―ヒ―とケ―キなんぞに\\n負けるはずが無い。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2654,
            "text_len": 44,
            "text_offset": 2666,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "となれば、私の宣伝に問題があるに違いない。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2742,
            "text_len": 76,
            "text_offset": 2754,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……よし、少しアンケ―トをとってみよう。\\n丁度いいから、そこの人にするか。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2854,
            "text_len": 38,
            "text_offset": 2866,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "すいません！　ちょっといいですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2936,
            "text_len": 8,
            "text_offset": 2946,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "はい？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2986,
            "text_len": 78,
            "text_offset": 2998,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "アンケ―トに答えて欲しいんですが……\\n大福はケ―キに負けていると思いますか？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3098,
            "text_len": 60,
            "text_offset": 3108,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "大福ですか？　そうですねぇ。\\n私は大福の方が好きですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3204,
            "text_len": 6,
            "text_offset": 3216,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3244,
            "text_len": 78,
            "text_offset": 3254,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "ですが、昨日大工カフェで食べたケ―キも\\n大福と同じくらい美味しかったですな！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3440,
            "text_len": 22,
            "text_offset": 3452,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "大福を仞めるな！！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3534,
            "text_len": 6,
            "text_offset": 3544,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3630,
            "text_len": 16,
            "text_offset": 3634,
            "warnings": 1
        },
        "speaker": "",
        "text": "えええ――！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3682,
            "text_len": 46,
            "text_offset": 3686,
            "warnings": 1
        },
        "speaker": "",
        "text": "オ、オッサンは大丈夫か！？\\n急いで確認を弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3778,
            "text_len": 16,
            "text_offset": 3786,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "えええ――！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3894,
            "text_len": 12,
            "text_offset": 3902,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "私ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3946,
            "text_len": 58,
            "text_offset": 3958,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "はい。私、実はここで\\n大福フェアをやってる者なんですが。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4038,
            "text_len": 70,
            "text_offset": 4050,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "率直に聞きますが、うちの大福フェアは\\n大工カフェと比べてどうですか？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4144,
            "text_len": 14,
            "text_offset": 4152,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "う―ん……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4192,
            "text_len": 12,
            "text_offset": 4204,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4242,
            "text_len": 8,
            "text_offset": 4250,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "地味？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4300,
            "text_len": 12,
            "text_offset": 4312,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4364,
            "text_len": 74,
            "text_offset": 4376,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（そんな、まさか大福の最大の武器が\\n　最大の欠点になっているとは……！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4506,
            "text_len": 22,
            "text_offset": 4518,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（だが……しかし！）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4572,
            "text_len": 24,
            "text_offset": 4584,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "じ、地味の何が悪い！！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4634,
            "text_len": 6,
            "text_offset": 4642,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4680,
            "text_len": 70,
            "text_offset": 4692,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだ、地味なんじゃない。\\nシックで落ち着いた、日本的なデザインだ！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4786,
            "text_len": 58,
            "text_offset": 4798,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "それに味は抜群なんだ！\\nほら、あんたも食べてみてくれ！！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4878,
            "text_len": 20,
            "text_offset": 4886,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "い、いただきます。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4938,
            "text_len": 30,
            "text_offset": 4946,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "……あ、ホントだ、おいしい。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5014,
            "text_len": 16,
            "text_offset": 5026,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだろう！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5068,
            "text_len": 64,
            "text_offset": 5080,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうさ、何を間違えていたんだろう。\\n大福はこのままでいいんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5248,
            "text_len": 48,
            "text_offset": 5260,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "私はこれからも\\nこのままのスタイルを貫くぞ―！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5328,
            "text_len": 50,
            "text_offset": 5336,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "でも……それって\\n問題の解決になってませんよね？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5412,
            "text_len": 6,
            "text_offset": 5424,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5488,
            "text_len": 72,
            "text_offset": 5500,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "くっ……やはり大福は大福。\\n地味という宿命からは逃れられないのか……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5594,
            "text_len": 30,
            "text_offset": 5602,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "ん―、でも私大福好きですよ？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5668,
            "text_len": 14,
            "text_offset": 5680,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "本当かね！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5716,
            "text_len": 70,
            "text_offset": 5724,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "やっぱり緑茶には和菓子の方があいますし、\\nあんこっておいしいですし。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5824,
            "text_len": 48,
            "text_offset": 5836,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そ、そうか！\\n大福の良さをわかってくれるか！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5922,
            "text_len": 62,
            "text_offset": 5934,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "なんて良いお嬢さんだ！\\nありがとう、何だか救われた気分だよ！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6018,
            "text_len": 12,
            "text_offset": 6026,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "いえいえ。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6064,
            "text_len": 72,
            "text_offset": 6076,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだ、良かったら寄ってってくれ！\\n出来る限りサ―ビスさせてもらうよ！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6174,
            "text_len": 60,
            "text_offset": 6182,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "あ、いいです。\\n今から友達と大工カフェでお茶する所なんで。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6274,
            "text_len": 6,
            "text_offset": 6286,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6328,
            "text_len": 30,
            "text_offset": 6336,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "じゃあ、そろそろ時間なんで。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6412,
            "text_len": 12,
            "text_offset": 6424,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6544,
            "text_len": 38,
            "text_offset": 6556,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "い、良いお嬢さん、だった……はずだ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6656,
            "text_len": 12,
            "text_offset": 6668,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6702,
            "text_len": 36,
            "text_offset": 6714,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そ、その。\\nじゃあ次の質問を……。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6778,
            "text_len": 8,
            "text_offset": 6786,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "はい？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6816,
            "text_len": 58,
            "text_offset": 6828,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "反対に大福のいい所を\\n思いつくだけ挙げてみてくれないか？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6908,
            "text_len": 12,
            "text_offset": 6916,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "ん―……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6950,
            "text_len": 16,
            "text_offset": 6958,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "特に無いかな！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7012,
            "text_len": 6,
            "text_offset": 7024,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7062,
            "text_len": 44,
            "text_offset": 7074,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そ、そうか……。\\nありがとう、もういいよ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7140,
            "text_len": 22,
            "text_offset": 7148,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "は―い、それじゃ―。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7216,
            "text_len": 12,
            "text_offset": 7228,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7348,
            "text_len": 30,
            "text_offset": 7360,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……大福は無力なんだろうか。\\n",
        "type": "dialog"
    }
]