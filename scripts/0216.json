[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2106,
            "text_len": 72,
            "text_offset": 2114,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ちょっ！？　どうしたんだよ！？\\nバディ！　コバディ！　どこ行くんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2212,
            "text_len": 24,
            "text_offset": 2220,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "ハッ！　ハッ！　ハッ！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2266,
            "text_len": 20,
            "text_offset": 2276,
            "warnings": 0
        },
        "speaker": "コバディ",
        "text": "ハッ、ハッ、ハッ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2326,
            "text_len": 42,
            "text_offset": 2334,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "バディ―！　コバディ―！\\n帰ってこ―い！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2402,
            "text_len": 12,
            "text_offset": 2410,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2444,
            "text_len": 40,
            "text_offset": 2452,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "……行っちゃった。\\nこれで何度目だろ？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2514,
            "text_len": 40,
            "text_offset": 2522,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "って、ほんと……何しに行ってるんだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2686,
            "text_len": 46,
            "text_offset": 2690,
            "warnings": 1
        },
        "speaker": "",
        "text": "……どうする？\\n番組以外に、何か手があるか？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2764,
            "text_len": 12,
            "text_offset": 2768,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2806,
            "text_len": 44,
            "text_offset": 2810,
            "warnings": 1
        },
        "speaker": "",
        "text": "おい、答えろよ！？　何とかならないのか！？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2876,
            "text_len": 50,
            "text_offset": 2880,
            "warnings": 1
        },
        "speaker": "",
        "text": "知るかよ！\\nこんなときアイツがいてくれたら……！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 16,
            "speaker_offset": 2960,
            "text_len": 36,
            "text_offset": 2978,
            "warnings": 0
        },
        "speaker": "？？？（８７番）",
        "text": "……それはもしかして、俺のことか？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3044,
            "text_len": 62,
            "text_offset": 3048,
            "warnings": 1
        },
        "speaker": "",
        "text": "おまえは、策士の８７番！？\\nスタジオ船にいたんじゃないのか？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3132,
            "text_len": 44,
            "text_offset": 3140,
            "warnings": 0
        },
        "speaker": "８７番",
        "text": "仲間のピンチだぜ？\\n策を講じるなら今だろ？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3206,
            "text_len": 44,
            "text_offset": 3210,
            "warnings": 1
        },
        "speaker": "",
        "text": "それでどうなんだ？\\n姫を喜ばす方法は……。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3276,
            "text_len": 72,
            "text_offset": 3284,
            "warnings": 0
        },
        "speaker": "８７番",
        "text": "ジョ―クもだめ、クイズもだめ、一発芸もだめ、\\nそして、番組も……だめ。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3384,
            "text_len": 30,
            "text_offset": 3392,
            "warnings": 0
        },
        "speaker": "８７番",
        "text": "なら残された手段はただ１つ！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3456,
            "text_len": 20,
            "text_offset": 3464,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "そ、それは……！？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3518,
            "text_len": 22,
            "text_offset": 3526,
            "warnings": 0
        },
        "speaker": "８７番",
        "text": "アルパカの真似だろ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3660,
            "text_len": 12,
            "text_offset": 3672,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "似とらん。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3966,
            "text_len": 28,
            "text_offset": 3970,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ……８７番までが……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4026,
            "text_len": 50,
            "text_offset": 4030,
            "warnings": 1
        },
        "speaker": "",
        "text": "……俺だ……\\nあいつが死んだのは……俺のせいだ！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4106,
            "text_len": 64,
            "text_offset": 4110,
            "warnings": 1
        },
        "speaker": "",
        "text": "俺が呼ばなければ……！\\n俺が頼んだりしなければ……ヤツは……！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4196,
            "text_len": 30,
            "text_offset": 4200,
            "warnings": 1
        },
        "speaker": "",
        "text": "……ん？　お、おい、みんな！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4260,
            "text_len": 28,
            "text_offset": 4264,
            "warnings": 1
        },
        "speaker": "",
        "text": "犬だ！　それも２匹いるぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4400,
            "text_len": 44,
            "text_offset": 4404,
            "warnings": 1
        },
        "speaker": "",
        "text": "励ましてくれるのか？\\n……けど、俺は……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4464,
            "text_len": 66,
            "text_offset": 4468,
            "warnings": 1
        },
        "speaker": "",
        "text": "なら、何とかしろ！　死んだみんなのために、\\n姫を喜ばせてみろ！！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4648,
            "text_len": 74,
            "text_offset": 4652,
            "warnings": 1
        },
        "speaker": "",
        "text": "……わかったよ。俺、やってみる。\\n死んだアイツラの分までがんばってみる！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4748,
            "text_len": 36,
            "text_offset": 4752,
            "warnings": 1
        },
        "speaker": "",
        "text": "その意気だ！　……で、何するんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4814,
            "text_len": 24,
            "text_offset": 4818,
            "warnings": 1
        },
        "speaker": "",
        "text": "アルパカの真似、かな。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5028,
            "text_len": 12,
            "text_offset": 5036,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5074,
            "text_len": 12,
            "text_offset": 5084,
            "warnings": 0
        },
        "speaker": "コバディ",
        "text": "…………。\\n",
        "type": "dialog"
    }
