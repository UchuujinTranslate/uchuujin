[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 80,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あのモヒカンめ……どこに行きおった？\\nこの中にいるのは間違いないんじゃが……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2248,
            "text_len": 66,
            "text_offset": 2256,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……ん？　あれはこの学校の先生……？\\nちょっと聞いてみようかの。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2454,
            "text_len": 28,
            "text_offset": 2462,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あのぉ……もし、そこの人。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2520,
            "text_len": 30,
            "text_offset": 2530,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "はい！？　だだ、誰ですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2582,
            "text_len": 52,
            "text_offset": 2590,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "いや……わたしは、\\n通りすがりのイタコですが……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2682,
            "text_len": 64,
            "text_offset": 2690,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（この反応……変じゃな。\\n　何か見られたくないものでも……？）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2776,
            "text_len": 42,
            "text_offset": 2784,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "失礼ですが、あなたはここの先生ですかな？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2848,
            "text_len": 58,
            "text_offset": 2858,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "は、はい、私はこの学校の教頭ですが、\\nな、何か問題でも？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2938,
            "text_len": 58,
            "text_offset": 2948,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "な、何もないのなら……\\nは、早くどこかに行ってください！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3032,
            "text_len": 68,
            "text_offset": 3040,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（ふむ……何やら怯えとる。\\n　なら、モヒカンから徴収する前に弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3130,
            "text_len": 40,
            "text_offset": 3138,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "教頭先生……ここだけの話なんですがね。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3200,
            "text_len": 16,
            "text_offset": 3210,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "は、はい……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3248,
            "text_len": 32,
            "text_offset": 3256,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "この学校、呪われてますぞ……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3324,
            "text_len": 72,
            "text_offset": 3334,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "呪！？　なな、何を突然……\\n私の学校に不吉なことを言わないでください！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3450,
            "text_len": 72,
            "text_offset": 3460,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "そうです！　私の学校のイメ―ジを\\n下げるようなことは言わないで欲しい！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3572,
            "text_len": 22,
            "text_offset": 3582,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "いえ！　私の学校だ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3626,
            "text_len": 46,
            "text_offset": 3636,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\nで、では、私たちの学校と言う事で……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3708,
            "text_len": 58,
            "text_offset": 3718,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ここは私の学校だ！\\nあなたは別の学校の校長になりなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3802,
            "text_len": 6,
            "text_offset": 3812,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3844,
            "text_len": 64,
            "text_offset": 3854,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "もう回りくどいことは止めだ！\\n力ずくで校長の席を譲ってもらう！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3944,
            "text_len": 42,
            "text_offset": 3954,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "きょ、教頭先生！？　ま、待ってくださ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4030,
            "text_len": 22,
            "text_offset": 4040,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "ぎゃあああああ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4142,
            "text_len": 12,
            "text_offset": 4146,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ―っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4190,
            "text_len": 62,
            "text_offset": 4194,
            "warnings": 1
        },
        "speaker": "",
        "text": "やばい！　あのメガネ本気だ！\\nこれじゃオッサンの命が危ない！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4286,
            "text_len": 32,
            "text_offset": 4290,
            "warnings": 1
        },
        "speaker": "",
        "text": "待て、危ないのは俺たちもだっ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4352,
            "text_len": 10,
            "text_offset": 4360,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4416,
            "text_len": 26,
            "text_offset": 4428,
            "warnings": 3
        },
        "speaker": "＆＆",
        "text": "うあああああああああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4546,
            "text_len": 74,
            "text_offset": 4554,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "嘘じゃありません。ここに棲みついた悪霊が\\n学校に呪いをかけているんです！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4650,
            "text_len": 50,
            "text_offset": 4660,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "い、一体何を言っているんだ？\\nバカバカしい……。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4732,
            "text_len": 50,
            "text_offset": 4740,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "信じてください。\\nそして……私に任せてください。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4812,
            "text_len": 30,
            "text_offset": 4822,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "任せる？　どういうことです？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4874,
            "text_len": 42,
            "text_offset": 4882,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "除霊です。\\n私がここから悪霊を……んん？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4946,
            "text_len": 22,
            "text_offset": 4956,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ど、どうかしました？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5000,
            "text_len": 16,
            "text_offset": 5008,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "い、いえ……。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5050,
            "text_len": 66,
            "text_offset": 5058,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（……って、この人が手に持ってるのは、\\n　まさか……まさか……）\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5168,
            "text_len": 16,
            "text_offset": 5176,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（藁人形！？）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5228,
            "text_len": 60,
            "text_offset": 5236,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（まさか……ヤバい人？\\n　それとも本当に悪霊がいて……？）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5320,
            "text_len": 64,
            "text_offset": 5330,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "どうしました？　顔色が悪いようですが……\\n体調が悪いんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5416,
            "text_len": 36,
            "text_offset": 5426,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "それとも……これが気になりますか？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5498,
            "text_len": 62,
            "text_offset": 5506,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（よ、予感的中……！\\n　エラいのに声をかけてしもうた……！）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5590,
            "text_len": 54,
            "text_offset": 5600,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "おやぁ、汗をかかれてますねぇ。\\n保健室に行きますか？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5716,
            "text_len": 62,
            "text_offset": 5724,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（保健室と言って、\\n　本当はどこに連れて行くつもりだ……？）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5812,
            "text_len": 68,
            "text_offset": 5820,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（油断すると命を持っていかれる……\\n　料金どころの話じゃない……）\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5914,
            "text_len": 62,
            "text_offset": 5922,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（だが、やられる訳には……\\n　やられる訳にはいかんのじゃ！）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6014,
            "text_len": 16,
            "text_offset": 6022,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "かぁぁぁあ！！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6074,
            "text_len": 6,
            "text_offset": 6084,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6112,
            "text_len": 52,
            "text_offset": 6120,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "この悪霊め！　来るなら来い！\\n成敗してくれるわ！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6204,
            "text_len": 42,
            "text_offset": 6214,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "失敬な！　わ、私は悪霊じゃない、校長だ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6278,
            "text_len": 48,
            "text_offset": 6286,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "佶されんぞ！\\nこれでもわたしはイタコの端く弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6366,
            "text_len": 36,
            "text_offset": 6374,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……えっ？　今さっき教頭って……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6446,
            "text_len": 34,
            "text_offset": 6456,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あっ！？　い、いやだから教頭だ！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6512,
            "text_len": 24,
            "text_offset": 6520,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "でも今、校長って……。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6566,
            "text_len": 64,
            "text_offset": 6576,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "こ、校長でもいいんだ！\\nそれで、お前が成敗したいのは校長か！？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6662,
            "text_len": 22,
            "text_offset": 6670,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "いや、別に校長は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6718,
            "text_len": 54,
            "text_offset": 6728,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "だからどれなんだ！？\\n校長か悪霊か藁人形か校長か！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6808,
            "text_len": 20,
            "text_offset": 6816,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6940,
            "text_len": 22,
            "text_offset": 6948,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "じゃあ、モヒカンで。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7038,
            "text_len": 42,
            "text_offset": 7046,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "いえ、結構ですから。\\nそれじゃこれで弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7110,
            "text_len": 70,
            "text_offset": 7120,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ちょっと待ってください。\\n具合の悪いお婆さんをほっとけないですから。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7212,
            "text_len": 38,
            "text_offset": 7220,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "結構です！　具合は悪くないですから！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7280,
            "text_len": 28,
            "text_offset": 7290,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "でしたら、なぜ顔色が……？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7340,
            "text_len": 66,
            "text_offset": 7348,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "そ、それは……んん！？　なんだこの匂い！？\\nまさか悪霊の弌弌！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7446,
            "text_len": 42,
            "text_offset": 7456,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "匂い？　……ああ、もしかしてこれですか？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7530,
            "text_len": 48,
            "text_offset": 7538,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ぐっ……やはり藁人形……\\n私を呪うつもりか！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7614,
            "text_len": 56,
            "text_offset": 7624,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "は？　何を言っているのです。\\nよく見なさい……ほら弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7712,
            "text_len": 10,
            "text_offset": 7720,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "納豆！？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7752,
            "text_len": 64,
            "text_offset": 7762,
            "warnings": 1
        },
        "speaker": "教頭先生",
        "text": "ええ。国産大豆１００、天然にがりのみ使用。\\n……食べますか？\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7854,
            "text_len": 14,
            "text_offset": 7862,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7898,
            "text_len": 30,
            "text_offset": 7906,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……タレとカラシは無いのか？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7968,
            "text_len": 10,
            "text_offset": 7978,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……あ。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8102,
            "text_len": 12,
            "text_offset": 8110,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "呪われろ！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8190,
            "text_len": 40,
            "text_offset": 8198,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "いや、結構ですから。では、これにて弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8264,
            "text_len": 58,
            "text_offset": 8274,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ちょ、ちょっと待ってください！\\nまだいいじゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8354,
            "text_len": 66,
            "text_offset": 8364,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……いろいろと話したいことがあるんですよ。\\nお付き合い願います。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8460,
            "text_len": 60,
            "text_offset": 8468,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "（ぐっ……やはり悪霊か。\\n　正に絶体絶命……どうする！？）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 8562,
            "text_len": 20,
            "text_offset": 8566,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8632,
            "text_len": 20,
            "text_offset": 8640,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ッ！？　なぜ鹿！？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8688,
            "text_len": 38,
            "text_offset": 8698,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "コ、コイツ……校長のみならず私まで！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 8776,
            "text_len": 18,
            "text_offset": 8780,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ！　ッ！　ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8820,
            "text_len": 64,
            "text_offset": 8828,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "これは天啓！　鹿、頼む！\\nコイツを……悪霊を何とかしてくれい！\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 9000,
            "text_len": 18,
            "text_offset": 9004,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッッッッッッッ！\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9038,
            "text_len": 46,
            "text_offset": 9046,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "おおおっ！　わかってくれたか！　恩に着るぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9250,
            "text_len": 12,
            "text_offset": 9258,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あうっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9298,
            "text_len": 18,
            "text_offset": 9306,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "な、なんで……？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9398,
            "text_len": 62,
            "text_offset": 9408,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "し、鹿が人間の言葉をわかるはずない……\\nそれに、この鹿は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9518,
            "text_len": 14,
            "text_offset": 9528,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ギャアッ！？\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9568,
            "text_len": 34,
            "text_offset": 9578,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……人イコ―ル……敵……だ……。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9772,
            "text_len": 70,
            "text_offset": 9780,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……あれ？　あんなところに鹿？\\nそれだけじゃないぞ、何か足もとに弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9958,
            "text_len": 54,
            "text_offset": 9966,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "弌弌教頭先生！？\\nあっちには、イタコのお婆さんも！？\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10036,
            "text_len": 42,
            "text_offset": 10044,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "こ、こういうとき……こういうときは弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10106,
            "text_len": 16,
            "text_offset": 10114,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10234,
            "text_len": 18,
            "text_offset": 10242,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……警察、かな。\\n",
        "type": "dialog"
    }
]