[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 62,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え―っと、ゆっこがバナジュ―で、\\n私は……なんにしようかな？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2234,
            "text_len": 36,
            "text_offset": 2240,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "う―ん……\\nオレンジって気分かな。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2310,
            "text_len": 20,
            "text_offset": 2316,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む？　貴様は……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2360,
            "text_len": 6,
            "text_offset": 2366,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2414,
            "text_len": 40,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（さ、さ、さ、笹原先輩―――――！！）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2482,
            "text_len": 78,
            "text_offset": 2488,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "近頃良く会うな。……おお、自販機と言えば、\\n先日は貴重な金子をすまなかった。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2588,
            "text_len": 20,
            "text_offset": 2594,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え、あ、ハイっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2636,
            "text_len": 62,
            "text_offset": 2642,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、やはり貴様もジュ―スを買うのか。\\nなにを所望するのだ？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2726,
            "text_len": 36,
            "text_offset": 2732,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え、えと……私は、オレンジを……。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2876,
            "text_len": 12,
            "text_offset": 2882,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2924,
            "text_len": 12,
            "text_offset": 2930,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3052,
            "text_len": 24,
            "text_offset": 3058,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あれ？　あれ？　あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3144,
            "text_len": 14,
            "text_offset": 3150,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3198,
            "text_len": 18,
            "text_offset": 3204,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（またか―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3258,
            "text_len": 56,
            "text_offset": 3264,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ……この自販機……\\n一時限りの戯れかと思ったが弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3342,
            "text_len": 34,
            "text_offset": 3348,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "どうして、稀代の傾奇者と見える。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3404,
            "text_len": 26,
            "text_offset": 3410,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ？　あっ、はいっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3468,
            "text_len": 64,
            "text_offset": 3474,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "止むを得ん、外の自販機に行こう。\\n……ちと遠いが、帯同するか？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3560,
            "text_len": 32,
            "text_offset": 3566,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ！？　は、はい！　喜んで！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3624,
            "text_len": 50,
            "text_offset": 3630,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（きゃ〜！\\n　笹原先輩と並んで歩けるなんて〜！）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3714,
            "text_len": 12,
            "text_offset": 3724,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3788,
            "text_len": 58,
            "text_offset": 3798,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……オレンジジュ―ス？\\nホットコ―ヒ―を選んだのに……。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3878,
            "text_len": 64,
            "text_offset": 3888,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "いや、これは冷たい物を飲むべきと言う\\n運命なのかもしれん……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3974,
            "text_len": 18,
            "text_offset": 3984,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "では、一気に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4028,
            "text_len": 30,
            "text_offset": 4038,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "あづ―――――――――！！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4176,
            "text_len": 48,
            "text_offset": 4186,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "（しまった！\\n　思わずカップを放り投げて弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4358,
            "text_len": 6,
            "text_offset": 4368,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4488,
            "text_len": 30,
            "text_offset": 4498,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "あづ―――――――――！！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4604,
            "text_len": 26,
            "text_offset": 4608,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ。\\nアンテナ、壊れた。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4676,
            "text_len": 12,
            "text_offset": 4680,
            "warnings": 1
        },
        "speaker": "",
        "text": "ホントだ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4788,
            "text_len": 42,
            "text_offset": 4794,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（けど……さすがにちょっとくやしいなぁ）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4862,
            "text_len": 66,
            "text_offset": 4868,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（私のお小遣いだって、\\n　そんなに多いわけじゃないし……よし！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4956,
            "text_len": 16,
            "text_offset": 4962,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "さ、笹原先輩！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5002,
            "text_len": 6,
            "text_offset": 5008,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5036,
            "text_len": 60,
            "text_offset": 5042,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "先輩って、詩、得意なんですよね。\\n一句詠んでくれませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5130,
            "text_len": 54,
            "text_offset": 5136,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ほう、貴様も風流を解すのか。\\nよかろう。う―む……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5226,
            "text_len": 14,
            "text_offset": 5232,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（今だ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5272,
            "text_len": 12,
            "text_offset": 5278,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "久方の弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5316,
            "text_len": 20,
            "text_offset": 5322,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ジョイヤ―ッ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5406,
            "text_len": 30,
            "text_offset": 5412,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "じょいや―響ける春の日に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5500,
            "text_len": 14,
            "text_offset": 5506,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ヘヤッ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5542,
            "text_len": 20,
            "text_offset": 5548,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "かかとめりこみ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5594,
            "text_len": 12,
            "text_offset": 5600,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "セアッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5666,
            "text_len": 20,
            "text_offset": 5672,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "じゅ―す賜わん〜。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5748,
            "text_len": 36,
            "text_offset": 5754,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ここまでやっても出ないか……！）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5818,
            "text_len": 50,
            "text_offset": 5824,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "どうであった？\\n我ながら良い出来だと思うのだが。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5896,
            "text_len": 54,
            "text_offset": 5902,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、あ〜、えっと……\\nもう一句お願いしていいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5984,
            "text_len": 52,
            "text_offset": 5990,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふふ、仭欲なやつめ。\\nしかし、よかろう。では……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6068,
            "text_len": 16,
            "text_offset": 6074,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ヘグラァッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6164,
            "text_len": 6,
            "text_offset": 6170,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6208,
            "text_len": 16,
            "text_offset": 6214,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………あっ。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6260,
            "text_len": 12,
            "text_offset": 6266,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6304,
            "text_len": 20,
            "text_offset": 6310,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え、あ、あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6352,
            "text_len": 34,
            "text_offset": 6358,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "自販機よ、金を返せと、殴り蹴る。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6414,
            "text_len": 50,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、傑作だ！！\\n貴様のおかげだ。感謝するぞ！！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6502,
            "text_len": 24,
            "text_offset": 6508,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "へっ！？　は、はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6558,
            "text_len": 52,
            "text_offset": 6564,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（せ、先輩が、\\n　私の手を握って〜〜〜〜〜〜！！）\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6642,
            "text_len": 42,
            "text_offset": 6648,
            "warnings": 1
        },
        "speaker": "みお",
        "text": "（わ、私、壊れちゃいそう〜〜〜〜〜〜）\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6794,
            "text_len": 6,
            "text_offset": 6800,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ぬ？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6854,
            "text_len": 20,
            "text_offset": 6860,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ど、どう……かな？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7012,
            "text_len": 66,
            "text_offset": 7018,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ、やった―！　先輩、出ました！\\nオレンジジュ―ス出ましたよ！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7110,
            "text_len": 20,
            "text_offset": 7116,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ほう、これは丐倖。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7248,
            "text_len": 24,
            "text_offset": 7254,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ん――、美味しいです。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7302,
            "text_len": 18,
            "text_offset": 7308,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "では、私も失敬。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7348,
            "text_len": 6,
            "text_offset": 7354,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7382,
