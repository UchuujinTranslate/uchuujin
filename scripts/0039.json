[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 40,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "いつになったら、この髪が元に戻るんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2214,
            "text_len": 78,
            "text_offset": 2222,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "今はまだ誰にもバレていないけど……\\nこのまま治らなければ、いつかきっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2322,
            "text_len": 76,
            "text_offset": 2330,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "こんな時こそ科学の出番だが……\\n勉強不足な僕には手も足も出ない症状だ……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2428,
            "text_len": 38,
            "text_offset": 2436,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……ん？　なんだここ？\\n東雲研究所？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2500,
            "text_len": 12,
            "text_offset": 2508,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2546,
            "text_len": 22,
            "text_offset": 2554,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ！　モヒカンだ！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2598,
            "text_len": 10,
            "text_offset": 2606,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2646,
            "text_len": 38,
            "text_offset": 2654,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははは、モヒカンだ！\\n初めて見た！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2718,
            "text_len": 52,
            "text_offset": 2726,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（なんだこの子……\\n　もしかしてこの研究所の子？）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2822,
            "text_len": 12,
            "text_offset": 2830,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2868,
            "text_len": 16,
            "text_offset": 2876,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わっ！　犬だ！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2920,
            "text_len": 26,
            "text_offset": 2928,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "助けて！　噛まれちゃう！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2978,
            "text_len": 60,
            "text_offset": 2986,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ？　ああ、大丈夫だよ。\\n犬はこうやって頭を撫でれば弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3124,
            "text_len": 34,
            "text_offset": 3128,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわあっ！　アンテナがピンチだ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3186,
            "text_len": 72,
            "text_offset": 3190,
            "warnings": 1
        },
        "speaker": "",
        "text": "耐えてくれ……耐えてくれ―っ！！\\nええい、もっと頑丈に作っておけよっ！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3284,
            "text_len": 80,
            "text_offset": 3288,
            "warnings": 1
        },
        "speaker": "",
        "text": "し、仕方ないだろ、予算が足りなかったんだ！\\n文句があるなら姫にでも言ってくれ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3394,
            "text_len": 36,
            "text_offset": 3398,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそ―っ！\\n姫のばか――――っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3476,
            "text_len": 46,
            "text_offset": 3480,
            "warnings": 1
        },
        "speaker": "",
        "text": "ええっ！？\\nまだ、壊れていなかったはずだぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3542,
            "text_len": 38,
            "text_offset": 3546,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あ、姫のとこに回線つながってた。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3604,
            "text_len": 24,
            "text_offset": 3612,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "姫のばか――――っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3728,
            "text_len": 38,
            "text_offset": 3736,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ねえお嬢ちゃん、ここ、研究所だよね？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3796,
            "text_len": 28,
            "text_offset": 3804,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。はかせの研究所だよ。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3854,
            "text_len": 28,
            "text_offset": 3862,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "その博士は今どこにいるの？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3912,
            "text_len": 22,
            "text_offset": 3920,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "はかせがはかせだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3968,
            "text_len": 30,
            "text_offset": 3976,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ？　お嬢ちゃんが博士！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4028,
            "text_len": 8,
            "text_offset": 4036,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4074,
            "text_len": 44,
            "text_offset": 4082,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（この歳で博士……？\\n　信じられない……）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4152,
            "text_len": 74,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（いや、だが、天才に年齢は関係ない。\\n　小さくても天才なんだ、この子は）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4256,
            "text_len": 66,
            "text_offset": 4264,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "じゃあ博士、お願いなんですけど……\\nこの髪の毛、見てくれますか？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4352,
            "text_len": 12,
            "text_offset": 4360,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "モヒカン！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4394,
            "text_len": 56,
            "text_offset": 4402,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "そう。だけどモヒカンの部分だけ\\n幽霊になってるんです。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4480,
            "text_len": 6,
            "text_offset": 4488,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4516,
            "text_len": 20,
            "text_offset": 4524,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "さわってもいい！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4566,
            "text_len": 70,
            "text_offset": 4574,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ？　あ、い、いいですけど、\\nでも、今は髪の毛が幽霊になってて弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4694,
            "text_len": 14,
            "text_offset": 4702,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あはははは！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4860,
            "text_len": 20,
            "text_offset": 4868,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "触れてる―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4924,
            "text_len": 54,
            "text_offset": 4932,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（ど、どういうことだ？\\n　僕は触れなかったのに……）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5008,
            "text_len": 28,
            "text_offset": 5016,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ちょ、ちょっとスイマセン。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5088,
            "text_len": 48,
            "text_offset": 5096,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……触れた。あっ……\\nでもまた触れなくなった。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5170,
            "text_len": 54,
            "text_offset": 5178,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（どういうことだ……？\\n　何か条件があるのか……？）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5254,
            "text_len": 72,
            "text_offset": 5262,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ハッ！？　そ、そうか！　科学の力で\\n幽霊になった髪に触ったんですね！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5356,
            "text_len": 12,
            "text_offset": 5364,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "なにそれ？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5402,
            "text_len": 18,
            "text_offset": 5410,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "違った―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5464,
            "text_len": 70,
            "text_offset": 5472,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（うう……触れたり触れなかったりなんて\\n　ハッキリしない幽霊だ……）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5564,
            "text_len": 74,
            "text_offset": 5572,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "くっ……だけど博士、博士ならきっと\\n科学の力でこの髪の毛を治せますよね？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5668,
            "text_len": 12,
            "text_offset": 5676,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "できるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5714,
            "text_len": 6,
            "text_offset": 5722,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5760,
            "text_len": 20,
            "text_offset": 5768,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ちょっと待ってて！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5810,
            "text_len": 16,
            "text_offset": 5818,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "は、はいっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5880,
            "text_len": 76,
            "text_offset": 5888,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（博士はすでに治し方がわかっているのか！？\\n　やっぱりこの子は天才だ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5990,
            "text_len": 30,
            "text_offset": 5998,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "はい！　この薬を頭につけて。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6050,
            "text_len": 8,
            "text_offset": 6058,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うん！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6180,
            "text_len": 56,
            "text_offset": 6188,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（これで治るんだ……\\n　科学が勝利する瞬間だ……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6302,
            "text_len": 6,
            "text_offset": 6310,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6342,
            "text_len": 24,
            "text_offset": 6350,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あはははははははは！！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6400,
            "text_len": 6,
            "text_offset": 6408,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6480,
            "text_len": 44,
            "text_offset": 6488,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははははは！\\nモヒカン３つ！　あははは！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6548,
            "text_len": 14,
            "text_offset": 6556,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6680,
            "text_len": 72,
            "text_offset": 6688,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ププ――！　カットパインが３つになった！\\nプププププ！　美味しそう！！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6776,
            "text_len": 14,
            "text_offset": 6784,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6814,
            "text_len": 10,
            "text_offset": 6822,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "は……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6854,
            "text_len": 12,
            "text_offset": 6862,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "……んん？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6982,
            "text_len": 52,
            "text_offset": 6990,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "生えた――――！！！\\n横からも髪が生えた――！！！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7064,
            "text_len": 6,
            "text_offset": 7072,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7094,
            "text_len": 50,
            "text_offset": 7102,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははは！\\n生えた生えた！　ニョキニョキ生えた！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7172,
            "text_len": 56,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うおおおおおお！\\n僕の毛根は死んでなかったんだ――！！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7344,
            "text_len": 18,
            "text_offset": 7352,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あはははははは！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7390,
            "text_len": 24,
            "text_offset": 7398,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うおおおおおおおお！！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7536,
            "text_len": 12,
            "text_offset": 7546,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7662,
            "text_len": 32,
            "text_offset": 7672,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……これ、止めた方がいいのか？\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7764,
            "text_len": 62,
            "text_offset": 7772,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（か、髪が、生えた……\\n　もう生えないと思われた場所に……）\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7854,
            "text_len": 80,
            "text_offset": 7862,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（僕の死んだ毛根にさえも効くなんて\\n　博士の発明した育毛剤はなんてすごいんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7978,
            "text_len": 34,
            "text_offset": 7986,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（だが……だが、しかし……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8132,
            "text_len": 62,
            "text_offset": 8140,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（新しい２つも触れない……！！\\n　傷口が拡がっただけだ……）\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8248,
            "text_len": 58,
            "text_offset": 8256,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははは！　変な髪型！\\nカットパインが３つも刺さってる！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8334,
            "text_len": 6,
            "text_offset": 8342,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8468,
            "text_len": 18,
            "text_offset": 8476,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あはははははは！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8516,
            "text_len": 14,
            "text_offset": 8524,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8558,
            "text_len": 78,
            "text_offset": 8566,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（なんてことだ……調査対象に目もくれず、\\n　モヒカンで遊ぶだなんて……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8664,
            "text_len": 84,
            "text_offset": 8672,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（だが、仕方ない。この子は確かに天才だけど\\n　まだまだ精神的に未熟。責められない）\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8776,
            "text_len": 70,
            "text_offset": 8784,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（責めるべくは自分の身に起こったことを\\n　自分で対処できない僕だ！）\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8874,
            "text_len": 72,
            "text_offset": 8882,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（こうなったら、自分一人で何とかしなくては！\\n　さようなら、はかせ！）\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9070,
            "text_len": 34,
            "text_offset": 9078,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははははは！　はあ、はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9134,
            "text_len": 22,
            "text_offset": 9142,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "はぁ……面白かった！\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9268,
            "text_len": 64,
            "text_offset": 9276,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "でもどっか行っちゃった。\\nせっかく治してあげようと思ったのに。\\n",
        "type": "dialog"
    }
]