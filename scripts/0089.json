[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 40,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "みんな席につけ―。\\n授業を始めるぞ―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2220,
            "text_len": 52,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（はぁ、高崎先生の授業か―。\\n　めんどくさいなぁ）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2302,
            "text_len": 64,
            "text_offset": 2312,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "まずは昨日出した宿題の答え合わせだな。\\nみんなやってきたか―？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2402,
            "text_len": 28,
            "text_offset": 2410,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あ―、宿題ね。宿題……）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2488,
            "text_len": 24,
            "text_offset": 2496,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（忘れてた―――！！）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2550,
            "text_len": 64,
            "text_offset": 2558,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ヤバイ！　また宿題忘れてた！\\n　い、今からでもやらなきゃ！）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2648,
            "text_len": 50,
            "text_offset": 2656,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（確か昨日の宿題は……\\n　あ、このプリントだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2732,
            "text_len": 12,
            "text_offset": 2740,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2778,
            "text_len": 50,
            "text_offset": 2786,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（わからない……\\n　１問たりともわからない……）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2872,
            "text_len": 60,
            "text_offset": 2880,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（短時間でこの難問を解くのは無理だ！\\n　ここはいつも通り）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2962,
            "text_len": 26,
            "text_offset": 2970,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "麻衣ちゃん、麻衣ちゃん。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3022,
            "text_len": 6,
            "text_offset": 3028,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3056,
            "text_len": 24,
            "text_offset": 3064,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "宿題見せて。お願いっ。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3114,
            "text_len": 12,
            "text_offset": 3120,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3172,
            "text_len": 54,
            "text_offset": 3182,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "相生、今日は宿題やってきたか？\\n早くプリントを出せ。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3262,
            "text_len": 46,
            "text_offset": 3270,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え、えっと……え？\\nプリントならここに……。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3338,
            "text_len": 38,
            "text_offset": 3346,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あれ？　ない？\\nさっきまでここに弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3418,
            "text_len": 12,
            "text_offset": 3426,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3460,
            "text_len": 38,
            "text_offset": 3468,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "弌弌あっ！　私のプリント！\\n返して！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3584,
            "text_len": 58,
            "text_offset": 3588,
            "warnings": 1
        },
        "speaker": "",
        "text": "おいおい、盗みは良くないぞ。\\nちゃんと返してやらないと。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3670,
            "text_len": 46,
            "text_offset": 3674,
            "warnings": 1
        },
        "speaker": "",
        "text": "何言ってるんだ。\\n俺はあの子を助けたんだぜ？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3742,
            "text_len": 66,
            "text_offset": 3746,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ、そうなのか？　どう見ても\\n困ってるようにしか見えないんだが。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3834,
            "text_len": 52,
            "text_offset": 3838,
            "warnings": 1
        },
        "speaker": "",
        "text": "……そっか。お前は頭良かったから\\nわからないのか。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3912,
            "text_len": 10,
            "text_offset": 3916,
            "warnings": 1
        },
        "speaker": "",
        "text": "んん？？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3948,
            "text_len": 72,
            "text_offset": 3952,
            "warnings": 1
        },
        "speaker": "",
        "text": "いや、なんでもない。ところで、\\n地球人はどんな勉強をしてるんだろうな。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4046,
            "text_len": 46,
            "text_offset": 4050,
            "warnings": 1
        },
        "speaker": "",
        "text": "奪ったプリントを見てみるか。\\nえ―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4138,
            "text_len": 12,
            "text_offset": 4146,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4192,
            "text_len": 26,
            "text_offset": 4196,
            "warnings": 1
        },
        "speaker": "",
        "text": "……知恵熱出てきた……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4244,
            "text_len": 12,
            "text_offset": 4248,
            "warnings": 1
        },
        "speaker": "",
        "text": "俺も……。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4302,
            "text_len": 28,
            "text_offset": 4306,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわっ！？　な、なんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4350,
            "text_len": 70,
            "text_offset": 4354,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ！　アンテナに高熱が加わってる！\\n一体どんな攻撃をされたんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4440,
            "text_len": 38,
            "text_offset": 4444,
            "warnings": 1
        },
        "speaker": "",
        "text": "……いや、きっとそれは攻撃じゃない。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4498,
            "text_len": 26,
            "text_offset": 4502,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ？　じゃあ、一体弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4544,
            "text_len": 12,
            "text_offset": 4548,
            "warnings": 1
        },
        "speaker": "",
        "text": "熱暴走だ。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4580,
            "text_len": 6,
            "text_offset": 4584,
            "warnings": 1
        },
        "speaker": "",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4686,
            "text_len": 12,
            "text_offset": 4692,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4730,
            "text_len": 26,
            "text_offset": 4738,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ん？　紙に何書いてるの？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4786,
            "text_len": 8,
            "text_offset": 4792,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "返事。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4826,
            "text_len": 12,
            "text_offset": 4834,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（返事？）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4876,
            "text_len": 12,
            "text_offset": 4882,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4932,
            "text_len": 28,
            "text_offset": 4940,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あっ！？　紙を投げた！）\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4998,
            "text_len": 6,
            "text_offset": 5006,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5032,
            "text_len": 24,
            "text_offset": 5040,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（中之条くんに返事？）\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5090,
            "text_len": 12,
            "text_offset": 5098,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5218,
            "text_len": 36,
            "text_offset": 5226,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あ、中之条くんも別の人に渡した）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5282,
            "text_len": 52,
            "text_offset": 5290,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（なになに？\\n　一体あの紙には何が書かれてるの？）\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5486,
            "text_len": 68,
            "text_offset": 5494,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あっ、まただ。みんな紙の中身を見たら\\n　すぐに別の人に渡してる）\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5582,
            "text_len": 80,
            "text_offset": 5590,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（一体麻衣ちゃんはどんなことを書いたの？\\n　見たい、あの紙の中身が見たい……）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5780,
            "text_len": 14,
            "text_offset": 5786,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ん？　手紙？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5826,
            "text_len": 40,
            "text_offset": 5834,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あっ、みおちゃんに手紙が渡ってきた）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5896,
            "text_len": 40,
            "text_offset": 5904,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ねえみおちゃん、私にもその手紙見せて？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5970,
            "text_len": 44,
            "text_offset": 5976,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "いいよ、というか、ゆっこ宛てだし。\\nはい。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6042,
            "text_len": 40,
            "text_offset": 6050,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、ありがとう。\\nえっと、なになに〜？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6132,
            "text_len": 12,
            "text_offset": 6140,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6198,
            "text_len": 16,
            "text_offset": 6206,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "回りくどいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6262,
            "text_len": 32,
            "text_offset": 6272,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うおっ！？　ど、どうした相生！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6330,
            "text_len": 70,
            "text_offset": 6338,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "回りくどいよ麻衣ちゃん！\\n私すぐ隣にいるじゃん！　喋れば１秒じゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6430,
            "text_len": 22,
            "text_offset": 6438,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや、と言うか……。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6486,
            "text_len": 56,
            "text_offset": 6494,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "さっきこの手紙見た人全員、\\nなんで以心伝心してるのさ！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6576,
            "text_len": 74,
            "text_offset": 6584,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "２文字！　断るの２文字しか書かれてなくて\\nどうして私まで辿り着いたの！？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6684,
            "text_len": 12,
            "text_offset": 6694,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6728,
            "text_len": 26,
            "text_offset": 6736,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや、それよりもさ……。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6788,
            "text_len": 50,
            "text_offset": 6796,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "麻衣ちゃ―ん、\\nなんで宿題見せてくれないの―！？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6872,
            "text_len": 60,
            "text_offset": 6880,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "もう麻衣ちゃんじゃなくていいから\\n誰か私に宿題見せてよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6962,
            "text_len": 28,
            "text_offset": 6972,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "宿題やってこなかったのか。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7022,
            "text_len": 6,
            "text_offset": 7030,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7140,
            "text_len": 22,
            "text_offset": 7150,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "廊下に立ってなさい。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7192,
            "text_len": 12,
            "text_offset": 7200,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7364,
            "text_len": 40,
            "text_offset": 7372,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ねえ麻衣ちゃん。あの紙に何を書いたの？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7434,
            "text_len": 18,
            "text_offset": 7440,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ゆっこへの返事。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7480,
            "text_len": 14,
            "text_offset": 7488,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "私への返事？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7528,
            "text_len": 74,
            "text_offset": 7536,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（でも麻衣ちゃん、他の人に回したし……\\n　う―ん、どういうことだろう？）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7640,
            "text_len": 20,
            "text_offset": 7646,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ゆっこ、手紙だよ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7688,
            "text_len": 8,
            "text_offset": 7696,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7730,
            "text_len": 58,
            "text_offset": 7738,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あっ、これはさっき麻衣ちゃんが\\n　何かを書いてた紙だ）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7822,
            "text_len": 32,
            "text_offset": 7830,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（何が書かれてるんだろう……）\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7888,
            "text_len": 12,
            "text_offset": 7896,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7944,
            "text_len": 12,
            "text_offset": 7952,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8000,
            "text_len": 18,
            "text_offset": 8008,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（回りくどっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8066,
            "text_len": 60,
            "text_offset": 8074,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（すぐ隣にいるのにどうしてこんなに\\n　回りくどい事を弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8160,
            "text_len": 34,
            "text_offset": 8168,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（弌弌回りくどい！？　そうか！）\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8228,
            "text_len": 62,
            "text_offset": 8236,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（回りくどい、つまり遠回し！\\n　私、遠回しに断られたんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8336,
            "text_len": 12,
            "text_offset": 8344,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8382,
            "text_len": 16,
            "text_offset": 8390,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8524,
            "text_len": 12,
            "text_offset": 8530,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8564,
            "text_len": 16,
            "text_offset": 8572,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8740,
            "text_len": 44,
            "text_offset": 8748,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "って、そんなのいいから、\\n宿題を見せてよ。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8814,
            "text_len": 8,
            "text_offset": 8820,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8850,
            "text_len": 48,
            "text_offset": 8858,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "おっ、ありがとう麻衣ちゃん。\\nじゃあ早速……。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8932,
            "text_len": 12,
            "text_offset": 8940,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8974,
            "text_len": 44,
            "text_offset": 8982,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "って、いやいや。\\nこのプリント別のやつ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9052,
            "text_len": 74,
            "text_offset": 9060,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ハッ！？　危ない、麻衣ちゃんのペ―スに\\n　巻き込まれるところだった！）\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9160,
            "text_len": 82,
            "text_offset": 9168,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（このパタ―ンは必死になればなるだけ\\n　私が痛い目を見るね。ここは大人しく……）\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9278,
            "text_len": 28,
            "text_offset": 9286,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "みおちゃん、ちょっといい？\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9338,
            "text_len": 14,
            "text_offset": 9344,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ん？　なに？\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9380,
            "text_len": 66,
            "text_offset": 9388,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いやぁ、ちょっとお願いがあって、\\n宿題を見せてほしいな―なんて。\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9476,
            "text_len": 30,
            "text_offset": 9482,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え―、また宿題やってないの？\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9546,
            "text_len": 48,
            "text_offset": 9556,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何！？\\n相生、また宿題をやってこなかったのか！\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9630,
            "text_len": 6,
            "text_offset": 9638,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9666,
            "text_len": 22,
            "text_offset": 9672,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、ご、ごめん……。\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9720,
            "text_len": 50,
            "text_offset": 9730,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "お前と言うやつはいつもいつも！\\n廊下に立ってろ！\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9802,
            "text_len": 14,
            "text_offset": 9810,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あう―……。\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9846,
            "text_len": 14,
            "text_offset": 9852,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……ゆっこ。\\n",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9892,
            "text_len": 22,
            "text_offset": 9900,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なに麻衣ちゃん……。\\n",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10026,
            "text_len": 42,
            "text_offset": 10032,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ゆっこのプリント、おとといの宿題のやつ。\\n",
        "type": "dialog"
    },
    {
        "id": 105,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10094,
            "text_len": 14,
            "text_offset": 10102,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！？\\n",
        "type": "dialog"
    }
]