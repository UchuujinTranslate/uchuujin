[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 60,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あと少しで体育祭だね―。\\nなんかテンション上がってくるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2234,
            "text_len": 52,
            "text_offset": 2240,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そうですね。\\n私は体育祭は初めてなので楽しみです。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2318,
            "text_len": 68,
            "text_offset": 2324,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "じゃあ少し練習した方がいいかもね。\\nほら、リレ―のバトン渡しとか。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2416,
            "text_len": 48,
            "text_offset": 2424,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "お、そうだね。\\nなのちゃんしたことないでしょ？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2494,
            "text_len": 8,
            "text_offset": 2500,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2530,
            "text_len": 52,
            "text_offset": 2538,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ少しだけ練習する？\\nバトンは携帯でいいよね。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2612,
            "text_len": 22,
            "text_offset": 2618,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい。お願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2762,
            "text_len": 18,
            "text_offset": 2768,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "じゃあ行くよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2808,
            "text_len": 22,
            "text_offset": 2814,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい、お願いします！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2864,
            "text_len": 16,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "よっ、と……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2932,
            "text_len": 12,
            "text_offset": 2938,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2972,
            "text_len": 74,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なのちゃん、みおちゃんが近くに来たら\\n走りながらバトンを受け取るんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3076,
            "text_len": 12,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "は、はい！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3122,
            "text_len": 28,
            "text_offset": 3128,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なのちゃん、バトン渡すよ。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3178,
            "text_len": 12,
            "text_offset": 3184,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はいっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3320,
            "text_len": 24,
            "text_offset": 3328,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "おっ、ナイスキャッチ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3374,
            "text_len": 50,
            "text_offset": 3380,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そのまま走って、\\n今度はゆっこにバトンを渡して！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3452,
            "text_len": 10,
            "text_offset": 3458,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3514,
            "text_len": 10,
            "text_offset": 3520,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3638,
            "text_len": 56,
            "text_offset": 3644,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いたたた……\\nどうしてこんなところにワンちゃんが……。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3722,
            "text_len": 48,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ！！　ご、ごめんなさい！\\nすぐに退くから！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3854,
            "text_len": 48,
            "text_offset": 3858,
            "warnings": 1
        },
        "speaker": "",
        "text": "ふぅ、間一髪だぜ。\\nあいつが助かってよかった。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3930,
            "text_len": 62,
            "text_offset": 3934,
            "warnings": 1
        },
        "speaker": "",
        "text": "とっさにバリアを展開したのか。\\nお前の犬好きには恐れ入るよ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4018,
            "text_len": 62,
            "text_offset": 4022,
            "warnings": 1
        },
        "speaker": "",
        "text": "へへっ。そういうお前も、\\n反重力場を発生させてたじゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4106,
            "text_len": 60,
            "text_offset": 4110,
            "warnings": 1
        },
        "speaker": "",
        "text": "ま、俺も犬は嫌いじゃないしな。\\n辛い目には遭わせたくない。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4192,
            "text_len": 48,
            "text_offset": 4196,
            "warnings": 1
        },
        "speaker": "",
        "text": "……その結果、\\n俺たちが辛い目に遭いそうだな。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4266,
            "text_len": 66,
            "text_offset": 4270,
            "warnings": 1
        },
        "speaker": "",
        "text": "所詮は試作機能だったからな。\\n負荷がかかりすぎちまったみたいだ。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4390,
            "text_len": 12,
            "text_offset": 4398,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4502,
            "text_len": 56,
            "text_offset": 4510,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "さあ急いでなのちゃん！\\nちんたらしてるとビリになるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4588,
            "text_len": 38,
            "text_offset": 4594,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！！\\n急いでバトンを渡します！！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 58,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これは練習だけど、練習だと思って\\n　手を抜いたら駄目）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4748,
            "text_len": 72,
            "text_offset": 4754,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これは本番だと思って全力で走る！\\n　全力で相生さんにバトンを渡す！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4856,
            "text_len": 18,
            "text_offset": 4862,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "うおおおおお！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4998,
            "text_len": 22,
            "text_offset": 5004,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "おおおおおおお！？？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5056,
            "text_len": 6,
            "text_offset": 5062,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5088,
            "text_len": 6,
            "text_offset": 5096,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5220,
            "text_len": 26,
            "text_offset": 5226,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（と、飛んでった―！？）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5284,
            "text_len": 32,
            "text_offset": 5290,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん！　避けてください！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5348,
            "text_len": 20,
            "text_offset": 5356,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はああああああ！！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5430,
            "text_len": 20,
            "text_offset": 5436,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "受け止めた――！？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5492,
            "text_len": 12,
            "text_offset": 5500,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5534,
            "text_len": 8,
            "text_offset": 5542,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5606,
            "text_len": 12,
            "text_offset": 5612,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5664,
            "text_len": 24,
            "text_offset": 5670,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あっ！？　手！！！）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5766,
            "text_len": 70,
            "text_offset": 5772,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（早くあの手をはめ直さないと\\n　片手が無い姿を多くの人に見られる！）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5864,
            "text_len": 32,
            "text_offset": 5870,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん！　止まってください！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5924,
            "text_len": 14,
            "text_offset": 5932,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5990,
            "text_len": 78,
            "text_offset": 5996,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ダメだ、止まらない……追いかけるにしても\\n　こんなに距離を離されたら……）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6106,
            "text_len": 54,
            "text_offset": 6112,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（いや！？　諦めるな！！\\n　まだ手は残っている！！）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6198,
            "text_len": 38,
            "text_offset": 6204,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（文字通り、片手だけは残っている！）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6288,
            "text_len": 24,
            "text_offset": 6294,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いっけぇえええええ！！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6426,
            "text_len": 6,
            "text_offset": 6434,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6460,
            "text_len": 20,
            "text_offset": 6468,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はああああああ！！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6632,
            "text_len": 24,
            "text_offset": 6638,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "また受け止められた！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6698,
            "text_len": 12,
            "text_offset": 6706,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6744,
            "text_len": 12,
            "text_offset": 6750,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6866,
            "text_len": 8,
            "text_offset": 6874,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6908,
            "text_len": 10,
            "text_offset": 6914,
            "warnings": 1
        },
        "speaker": "なの",
        "text": "！！！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6996,
            "text_len": 70,
            "text_offset": 7002,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（手を相生さんに持っていかれた！\\n　……いや、それよりも問題は……）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7102,
            "text_len": 12,
            "text_offset": 7108,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7146,
            "text_len": 50,
            "text_offset": 7152,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（手が飛ぶところを\\n　長野原さんに見られた……）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7244,
            "text_len": 80,
            "text_offset": 7250,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これで私がロボだということが広まったら\\n　もう学校に通うことはできない……）\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7356,
            "text_len": 12,
            "text_offset": 7362,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7396,
            "text_len": 18,
            "text_offset": 7402,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "長野原さん……。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7446,
            "text_len": 22,
            "text_offset": 7452,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ！？　な、なに？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7496,
            "text_len": 18,
            "text_offset": 7502,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……見ましたか？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7546,
            "text_len": 12,
            "text_offset": 7552,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7590,
            "text_len": 12,
            "text_offset": 7596,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7630,
            "text_len": 20,
            "text_offset": 7636,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……な、何のこと？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7678,
            "text_len": 20,
            "text_offset": 7684,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "それはもちろん弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7730,
            "text_len": 72,
            "text_offset": 7736,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ああ、なのちゃんのバトンを受け取る\\nタイミングは見たよ。１００点満点。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7836,
            "text_len": 14,
            "text_offset": 7842,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7886,
            "text_len": 46,
            "text_offset": 7892,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（良かった！\\n　手が飛ぶ所を見られてない！）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7960,
            "text_len": 60,
            "text_offset": 7966,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ、ありがとうございます！\\nじゃあもう一度練習しましょう！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8048,
            "text_len": 36,
            "text_offset": 8054,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、うん、ゆっこ呼んでこないとね。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8112,
            "text_len": 46,
            "text_offset": 8118,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "私行ってきます！\\nちょっと待っててください。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8186,
            "text_len": 20,
            "text_offset": 8192,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "う、うん、お願い。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8238,
            "text_len": 56,
            "text_offset": 8244,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（私はまた学校に通えるんだ……\\n　本当に嘘みたい……）\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8404,
            "text_len": 22,
            "text_offset": 8410,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……飛ぶんだ、あれ。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8458,
            "text_len": 6,
            "text_offset": 8464,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8524,
            "text_len": 34,
            "text_offset": 8530,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "な、なのちゃん……今、手が……。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8590,
            "text_len": 52,
            "text_offset": 8596,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "え？　手が、ど、どうしたんですか？\\n痛いんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8680,
            "text_len": 36,
            "text_offset": 8686,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ？\\nいや、なのちゃんの手が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8744,
            "text_len": 64,
            "text_offset": 8750,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん、どこまで行ったんでしょうね。\\n全然帰ってきませんよ？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8840,
            "text_len": 6,
            "text_offset": 8846,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8898,
            "text_len": 24,
            "text_offset": 8906,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はっ、はっ、はっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8956,
            "text_len": 58,
            "text_offset": 8962,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、お帰りなさい相生さん。\\nどこまで行ってたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9048,
            "text_len": 60,
            "text_offset": 9056,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、ちょっとそこまで……\\nところでなのちゃん……この手弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9138,
            "text_len": 28,
            "text_offset": 9144,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、ちょっとすみません。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9220,
            "text_len": 8,
            "text_offset": 9226,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！！！\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9304,
            "text_len": 6,
            "text_offset": 9318,
            "warnings": 0
        },
        "speaker": "ゆっこ＆みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9360,
            "text_len": 38,
            "text_offset": 9366,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "弌弌えっと、なんでしょうか相生さん。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9430,
            "text_len": 12,
            "text_offset": 9438,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9478,
            "text_len": 12,
            "text_offset": 9484,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9600,
            "text_len": 26,
            "text_offset": 9608,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "体育祭、頑張ろうね……。\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9650,
            "text_len": 10,
            "text_offset": 9656,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はいっ。\\n",
        "type": "dialog"
    }
]