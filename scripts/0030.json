[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2148,
            "text_len": 56,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お、どうしたんだ関口。\\nこんな朝から部室にいるなんて。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2232,
            "text_len": 32,
            "text_offset": 2238,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ここ、静かで読書にいいんです。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2292,
            "text_len": 28,
            "text_offset": 2298,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長こそどうしたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2348,
            "text_len": 42,
            "text_offset": 2354,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ちょっと朝練でもしてみようかと思ってな！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2428,
            "text_len": 28,
            "text_offset": 2434,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……囲碁サッカ―のですか？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2488,
            "text_len": 64,
            "text_offset": 2494,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "当たり前だろ！　囲碁サッカ―部なんだ。\\nそれ以外のわけがない！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2580,
            "text_len": 8,
            "text_offset": 2586,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2616,
            "text_len": 16,
            "text_offset": 2622,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "どうした関口。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2670,
            "text_len": 28,
            "text_offset": 2676,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\nいえ、あそこに犬が。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2734,
            "text_len": 28,
            "text_offset": 2740,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（話、聞いてなかった！？）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2790,
            "text_len": 14,
            "text_offset": 2796,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……可愛い。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2832,
            "text_len": 54,
            "text_offset": 2838,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口、あんま近づくなよ。\\n野良だったら危ないからな。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2914,
            "text_len": 76,
            "text_offset": 2920,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "大丈夫ですよ。首輪はしていませんけど、\\nアンテナみたいな物がついてますし。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3018,
            "text_len": 14,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "アンテナ！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3064,
            "text_len": 62,
            "text_offset": 3070,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……おお、すげぇ。\\nアンテナだ。誰がどう見てもアンテナだな。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3158,
            "text_len": 34,
            "text_offset": 3164,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、部長。\\nあんまり触ったら弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3308,
            "text_len": 8,
            "text_offset": 3314,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あ！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3338,
            "text_len": 34,
            "text_offset": 3344,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "弌弌折れるって言おうとしたのに。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3458,
            "text_len": 72,
            "text_offset": 3462,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おい、どういうことだ！\\n今回のアンテナは試作強化タイプのはずだろ！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3560,
            "text_len": 56,
            "text_offset": 3564,
            "warnings": 1
        },
        "speaker": "",
        "text": "おう、それぞれのパ―ツは\\n格段に強度アップしているぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3642,
            "text_len": 18,
            "text_offset": 3646,
            "warnings": 1
        },
        "speaker": "",
        "text": "なら、どうして？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3706,
            "text_len": 38,
            "text_offset": 3710,
            "warnings": 1
        },
        "speaker": "",
        "text": "それが、なぜかビスが余ったんだよな！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3840,
            "text_len": 34,
            "text_offset": 3846,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "急に練習とか、どうしたんですか。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3902,
            "text_len": 60,
            "text_offset": 3908,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや―、桜井くんが入部しただろ？\\nおかげでやる気が出てさ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3990,
            "text_len": 72,
            "text_offset": 3996,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "まだどんな競技かよくわからんが、\\nとりあえず部活といえば朝練だからな！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4094,
            "text_len": 12,
            "text_offset": 4100,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4216,
            "text_len": 56,
            "text_offset": 4222,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ―し、とりあえずリフティングから。\\nよっ……はっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4306,
            "text_len": 70,
            "text_offset": 4312,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そしてここで……碁石だったっけ。\\nおっと……ははっ、意外と難しいな。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4398,
            "text_len": 58,
            "text_offset": 4404,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "でも楽しくなってきたぞ。\\nよ―し、今度は碁石３つで……。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4478,
            "text_len": 8,
            "text_offset": 4484,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4606,
            "text_len": 22,
            "text_offset": 4612,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん？　どうした関口。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4660,
            "text_len": 52,
            "text_offset": 4666,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "碁石とサッカ―に\\n分けるわけにはいかないんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4784,
            "text_len": 20,
            "text_offset": 4790,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "何言ってんだ関口！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4846,
            "text_len": 6,
            "text_offset": 4852,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4880,
            "text_len": 60,
            "text_offset": 4886,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "オレだけが考えた競技ならいい。\\nだけど、実在したんだぞ！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4968,
            "text_len": 50,
            "text_offset": 4974,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それなら、\\n勝手に分割するわけにはいかないだろ！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5046,
            "text_len": 22,
            "text_offset": 5052,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……す、すみません。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5096,
            "text_len": 56,
            "text_offset": 5102,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "しかも中学ＬＵＯの桜井くんまで\\n入部してくれたんだぞ？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5180,
            "text_len": 54,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そうですね。\\nすみません、変なことを言ってしまって。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5262,
            "text_len": 66,
            "text_offset": 5268,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、わかってくれればいい。\\nオレもちょっとキツく言い過ぎたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5356,
            "text_len": 28,
            "text_offset": 5362,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ただちょっと悩んでてな―。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5412,
            "text_len": 16,
            "text_offset": 5418,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "悩み、ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5456,
            "text_len": 66,
            "text_offset": 5462,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、お遊びで作った部が\\n何か凄いことになってきて正直めん……。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5550,
            "text_len": 44,
            "text_offset": 5556,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、楽しいことになってきたなと思ってな！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5704,
            "text_len": 12,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5784,
            "text_len": 40,
            "text_offset": 5790,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……なるほど。\\n関口の言うとおりだな。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5858,
            "text_len": 66,
            "text_offset": 5864,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "初心者のオレは無理をせず\\nそれぞれ個別に練習した方がいいもんな！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5962,
            "text_len": 40,
            "text_offset": 5968,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "い、いえ、そういう意味じゃなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6030,
            "text_len": 46,
            "text_offset": 6036,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "さすが関口だ！\\n頼りになるな、ありがとう！！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6108,
            "text_len": 12,
            "text_offset": 6114,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6148,
            "text_len": 26,
            "text_offset": 6154,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、どういたしまして。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6210,
            "text_len": 48,
            "text_offset": 6216,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それじゃ早速練習だ！\\nまずはリフティングから！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6368,
            "text_len": 50,
            "text_offset": 6374,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よっ……おっと！\\nくっ、意外と……難しいなコレ。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6440,
            "text_len": 46,
            "text_offset": 6446,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "くそっ、ダメだ！\\n連続３０回が精一杯か……。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6508,
            "text_len": 70,
            "text_offset": 6514,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "この状態で碁石を使うなんて、\\n囲碁サッカ―ってのは凄いスポ―ツだな。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6604,
            "text_len": 12,
            "text_offset": 6610,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6726,
            "text_len": 84,
            "text_offset": 6732,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……ところで関口。サッカ―部分はいいとして、\\n碁石部分はどう練習すればいいと思う？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6832,
            "text_len": 10,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……あ。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6910,
            "text_len": 52,
            "text_offset": 6916,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口〜。\\nそれじゃ囲碁サッカ―じゃなくなるだろ―？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6996,
            "text_len": 16,
            "text_offset": 7002,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、はい……。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7040,
            "text_len": 62,
            "text_offset": 7046,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "まったく、しっかりしてくれよ？\\n囲碁サッカ―部員としてさぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7130,
            "text_len": 14,
            "text_offset": 7136,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "すみません。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7172,
            "text_len": 58,
            "text_offset": 7178,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "まあいいけど。\\nさて、そろそろル―ルブックでも読むかな。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7258,
            "text_len": 36,
            "text_offset": 7264,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ル―ルブックなんてあったんですか？\\n",
