[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2102,
            "text_len": 38,
            "text_offset": 2108,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はぁ……\\n結局ネジはついたままかぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2174,
            "text_len": 70,
            "text_offset": 2180,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ネジの無い普通の女の子になれる日は\\nいつになったら来るんだろう……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2272,
            "text_len": 24,
            "text_offset": 2280,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "お―い、なのちゃ―ん。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2326,
            "text_len": 40,
            "text_offset": 2332,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "おはよ―なのちゃん。\\n風邪は治ったの？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2398,
            "text_len": 54,
            "text_offset": 2404,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、おはようございます。\\n風邪はもう治りましたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2480,
            "text_len": 52,
            "text_offset": 2486,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そっか。良かった。\\n心配してたんだよ。ね、ゆっこ？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2562,
            "text_len": 72,
            "text_offset": 2570,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "うんうん、なのちゃんが落ち込んでると\\nこっちまで元気なくなっちゃうし。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2658,
            "text_len": 28,
            "text_offset": 2664,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "相生さん、長野原さん……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2726,
            "text_len": 72,
            "text_offset": 2732,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ネジはついたままだけど……でも、\\n　私にはこんなに優しい友達がいる）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2830,
            "text_len": 70,
            "text_offset": 2836,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（それだけでいいや。ネジがあっても、\\n　私は普通の女の子なんだから）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2928,
            "text_len": 64,
            "text_offset": 2934,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ありがとうございます。\\nまた、今日からよろしくお願いしますね。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3024,
            "text_len": 22,
            "text_offset": 3032,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "うんうん、よろしく。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3078,
            "text_len": 26,
            "text_offset": 3084,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "よろしくね、なのちゃん。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3132,
            "text_len": 10,
            "text_offset": 3138,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3182,
            "text_len": 68,
            "text_offset": 3188,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ああ、これが友情というものなんだ。\\n　なんて素晴らしいんだろう）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3282,
            "text_len": 78,
            "text_offset": 3288,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（でも私がロボットであることは秘密だから\\n　少し後ろめたい気もするけど……）\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3392,
            "text_len": 82,
            "text_offset": 3398,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（でも、こればっかりは教えられないから\\n　許してください、相生さん、長野原さん）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3506,
            "text_len": 52,
            "text_offset": 3512,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なのちゃん、\\nちゃんと修理してもらったみたいだね。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3586,
            "text_len": 30,
            "text_offset": 3594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "うんうん。直ってよかったよ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3648,
            "text_len": 12,
            "text_offset": 3654,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3778,
            "text_len": 12,
