[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 48,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん―、いいお天気。\\nこんな日はお散歩もいいね。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2216,
            "text_len": 44,
            "text_offset": 2224,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "せっかくだから、\\n少し遊んでいこうかな―。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2290,
            "text_len": 10,
            "text_offset": 2298,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "……ん？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2430,
            "text_len": 30,
            "text_offset": 2438,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あの人、何やってるんだろう？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2572,
            "text_len": 30,
            "text_offset": 2580,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よっと。１、２、３、４……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2630,
            "text_len": 12,
            "text_offset": 2638,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2666,
            "text_len": 52,
            "text_offset": 2674,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よし。これから碁石を追加して……\\n１、２、３……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2742,
            "text_len": 12,
            "text_offset": 2750,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2778,
            "text_len": 30,
            "text_offset": 2786,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よし、いい感じ。このまま弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2832,
            "text_len": 12,
            "text_offset": 2840,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2872,
            "text_len": 12,
            "text_offset": 2880,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3002,
            "text_len": 20,
            "text_offset": 3010,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え、え―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3066,
            "text_len": 44,
            "text_offset": 3074,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（何なんだ……？\\n　すごい見られてる……）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3144,
            "text_len": 80,
            "text_offset": 3152,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（囲碁サッカ―に興味があるのかな？\\n　それともリフティングが続くのが楽しい？）\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3258,
            "text_len": 50,
            "text_offset": 3266,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あっ、強く蹴りすぎた！\\n……ああっ、あぶない！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3370,
            "text_len": 14,
            "text_offset": 3378,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "キャイン！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3468,
            "text_len": 54,
            "text_offset": 3472,
            "warnings": 1
        },
        "speaker": "",
        "text": "ミスキックのボ―ルが激突！\\nアンテナ、破損しました！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3568,
            "text_len": 20,
            "text_offset": 3572,
            "warnings": 1
        },
        "speaker": "",
        "text": "く、なんて不運な！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3608,
            "text_len": 12,
            "text_offset": 3612,
            "warnings": 1
        },
        "speaker": "",
        "text": "ふう―ん。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3644,
            "text_len": 8,
            "text_offset": 3648,
            "warnings": 1
        },
        "speaker": "",
        "text": "え！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3748,
            "text_len": 30,
            "text_offset": 3756,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あれ？　なんでやめちゃうの？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3812,
            "text_len": 24,
            "text_offset": 3820,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "えっ？　見てたかった？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3866,
            "text_len": 56,
            "text_offset": 3874,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。すごく楽しそうだったもん。\\nこれサッカ―だよね？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3952,
            "text_len": 68,
            "text_offset": 3960,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "う―ん、ちょっと違うかな。\\nサッカ―じゃなくて、囲碁サッカ―だよ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4050,
            "text_len": 32,
            "text_offset": 4058,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "試しにやってみる？\\n楽しいよ？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4112,
            "text_len": 10,
            "text_offset": 4120,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "本当！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4152,
            "text_len": 22,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うん。頑張ってみて。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4204,
            "text_len": 20,
            "text_offset": 4212,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "よ―し！　せ―の！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4336,
            "text_len": 46,
            "text_offset": 4344,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あれ？　おにいさんみたいに\\n上手くできない。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4408,
            "text_len": 74,
            "text_offset": 4416,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "リフティングは難しかったかな？\\nでも練習すればすぐにできるようになるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4506,
            "text_len": 46,
            "text_offset": 4514,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "む―……\\nはかせ今すぐできるようになりたい。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4704,
            "text_len": 60,
            "text_offset": 4712,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そうだ！　囲碁サッカ―のためのロボットを\\n作ればいいんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4814,
            "text_len": 12,
            "text_offset": 4822,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ロボット？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4856,
            "text_len": 34,
            "text_offset": 4864,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん！　ちょっとボ―ル借りるね。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4920,
            "text_len": 20,
            "text_offset": 4928,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え、う、うん……。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5052,
            "text_len": 18,
            "text_offset": 5060,
            "warnings": 1
        },
        "speaker": "はかせ",
        "text": "これをこ―して\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5098,
            "text_len": 24,
            "text_offset": 5106,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわっ！？　ちょっと！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5244,
            "text_len": 52,
            "text_offset": 5252,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃじゃ―ん！　完成―！\\n自動リフティングボ―ル！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5336,
            "text_len": 28,
            "text_offset": 5344,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "このスイッチを押せば……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5476,
            "text_len": 46,
            "text_offset": 5484,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ほらほら！　はかせもリフティング\\nできるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5550,
            "text_len": 12,
            "text_offset": 5558,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5710,
            "text_len": 64,
            "text_offset": 5718,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ははは、すぐには無理だよ。\\n囲碁サッカ―は難しい競技だからね。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5804,
            "text_len": 54,
            "text_offset": 5812,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "オレだってここまでくるのに\\n何年も練習したんだから。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5894,
            "text_len": 62,
            "text_offset": 5902,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあはかせもいっぱい練習したら\\n囲碁サッカ―を極められる？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5986,
            "text_len": 64,
            "text_offset": 5994,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うん。オレもまだ極めてないから\\nどっちが先に極めるか勝負だね。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6080,
            "text_len": 38,
            "text_offset": 6088,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "勝負する！　はかせ負けないからね―！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6148,
            "text_len": 70,
            "text_offset": 6156,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よし。じゃあキミにこのボ―ルをあげるよ。\\nこれでいっぱい練習してね。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6248,
            "text_len": 22,
            "text_offset": 6256,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん！　ありがとう！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 6390,
            "text_len": 80,
            "text_offset": 6410,
            "warnings": 0
        },
        "speaker": "ナレ―タ―お姉さん",
        "text": "数日後、このボ―ルは物置に収められ、\\n永久に忘れられることになったといいます。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6640,
            "text_len": 58,
            "text_offset": 6648,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "今すぐか―。囲碁サッカ―は奥が深いから\\n難しいと思うよ。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6728,
            "text_len": 58,
            "text_offset": 6736,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "囲碁サッカ―ってどんなことするの？\\nさっきみたいなこと？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6816,
            "text_len": 48,
            "text_offset": 6824,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ん―、そうだね。\\n一連の流れを見せてあげるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6990,
            "text_len": 34,
            "text_offset": 6998,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よっ。これが普通のリフティング。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7058,
            "text_len": 22,
            "text_offset": 7066,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わくわく。わくわく。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7110,
            "text_len": 30,
            "text_offset": 7118,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "それで、ここから碁石を追加。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7164,
            "text_len": 10,
            "text_offset": 7172,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "おお―！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7198,
            "text_len": 56,
            "text_offset": 7206,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ははは。そしてここからが\\n囲碁サッカ―、最大の見せ場！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7304,
            "text_len": 18,
            "text_offset": 7312,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "一人ジャ―マン！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7356,
            "text_len": 6,
            "text_offset": 7364,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7396,
            "text_len": 24,
            "text_offset": 7404,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "これぞ定石・三連星！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7450,
            "text_len": 6,
            "text_offset": 7458,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7480,
            "text_len": 12,
            "text_offset": 7488,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7516,
            "text_len": 12,
            "text_offset": 7524,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7640,
            "text_len": 32,
            "text_offset": 7648,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "……囲碁サッカ―するのやめる。\\n",
        "type": "dialog"
    }
]