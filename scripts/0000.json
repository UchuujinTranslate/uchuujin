[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 18,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "んっ、んん……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2184,
            "text_len": 16,
            "text_offset": 2192,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、起きた。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2322,
            "text_len": 46,
            "text_offset": 2328,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "うう……はかせ？\\nもう起きてたんですか……？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2412,
            "text_len": 12,
            "text_offset": 2418,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2466,
            "text_len": 40,
            "text_offset": 2472,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（はかせの持ってるボタンって……何？）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2536,
            "text_len": 18,
            "text_offset": 2544,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "おはよ―、なの。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2584,
            "text_len": 48,
            "text_offset": 2590,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "お、おはようございます。\\nところではかせ……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2660,
            "text_len": 10,
            "text_offset": 2668,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "なに―？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2708,
            "text_len": 72,
            "text_offset": 2714,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "起きていきなりでアレですけど、\\nそのボタンで私に何をするつもりですか？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2810,
            "text_len": 70,
            "text_offset": 2818,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "なののネジをパワ―アップさせたから\\n新しい機能を試すところだったの。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2914,
            "text_len": 46,
            "text_offset": 2920,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えぇっ！？\\nもう何かした後だったんですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2988,
            "text_len": 42,
            "text_offset": 2996,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "何かじゃないよ！\\nパワ―アップさせたの！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3070,
            "text_len": 58,
            "text_offset": 3076,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "しないでくださいよ〜。\\nただでさえネジで困ってるのに〜。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3156,
            "text_len": 24,
            "text_offset": 3164,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "でもパワ―アップだよ？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3210,
            "text_len": 64,
            "text_offset": 3216,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "一番なくしてほしいネジを\\nパワ―アップされても困るんですよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3312,
            "text_len": 38,
            "text_offset": 3318,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "阪本さんも何とか言ってくださいよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3382,
            "text_len": 12,
            "text_offset": 3390,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3436,
            "text_len": 22,
            "text_offset": 3450,
            "warnings": 0
        },
        "speaker": "なの＆はかせ",
        "text": "！？　犬だ―――！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3552,
            "text_len": 58,
            "text_offset": 3556,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそっ、猫がいるんだから\\n犬でも誤魔化せると思ったのに！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3650,
            "text_len": 30,
            "text_offset": 3654,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ！　アンテナが折られた！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3726,
            "text_len": 72,
            "text_offset": 3730,
            "warnings": 1
        },
        "speaker": "",
        "text": "……判断を間違えたな。俺よりも\\n地球人たちの方が一枚上手だったらしい。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3818,
            "text_len": 12,
            "text_offset": 3822,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ……。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3850,
            "text_len": 60,
            "text_offset": 3854,
            "warnings": 1
        },
        "speaker": "",
        "text": "でも、なんで犬で行ったんだ？\\n最初からいた猫でよくないか？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3934,
            "text_len": 6,
            "text_offset": 3938,
            "warnings": 1
        },
        "speaker": "",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3960,
            "text_len": 48,
            "text_offset": 3964,
            "warnings": 1
        },
        "speaker": "",
        "text": "……俺よりもお前の方が\\n一枚上手だったらしい。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4032,
            "text_len": 14,
            "text_offset": 4036,
            "warnings": 1
        },
        "speaker": "",
        "text": "えええっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4142,
            "text_len": 50,
            "text_offset": 4148,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "改造は許しますから\\nせめてネジを取ってください。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4222,
            "text_len": 46,
            "text_offset": 4230,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、そうだなの―。\\nはかせ、おなかすいた。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4298,
            "text_len": 50,
            "text_offset": 4304,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "誤魔化さないで、\\n今すぐネジを取ってくださいよ。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4380,
            "text_len": 52,
            "text_offset": 4388,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ごまかしてない！\\nはかせ本当にお腹すいたんだもん！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4462,
            "text_len": 22,
            "text_offset": 4468,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そうじゃなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4552,
            "text_len": 44,
            "text_offset": 4558,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ネジです。ネ・ジ！\\nネジを取ってください！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4628,
            "text_len": 46,
            "text_offset": 4636,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ちがうもん！\\nネジじゃなくてお腹がすいたの！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4708,
            "text_len": 58,
            "text_offset": 4714,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "だから、ご飯の前にネジを\\n取ってほしいと言ってるんです！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4798,
            "text_len": 8,
            "text_offset": 4806,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "やだ！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4840,
            "text_len": 62,
            "text_offset": 4846,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "私ははかせがネジを取ってくれるまで\\n朝ご飯作りませんからね！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4934,
            "text_len": 52,
            "text_offset": 4942,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "いいもん！\\nなぜなら、お菓子がいっぱいあるから―！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5020,
            "text_len": 40,
            "text_offset": 5026,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "お菓子を朝ご飯代わりにしちゃダメです！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5092,
            "text_len": 24,
            "text_offset": 5100,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあ、朝ご飯作って！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5150,
            "text_len": 16,
            "text_offset": 5156,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わかりました！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5296,
            "text_len": 24,
            "text_offset": 5302,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！　朝ご飯ですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5348,
            "text_len": 28,
            "text_offset": 5356,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わ―い！\\nいただきま―す！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5412,
            "text_len": 12,
            "text_offset": 5418,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5460,
            "text_len": 38,
            "text_offset": 5466,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（何、普通に朝ご飯作ったの、私！？）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5540,
            "text_len": 70,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（違う、そうじゃない。ネジを\\n　取ってもらわないと私は学校にも弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5642,
            "text_len": 26,
            "text_offset": 5648,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "学校！？\\nい、今何時！？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5696,
            "text_len": 62,
            "text_offset": 5702,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ああああ――！？　もう遅刻寸前！！\\nはかせ、行ってきま―す！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5878,
            "text_len": 22,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "行ってらっしゃ―い！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5956,
            "text_len": 66,
            "text_offset": 5962,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ネジを取ってください。\\nこれがある限り私は家事をしませんからね。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6054,
            "text_len": 34,
            "text_offset": 6062,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ならまた別の子作るからいいもん！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6138,
            "text_len": 6,
            "text_offset": 6144,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6194,
            "text_len": 12,
            "text_offset": 6200,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6234,
            "text_len": 12,
            "text_offset": 6242,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6276,
            "text_len": 44,
            "text_offset": 6284,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "嘘だよ。はかせは、\\nなの以外は作らないよ。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6364,
            "text_len": 52,
            "text_offset": 6370,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……本当ですか？\\n私、いらない子じゃないんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6444,
            "text_len": 60,
            "text_offset": 6452,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "いらなくないよ！\\nはかせ、なのにご飯食べさせて欲しいもん！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6544,
            "text_len": 44,
            "text_offset": 6550,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はかせ……！\\nはい！　私、朝ご飯作ります！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6712,
            "text_len": 18,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ごちそうさま―！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6764,
            "text_len": 24,
            "text_offset": 6770,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい。お粗末さまです。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6816,
            "text_len": 40,
            "text_offset": 6824,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、なの。\\nそろそろ学校の時間だよ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6886,
            "text_len": 50,
            "text_offset": 6892,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、本当ですね。\\nじゃあはかせ、行ってきます。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7046,
            "text_len": 22,
            "text_offset": 7054,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "行ってらっしゃ―い。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7124,
            "text_len": 48,
            "text_offset": 7130,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "じゃあ、ネジを取ってくれたら\\n朝ご飯作ります。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7210,
            "text_len": 8,
            "text_offset": 7218,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "やだ！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7258,
            "text_len": 44,
            "text_offset": 7264,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "何でですか？\\n朝ご飯食べられないんですよ？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7330,
            "text_len": 56,
            "text_offset": 7338,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だってはかせ、ネジのために\\n早起きして頑張ったんだよ？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7416,
            "text_len": 46,
            "text_offset": 7422,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "こんなことのために\\n早起きしたんですか……。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7490,
            "text_len": 50,
            "text_offset": 7498,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "せっかく頑張ったのに\\nもう取っちゃうなんてヤダ！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7570,
            "text_len": 74,
            "text_offset": 7576,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……仕方ないですね。それじゃあ、\\n一回だけ新機能を試してもいいですから。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7682,
            "text_len": 24,
            "text_offset": 7690,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "はかせお腹減った……。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7736,
            "text_len": 52,
            "text_offset": 7744,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "疲れてお腹も減ってるのに、\\n朝ご飯食べられないの？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7818,
            "text_len": 12,
            "text_offset": 7824,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "うっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7858,
            "text_len": 40,
            "text_offset": 7864,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……わかりました。\\n先に朝ご飯ですね。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7926,
            "text_len": 28,
            "text_offset": 7934,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わ―い！\\nなの大好き――！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8080,
            "text_len": 18,
            "text_offset": 8088,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ごちそうさま―！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8132,
            "text_len": 34,
            "text_offset": 8138,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "それじゃあはかせ、早くネジを弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8194,
            "text_len": 46,
            "text_offset": 8202,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、なの、もう時間だよ？\\n学校行かないと。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8274,
            "text_len": 54,
            "text_offset": 8280,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わわわっ！？　もうこんな時間！\\nで、でもネジが……。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8356,
            "text_len": 36,
            "text_offset": 8362,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わ―ん！\\nはかせ、行ってきま―す！\\n",
