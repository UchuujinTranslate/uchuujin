            "speaker_offset": 7726,
            "text_len": 14,
            "text_offset": 7732,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "（楽だ……）\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7778,
            "text_len": 18,
            "text_offset": 7786,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ほおああああ！！\\n",
        "type": "dialog"
