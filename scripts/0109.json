[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 64,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "う―、見つからないな―……\\nこっちに来たと思ったんだけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2238,
            "text_len": 52,
            "text_offset": 2246,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "気のせいだったのかなぁ。\\n鹿がいるわけないもんね。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2320,
            "text_len": 30,
            "text_offset": 2328,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "帰ってマンガの続き読もっと。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2380,
            "text_len": 8,
            "text_offset": 2388,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2418,
            "text_len": 40,
            "text_offset": 2426,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ん？　おお、なのちゃんちのちっこいの。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2492,
            "text_len": 14,
            "text_offset": 2500,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "はかせだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2536,
            "text_len": 52,
            "text_offset": 2544,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そうそう、はかせだ。\\n私は覚えてる？　ゆっこだよ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2618,
            "text_len": 44,
            "text_offset": 2626,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん、ゆっこ！\\nゆっこのうちもこの辺なの？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2692,
            "text_len": 56,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ううん。暇だったから\\nなんとなくこっちに来てみただけ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2778,
            "text_len": 30,
            "text_offset": 2786,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "じゃあうちで遊んでいかない？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2838,
            "text_len": 40,
            "text_offset": 2846,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "お、いいね。\\nじゃあ遊びに行こうかな。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2908,
            "text_len": 12,
            "text_offset": 2916,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "やった―！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2976,
            "text_len": 12,
            "text_offset": 2984,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 3036,
            "text_len": 6,
            "text_offset": 3052,
            "warnings": 0
        },
        "speaker": "はかせ＆ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3088,
            "text_len": 34,
            "text_offset": 3096,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ゆ、ゆっこ……犬……退治できる？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3156,
            "text_len": 50,
            "text_offset": 3164,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "う、うう……厳しい……\\nでも、二人一緒なら……！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3236,
            "text_len": 28,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わ、わかった、二人一緒に！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 3310,
            "text_len": 20,
            "text_offset": 3326,
            "warnings": 0
        },
        "speaker": "はかせ＆ゆっこ",
        "text": "うわああああ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3426,
            "text_len": 12,
            "text_offset": 3430,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3484,
            "text_len": 54,
            "text_offset": 3488,
            "warnings": 1
        },
        "speaker": "",
        "text": "……地球の女の子は犬を見たら\\n戦う習性でもあるのか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3558,
            "text_len": 28,
            "text_offset": 3562,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あるんだろう。きっと。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3682,
            "text_len": 50,
            "text_offset": 3690,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そうだはかせ。\\nなのちゃんはもう家に帰ってるの？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3764,
            "text_len": 24,
            "text_offset": 3772,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ううん、なのはまだ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3822,
            "text_len": 42,
            "text_offset": 3830,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（っとと。ゆっこはなのと仲良いんだった）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3898,
            "text_len": 72,
            "text_offset": 3906,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（なのになのがいないなんて知ったら\\n　遊びに来てくれないかもしれない）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4004,
            "text_len": 70,
            "text_offset": 4012,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（はかせはまたゆっこと遊びたいし、\\n　本当のことは言っちゃダメかも）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4104,
            "text_len": 40,
            "text_offset": 4112,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あれ？　なのちゃんはまだ帰ってないの？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4210,
            "text_len": 40,
            "text_offset": 4218,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "い、いるよ！\\nちゃんと、うちにいるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4280,
            "text_len": 72,
            "text_offset": 4288,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そうなんだ。じゃあ、はかせとなのちゃんに\\n私がマジックを見せてあげる。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4382,
            "text_len": 28,
            "text_offset": 4390,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "マジック！？　できるの！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4440,
            "text_len": 54,
            "text_offset": 4448,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ま、かじった程度だけどね。\\n最近やるようになってさ。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4524,
            "text_len": 32,
            "text_offset": 4532,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "すごいすごい！\\n見せて見せて！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4586,
            "text_len": 70,
            "text_offset": 4594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "それは後のお楽しみってね。\\nなのちゃん待たせても悪いし、早く行こう。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4686,
            "text_len": 54,
            "text_offset": 4694,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あっ、で、でも、なのはいいの！\\nなのは見られないの！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4770,
            "text_len": 8,
            "text_offset": 4778,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4812,
            "text_len": 34,
            "text_offset": 4820,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "なの、今メンテナンスしてるから！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4886,
            "text_len": 48,
            "text_offset": 4894,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "メンテナンス！？　マジ！？\\nやっぱりするんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4964,
            "text_len": 64,
            "text_offset": 4972,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そう！　だからなのはちょっと\\n起きられないから、見られないの！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5058,
            "text_len": 44,
            "text_offset": 5066,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そっか―。\\nすぐには終わりそうにないんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5132,
            "text_len": 64,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん！　メンテナンスは時間かかるの！\\n明日までかかっちゃうの！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5230,
            "text_len": 60,
            "text_offset": 5238,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ邪魔しても悪いし、\\n遊びに行くのはまた今度にするよ。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5344,
            "text_len": 20,
            "text_offset": 5352,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（あれ―――！？）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5476,
            "text_len": 22,
            "text_offset": 5484,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあね―はかせ―。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5526,
            "text_len": 12,
            "text_offset": 5534,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5600,
            "text_len": 84,
            "text_offset": 5608,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あのね、なのはね、帰ってきてたんだよ？\\nでもね、今は夕飯の準備してるからいないの。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5714,
            "text_len": 34,
            "text_offset": 5722,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "へ―。なのちゃんがやってるんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5778,
            "text_len": 70,
            "text_offset": 5786,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そ、そう。でもちょっと時間がかかるから\\n帰ってくるのは遅くなるかも。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5878,
            "text_len": 56,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "それなら買い物手伝おうか？\\n二人なら早く終わるでしょ。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5964,
            "text_len": 16,
            "text_offset": 5972,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "う、うん……。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6014,
            "text_len": 62,
            "text_offset": 6022,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "（ど、どうしよう。本当は買い物なんて\\n　行かせてないし……）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6106,
            "text_len": 46,
            "text_offset": 6114,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なのちゃん、今どこかな？\\n連絡取れたりする？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6190,
            "text_len": 36,
            "text_offset": 6198,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "と、取れるけど、電波届かないかも！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6256,
            "text_len": 42,
            "text_offset": 6264,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えっ！？\\nなのちゃんどこに行ってるの！？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6328,
            "text_len": 66,
            "text_offset": 6336,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……あっ、地下だからか。\\nふ―、密林や樹海に行ってるかと思った。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6428,
            "text_len": 14,
            "text_offset": 6436,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "行ってるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6476,
            "text_len": 14,
            "text_offset": 6484,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えええっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6530,
            "text_len": 64,
            "text_offset": 6538,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だ、だからなのには連絡取れないし、\\n帰ってくるのも遅くなるの。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6624,
            "text_len": 72,
            "text_offset": 6632,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "夕飯のためにそんなところ行くなんて……\\nはかせの家、大変なんだね……。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6726,
            "text_len": 22,
            "text_offset": 6734,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "う、うん、大変なの。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6778,
            "text_len": 70,
            "text_offset": 6786,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なのちゃん帰ってくるまで\\nずっと遊んであげるから。大人しく待とうね。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6878,
            "text_len": 8,
            "text_offset": 6886,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6998,
            "text_len": 38,
            "text_offset": 7006,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "世知辛い世の中になったんだなぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7102,
            "text_len": 22,
            "text_offset": 7110,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "えっと、えっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
