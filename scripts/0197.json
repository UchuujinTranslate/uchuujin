[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2094,
            "text_len": 12,
            "text_offset": 2106,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2142,
            "text_len": 24,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "父さん、買ってきたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2198,
            "text_len": 8,
            "text_offset": 2210,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ああ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2244,
            "text_len": 60,
            "text_offset": 2256,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（……うちと大工カフェの差を\\n　はっきりと知る必要がある）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2342,
            "text_len": 58,
            "text_offset": 2354,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（そのためには一度くらいは\\n　商品を試さないといけない）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2434,
            "text_len": 36,
            "text_offset": 2446,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "それで、何を買ってきてくれたんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2504,
            "text_len": 64,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ヘイゼルキャラメルマキア―トと\\nケ―キをいくつか買ってきたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2598,
            "text_len": 32,
            "text_offset": 2610,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ヘイゼルキャラメルマキア―ト？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2668,
            "text_len": 56,
            "text_offset": 2680,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（わけがわからん……\\n　なんだその長ったらしい名前は）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2758,
            "text_len": 66,
            "text_offset": 2770,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……名前通りキャラメルのいい匂いがするな。\\n味も悪くはない……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2858,
            "text_len": 54,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "さて、ケ―キはと……\\nな、何だこの目に痛い色合いは！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2946,
            "text_len": 22,
            "text_offset": 2958,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……わけがわからん。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3002,
            "text_len": 68,
            "text_offset": 3014,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "これなら白くてシンプルな大福の方が\\nよっぽど素晴らしいじゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3104,
            "text_len": 34,
            "text_offset": 3112,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "父さん、とりあえず食べてみなよ。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3168,
            "text_len": 12,
            "text_offset": 3180,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだな。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3214,
            "text_len": 48,
            "text_offset": 3226,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……うまい。\\nだが、少し甘すぎるような……！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3304,
            "text_len": 54,
            "text_offset": 3316,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "いや、こ、これは……！\\nコ―ヒ―と絶妙なバランス！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3392,
            "text_len": 66,
            "text_offset": 3404,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "くっ、悔しいがこれはうまい。\\nこれだけうまければ流行るのも……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3496,
            "text_len": 82,
            "text_offset": 3508,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "（しまった！　自分は何を考えているんだ！\\n　大福に対して失礼だと思わないか！！）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3616,
            "text_len": 32,
            "text_offset": 3628,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "くっ……！　私は、私は……！！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3714,
            "text_len": 16,
            "text_offset": 3722,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "と、父さん！？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3764,
            "text_len": 50,
            "text_offset": 3776,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "すまない、この通りだ！\\n許してくれ、大福の神よ！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3852,
            "text_len": 30,
            "text_offset": 3860,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "父さん、やめてよ！　父さん！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3916,
            "text_len": 74,
            "text_offset": 3928,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "二度とケ―キには！　悪魔の誘惑には乗らない！\\nだから、だからお許しをっ！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4056,
            "text_len": 12,
            "text_offset": 4064,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4180,
            "text_len": 18,
            "text_offset": 4188,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………父さん。\\n",
        "type": "dialog"
    }
]