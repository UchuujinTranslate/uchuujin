[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2152,
            "text_len": 28,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はぁ……やっと帰れる……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2212,
            "text_len": 52,
            "text_offset": 2218,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（すぐに郵便局行って\\n　この原稿を送らなきゃ……）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2296,
            "text_len": 56,
            "text_offset": 2302,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（……っと、その前に、\\n　念のため中を確認しとこっと）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2384,
            "text_len": 72,
            "text_offset": 2390,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（…………よし。周りに誰もいない。\\n　今の内に……１……４……８……）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2484,
            "text_len": 44,
            "text_offset": 2490,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……ん？\\n数え間違えたかな。もう一回……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2564,
            "text_len": 12,
            "text_offset": 2570,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2610,
            "text_len": 14,
            "text_offset": 2616,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2664,
            "text_len": 28,
            "text_offset": 2670,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（な――――――いっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2734,
            "text_len": 58,
            "text_offset": 2740,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（いや違う、正確には、\\n　足りな―――――――いっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2824,
            "text_len": 50,
            "text_offset": 2830,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（やっぱりあれだ！\\n　ゆっことぶつかった時！！）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2906,
            "text_len": 68,
            "text_offset": 2912,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（くっ、タイムリミットは！？\\n　ううっ！？　あと数時間しかない！）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3006,
            "text_len": 68,
            "text_offset": 3012,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ええい、悔やんでいる間も惜しい！\\n　今からでも探し出してやる！）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3202,
            "text_len": 54,
            "text_offset": 3212,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、長野原さん。\\nちょっと相談があるんですけど弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3292,
            "text_len": 66,
            "text_offset": 3298,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え―っ！？　すみません、\\nいま急いでるんで今度にしてください！！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3386,
            "text_len": 64,
            "text_offset": 3396,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あの、お時間は取らせませんから……\\nちょっとだけお願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3482,
            "text_len": 28,
            "text_offset": 3488,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "うぅ〜……な、なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3538,
            "text_len": 40,
            "text_offset": 3548,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はい、実は……あれのことなんですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3610,
            "text_len": 8,
            "text_offset": 3616,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3738,
            "text_len": 12,
            "text_offset": 3744,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3786,
            "text_len": 84,
            "text_offset": 3796,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "長野原さんの絵がとても素敵だったので、\\n生活指導の垂れ幕に使わせてもらったんです。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3992,
            "text_len": 12,
            "text_offset": 3998,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4034,
            "text_len": 72,
            "text_offset": 4044,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "美術部の子が描いてくれたんです。\\n元の絵そっくりで良くできてますよね。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4144,
            "text_len": 12,
            "text_offset": 4150,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4184,
            "text_len": 8,
            "text_offset": 4194,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "？？？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4230,
            "text_len": 56,
            "text_offset": 4236,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（終わった……\\n　私の平穏な学園生活が崩れていく……）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4314,
            "text_len": 14,
            "text_offset": 4320,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………ん？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4442,
            "text_len": 6,
            "text_offset": 4448,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4484,
            "text_len": 50,
            "text_offset": 4490,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あそこに引っかかってるの、\\n　私の原稿だ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4666,
            "text_len": 56,
            "text_offset": 4672,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ！？　こっちに落ちてくる！！\\nやった、そのまま弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4750,
            "text_len": 12,
            "text_offset": 4760,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4798,
            "text_len": 6,
            "text_offset": 4804,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4840,
            "text_len": 24,
            "text_offset": 4846,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "見るな――――――！！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4958,
            "text_len": 6,
            "text_offset": 4968,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5056,
            "text_len": 10,
            "text_offset": 5060,
            "warnings": 1
        },
        "speaker": "",
        "text": "見えた！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5094,
            "text_len": 14,
            "text_offset": 5098,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ、嘘！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5134,
            "text_len": 32,
            "text_offset": 5138,
            "warnings": 1
        },
        "speaker": "",
        "text": "マジだ！\\nほんの一瞬だけどな。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5192,
            "text_len": 48,
            "text_offset": 5196,
            "warnings": 1
        },
        "speaker": "",
        "text": "くっそおおおお！\\n俺、全く見えなかったぁああ！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5266,
            "text_len": 50,
            "text_offset": 5270,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おい、教えてくれよ。\\nどんなのが見えたんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5342,
            "text_len": 68,
            "text_offset": 5346,
            "warnings": 1
        },
        "speaker": "",
        "text": "それが……なんかモヒカンのやつが\\n服を脱いでる絵だったんだけどさ。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5436,
            "text_len": 16,
            "text_offset": 5440,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5478,
            "text_len": 8,
            "text_offset": 5482,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5536,
            "text_len": 16,
            "text_offset": 5544,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5652,
            "text_len": 22,
            "text_offset": 5658,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ああっ！！　飛ぶ！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5794,
            "text_len": 26,
            "text_offset": 5804,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？　な、何がですか？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5856,
            "text_len": 66,
            "text_offset": 5862,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あっ、堪えた！　まだ飛ばされずに\\n　ポスタ―にくっついてる！）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5950,
            "text_len": 32,
            "text_offset": 5956,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あれなら……屋上に行けば……。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6010,
            "text_len": 36,
            "text_offset": 6020,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "屋上？　屋上がどうかしたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6078,
            "text_len": 34,
            "text_offset": 6084,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ごめんなさい！！\\n私、行きます！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6160,
            "text_len": 24,
            "text_offset": 6170,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きゃ！　長野原さん！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6216,
            "text_len": 28,
            "text_offset": 6226,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "飛ぶ……屋上……逝く……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6290,
            "text_len": 14,
            "text_offset": 6300,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6430,
            "text_len": 42,
            "text_offset": 6436,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はぁ、はぁ……！\\nげ、原稿は…………！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6500,
            "text_len": 54,
            "text_offset": 6506,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ！！　良かった〜、\\nまだ風に飛ばされてなかった！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6584,
            "text_len": 60,
            "text_offset": 6590,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "回収回収っと。……あっ！\\nこのシ―ン一番重要なとこじゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6672,
            "text_len": 62,
            "text_offset": 6678,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "見られたら一番ヤバいシ―ンだよ……\\nホントに危なかった……。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6762,
            "text_len": 46,
            "text_offset": 6768,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "さて、誰かに見られる前に\\nさっさと退散を弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6864,
            "text_len": 40,
            "text_offset": 6874,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "長野原さん！　待って！　早まらないで！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6940,
            "text_len": 38,
            "text_offset": 6950,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "こら、長野原！　馬鹿な真似はやめろ！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7018,
            "text_len": 22,
            "text_offset": 7024,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ワオ――――！？）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7112,
            "text_len": 62,
            "text_offset": 7122,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "飛び降りなんてバカなこと考えるな！\\n今すぐこっちに来るんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7214,
            "text_len": 66,
            "text_offset": 7220,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（えええっ！？　飛び降り！？\\n　何でそんなことになってるの！？）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7308,
            "text_len": 40,
            "text_offset": 7318,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "その遺書も先生に渡すんだ！　さあ早く！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7380,
            "text_len": 36,
            "text_offset": 7386,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\nい、いや、これは遺書じゃ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7448,
            "text_len": 70,
            "text_offset": 7458,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "わかってる！　お前の気持ちはわかっている！\\nだから早まるんじゃない！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7558,
            "text_len": 30,
            "text_offset": 7564,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（わかってな――――い！！）\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7630,
            "text_len": 70,
            "text_offset": 7636,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（飛び降りなんかしないよ！\\n　というか原稿見られたら飛び降りるよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7728,
            "text_len": 12,
            "text_offset": 7738,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "長野原！！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7774,
            "text_len": 16,
            "text_offset": 7784,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "長野原さん！！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7822,
            "text_len": 14,
            "text_offset": 7828,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7868,
            "text_len": 64,
            "text_offset": 7874,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（原稿を見られるわけには……！\\n　だけど逃げるには無理が……）\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7968,
            "text_len": 56,
            "text_offset": 7974,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ハッ！？　そ、そうか！\\n　道はまだ１つだけある！！）\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8138,
            "text_len": 78,
            "text_offset": 8144,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あの垂れ幕……あれをカンフ―アクション\\n　みたいに裂きながら降りれば弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8242,
            "text_len": 74,
            "text_offset": 8248,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（そう、垂れ幕を抹消できて、脱出も出来る。\\n　……まさに一石二鳥だよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8440,
            "text_len": 24,
            "text_offset": 8446,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "うおおおおおおおお！！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 8510,
            "text_len": 12,
            "text_offset": 8530,
            "warnings": 0
        },
        "speaker": "桜井先生＆高崎先生",
        "text": "！！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8576,
            "text_len": 18,
            "text_offset": 8582,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え、絡まった！？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 8640,
            "text_len": 12,
            "text_offset": 8660,
            "warnings": 0
        },
        "speaker": "桜井先生＆高崎先生",
        "text": "宙づり！？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8786,
            "text_len": 38,
            "text_offset": 8810,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒１）",
        "text": "おい、あそこに誰かぶら下がってるぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8864,
            "text_len": 66,
            "text_offset": 8888,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒２）",
        "text": "本当だ、何してるんだ？\\nっていうか、あんな所に垂れ幕あったのか。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 8970,
            "text_len": 78,
            "text_offset": 8994,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒１）",
        "text": "そういやそうだな、今まで知らなかったけど。\\n……へぇ、誰が描いたんだろうな。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9092,
            "text_len": 66,
            "text_offset": 9098,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（い、一石二鳥どころか……\\n　恥ずかしさのダブルパンチだよ……）\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9226,
            "text_len": 58,
            "text_offset": 9232,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ま、まだ何か用が！？\\n　もう勘弁してほしいんだけど！）\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9320,
            "text_len": 48,
            "text_offset": 9330,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "長野原！　飛び降りなんて\\nバカなことはやめろ！\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9400,
            "text_len": 32,
            "text_offset": 9406,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "は、はい！？　と、飛び降り！？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9460,
            "text_len": 52,
            "text_offset": 9470,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだ！　こんなことで死ぬなんて\\n馬鹿げているぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9548,
            "text_len": 12,
            "text_offset": 9554,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9592,
            "text_len": 72,
            "text_offset": 9598,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（よくわからないけど、飛び降りるために\\n　屋上に来たって思われてる？）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9692,
            "text_len": 72,
            "text_offset": 9698,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ち、違います。飛び降りじゃなくて\\n無くした原稿を取りに来ただけですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9792,
            "text_len": 16,
            "text_offset": 9802,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "げ、原稿……？\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9840,
            "text_len": 38,
            "text_offset": 9846,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はい。\\nだから心配しないでください。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9908,
            "text_len": 30,
            "text_offset": 9918,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そ、そうだったんですか……。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9970,
            "text_len": 8,
            "text_offset": 9976,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10016,
            "text_len": 22,
            "text_offset": 10022,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "弌弌あっ！　原稿！！\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10068,
            "text_len": 56,
            "text_offset": 10078,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "おっと。危ないぞ長野原。\\nしっかり掴んでおかないと弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10170,
            "text_len": 10,
            "text_offset": 10180,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10232,
            "text_len": 10,
            "text_offset": 10238,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10278,
            "text_len": 12,
            "text_offset": 10288,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10322,
            "text_len": 30,
            "text_offset": 10332,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "わ―っ！　面白そうですね―！\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 14,
            "speaker_offset": 10470,
            "text_len": 10,
            "text_offset": 10486,
            "warnings": 0
        },
        "speaker": "みお＆高崎先生",
        "text": "！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10568,
            "text_len": 52,
            "text_offset": 10578,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ごめんなさい！\\n私が勝手なことしちゃったから……！\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10654,
            "text_len": 48,
            "text_offset": 10664,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井先生も反省してるんだ！\\n許してあげてくれ！\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10738,
            "text_len": 12,
            "text_offset": 10744,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10778,
            "text_len": 26,
            "text_offset": 10784,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……えっ？　何がですか？\\n",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10834,
            "text_len": 6,
            "text_offset": 10844,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "？？\\n",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10874,
            "text_len": 26,
            "text_offset": 10884,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "えっ？　飛び降りは……？\\n",
        "type": "dialog"
    },
    {
        "id": 105,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10932,
            "text_len": 32,
            "text_offset": 10938,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "飛び降り？　誰がするんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 106,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10992,
            "text_len": 8,
            "text_offset": 11002,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 107,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11034,
            "text_len": 8,
            "text_offset": 11044,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 108,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 11074,
            "text_len": 14,
            "text_offset": 11080,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………？？\\n",
        "type": "dialog"
    },
    {
        "id": 109,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11122,
            "text_len": 12,
            "text_offset": 11132,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 110,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11168,
            "text_len": 20,
            "text_offset": 11178,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "勘違い……ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 111,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 11220,
            "text_len": 16,
            "text_offset": 11226,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………多分。\\n",
        "type": "dialog"
    },
    {
        "id": 112,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11270,
            "text_len": 12,
            "text_offset": 11280,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 113,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11396,
            "text_len": 28,
            "text_offset": 11406,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "お騒がせ……しました……。\\n",
        "type": "dialog"
    }
]