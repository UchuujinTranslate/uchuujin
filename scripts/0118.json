[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2152,
            "text_len": 32,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "こんにちは……今日も一番かな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2216,
            "text_len": 60,
            "text_offset": 2222,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（すぐ来るだろうし、\\n　いつも通りにして待ってればいいか）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2306,
            "text_len": 46,
            "text_offset": 2312,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……あれ？\\nボ―ル、昨日片付け忘れてたっけ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2384,
            "text_len": 12,
            "text_offset": 2390,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2424,
            "text_len": 34,
            "text_offset": 2430,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ちょっとだけ、遊んでみようかな？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2492,
            "text_len": 10,
            "text_offset": 2498,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "えいっ。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2530,
            "text_len": 24,
            "text_offset": 2536,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ……意外と続くかも。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2576,
            "text_len": 28,
            "text_offset": 2582,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "９、１０、１１、１２……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2714,
            "text_len": 16,
            "text_offset": 2722,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こんにちは―。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2754,
            "text_len": 18,
            "text_offset": 2760,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、こんにちは。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2794,
            "text_len": 26,
            "text_offset": 2802,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あれ、関口さんそれ……。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2858,
            "text_len": 32,
            "text_offset": 2864,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、遊んでただけなので弌弌。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2924,
            "text_len": 26,
            "text_offset": 2932,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "すっごく上手いですよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2990,
            "text_len": 10,
            "text_offset": 2996,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "弌弌え？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3028,
            "text_len": 74,
            "text_offset": 3036,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ボ―ルコントロ―ルに才能を感じます！\\nもうちょっと見せてくれませんか！？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3142,
            "text_len": 22,
            "text_offset": 3148,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、私はその……。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3196,
            "text_len": 16,
            "text_offset": 3204,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "お願いします！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3242,
            "text_len": 22,
            "text_offset": 3248,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……少しだけですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3384,
            "text_len": 20,
            "text_offset": 3390,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "えい……っ、やっ！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3426,
            "text_len": 44,
            "text_offset": 3434,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うまいうまい。\\nやっぱり才能ありますって。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3494,
            "text_len": 44,
            "text_offset": 3500,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そうですか？\\n弌弌あっ、強く蹴りすぎ……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3584,
            "text_len": 12,
            "text_offset": 3592,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "キャン！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3640,
            "text_len": 10,
            "text_offset": 3654,
            "warnings": 0
        },
        "speaker": "関口＆桜井誠",
        "text": "あ……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3744,
            "text_len": 22,
            "text_offset": 3748,
            "warnings": 1
        },
        "speaker": "",
        "text": "ア、アンテナ破損！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3812,
            "text_len": 70,
            "text_offset": 3816,
            "warnings": 1
        },
        "speaker": "",
        "text": "おい、今のボ―ルは避けられただろ！？\\n何でちゃんと見てなかったんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3906,
            "text_len": 50,
            "text_offset": 3910,
            "warnings": 1
        },
        "speaker": "",
        "text": "え、だ、だって……\\n動物って視点が低いから……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3976,
            "text_len": 50,
            "text_offset": 3980,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あ、まさかお前、\\n視点の低さを利用して……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4050,
            "text_len": 58,
            "text_offset": 4054,
            "warnings": 1
        },
        "speaker": "",
        "text": "ち、違う！\\n見ないようにモニタ―から目を離してたら……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4128,
            "text_len": 22,
            "text_offset": 4132,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………純なやつめ。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4328,
            "text_len": 60,
            "text_offset": 4336,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うん、やっぱり上手い。\\n関口さん、本当に初心者なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4412,
            "text_len": 58,
            "text_offset": 4418,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "サッカ―ボ―ルなんて、\\n授業以外で触ったことないですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4492,
            "text_len": 48,
            "text_offset": 4500,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "それでこの実力弌弌\\n本物だ、本物の天才が……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 32,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お―っす、お、二人とも早いな。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4722,
            "text_len": 60,
            "text_offset": 4730,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長！　見てくださいよこれ！！\\n関口さん凄くないですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4814,
            "text_len": 60,
            "text_offset": 4820,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おおっ、確かにすげぇ！\\n関口リフティング上手いじゃないか！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4904,
            "text_len": 18,
            "text_offset": 4910,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……そうですか？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4954,
            "text_len": 34,
            "text_offset": 4960,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（褒められるのは、悪くないかも）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5020,
            "text_len": 48,
            "text_offset": 5026,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ただ、欠点を言うなら……。\\n見えてるんだよな。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5106,
            "text_len": 12,
            "text_offset": 5112,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5204,
            "text_len": 22,
            "text_offset": 5210,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（み、見られた！？）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5254,
            "text_len": 20,
            "text_offset": 5262,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長っ、危ない！！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5336,
            "text_len": 12,
            "text_offset": 5342,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "がはっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5428,
            "text_len": 30,
            "text_offset": 5436,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長っ、大丈夫ですか部長！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5490,
            "text_len": 46,
            "text_offset": 5496,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……す、すみません。\\n足が滑っちゃって……。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5566,
            "text_len": 38,
            "text_offset": 5572,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いたたたた……関口、ナイスシュ―ト。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5644,
            "text_len": 22,
            "text_offset": 5652,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "弌弌ぶ、部長これ！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5704,
            "text_len": 56,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うん？　こ、これは！？\\nボ―ルの上に碁石が並んでる！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5798,
            "text_len": 70,
            "text_offset": 5806,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "これは伝説の大技、九連星弌弌\\nそう、紅白碁石合戦（オ―ルスタ―）っ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5910,
            "text_len": 26,
            "text_offset": 5916,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そんな技、あるんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5968,
            "text_len": 74,
            "text_offset": 5976,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こんなのプロでも滅多に見られませんよ！\\n関口さん、本当に初心者ですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6072,
            "text_len": 16,
            "text_offset": 6078,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "本当ですって。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6116,
            "text_len": 24,
            "text_offset": 6124,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "凄い才能だ……部長っ！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6178,
            "text_len": 66,
            "text_offset": 6184,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おう！　関口、今日からお前も練習な！\\n一緒に全国を目指そうぜ！！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6272,
            "text_len": 10,
            "text_offset": 6278,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "嫌です。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6314,
            "text_len": 52,
            "text_offset": 6320,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そう言うなって！\\nよ―し、早速リフティングからだ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6402,
            "text_len": 34,
            "text_offset": 6410,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "関口さん、一緒に頑張りましょう！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6562,
            "text_len": 12,
            "text_offset": 6568,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6636,
            "text_len": 16,
            "text_offset": 6642,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "っっっ！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6718,
            "text_len": 12,
            "text_offset": 6724,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "んげッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6806,
            "text_len": 14,
            "text_offset": 6814,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ぶ、部長！？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6858,
            "text_len": 12,
            "text_offset": 6864,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……ネジ？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6910,
            "text_len": 28,
            "text_offset": 6918,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "しっかりしてください部長！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7010,
            "text_len": 22,
            "text_offset": 7016,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ハラホロヒレハレ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7060,
            "text_len": 34,
            "text_offset": 7066,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……部長、しっかりしてください。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7122,
            "text_len": 52,
            "text_offset": 7128,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん、ああ……いたたた。\\nあれ？　今どうなったんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7202,
            "text_len": 52,
            "text_offset": 7210,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "どこからかネジが飛んできて\\n部長に直撃したんです。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7284,
            "text_len": 60,
            "text_offset": 7292,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長ってばついてないですよね。\\n部室に来ていきなりですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7382,
            "text_len": 6,
            "text_offset": 7388,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7422,
            "text_len": 70,
            "text_offset": 7428,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あれ……？　あ―、そっか。ドア開けて\\nいきなりネジが飛んできたのか。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7520,
            "text_len": 56,
            "text_offset": 7528,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そうですよ！\\n来ていきなり！　オレたちが挨拶してすぐ！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7606,
            "text_len": 24,
            "text_offset": 7612,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "だよな。いたたた……。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7660,
            "text_len": 26,
            "text_offset": 7666,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ハンカチ濡らしてきます。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7714,
            "text_len": 20,
            "text_offset": 7720,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "サンキュ―、関口。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7848,
            "text_len": 66,
            "text_offset": 7854,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（……何だかわからないけど、\\n　ありがとうネジ。それと桜井くん）\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7974,
            "text_len": 52,
            "text_offset": 7982,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ぶ、部長！\\n流石にそれはちょっとデリカシ―が……。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8060,
            "text_len": 14,
            "text_offset": 8066,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いいですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8116,
            "text_len": 6,
            "text_offset": 8124,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8152,
            "text_len": 30,
            "text_offset": 8158,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "どうせ体操着下に着てますし。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8224,
            "text_len": 12,
            "text_offset": 8230,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "な―んだ。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8268,
            "text_len": 38,
            "text_offset": 8276,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いや、でもやっぱり気になりませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8336,
            "text_len": 16,
            "text_offset": 8342,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "それほどでも。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8384,
            "text_len": 52,
            "text_offset": 8390,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口ってそんな感じだもんな。\\nク―ルっていうかさ。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8466,
            "text_len": 14,
            "text_offset": 8474,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "う―ん……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8516,
            "text_len": 44,
            "text_offset": 8522,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "でも、気にしないのと怒らないのは別なので。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8598,
            "text_len": 6,
            "text_offset": 8604,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8652,
            "text_len": 28,
            "text_offset": 8658,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "二人とも正座してください。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8712,
            "text_len": 18,
            "text_offset": 8718,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "えぇ―――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8764,
            "text_len": 16,
            "text_offset": 8772,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え、オレも！？\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8810,
            "text_len": 8,
            "text_offset": 8816,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "正座。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8850,
            "text_len": 12,
            "text_offset": 8856,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8894,
            "text_len": 12,
            "text_offset": 8902,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
