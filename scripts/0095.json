[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 6,
            "text_offset": 2146,
            "warnings": 1
        },
        "speaker": "みさと",
        "text": "〜\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2200,
            "text_len": 12,
            "text_offset": 2208,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "きゃっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2242,
            "text_len": 46,
            "text_offset": 2250,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "いったた……\\nちょっと、どこ見て歩いてるの？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2318,
            "text_len": 38,
            "text_offset": 2326,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、す、すいません！\\n大丈夫ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2384,
            "text_len": 60,
            "text_offset": 2392,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（あ……このモヒカン……\\n　確か、ウェボシ―の好きな……）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2474,
            "text_len": 34,
            "text_offset": 2482,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ホントすいません。\\n立てますか？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2542,
            "text_len": 24,
            "text_offset": 2550,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、ありが……と……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2596,
            "text_len": 82,
            "text_offset": 2604,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "怪我は……ないみたいですね。良かった。\\nあ〜教科書が散らばって……すぐ拾います！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2708,
            "text_len": 16,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "う、うん……。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2758,
            "text_len": 66,
            "text_offset": 2766,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（な、なによこいつ！？\\n　モヒカンな不良のくせに優しいじゃない）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2854,
            "text_len": 40,
            "text_offset": 2862,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ふう、これでよし、と。\\nはい、どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2924,
            "text_len": 26,
            "text_offset": 2932,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、うん、ありがと……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2994,
            "text_len": 68,
            "text_offset": 3002,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（あ、でも前にフェっちゃんが\\n　花壇に水やってたって言ってたっけ）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3092,
            "text_len": 46,
            "text_offset": 3100,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "？　なんですか？\\n僕の顔に何かついてますか？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3172,
            "text_len": 78,
            "text_offset": 3180,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（こんな髪型してるけど、もしかしてこの人、\\n　普通に良いヤツだったり……？）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3298,
            "text_len": 24,
            "text_offset": 3310,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "みさと？　何してんの？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3360,
            "text_len": 22,
            "text_offset": 3368,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！？　ウェボシ―！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3412,
            "text_len": 28,
            "text_offset": 3424,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "あれ、そこにいるのは……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3474,
            "text_len": 40,
            "text_offset": 3482,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あっ、違うの！\\nこれは違うのよ―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3572,
            "text_len": 8,
            "text_offset": 3580,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "ッ！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3612,
            "text_len": 50,
            "text_offset": 3620,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あっ！？\\nご、ごめんねワンちゃん！　じゃあね―！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3714,
            "text_len": 32,
            "text_offset": 3726,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "……なんなのよ、みさとったら。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3836,
            "text_len": 26,
            "text_offset": 3840,
            "warnings": 1
        },
        "speaker": "",
        "text": "おっと！　犬は大丈夫か？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3890,
            "text_len": 46,
            "text_offset": 3894,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、犬なら、ほんのちょっとかすっただけさ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3962,
            "text_len": 12,
            "text_offset": 3966,
            "warnings": 1
        },
        "speaker": "",
        "text": "……なあ？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4000,
            "text_len": 16,
            "text_offset": 4004,
            "warnings": 1
        },
        "speaker": "",
        "text": "ん？　なんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4042,
            "text_len": 28,
            "text_offset": 4046,
            "warnings": 1
        },
        "speaker": "",
        "text": "ちょっと思ったんだけどよ。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4096,
            "text_len": 20,
            "text_offset": 4100,
            "warnings": 1
        },
        "speaker": "",
        "text": "だから、何をだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4162,
            "text_len": 22,
            "text_offset": 4166,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナ、弱すぎね？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4292,
            "text_len": 60,
            "text_offset": 4298,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ん？　立花みさとではないか。\\nこんなところで何をしている？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4374,
            "text_len": 26,
            "text_offset": 4382,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "さ、さ、さ、さ、笹原！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4430,
            "text_len": 42,
            "text_offset": 4436,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む？　そこの者は立花みさとの知り合いか？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4504,
            "text_len": 14,
            "text_offset": 4512,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、僕は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4546,
            "text_len": 6,
            "text_offset": 4554,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4674,
            "text_len": 6,
            "text_offset": 4680,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4722,
            "text_len": 40,
            "text_offset": 4730,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ち、違うの笹原！！\\n何でもないのよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4792,
            "text_len": 86,
            "text_offset": 4798,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……立花みさとの知己ならば、私にとっても\\n竹馬の友だ。ぜひ紹介してほしかったのだが。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4910,
            "text_len": 42,
            "text_offset": 4918,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "だから、ホントに何でもないんだってば！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4982,
            "text_len": 62,
            "text_offset": 4988,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そうか……なら良いのだが、\\n先程から何を恥ずかしがっている？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5072,
            "text_len": 16,
            "text_offset": 5080,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "べ、別に……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5118,
            "text_len": 28,
            "text_offset": 5124,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……む、いや、これは失敬。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5174,
            "text_len": 8,
            "text_offset": 5182,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5212,
            "text_len": 72,
            "text_offset": 5218,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そうか、私に紹介するのが恥ずかしいほど\\n抜き差しならない間柄なのだな。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5318,
            "text_len": 6,
            "text_offset": 5326,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5364,
            "text_len": 68,
            "text_offset": 5370,
            "warnings": 1
        },
        "speaker": "笹原",
        "text": "気が利かずすまなかったな。\\nでは、邪魔者は早に立ち去るとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5460,
            "text_len": 20,
            "text_offset": 5468,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "な、な、な……！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5596,
            "text_len": 38,
            "text_offset": 5602,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……立花みさとよ、私が一体何をした。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5678,
            "text_len": 34,
            "text_offset": 5686,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なんですぐに諦めようとするのよ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5746,
            "text_len": 12,
            "text_offset": 5752,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5790,
            "text_len": 66,
            "text_offset": 5798,
            "warnings": 1
        },
        "speaker": "みさと",
        "text": "怒るとか、慌てるとか、嫉妬するとか、\\n色やることはあるでしょ！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5886,
            "text_len": 52,
            "text_offset": 5894,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なによ！　いつもは\\n人に迷惑ばかりかけてるくせに！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5972,
            "text_len": 50,
            "text_offset": 5980,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なんでこんな時に限って気ぃ利かせたり\\nすんのよ！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6052,
            "text_len": 56,
            "text_offset": 6058,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……良くわからんが、\\n気を利かせたのがまずかったのか。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6136,
            "text_len": 50,
            "text_offset": 6142,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "では気の利かないことを……\\nといっても難しいな。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6214,
            "text_len": 36,
            "text_offset": 6220,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、これはなかなかに深い問題だ。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6278,
            "text_len": 58,
            "text_offset": 6284,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "あえて空気を読まぬ態度というのは\\nどのようなものだろう。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6364,
            "text_len": 38,
            "text_offset": 6372,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "だから、怒ったり、慌てたり、嫉妬弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6442,
            "text_len": 28,
            "text_offset": 6450,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "〜〜〜〜〜〜〜〜〜〜〜！！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6540,
            "text_len": 12,
            "text_offset": 6546,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6594,
            "text_len": 76,
            "text_offset": 6602,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なによ！　私は別に、あんたがどう思ってるか\\nなんて気にしないんだからね！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6724,
            "text_len": 12,
            "text_offset": 6730,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6764,
            "text_len": 26,
            "text_offset": 6770,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……待つのだ立花みさ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6906,
            "text_len": 12,
            "text_offset": 6912,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6986,
            "text_len": 46,
            "text_offset": 6992,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……はて、\\nところで件の１年は何処へ行った？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7060,
            "text_len": 18,
            "text_offset": 7068,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え？　あ、あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7110,
            "text_len": 54,
            "text_offset": 7116,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "高崎先生―！\\n中之条くんが２階から飛び降りました―！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7212,
            "text_len": 12,
            "text_offset": 7220,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7248,
            "text_len": 70,
            "text_offset": 7258,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "中之条は飛び降りてなどいない！\\nそうだ、スカイダイビングかもしれん！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7366,
            "text_len": 12,
            "text_offset": 7372,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7408,
            "text_len": 14,
            "text_offset": 7416,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "逃げなきゃ！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7452,
            "text_len": 50,
            "text_offset": 7458,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "いやいや立花みさとよ。\\nそれは良くないであろう。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7530,
            "text_len": 50,
            "text_offset": 7536,
            "warnings": 1
        },
        "speaker": "笹原",
        "text": "きちんと現場へはせ参じ、\\n堂と釈明するべきだ。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7608,
            "text_len": 58,
            "text_offset": 7614,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "私にも責任の一端はあるからな。\\n一緒について行ってやる。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7704,
            "text_len": 60,
            "text_offset": 7712,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原……うぅ……\\nあ、ありがとうなんて言わないんだからね！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7794,
            "text_len": 28,
            "text_offset": 7800,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "はっはっは。さて行こうか。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7950,
            "text_len": 54,
            "text_offset": 7956,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "この者は自分で飛び降りた。\\n私と立花みさとが証人だ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8034,
            "text_len": 26,
            "text_offset": 8042,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（ええええ〜〜〜〜！？）\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8130,
            "text_len": 22,
            "text_offset": 8136,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……なにか用事でも？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8184,
            "text_len": 12,
            "text_offset": 8192,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8260,
            "text_len": 46,
            "text_offset": 8266,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\nそれはどういう意味なのだ、立花みさ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8376,
            "text_len": 12,
            "text_offset": 8382,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8426,
            "text_len": 12,
            "text_offset": 8434,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8468,
            "text_len": 64,
            "text_offset": 8474,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふ、さすがは立花みさと。\\nクマの被り物で攻撃とは、なかなか弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8598,
            "text_len": 20,
            "text_offset": 8604,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "なか……なか……。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8646,
            "text_len": 16,
            "text_offset": 8654,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8692,
            "text_len": 44,
            "text_offset": 8698,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "まあ良い。\\nそれでは私は、これにて失敬弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8806,
            "text_len": 12,
            "text_offset": 8812,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8850,
            "text_len": 12,
            "text_offset": 8858,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8892,
            "text_len": 70,
            "text_offset": 8898,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "とりあえず、立花みさとよ。\\nその頭のクマは外してもらえないだろうか。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8994,
            "text_len": 12,
            "text_offset": 9002,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9142,
            "text_len": 26,
            "text_offset": 9148,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……世の中、ままならぬ。\\n",
        "type": "dialog"
    }
]