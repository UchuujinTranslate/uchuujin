[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 80,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ふう……昼食がすっかり遅くなってしまった。\\nさて、もうひと頑張りするとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2256,
            "text_len": 86,
            "text_offset": 2266,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "しかし相生はまた宿題をやってこなかったが、\\nどうすれば反省してくれるんだろうか……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2374,
            "text_len": 34,
            "text_offset": 2384,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "やはり一度じっくりと説教を……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2440,
            "text_len": 20,
            "text_offset": 2446,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あっ、高崎先生！！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2492,
            "text_len": 10,
            "text_offset": 2502,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "安中！？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2538,
            "text_len": 20,
            "text_offset": 2544,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "大変なんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2586,
            "text_len": 32,
            "text_offset": 2596,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ということは、また中之条か！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2668,
            "text_len": 46,
            "text_offset": 2674,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "それが今回は桜井先生が行方不明なんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2752,
            "text_len": 20,
            "text_offset": 2762,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2818,
            "text_len": 28,
            "text_offset": 2824,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "すぐ来て欲しいんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2884,
            "text_len": 26,
            "text_offset": 2894,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "わかった、すぐに行くぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2962,
            "text_len": 38,
            "text_offset": 2968,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あっ、先生そっちじゃないんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3028,
            "text_len": 44,
            "text_offset": 3034,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "っていうかそっちには猫がいるんですけど！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3200,
            "text_len": 16,
            "text_offset": 3210,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うおぉぉっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3304,
            "text_len": 42,
            "text_offset": 3308,
            "warnings": 1
        },
        "speaker": "",
        "text": "なあ、いいニュ―スと悪いニュ―スがある。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3374,
            "text_len": 28,
            "text_offset": 3378,
            "warnings": 1
        },
        "speaker": "",
        "text": "……いいニュ―スから頼む。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3428,
            "text_len": 30,
            "text_offset": 3432,
            "warnings": 1
        },
        "speaker": "",
        "text": "猫はバリアのおかげで無事だ。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3484,
            "text_len": 22,
            "text_offset": 3488,
            "warnings": 1
        },
        "speaker": "",
        "text": "……悪いニュ―スは？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3532,
            "text_len": 30,
            "text_offset": 3536,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナを守るのを忘れてた。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3608,
            "text_len": 16,
            "text_offset": 3612,
            "warnings": 1
        },
        "speaker": "",
        "text": "……なるほど。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3720,
            "text_len": 60,
            "text_offset": 3730,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "わかった！　桜井先生の危機と聞いたら\\nこうしてはいられん！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3812,
            "text_len": 16,
            "text_offset": 3822,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "行くぞ安中！！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3860,
            "text_len": 14,
            "text_offset": 3866,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "あっ、先生！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3904,
            "text_len": 14,
            "text_offset": 3914,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何だ安中！！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3950,
            "text_len": 44,
            "text_offset": 3956,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "桜井先生の居場所に心当たりないんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4032,
            "text_len": 8,
            "text_offset": 4042,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4082,
            "text_len": 32,
            "text_offset": 4088,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "高崎先生、知ってるんですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4176,
            "text_len": 12,
            "text_offset": 4186,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4224,
            "text_len": 20,
            "text_offset": 4234,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "いいか、よく聞け。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4276,
            "text_len": 8,
            "text_offset": 4282,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "はい！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4320,
            "text_len": 32,
            "text_offset": 4330,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井先生の居場所は……知らん！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4394,
            "text_len": 20,
            "text_offset": 4400,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4456,
            "text_len": 32,
            "text_offset": 4466,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだ！！　ダウジングしろ！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4520,
            "text_len": 16,
            "text_offset": 4526,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "やってみます。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4568,
            "text_len": 20,
            "text_offset": 4578,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4634,
            "text_len": 14,
            "text_offset": 4644,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ま、待て！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4684,
            "text_len": 20,
            "text_offset": 4690,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4742,
            "text_len": 30,
            "text_offset": 4752,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "手がかりが無いからって何だ！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4808,
            "text_len": 76,
            "text_offset": 4818,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "教育者として人が行方不明なら何が何でも探す！\\nそれが……その、教育者だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4916,
            "text_len": 44,
            "text_offset": 4922,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "でも、何か今桜井先生が歩いてたんですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4998,
            "text_len": 20,
            "text_offset": 5008,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ――――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5060,
            "text_len": 42,
            "text_offset": 5066,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "私の勘違い……だったみたいなんですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5134,
            "text_len": 12,
            "text_offset": 5144,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5182,
            "text_len": 12,
            "text_offset": 5188,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5304,
            "text_len": 28,
            "text_offset": 5314,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "安中、職員室まで来なさい。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5358,
            "text_len": 8,
            "text_offset": 5364,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ハイ。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5430,
            "text_len": 58,
            "text_offset": 5440,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ヒント無しか……。\\nこれは地道に聞き込むしか……そうだ！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5530,
            "text_len": 56,
            "text_offset": 5540,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "安中、中之条を呼んできてくれ！\\n失价といえばアイツだ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5622,
            "text_len": 16,
            "text_offset": 5628,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "いってきます！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5772,
            "text_len": 16,
            "text_offset": 5780,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "何ですか先生。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5818,
            "text_len": 68,
            "text_offset": 5828,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "中之条、桜井先生が失价した。\\nお前、居場所を知ってるんじゃないか？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5918,
            "text_len": 28,
            "text_offset": 5926,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、ハイ。知ってますけど。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5994,
            "text_len": 60,
            "text_offset": 6004,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ええ―――っ！！？\\n　勢いで言ったのに知ってるのかよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6086,
            "text_len": 36,
            "text_offset": 6094,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "今こっちに来る時、すれ違いました。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6158,
            "text_len": 6,
            "text_offset": 6168,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6198,
            "text_len": 36,
            "text_offset": 6204,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "……あ、そういえば見たような気が。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6272,
            "text_len": 52,
            "text_offset": 6278,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ご、ごめんなさいっ！\\n私の勘違いだったみたいです！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6352,
            "text_len": 22,
            "text_offset": 6362,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……気にするな安中。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6492,
            "text_len": 80,
            "text_offset": 6502,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（そうさ、安中も悪気があったわけじゃない。\\n　むしろ桜井先生を心配した結果だ）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6602,
            "text_len": 58,
            "text_offset": 6612,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（それを怒れるはずがない。\\n　それが教師ってもんだろ？）\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6784,
            "text_len": 38,
            "text_offset": 6792,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……あの―、呼ばれたのってそれだけ？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6878,
            "text_len": 50,
            "text_offset": 6888,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……知らん！！\\nだが、１つだけわかることがある。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6960,
            "text_len": 16,
            "text_offset": 6966,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "な、何ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7010,
            "text_len": 40,
            "text_offset": 7020,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井先生の居場所を見つける手がかりだ。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7082,
            "text_len": 58,
            "text_offset": 7092,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "今日の昼休み、\\nオレは桜井先生と話をしていたんだが……。\\n",
