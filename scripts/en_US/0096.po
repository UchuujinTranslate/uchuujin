#  speaker 
#  warnings: 0 
#: sc/0096:0 
msgid "中村先生"
msgstr "Nakamura-sensei"

#  text 
#  warnings: 0 
#: sc/0096:0 
msgid "う―む。東雲なのの捕獲……\n一体どうしたらできるのだろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:1 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:1 
msgid "今まで試した方法はダメだ。\nそれ以外で何か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:2 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:2 
msgid "おっと。ポケットから５円が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:3 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:3 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0096:4 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:4 
msgid "そうだ！　催眠術だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:5 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:5 
msgid "５円に紐を通して……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:6 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:6 
msgid "よし。後は５円を揺らしながら、\nあなたはだんだん眠くなる……か？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:7 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0096:7 
msgid "失礼しま―す。\n"
msgstr "Excuse me—.\n"

#  speaker 
#  warnings: 0 
#: sc/0096:8 
msgid "田中"
msgstr "Tanaka"

#  text 
#  warnings: 0 
#: sc/0096:8 
msgid "せんせ―。\n授業で使った道具、返しに来たよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:9 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:9 
msgid "ああ、その辺に置いてくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:10 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:10 
msgid "は―い。\n"
msgstr "Okay.\n"

#  speaker 
#  warnings: 0 
#: sc/0096:11 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:11 
msgid "（そうだ、東雲に試す前に\n　こいつらにやってみるか）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:12 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:12 
msgid "二人とも、すまない。\n少しだけ実験に付き合ってもらえるか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:13 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:13 
msgid "実験ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:14 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:14 
msgid "いいよ―。何やるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:15 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:15 
msgid "催眠術だ。行くぞ。\nこの５円玉をじっくり見ていろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:16 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:16 
msgid "あなたはだんだん眠くな―る―。\n眠くな―る―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:17 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:17 
msgid "あはは。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:18 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:18 
msgid "よくテレビで見るやつだね―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:19 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:19 
msgid "だんだん眠くな―る―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:20 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:20 
msgid "あれ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:21 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:21 
msgid "なんか、眠く……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:22 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:22 
msgid "眠くな―る！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:23 
msgid "中之条"
msgstr "Nakanojou"

#  text 
#  warnings: 0 
#: sc/0096:23 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:24 
msgid "田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:24 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:25 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:25 
msgid "！？　や、やった！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:26 
msgid "校長先生"
msgstr "Principal"

#  text 
#  warnings: 0 
#: sc/0096:26 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:27 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:27 
msgid "うわっ！　こ、校長先生！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:28 
msgid "校長先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:28 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:29 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:29 
msgid "……校長？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:30 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:30 
msgid "……眠ってる。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:31 
msgid "なにぃ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:32 
msgid "……一体、何がおこったんだ！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:33 
msgid "わからん！　急にオッサンが倒れて……\nその衝撃でアンテナも壊れたんだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:34 
msgid "しかし、なぜ急に？\nま、まさか……魔法？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:35 
msgid "バ、バカ言うな！\nそんなものがあるわけ……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:36 
msgid "だが、実際にオッサンは倒れた。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0096:37 
msgid "ｎｇ、マジカル！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:38 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:38 
msgid "１発で成功するとは思わなかったが……\nこの分なら本番も大丈夫そうだな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:39 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:39 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:40 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:40 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:41 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:41 
msgid "ちょっと遊んでみるか。\nえ―っと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:42 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:42 
msgid "二人の髪型が入れ替わ―る―。\n……なんてな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:43 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:43 
msgid "うおおっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:44 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:44 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:45 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:45 
msgid "か、髪型が入れ替わった……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:46 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:46 
msgid "催眠術だからそういう風に思い込むくらいだと\n思っていたのだが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:47 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:47 
msgid "いや、それより……\nもしかして私、催眠術の能力が？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:48 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:48 
msgid "フ、フフフ……すごいぞ私。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:49 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:49 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:50 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:50 
msgid "……すごいが、喜ぶのは後だな。\nまずはこの二人の髪を何とかしないと……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:51 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:51 
msgid "まずはこいつから。\n田中の髪がアフロにな―る―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:52 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:52 
msgid "よし。次は\n中之条の髪がモヒカンにな―る―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:53 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:53 
msgid "うわっ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:54 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:54 
msgid "えっ？　アフロがデカくなった？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:55 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:55 
msgid "うぅ……やってみたい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:56 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:56 
msgid "……いや、ダメだ。\nやったら戻れなくなるかも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:57 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:57 
msgid "だ、だが、やってみたい……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:58 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:59 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:59 
msgid "……た、田中のアフロがデカくな―る―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:60 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:60 
msgid "おお！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:61 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:61 
msgid "田中のアフロが\n更に更にデカくな―る―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:62 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:62 
msgid "うわっ！？\n"
msgstr "Uwah!?\n"

#  speaker 
#  warnings: 0 
#: sc/0096:63 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:63 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:64 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:64 
msgid "……キモっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:65 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:65 
msgid "まあ、戻すのは簡単だ。\n二人の髪型がだんだん元に戻っていく―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:66 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:66 
msgid "よし。これでいい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:67 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:67 
msgid "さて、練習はこれで終わりにして\n東雲を捕まえに行くか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:68 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:68 
msgid "っと、二人とも。起きていいぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:69 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:69 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:70 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:70 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:71 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:71 
msgid "……あれ？　もしかして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:72 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:72 
msgid "中之条、田中、起きろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:73 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:73 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:74 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:74 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:75 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:75 
msgid "催眠が解けない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:76 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:77 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:77 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:78 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:78 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0096:79 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:79 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:80 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:80 
msgid "……ま、いっか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:81 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:81 
msgid "それより、本屋に行って\n催眠術の本を買い漁ってこよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:82 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:82 
msgid "……ｙｙＹ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0096:83 
msgid "中之条＆田中"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:83 
msgid "……ｙｙＹ\n"
msgstr ""
