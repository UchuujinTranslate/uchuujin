msgstr ""
"No, but rather……\n"
"What kind of hypnosis powers do I possess?\n"

#  speaker 
#  warnings: 0 
#: sc/0096:48 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:48 
