msgstr ""
"It's hypnosis. Alright, here we go.\n"
"Gaze at this swinging five yen coin.\n"

#  speaker 
#  warnings: 0 
#: sc/0096:16 
msgid "中村先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0096:16 
