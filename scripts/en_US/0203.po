#  speaker 
#  warnings: 0 
#: sc/0203:0 
msgid "イタコ"
msgstr "Itako"

#  text 
#  warnings: 0 
#: sc/0203:0 
msgid "……おや、もうこんな時間かいな。\nそろそろ戻らんとなあ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:1 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:1 
msgid "だが、ここから恐山までは遠い。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:2 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:2 
msgid "どうやったら楽して帰れるか……むう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:3 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:3 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0203:4 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:4 
msgid "とりあえず土産を買おう。\nそれから考えるとするか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:5 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:5 
msgid "さて、何がええかの。\nできりゃ食べ物のほうがいいんじゃが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:6 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:6 
msgid "……ん？　あそこで売っとるのは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:7 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0203:7 
msgid "甘いよ―、安いよ―、白いよ―。大福だよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:8 
msgid "？？？（みお）"
msgstr "??? (Mio)"

#  text 
#  warnings: 0 
#: sc/0203:8 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:9 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:9 
msgid "ほう……大福か。\nそういや最近とんと食べてないの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:10 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:10 
msgid "……しかし、さっきから踊っている\nあの変な着ぐるみは何じゃろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:11 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:11 
msgid "……まあええ。\nとりあえず一包み買って帰るとしようか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:12 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:12 
msgid "いらっしゃいませ―。\nおいくつ包みましょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:13 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:13 
msgid "そうじゃのう……５個は少ないし……\nかといって１０個は多すぎるし……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:14 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0203:14 
msgid "長野原さん、交代の時間だよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:15 
msgid "？？？（みお）"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:15 
msgid "ほんと！？　ありがと！　もう限界だったんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:16 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0203:16 
msgid "ふう……あ―、暑かった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:17 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:17 
msgid "ごめんね。向こうで休んでいいよ。\n冷たいお茶があるからさ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:18 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:18 
msgid "あ……あいつは料金を踏み倒した小僧……！\nここで会ったが百年目っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:19 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:19 
msgid "これ、そこのお兄さんっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:20 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:20 
msgid "うああああああああああっ！？\nイ、イ、イタコのお婆さん！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:21 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:21 
msgid "料金。１万円です。\n延滞料金と出張料金で３万円となります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:22 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:22 
msgid "ちょっ！？　高すぎますよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:23 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:23 
msgid "第一、マイクテスとか勘弁してくださいとか、\n前のあれは絶対に信じられません！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:24 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:24 
msgid "しかし現実……真実です。\nそして料金も弌弌払ってください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:25 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:25 
msgid "で、でも弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:26 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:26 
msgid "僕は弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:27 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:27 
msgid "断りますっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:28 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:28 
msgid "くっ、逃げられたか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:29 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:29 
msgid "……しかし、私も恐山のイタコ！\n何度もおめおめと逃がしてなるものか弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:30 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:30 
msgid "はあ……逃げられたか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:31 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:31 
msgid "どうしました、お婆さん。逃げられたって……\nまさかひったくりですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:32 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:32 
msgid "あ、いや、料金が……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:33 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:33 
msgid "………………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:34 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:34 
msgid "そうです、ひったくりです。\nでも、車なら追いつけるはずです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:35 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:35 
msgid "車なら……分かりました、パトカ―を回します。\nお婆さん、いっしょに乗ってもらえますか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:36 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:36 
msgid "ええ、もちろん。\nそれで犯人が逃げた先なのですが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:37 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:37 
msgid "どちらですか？\n駅の方ですか？　川のほうですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:38 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:38 
msgid "いいえ、恐山です。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:39 
msgid "警察"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:39 
msgid "了解！　恐山まで弌弌って、恐山！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0203:40 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:40 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0203:41 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0203:41 
msgid "恐山です。\n"
msgstr ""
