#  speaker 
#  warnings: 0 
#: sc/0198:0 
msgid "きよし"
msgstr "Kiyoshi"

#  text 
#  warnings: 0 
#: sc/0198:0 
msgid "ただいま〜……。\n"
msgstr "I'm home～…….\n"

#  speaker 
#  warnings: 0 
#: sc/0198:1 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:1 
msgid "はぁ……疲れた……\nバイトってこんなに大変なんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:2 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:2 
msgid "もう動けないよ……\nおじいちゃんたちを捜しにも行けない……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:3 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:3 
msgid "おお、おかえり。\n"
msgstr "Aah, welcome home.\n"

#  speaker 
#  warnings: 0 
#: sc/0198:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:4 
msgid "うん、おじいちゃん。\nただいま―。\n"
msgstr ""
"Yeah, grandpa.\n"
"I'm home—.\n"

#  speaker 
#  warnings: 0 
#: sc/0198:5 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:5 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0198:6 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:6 
msgid "あれ！？　おじいちゃん！？\n"
msgstr "What!? Grandpa!?\n"

#  speaker 
#  warnings: 0 
#: sc/0198:7 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0198:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:8 
msgid "コバディ"
msgstr "Lil' Buddy"

#  text 
#  warnings: 0 
#: sc/0198:8 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:9 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:9 
msgid "バディ！　コバディ！　お前たちも！\n帰ってたのか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:10 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:10 
msgid "夕方には帰っておったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:11 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:11 
msgid "そんな……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:12 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:12 
msgid "僕、おじいちゃんたちを捜しに行って、\nこんなに疲れるまで働いて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:13 
msgid "ジェントルマン"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:13 
msgid "…………？\n"
msgstr "…………?\n"

#  speaker 
#  warnings: 0 
#: sc/0198:14 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:14 
msgid "（あっ、バイトのことを言ったら\n　怒られるかも）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:15 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:15 
msgid "えっと、なんでもないよ。\nそれよりみんなが無事で良かった。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:16 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:16 
msgid "（いっぱい心配して、街を走り回って、\n　バイトでくたくたになるまで働いて……）\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0198:17 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0198:17 
msgid "（色あったけど、でも、みんな無事だから\n　結果的にこれで良かっ弌弌）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:18 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:18 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0198:19 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:19 
msgid "（良かったんだ。良かったんだけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:20 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:20 
msgid "（バディたちの心配をして、おじいちゃんの\n　心配もして、バイトすることになって……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:21 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:21 
msgid "（何だか、今日一番損しているのは\n　僕だった気がする……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:22 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:22 
msgid "（バディたちはおじいちゃんに任せて\n　遊びに行けばよかったんじゃ……！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:23 
msgid "ジェントルマン"
msgstr "Gentleman"

#  text 
#  warnings: 0 
#: sc/0198:23 
msgid "……どうしたんじゃ？　かずし？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0198:24 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0198:24 
msgid "きよしだよ！！\n"
msgstr "It's Kiyoshi\n"
