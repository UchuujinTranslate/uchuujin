#  speaker 
#  warnings: 0 
#: sc/0022:0 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:0 
msgid "あれ？　あの後姿は……。\n"
msgstr "Wait, what? Who's that…….\n"

#  speaker 
#  warnings: 0 
#: sc/0022:1 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:1 
msgid "笹原じゃない。\nあいつ、こんなところで何してんのよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:2 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:2 
msgid "まさか、\nまたはた迷惑なことしてるんじゃ弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:3 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:3 
msgid "外で食事！！\nしかもあれは……カニ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:4 
msgid "笹原"
msgstr "Sasahara"

#  text 
#  warnings: 0 
#: sc/0022:4 
msgid "バリッ！　ベキッ！　ホジホジ……\nパクリ……モグモグ……ゴクン。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:5 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:5 
msgid "うむ、美味。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:6 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:6 
msgid "笹原！　あんた、\nこんなところで何やってんのよ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:7 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:7 
msgid "？　私は一人静かに食事をしていただけだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:8 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:8 
msgid "なにが静かによ！\nカニ解体する音がまわりに響いてるわよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:9 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:9 
msgid "……そうであったか。\nそれはすまないことをした。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:10 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:10 
msgid "（カニ三昧フルコ―スが\n　もれなく焼きガニになってしまった）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:11 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0022:11 
msgid "お、カニか！　美味そうだな。\n一度食ってみたかったんだよな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:12 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:12 
msgid "……え？\n"
msgstr "……Eh?\n"

#  speaker 
#  warnings: 0 
#: sc/0022:13 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:13 
msgid "むっ？\n"
msgstr "Hmph?\n"

#  speaker 
#  warnings: 0 
#: sc/0022:14 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:14 
msgid "（し、しまった！\n　つい匂いにつられて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:15 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:15 
msgid "こやつ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:16 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:16 
msgid "に、にゃ〜。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:17 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:17 
msgid "ちょ、ちょっと笹原……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:18 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:18 
msgid "猫の分際で大儀である。\n一杯進ぜよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:19 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:19 
msgid "（え、喋ったことはスル―！？\n　……ま、いいや、ラッキ―！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:20 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:20 
msgid "やめなさいよ！\nあんた、何考えてんのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:21 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:21 
msgid "猫にカニは良くないのよ！\nビタミンＡ１分解酵素がどうとかで！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:22 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:22 
msgid "（え、そうなのか！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:23 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:23 
msgid "だから、ごめんね。ところで笹原弌弌\nあ、死にたくなかったらあっち行ってね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:24 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:24 
msgid "（ぬおおおおお〜〜〜〜っ！？\n　警告してすぐ撃つんじゃねえ〜〜！）\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0022:25 
msgid "ふう、危ないなぁ。\nぎりぎり避けられてよかったぜ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0022:26 
msgid "……あっ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0022:27 
msgid "ん？　どうした？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0022:28 
msgid "アンテナに……命中してた。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:29 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:29 
msgid "ところで立花みさとよ。\n１つ聞きたいことがあるのだが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:30 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:30 
msgid "え？　な、なによ……。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0022:31 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0022:31 
msgid "俗世にはカニカマなる庶民の食品が\nあるらしいが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:32 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:32 
msgid "あれは何なのだ？\nカニなのか、カマボコなのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:33 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:33 
msgid "一体どんな味がするのものなのか……\nまったく想像できんのだ。教えてくれ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:34 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:34 
msgid "ど、どんなって言われても……\nカニっぽい味のするカマボコよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:35 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:35 
msgid "カニっぽい味のカマボコと言われても……\nどうも解せんな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:36 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:36 
msgid "カニの味を楽しみたいなら\nカニを食べれば良いではないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:37 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:37 
msgid "カマボコを食べたいなら\nカマボコを食べれば良いはずだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:38 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:38 
msgid "あえてカニカマを食すのは、カニカマ独自の\n味わいを求めているからではないのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:39 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:39 
msgid "私はそれが知りたい。\nカニカマとは、いかなる味がするものなのか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:40 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:40 
msgid "ってか、気になるなら\n実際食べてみればいいでしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:41 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:41 
msgid "何を馬鹿な。笹原家の長男ともあろう者が、\nあのような庶民の食品を買うなど考えられん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:42 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:42 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0022:43 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:43 
msgid "何が庶民の食品よ！\nカニカマ、馬鹿にしないでよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:44 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:44 
msgid "っていうか、\nあんたん家だってただの農家でしょ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:45 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:45 
msgid "農家だからこそだ。漁師ならば\nカニカマも食す機会があるのだろうが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:46 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:46 
msgid "関係ないでしょ！\n漁師が水揚げしてるわけじゃないんだから！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:47 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:47 
msgid "漁師が……水揚げ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:48 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:48 
msgid "……ぷっ、はははは。これは愉快だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:49 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:49 
msgid "立花みさとにも可愛いところがあるのだな。\nカニカマが海で泳いでいる、とは！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:50 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:50 
msgid "なっ！？\nな、な、な、何言ってるのよ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:51 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:51 
msgid "なに、恥ずかしがることはない。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:52 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:52 
msgid "女人であれば、メルヘンチックな空想も\n許されようというもの。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:53 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:53 
msgid "しかし……立花みさとがな……\nそうか……人には意外な一面がある弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:54 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:54 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:55 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:55 
msgid "なに馬鹿なこと言ってるのよ！\nいい加減なこと言わないで！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:56 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:56 
msgid "私が可愛いだなんて、\nそんな、そんな、そんな〜〜〜〜〜〜っ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:57 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:57 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:58 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:58 
msgid "公共の公園で下らないこと考えないでよ！\n近所迷惑なのよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:59 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:59 
msgid "いやしかし、疑問は解決するに如かず。\n気になるものは気になるのだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:60 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:60 
msgid "あ―もうめんどくさいわね！\nちょっとそこで待ってなさいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:61 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:61 
msgid "？\n"
msgstr "?\n"

#  speaker 
#  warnings: 0 
#: sc/0022:62 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:62 
msgid "ほら、買ってきてあげたわ。\nこれがカニカマよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:63 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:63 
msgid "そうか、これが夢にまで見た……\n立花みさとよ、わざわざすまぬな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:64 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:64 
msgid "い、いいから！\nさっさと食べなさいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:65 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:65 
msgid "うむ、ではさっそく。馳走になろう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:66 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:66 
msgid "（あ、あれ！？　しまった！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:67 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:67 
msgid "（間違えて笹かま\n　買って来ちゃった〜〜〜〜っ！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:68 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:68 
msgid "ふむ、これがカニカマか。\nしかし、これの一体どこがカニなのやら。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:69 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:69 
msgid "ち、違うの！　そ、その……\nそれはカニカマじゃなくて笹かまよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:70 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:70 
msgid "笹原のこと考えてたら……\n笹ってのが頭にあったから……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:71 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:71 
msgid "なるほど、これは笹かまか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:72 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:72 
msgid "カニカマがカニのようなカマボコなら、\n笹かまは私のようなカマボコなのだろうな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:73 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:73 
msgid "は？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:74 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:74 
msgid "気に入った！\n今日からこの笹かまの名は笹原カマジロ―だ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:75 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:76 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:76 
msgid "うむ。と言うわけで、今日の記念にカマジロ―を\n立花みさとに進ぜよう。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:77 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:77 
msgid "え？　わ、私にくれるの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:78 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:78 
msgid "うむ。せいぜい可愛がってやってくれ。\n……では失礼。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:79 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:79 
msgid "笹原の……笹かま……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:80 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:80 
msgid "ぱくっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:81 
msgid "執事"
msgstr "Butler"

#  text 
#  warnings: 0 
#: sc/0022:81 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:82 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:82 
msgid "（あ、カニカマ……\n　あるならさっさと出しなさいよね）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:83 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:83 
msgid "ふむ、これがカニカマというものか。\nではさっそく食してみるかな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:84 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:84 
msgid "赤い部分が甲羅か。\nなるほど、良くできているな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:85 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:85 
msgid "！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:86 
msgid "執事"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:86 
msgid "！！\n"
msgstr "!!\n"

#  speaker 
#  warnings: 0 
#: sc/0022:87 
msgid "みさと"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:87 
msgid "（あ、赤いところを避けて食べてる……\n　甲羅だと思って……？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:88 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:88 
msgid "ふむ、これは……なんというか……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0022:89 
msgid "みさと"
msgstr "Misato"

#  text 
#  warnings: 0 
#: sc/0022:89 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0022:90 
msgid "笹原"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0022:90 
msgid "……なんとも、言えんな。\n"
msgstr ""
