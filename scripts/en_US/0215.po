#  speaker 
#  warnings: 0 
#: sc/0215:0 
msgid "なの＆はかせ"
msgstr "Nano & Hakase"

#  text 
#  warnings: 0 
#: sc/0215:0 
msgid "ご馳走様でした―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:1 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0215:1 
msgid "お洗濯はもう終わってるから……\n洗い物しよっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:2 
msgid "はかせ"
msgstr "Hakase"

#  text 
#  warnings: 0 
#: sc/0215:2 
msgid "はかせは研究しよっと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:3 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:3 
msgid "待ってください。\n後ろに隠したお菓子を置いていってください。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:4 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:4 
msgid "そ、そんなのないもん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:5 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:5 
msgid "あります。\nさ、早く。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:6 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:6 
msgid "うう……なのの意地悪―……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:7 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0215:7 
msgid "朝からあいつらは何やってるんだか。\n……ま、いつもと同じか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:8 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:8 
msgid "ふあぁ―。\nそろそろお昼か弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:9 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:9 
msgid "弌弌ん、なんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:10 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:10 
msgid "雨か……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:11 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:11 
msgid "……って、おい、娘！\n洗濯物を干したんじゃないのか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:12 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:12 
msgid "………………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0215:13 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:13 
msgid "お―い、娘！　どこだ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:14 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:14 
msgid "台所ですよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:15 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:15 
msgid "雨が降ってるぞ―！\n洗濯物、濡れちまうぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:16 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:16 
msgid "でも、今は手が泡だらけで……\nはかせ―！　お願いしま―す！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:17 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:17 
msgid "だめ―！　今、研究中―！　手が離せない！\n阪本やって―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:18 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:18 
msgid "猫にできるか！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:19 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:19 
msgid "どっちでもいい！\n早くしないと洗濯物が濡れちまうぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:20 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:20 
msgid "はかせ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:21 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:21 
msgid "阪本―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:22 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:22 
msgid "できるか―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:23 
msgid "ナレ―タ―お姉さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:23 
msgid "そして結局……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:24 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:24 
msgid "通り雨だったが弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:25 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:25 
msgid "……洗濯物……全滅してました……\n一緒に干しておいたお布団も……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:26 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:26 
msgid "弌弌結構な降りだったからな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:27 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:27 
msgid "うそ！？　どうやって寝るの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:28 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:28 
msgid "毛布に包まって寝るしかないですね……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:29 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:29 
msgid "なんで洗濯物入れなかったの！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:30 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:30 
msgid "だって洗い物してたんですよ！？\n手が泡だらけだったんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:31 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:31 
msgid "はかせだってそうだもん！\n研究中だったんだもん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:32 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:32 
msgid "……ポテチの研究、ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:33 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:33 
msgid "ドキ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:34 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:34 
msgid "口の周りにポテチのカスがついてますよ？\n本当に研究してたんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:35 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:35 
msgid "きゅ、休憩中だったんだもん！\n仕方ないんだよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:36 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:36 
msgid "仕方なくないです！\n休憩中だったら入れてください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:37 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:37 
msgid "休憩中はお菓子食べるのが仕事だもの！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:38 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:38 
msgid "二人共落ち着け。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:39 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:39 
msgid "でも、はかせが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:40 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:40 
msgid "でも、なのが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:41 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:41 
msgid "ケンカして洗濯物が乾くのか？\n違うだろ？　それに責任転嫁するな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:42 
msgid "なの＆はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:42 
msgid "は―い……。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0215:43 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0215:43 
msgid "返事ははいだろ！\n気抜けた声を出すな！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:44 
msgid "なの＆はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:44 
msgid "はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:45 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:45 
msgid "それでよし。\nやればできるのに、なんでやんないんだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:46 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:46 
msgid "さっきだってそうだ。\n弌弌娘。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:47 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:47 
msgid "は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:48 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:48 
msgid "手が泡だらけでも、洗えばいいはずだ。\n……１０秒あればできるだろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:49 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:49 
msgid "はい……そうです……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:50 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:50 
msgid "次、ガキ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:51 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:51 
msgid "は、はい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:52 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:52 
msgid "他人に甘えんな。\nおまえの悪いくせだぞ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:53 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:53 
msgid "は、は―い……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:54 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:54 
msgid "それから、大人になりたいなら、\n自分勝手な解釈はやめたほうがいいぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:55 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:55 
msgid "……ごめんなさい。\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0215:56 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0215:56 
msgid "あと、この際だから言わせてもらうが、\nおまえには色と悪いところが弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:57 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:57 
msgid "う……う……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:58 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:58 
msgid "やめてくださ―い！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:59 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:59 
msgid "はかせは悪いとこばかりじゃないんです。\n良いとこだってあるんですよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:60 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:60 
msgid "は？　いや、そんなことはわかってるが……\nこれは説教だから弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:61 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:61 
msgid "……なの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:62 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:62 
msgid "はかせ、今日は私の毛布も使ってください。\n敷き布団みたいにすれば十分温かいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:63 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:63 
msgid "でも、なのが……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:64 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:64 
msgid "私は大丈夫です。\nロボットだから、寒さなんてへっちゃらです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:65 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:65 
msgid "なの……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:66 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:66 
msgid "ごめんなさい……\nはかせが悪かったの……ごめんなさい！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:67 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:67 
msgid "はかせだけじゃありません！\n私だって悪かったんです！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:68 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:68 
msgid "な―の―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:69 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:69 
msgid "はかせ―！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:70 
msgid "なの＆はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:70 
msgid "うあ―ん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:71 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:71 
msgid "弌弌ふう。\nま、わかったならいいか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:72 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:72 
msgid "あ、はかせ、お昼は何がいいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:73 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:73 
msgid "オムライス―！\nあとハンバ―グ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:74 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:74 
msgid "それじゃ今日は特別。\nレストランに行っちゃいましょうか！\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0215:75 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0215:75 
msgid "やった―！　だからなの大好き―\n"
msgstr ""

#  speaker 
#  warnings: 1 
#: sc/0215:76 
msgid "なの"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0215:76 
msgid "もう、はかせったら……えへへへ\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:77 
msgid "はかせ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:77 
msgid "それじゃ阪本、出かけてくるね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:78 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:78 
msgid "お留守番、お願いします。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:79 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:79 
msgid "おう！\n仲良く行ってこいよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:80 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:80 
msgid "………………ん？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0215:81 
msgid "阪本さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0215:81 
msgid "なんで俺がメシ抜きなんだ？\n"
msgstr ""
