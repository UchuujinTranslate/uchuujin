#  speaker 
#  warnings: 0 
#: sc/0040:61 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:61 
msgid "（またこのパタ―ンか……）\n"
msgstr "(So it's pattern again……)\n"

#  speaker 
#  warnings: 0 
#: sc/0040:62 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:62 
msgid "大福で―す。\n血液型は９型〜。\n"
msgstr ""
"Hey I'm Daifuku. \n"
"My blood type is A~\n"

#  speaker 
#  warnings: 0 
#: sc/0040:63 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:63 
msgid "！？\n"
msgstr "!  ?\n"

#  speaker 
#  warnings: 0 
#: sc/0040:64 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:64 
msgid "９ＭＭＪＮだけに９型―。\nなんちゃって―。\n"
msgstr ""
"A as in \"ANNKO\"\n"
"am I right\n"

#  speaker 
#  warnings: 0 
#: sc/0040:65 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:65 
