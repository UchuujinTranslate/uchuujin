msgstr ""
"(I- I won……!\n"
"With this I don't have to pay her money…)\n"

#  speaker 
#  warnings: 0 
#: sc/0040:43 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:43 
msgid "…………！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0040:44 
msgid "イタコ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0040:44 
