#  speaker 
#  warnings: 0 
#: sc/0117:0 
msgid "桜井誠"
msgstr "Makoto"

#  text 
#  warnings: 0 
#: sc/0117:0 
msgid "１３６、１３７、１３８……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:1 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:1 
msgid "何とか落とさず教室まで行けそう……っと。\n……あれ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:2 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:2 
msgid "向こうから来るのって関口さん？\n本読んでるから顔は見えないけど……多分。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:3 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:3 
msgid "すみませ―ん、関口さ―ん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:4 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:4 
msgid "……あ、桜井くん。どうも。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:5 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:5 
msgid "こんにちは。歩きながら\n本読んでる人が居るからびっくりしました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:6 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:6 
msgid "ほんとにいつも本読んでるんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:7 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:7 
msgid "私もリフティングしながら\n歩いてくる人がいるから驚きました。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:8 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:8 
msgid "ほんとにいつもボ―ル蹴ってるんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:9 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:9 
msgid "もちろんですよ。\n囲碁サッカ―、大好きですから。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:10 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:10 
msgid "なるほど。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:11 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:11 
msgid "でも、少しは前を見たほうがいいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:12 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:12 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:13 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:13 
msgid "ほら、足元。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:14 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:14 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:15 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:15 
msgid "まさかオレ、踏みかけてた！？\n……ってこの猫、何で逃げないんだろう？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0117:16 
msgid "くそっ、こいつ！\nアンテナが折れちまっただろ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0117:17 
msgid "バリアで猫は平気だが、万一のことがあったら\nどうするんだよ！　とっちめてやる！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0117:18 
msgid "……これから？　どうやって？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0117:19 
msgid "どうやってて、そりゃお前、\n今、アンテナが壊れたんだから弌弌あれ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0117:20 
msgid "…………。\n"
msgstr ".........\n"

#  text 
#  warnings: 1 
#: sc/0117:21 
msgid "やるせねぇ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:22 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:22 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:23 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:23 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:24 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:24 
msgid "（あれ……？　会話終わった？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:25 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:25 
msgid "（そういえば、\n　いつもは関口さんと何を話してたっけ？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:26 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:26 
msgid "（……そうか！\n　いつもは間に部長が居るんだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:27 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:27 
msgid "（で、原因がわかった所でどうしよう。\n　挨拶しておいて何も話さないのも変だし）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:28 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:28 
msgid "（ここは一番得意なジャンルで勝負！\n　とりあえずさっきの話を膨らませて……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:29 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:29 
msgid "そ、そう！　囲碁サッカ―が大好きだから\nいつだって技を出す準備も出来てるんですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:30 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:30 
msgid "はぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:31 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:31 
msgid "例えばこうやってボ―ルを高く浮かせて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:32 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:32 
msgid "ここで碁石！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:33 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:33 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:34 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:34 
msgid "そして碁石と高さをあわせて！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:35 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:35 
msgid "あの、まさかとは思いますけど\nこんな所でシュ―トなんてしたら……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:36 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:36 
msgid "シュ――――ト！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:37 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:37 
msgid "あ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:38 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:38 
msgid "碁石も蹴ったように見せかけて\n実はボ―ルしか蹴らない！　これが必殺の弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:39 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:39 
msgid "あ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:40 
msgid "富岡先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:40 
msgid "あ、赤城先生大丈夫ですか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:41 
msgid "富岡先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:41 
msgid "ダメだ、白目剥いてる……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:42 
msgid "富岡先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:42 
msgid "誰だ！　こんな所でボ―ルを蹴ったのは！！\nさては、わざと赤城先生を狙ったな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:43 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:43 
msgid "ど、どうしよう、わざとじゃないのに……\nあの関口さん、一緒に誤解を弌弌居ない！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:44 
msgid "富岡先生"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:44 
msgid "あっ、犯人はそこのお前か！？\nちょっと来い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:45 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:45 
msgid "……はい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:46 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:46 
msgid "（あ、そうだ！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:47 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:47 
msgid "あの、関口さん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:48 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:48 
msgid "なんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:49 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:49 
msgid "良かったらその本、\nちょっと読ませて貰ってもいいですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:50 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:50 
msgid "いいですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:51 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:51 
msgid "ありがとう。\nそれじゃ、代わりにボ―ルどうぞ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:52 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:52 
msgid "え？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:53 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:53 
msgid "オレだけ借りてたら不公平でしょ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:54 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:54 
msgid "いえ、私は別に……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:55 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:55 
msgid "使いすぎてボロボロですけど、\n初めて買ったボ―ルなんで許してくださいね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:56 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:56 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:57 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:57 
msgid "そんな大事なもの、私が使うわけには。\nそもそも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:58 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:58 
msgid "いえ、使ってください！\nむしろ使って欲しいんです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:59 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:59 
msgid "関口さんは大切な囲碁サッカ―部の\n仲間ですから！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:60 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:60 
msgid "（そうだ、話しにくいだなんて失礼な。\n　仲間なんだからもっと親しくしないと！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:61 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:61 
msgid "！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:62 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:63 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:63 
msgid "ありがとうございます。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:64 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:64 
msgid "（そうだ、囲碁サッカ―に興味を持ってもらう\n　いいチャンスじゃないか！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:65 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:65 
msgid "（よ―し、そうと決まればその路線で）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:66 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:66 
msgid "いや―、思わず廊下でリフティングするぐらい\n囲碁サッカ―って本当に面白いんですよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:67 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:67 
msgid "学校に囲碁サッカ―部があったのが嬉しくて、\n最近は特にテンションあがってるんですよね！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:68 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:68 
msgid "はぁ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:69 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:69 
msgid "そ、それにプロリ―グも盛り上がってて、\nオリンピック競技になるかもしれないですし！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:70 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:70 
msgid "そうなんですか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:71 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:71 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:72 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:72 
msgid "（あ、あれ〜？\n　何かリアクション薄いんだけど？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:73 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:73 
msgid "（よし、こうなったら単刀直入に！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:74 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:74 
msgid "あの……もしかして関口さんって、\n囲碁サッカ―あんまり興味ないんですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:75 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:75 
msgid "無いですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:76 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:76 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:77 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:77 
msgid "そろそろ授業なので行きますね。\nまた部室で。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:78 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:78 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0117:79 
msgid "桜井誠"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0117:79 
msgid "うちの部……ダメかも。\n"
msgstr ""
