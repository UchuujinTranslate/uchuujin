#  speaker 
#  warnings: 0 
#: sc/0180:0 
msgid "なの"
msgstr "Nano"

#  text 
#  warnings: 0 
#: sc/0180:0 
msgid "はぁ……\n結局ネジはついたままかぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:1 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:1 
msgid "ネジの無い普通の女の子になれる日は\nいつになったら来るんだろう……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:2 
msgid "ゆっこ"
msgstr "Yuuko"

#  text 
#  warnings: 0 
#: sc/0180:2 
msgid "お―い、なのちゃ―ん。\n"
msgstr "Hey Nano-chan!\n"

#  speaker 
#  warnings: 0 
#: sc/0180:3 
msgid "みお"
msgstr "Mio"

#  text 
#  warnings: 0 
#: sc/0180:3 
msgid "おはよ―なのちゃん。\n風邪は治ったの？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:4 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:4 
msgid "あっ、おはようございます。\n風邪はもう治りましたよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:5 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:5 
msgid "そっか。良かった。\n心配してたんだよ。ね、ゆっこ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:6 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:6 
msgid "うんうん、なのちゃんが落ち込んでると\nこっちまで元気なくなっちゃうし。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:7 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:7 
msgid "相生さん、長野原さん……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:8 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:8 
msgid "（ネジはついたままだけど……でも、\n　私にはこんなに優しい友達がいる）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:9 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:9 
msgid "（それだけでいいや。ネジがあっても、\n　私は普通の女の子なんだから）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:10 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:10 
msgid "ありがとうございます。\nまた、今日からよろしくお願いしますね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:11 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:11 
msgid "うんうん、よろしく。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:12 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:12 
msgid "よろしくね、なのちゃん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:13 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:13 
msgid "はいっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:14 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:14 
msgid "（ああ、これが友情というものなんだ。\n　なんて素晴らしいんだろう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:15 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:15 
msgid "（でも私がロボットであることは秘密だから\n　少し後ろめたい気もするけど……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:16 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:16 
msgid "（でも、こればっかりは教えられないから\n　許してください、相生さん、長野原さん）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:17 
msgid "みお"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:17 
msgid "なのちゃん、\nちゃんと修理してもらったみたいだね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:18 
msgid "ゆっこ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:18 
msgid "うんうん。直ってよかったよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0180:19 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:19 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0180:20 
msgid "なの"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0180:20 
msgid "…………。\n"
msgstr "………….\n"
