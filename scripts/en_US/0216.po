#  text 
#  warnings: 0 
#: sc/0216:0 
msgid "ちょっ！？　どうしたんだよ！？\nバディ！　コバディ！　どこ行くんだ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:1 
msgid "バディ"
msgstr "Buddy"

#  text 
#  warnings: 0 
#: sc/0216:1 
msgid "ハッ！　ハッ！　ハッ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:2 
#, fuzzy
msgid "コバディ"
msgstr "Co-Buddy"

#  text 
#  warnings: 0 
#: sc/0216:2 
msgid "ハッ、ハッ、ハッ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:3 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:3 
msgid "バディ―！　コバディ―！\n帰ってこ―い！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:4 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:4 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0216:5 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:5 
msgid "……行っちゃった。\nこれで何度目だろ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:6 
msgid "きよし"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:6 
msgid "って、ほんと……何しに行ってるんだろ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:7 
msgid "……どうする？\n番組以外に、何か手があるか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:8 
msgid "…………。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:9 
msgid "おい、答えろよ！？　何とかならないのか！？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:10 
msgid "知るかよ！\nこんなときアイツがいてくれたら……！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:11 
msgid "？？？（８７番）"
msgstr "??? (#87)"

#  text 
#  warnings: 0 
#: sc/0216:11 
msgid "……それはもしかして、俺のことか？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:12 
msgid "おまえは、策士の８７番！？\nスタジオ船にいたんじゃないのか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:13 
msgid "８７番"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:13 
msgid "仲間のピンチだぜ？\n策を講じるなら今だろ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:14 
msgid "それでどうなんだ？\n姫を喜ばす方法は……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:15 
msgid "８７番"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:15 
msgid "ジョ―クもだめ、クイズもだめ、一発芸もだめ、\nそして、番組も……だめ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:16 
msgid "８７番"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:16 
msgid "なら残された手段はただ１つ！\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0216:17 
msgid "＆"
msgstr "&"

#  text 
#  warnings: 2 
#: sc/0216:17 
msgid "そ、それは……！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:18 
msgid "８７番"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:18 
msgid "アルパカの真似だろ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:19 
msgid "スタ―ラ姫"
msgstr "Princess Starla"

#  text 
#  warnings: 0 
#: sc/0216:19 
msgid "似とらん。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:20 
msgid "ああ……８７番までが……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:21 
msgid "……俺だ……\nあいつが死んだのは……俺のせいだ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:22 
msgid "俺が呼ばなければ……！\n俺が頼んだりしなければ……ヤツは……！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:23 
msgid "……ん？　お、おい、みんな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:24 
msgid "犬だ！　それも２匹いるぞ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:25 
msgid "励ましてくれるのか？\n……けど、俺は……。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:26 
msgid "なら、何とかしろ！　死んだみんなのために、\n姫を喜ばせてみろ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:27 
msgid "……わかったよ。俺、やってみる。\n死んだアイツラの分までがんばってみる！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:28 
msgid "その意気だ！　……で、何するんだ？\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0216:29 
msgid "アルパカの真似、かな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0216:30 
msgid "バディ"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0216:30 
msgid "…………。\n"
msgstr ""

