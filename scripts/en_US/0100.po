#  speaker 
#  warnings: 0 
#: sc/0100:0 
msgid "大工"
msgstr "Daiku"

#  text 
#  warnings: 0 
#: sc/0100:0 
msgid "ん―、暇だなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:1 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:1 
msgid "桜井くんは用事を済ませているらしいし、\n……二人っきりじゃな―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:2 
msgid "関口"
msgstr "Sekiguchi"

#  text 
#  warnings: 0 
#: sc/0100:2 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0100:3 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:3 
msgid "な―関口―。\nなんか面白いことないか―？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:4 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:4 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:5 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:5 
msgid "関口―、反応ぐらいしてくれよ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:6 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:6 
#, fuzzy
msgid "面白いことって、たとえば何ですか？\n"
msgstr "Give me an example of an interesting thing.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:7 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:7 
msgid "たとえば？\nえ―っと、そうだな……例えば。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:8 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:8 
msgid "例えば、突然ここにアンテナをつけた\n猫が現れるとかどうだ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:9 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:9 
msgid "しかもそのアンテナは宇宙人の陰謀で\n取り付けられた人類監視装置なんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:10 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:10 
msgid "……猫ですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:11 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:11 
msgid "ああ。\nやつらは人類を監視して……え―っと。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:12 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:12 
msgid "そうだ！\nバラエティ―番組をやってるってのは？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:13 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:13 
msgid "……面白いです。\n"
msgstr "……that’s interesting.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:14 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:14 
#, fuzzy
msgid "だろ？\n"
msgstr "Is it?\n"

#  speaker 
#  warnings: 0 
#: sc/0100:15 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:15 
msgid "じゃあ、その猫がそうなんですね。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:16 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0100:16 
msgid "！？\n"
msgstr "!?\n"

#  text 
#  warnings: 1 
#: sc/0100:17 
#, fuzzy
msgid "ヤバイ！　今すぐ猫を移動させるんだ！\n"
msgstr "Oh no! Let’s make the cat move immediately!\n"

#  text 
#  warnings: 1 
#: sc/0100:18 
msgid "ああっ！　遅かった！\n捕まえられた―――！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0100:19 
msgid "アンテナ折られた――！！\n"
msgstr "The antenna broke—!!\n"

#  text 
#  warnings: 1 
#: sc/0100:20 
msgid "ちくしょう！　この男、いったい何者なんだ！？\n俺たちの行動を監視してるのか！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:21 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:21 
msgid "例えば……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:22 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:22 
msgid "関口、画期的な案をひらめいたぞ！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:23 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:23 
msgid "画期的？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:24 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:24 
msgid "題して面白いことしりとりだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:25 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:25 
msgid "お互いに面白いと思った言葉を使って\nしりとりをする。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:26 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:26 
msgid "そうして終わったら\n面白そうなことがたくさん集まってるわけだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:27 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:27 
msgid "まさに一石二鳥だと思わないか関口！\n早速やってみようぜ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:28 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:28 
#, fuzzy
msgid "知らん。\n"
msgstr "I donno.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:29 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:29 
msgid "！？\n"
msgstr "! ?\n"

#  speaker 
#  warnings: 0 
#: sc/0100:30 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:30 
msgid "（知らん？　今の発言はどういう事だ？\n　関口がそんな冷たいこと言うはずが……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:31 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:31 
msgid "（なるほど、そうか！）\n"
msgstr "(I see. I get it now!)\n"

#  speaker 
#  warnings: 0 
#: sc/0100:32 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:32 
msgid "（これはツッコミ待ちに違いない！！）\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0100:33 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:33 
msgid "知らんって、\nそれじゃんがついて終わりだろ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:34 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:34 
#, fuzzy
msgid "あ、ほんとですね。\nすみません。\n"
msgstr ""
"Ah, it really is.\n"
"Sorry.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:35 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:35 
msgid "も―、しっかりしてくれよ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:36 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:36 
msgid "いや……でもこれ、アリかもしれない。\n"
msgstr "No way…… But that’s probably an ant.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:37 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:37 
msgid "……え？\n"
msgstr "……Eh?\n"

#  speaker 
#  warnings: 2 
#: sc/0100:38 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:38 
msgid "そうだ、んで終わる\n言葉しりとりをしよう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:39 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:39 
msgid "……なんですかそれ。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0100:40 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:40 
#, fuzzy
msgid "ル―ルは簡単！\n絶対にんで終わること！\n"
msgstr ""
"The rules are simple!\n"
"Absolutely end it!\n"

#  speaker 
#  warnings: 2 
#: sc/0100:41 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:41 
msgid "次の言葉はんの\n手前の文字を取って言う！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:42 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:42 
#, fuzzy
msgid "どうだコレ？\n面白そうじゃないか！？\n"
msgstr ""
"How is this thing?\n"
"Doesn’t it look interesting!?\n"

#  speaker 
#  warnings: 0 
#: sc/0100:43 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:43 
msgid "悪くないんじゃないですか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:44 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:44 
msgid "だよな！？\n我ながらいい遊びができたぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:45 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:45 
msgid "よ―し決めた！　今日からここは\nおもしろしりとり研究会にしよう！\n"
msgstr ""
"Alright- it’s decided! \n"
"From here on we will be the funny Shiritori research team!\n"

#  speaker 
#  warnings: 0 
#: sc/0100:46 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:46 
msgid "！！！？\n"
msgstr "!!!?\n"

#  speaker 
#  warnings: 0 
#: sc/0100:47 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:47 
msgid "知らん……おじさんが！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:48 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:48 
msgid "……近くの。\n"
msgstr "\n"

#  speaker 
#  warnings: 0 
#: sc/0100:49 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:49 
msgid "保健所で！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:50 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:50 
msgid "焼きそばを。\n"
msgstr "The Yakisoba.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:51 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:51 
msgid "色っぽく。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:52 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:52 
msgid "こぼしてた。\n"
msgstr "Was spilling.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:53 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:53 
msgid "完成！\n"
msgstr "Complete!\n"

#  speaker 
#  warnings: 2 
#: sc/0100:54 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:54 
msgid "知らんおじさんが近くの保健所で\n　焼きそばを色っぽくこぼしてた！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:55 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:55 
msgid "ははははははっ！！\n何だこれ、わけわかんね―！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:56 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:56 
msgid "ははははははははははっ！！\n"
msgstr "Hahahahahahahahahahaa!!\n"

#  speaker 
#  warnings: 0 
#: sc/0100:57 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:57 
msgid "いや―、関口ナイス。\nまさか二人で面白い文章を作る遊びなんて。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:58 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:58 
msgid "……部長、よくわかりましたね。\n"
msgstr "……President, I understand well.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:59 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:59 
msgid "あったりまえだろ、どれだけの間\nお前と二人っきりで部室に居たと思ってんだ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:60 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:60 
msgid "…………わかってもらえて嬉しいです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:61 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:61 
msgid "あ、でも関口。これしりとりじゃないよな。\n"
msgstr "Ah, but Sekiguchi. This is not Shiritori you know.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:62 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:62 
msgid "！！\n"
msgstr "!!\n"

#  speaker 
#  warnings: 0 
#: sc/0100:63 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:63 
msgid "……あ、そういえば。\n"
msgstr "……ah, now that you mention it.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:64 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:64 
msgid "いや、知らんで思い出したんだけどさ、\nこないだうちに知らん人から電話がきてさ。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0100:65 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:65 
msgid "ユミコはどこだ！？\n"
msgstr "Where is Yumiko!?\n"

#  speaker 
#  warnings: 2 
#: sc/0100:66 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:66 
msgid "そばにいるのは知ってるんだからな！\n　この野郎！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:67 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:67 
msgid "って電話が来てさ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:68 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:68 
#, fuzzy
msgid "へぇ。\n"
msgstr "Huh.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:69 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:69 
msgid "そんで違いますって言ったら。\n"
msgstr ""

#  speaker 
#  warnings: 2 
#: sc/0100:70 
msgid "大工"
msgstr ""

#  text 
#  warnings: 2 
#: sc/0100:70 
msgid "嘘ついても証拠は挙がってるんだ！\n　今からお前の家に行くからな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:71 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:71 
msgid "ってさ。\nホント、リアクションに困る電話だったなぁ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:72 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:72 
msgid "あの、部長。\n"
msgstr "Uhm, President.\n"

#  speaker 
#  warnings: 0 
#: sc/0100:73 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:73 
msgid "ん―？\n"
msgstr "Hmm-?\n"

#  speaker 
#  warnings: 0 
#: sc/0100:74 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:74 
msgid "その話を聞いた私も\nリアクションに困るんですが。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:75 
msgid "大工"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:75 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0100:76 
msgid "関口"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0100:76 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0100:77 
msgid "大工"
msgstr "Daiku"

#  text 
#  warnings: 0 
#: sc/0100:77 
msgid "暇だよな―。\n"
msgstr ""
