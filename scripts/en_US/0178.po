#  speaker 
#  warnings: 0 
#: sc/0178:0 
msgid "玉村"
msgstr "Tamamura"

#  text 
#  warnings: 0 
#: sc/0178:0 
msgid "いらっしゃいませ―。\n"
msgstr "Welcome!\n"

#  speaker 
#  warnings: 0 
#: sc/0178:1 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0178:1 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0178:2 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:2 
msgid "（……あ、あのお客さん、\n　いつも、すぐそこで大福売ってる人！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:3 
msgid "……ふ―む……。\n"
msgstr "……Hmm…….\n"

#  speaker 
#  warnings: 0 
#: sc/0178:4 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:4 
msgid "（こういう喫茶店にも来るんだ。\n　ケ―キは苦手だと思ってたのに……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:5 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:5 
msgid "これが……だが、こんな店では……\nしかし、まずは弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:6 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:6 
msgid "（な、何か、独り言を言ってる！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:7 
msgid "阪本さん"
msgstr "Sakamoto-san"

#  text 
#  warnings: 0 
#: sc/0178:7 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:8 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:8 
msgid "……あら、あの人と一緒にいるのは？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:9 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:9 
msgid "あの、お客様？　申し訳ございませんが当店は\nペットとの入店はお断りしておりまして……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:10 
msgid "……ペット？\nあっ、なんだこの猫は、シッシッ！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:11 
msgid "危ないっ！　ぬぉぉりゃああ！！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:12 
msgid "おおっ！　ナイス回避！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:13 
msgid "……ふぅ。\n猫を蹴ろうとするとは、なんてひどいやつだ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:14 
msgid "だが、お前の丕丶の反応のおかげで\n猫は蹴られずに済んだな！\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:15 
msgid "でも、アンテナが蹴られてちゃったけどな。\nハハッ。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:16 
msgid "ハハハハハ、ドンマイ、ドンマイ。\nまだまだ練習が足りなかったな。\n"
msgstr ""

#  text 
#  warnings: 1 
#: sc/0178:17 
msgid "おまえら二人、軽すぎるわっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:18 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:18 
msgid "（でも、お客様だから……\n　なんだかちょっと怖いけど、笑顔、笑顔）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:19 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:19 
msgid "お客様、ご注文はお決まりでしょうか？\n"
msgstr "Pardon me, but are we ready to order?\n"

#  speaker 
#  warnings: 0 
#: sc/0178:20 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:20 
msgid "大福。\n"
msgstr "Daifuku.\n"

#  speaker 
#  warnings: 0 
#: sc/0178:21 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:21 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:22 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:22 
msgid "も、申し訳ありません、\n当店では大福は置いていないのですが……。\n"
msgstr ""
"I, I must apologize, we do not\n"
"have daifuku at this location…….\n"

#  speaker 
#  warnings: 0 
#: sc/0178:23 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:23 
msgid "なんで大福が置いていないんだ！\n"
msgstr "Why don't you have any daifuku here!?\n"

#  speaker 
#  warnings: 0 
#: sc/0178:24 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:24 
msgid "（えええ―――！？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:25 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:25 
msgid "も、申し訳ございません。当店で扱っている\n商品とは方向性が合いませんので弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:26 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:26 
msgid "大福を仞めるな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:27 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:27 
msgid "（えええ―――！？）\n"
msgstr "(Ehhh———!?)\n"

#  speaker 
#  warnings: 0 
#: sc/0178:28 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:28 
msgid "大福は最高の物だ！\n合わないわけがないだろう！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:29 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:29 
msgid "ええと、お味ではなくて……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:30 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:30 
msgid "合わないわけがない！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:31 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:31 
msgid "……で、でしたらお客様、\nエスプレッソはいかがでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:32 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:32 
msgid "えすぷれっそ？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:33 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:33 
msgid "はい。苦く癖のある飲み物ですが、\n大福の甘さが調和してくれると思います。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:34 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:34 
msgid "ふむ。ではそれを１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:35 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:35 
msgid "はい。\nエスプレッソのドッピオ１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:36 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:36 
msgid "（……どっぴお？）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:37 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:37 
msgid "お待たせしました。エスプレッソです。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:38 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:38 
msgid "ふむ……では一口……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:39 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:39 
msgid "苦っ！？\n……しかし、これなら大福にも弌弌\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:40 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:40 
msgid "……お味は、いかがでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:41 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:41 
msgid "ま、まあまあだ弌弌だが、所詮は西洋の飲み物。\n大福とは方向性が合っておらんなっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:42 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:42 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:43 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:43 
msgid "（……私は、どうすればいいんだろう）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:44 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:44 
msgid "も、申し訳ございません……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:45 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:45 
msgid "（大福なら自分の店で食べればいいのに……\n　で、でも、そんなこと言えないよね）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:46 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:46 
msgid "全く、大福も置いていないとは\nふざけた店だ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:47 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:47 
msgid "ではお客様、こちらのロ―ルケ―キを\n替わりにいかがでしょうか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:48 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:48 
msgid "フン、そんなもの大福の足元にも及ばん。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:49 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:49 
msgid "はい。ですが、こちらは\n当店のオススメ商品となっております。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:50 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:50 
msgid "大福には勝てないと思いますが、\nたまには、このような物もいかがでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:51 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:51 
msgid "う、うむ、そうか、勝てないか……。\nでは、ロ―ルケ―キを１つ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:52 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:52 
msgid "３００円になりま―す。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:53 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:53 
msgid "うむ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:54 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:54 
msgid "はい、お待たせいたしました。\nこちらがロ―ルケ―キになります。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:55 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:55 
msgid "ふむ、これがこの店のオススメか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:56 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:56 
msgid "どれどれ……もぐっ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:57 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:57 
msgid "…………うまっ。\n"
msgstr "………… Sweet.\n"

#  speaker 
#  warnings: 0 
#: sc/0178:58 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:58 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:59 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:59 
msgid "！？\nも、もちろん、大福の方が遥かにうまいがな！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:60 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:60 
msgid "はい。\nそれでは、ごゆっくりお召し上がり下さい。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:61 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:61 
msgid "…………うまい。\n"
msgstr "…………Sweet.\n"

#  speaker 
#  warnings: 0 
#: sc/0178:62 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:62 
msgid "……よし。\n"
msgstr "……Alright.\n"

#  speaker 
#  warnings: 0 
#: sc/0178:63 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:63 
msgid "仞めておりませんが、\n当店に大福はございませんので……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:64 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:64 
msgid "じゃあ、何ならあるんだ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:65 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:65 
msgid "ええと……それでは、\nこちらのメニュ―をご覧ください！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:66 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:66 
msgid "う、うむ……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:67 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:67 
msgid "…………。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:68 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:68 
msgid "（……なんだか、お客さんが予想外すぎて、\n　頭が混乱して……ああ、どうすれば……）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:69 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:69 
msgid "…………。\n"
msgstr "………….\n"

#  speaker 
#  warnings: 0 
#: sc/0178:70 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:70 
msgid "お、お決まりでしょうか？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:71 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:71 
msgid "フン、ろくなもんがないな。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:72 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:72 
msgid "そ、それではまた、\n次回のチャレンジをお待ちしております！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:73 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:73 
msgid "チャレンジ！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:74 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:74 
msgid "次にお待ちのお客様、どうぞ―。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:75 
msgid "中之条さん"
msgstr "Nakanojou-san"

#  text 
#  warnings: 0 
#: sc/0178:75 
msgid "終了！？\n……くっ、仕方が無い、またくるぞ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0178:76 
msgid "玉村"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0178:76 
msgid "え？\n……あれ？　あれれ―？\n"
msgstr ""
