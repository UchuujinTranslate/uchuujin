msgstr ""
"You shouldn't enter here,\n"
"Also don't be violent, okay?\n"

#  speaker 
#  warnings: 0 
#: sc/0163:46 
msgid "鹿"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0163:46 
