#  speaker 
#  warnings: 0 
#: sc/0197:0 
msgid "中之条さん"
msgstr "Nakanojo-san"

#  text 
#  warnings: 0 
#: sc/0197:0 
msgid "…………。\n"
msgstr ".........\n"

#  speaker 
#  warnings: 0 
#: sc/0197:1 
msgid "中之条"
msgstr "Nakanojo"

#  text 
#  warnings: 0 
#: sc/0197:1 
msgid "父さん、買ってきたよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:2 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:2 
msgid "ああ。\n"
msgstr "Okay.\n"

#  speaker 
#  warnings: 0 
#: sc/0197:3 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:3 
msgid "（……うちと大工カフェの差を\n　はっきりと知る必要がある）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:4 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:4 
msgid "（そのためには一度くらいは\n　商品を試さないといけない）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:5 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:5 
msgid "それで、何を買ってきてくれたんだ？\n"
msgstr "Well, what did you buy for me?\n"

#  speaker 
#  warnings: 0 
#: sc/0197:6 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:6 
msgid "ヘイゼルキャラメルマキア―トと\nケ―キをいくつか買ってきたよ。\n"
msgstr "I bought a hazelnut caramel macchiato and some cake.\n"

#  speaker 
#  warnings: 0 
#: sc/0197:7 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:7 
msgid "ヘイゼルキャラメルマキア―ト？\n"
msgstr "A hazel-caramel-macchiato?\n"

#  speaker 
#  warnings: 0 
#: sc/0197:8 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:8 
#, fuzzy
msgid "（わけがわからん……\n　なんだその長ったらしい名前は）\n"
msgstr ""
"(I don't understand.....\n"
"What is that long name?)\n"

#  speaker 
#  warnings: 0 
#: sc/0197:9 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:9 
msgid "……名前通りキャラメルのいい匂いがするな。\n味も悪くはない……。\n"
msgstr ""
"......As the name suggests, it has a nice caramel smell.\n"
"And the taste isn't bad either.......\n"

#  speaker 
#  warnings: 0 
#: sc/0197:10 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:10 
#, fuzzy
msgid "さて、ケ―キはと……\nな、何だこの目に痛い色合いは！\n"
msgstr ""
"Now, the cake......\n"
"Hey, what is this color? It hurts my eyes!\n"

#  speaker 
#  warnings: 0 
#: sc/0197:11 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:11 
msgid "……わけがわからん。\n"
msgstr "......I don't get it.\n"

#  speaker 
#  warnings: 0 
#: sc/0197:12 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:12 
msgid "これなら白くてシンプルな大福の方が\nよっぽど素晴らしいじゃないか。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:13 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:13 
msgid "父さん、とりあえず食べてみなよ。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:14 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:14 
#, fuzzy
msgid "そうだな。\n"
msgstr "I see.\n"

#  speaker 
#  warnings: 0 
#: sc/0197:15 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:15 
msgid "……うまい。\nだが、少し甘すぎるような……！？\n"
msgstr ""
"...... Yum.\n"
"But, it is a little to sweet......!?\n"

#  speaker 
#  warnings: 0 
#: sc/0197:16 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:16 
msgid "いや、こ、これは……！\nコ―ヒ―と絶妙なバランス！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:17 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:17 
msgid "くっ、悔しいがこれはうまい。\nこれだけうまければ流行るのも……。\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:18 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:18 
msgid "（しまった！　自分は何を考えているんだ！\n　大福に対して失礼だと思わないか！！）\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:19 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:19 
msgid "くっ……！　私は、私は……！！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:20 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:20 
msgid "と、父さん！？\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:21 
msgid "中之条さん"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:21 
msgid "すまない、この通りだ！\n許してくれ、大福の神よ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:22 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:22 
msgid "父さん、やめてよ！　父さん！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:23 
msgid "中之条さん"
msgstr "Nakanojou-san"

#  text 
#  warnings: 0 
#: sc/0197:23 
msgid "二度とケ―キには！　悪魔の誘惑には乗らない！\nだから、だからお許しをっ！\n"
msgstr ""

#  speaker 
#  warnings: 0 
#: sc/0197:24 
msgid "中之条"
msgstr ""

#  text 
#  warnings: 0 
#: sc/0197:24 
msgid "…………。\n"
msgstr "…………\n"

#  speaker 
#  warnings: 0 
#: sc/0197:25 
msgid "中之条"
msgstr "Nakanojou"

#  text 
#  warnings: 0 
#: sc/0197:25 
msgid "…………父さん。\n"
msgstr "…………Dad.\n"
