[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2146,
            "text_len": 74,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌ただいまより臨時全校集会を始めます。\\nまずは校長先生のお話からです。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2340,
            "text_len": 76,
            "text_offset": 2350,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "教頭先生、町内を亂乘している野生動物に\\n注意するよう言えばいいんですよね？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2458,
            "text_len": 38,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ええ、手短にお願いします。\\n手短に。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2528,
            "text_len": 34,
            "text_offset": 2538,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "はっはっはっ、わかっていますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2598,
            "text_len": 56,
            "text_offset": 2608,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……と言っても、\\n　絶対にまた長引くに決まっている）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2690,
            "text_len": 70,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（しかし、それでいい……\\n　長引けばまた今日も貧血でみんな、倒れる）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2796,
            "text_len": 78,
            "text_offset": 2806,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（それが続けば、校長は責任を取ってクビに！\\n　そうなれば私が、再び校長に！）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2916,
            "text_len": 28,
            "text_offset": 2926,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……教頭先生……教頭先生。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2976,
            "text_len": 38,
            "text_offset": 2986,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌え！？\\nは、はい、何でしょう！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3046,
            "text_len": 28,
            "text_offset": 3056,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "そろそろ始めていいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3106,
            "text_len": 52,
            "text_offset": 3116,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "え、ええ、いつでもどうぞ。\\nよろしくお願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3190,
            "text_len": 18,
            "text_offset": 3200,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "それじゃ早速弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3342,
            "text_len": 62,
            "text_offset": 3352,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "弌弌おはようございます。\\nいやもう、こんちには、でしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3444,
            "text_len": 16,
            "text_offset": 3454,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3486,
            "text_len": 68,
            "text_offset": 3496,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "おっと。こんにちはと言うところを\\nこんちにはと言ってしまいました。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3594,
            "text_len": 20,
            "text_offset": 3604,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3744,
            "text_len": 50,
            "text_offset": 3754,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……おや？　どこからか紙が……\\nこれはいったい？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3940,
            "text_len": 56,
            "text_offset": 3948,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ちょっ！？\\nみおちゃん、あれってあんたの原稿じゃない！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4028,
            "text_len": 68,
            "text_offset": 4034,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え？　ほんとだ！　どこに行ったと思ったら……\\n早く取り戻さないと！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4206,
            "text_len": 68,
            "text_offset": 4216,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "これは……マンガかな？\\nえ―となになに……前略、こちらくちびる弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4392,
            "text_len": 14,
            "text_offset": 4398,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "しゃあっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4438,
            "text_len": 24,
            "text_offset": 4448,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "うわああああああっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4614,
            "text_len": 16,
            "text_offset": 4624,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4722,
            "text_len": 12,
            "text_offset": 4726,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ―っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4762,
            "text_len": 40,
            "text_offset": 4766,
            "warnings": 1
        },
        "speaker": "",
        "text": "うおお！？\\nシステムが暴走を始めたぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4836,
            "text_len": 42,
            "text_offset": 4840,
            "warnings": 1
        },
        "speaker": "",
        "text": "は、早く止めろっ！\\nじゃないと船が弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4914,
            "text_len": 30,
            "text_offset": 4918,
            "warnings": 1
        },
        "speaker": "",
        "text": "そ、そんなこと、言われても！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4994,
            "text_len": 26,
            "text_offset": 5006,
            "warnings": 3
        },
        "speaker": "＆＆",
        "text": "うあああああああああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5232,
            "text_len": 38,
            "text_offset": 5242,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "弌弌ああっ！？　長野原さんが倒れて！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5344,
            "text_len": 42,
            "text_offset": 5354,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "安中もです！\\n弌弌うおっ！？　田中まで！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5440,
            "text_len": 66,
            "text_offset": 5450,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "大変なことになってきましたな。\\n校長先生、話が長いですから……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5542,
            "text_len": 58,
            "text_offset": 5552,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（ふふふ、この調子だ！\\n　このままいけば全員、倒れる！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5652,
            "text_len": 56,
            "text_offset": 5662,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "教頭先生、校長先生を止めましょう！\\nでないと危険です！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5748,
            "text_len": 36,
            "text_offset": 5758,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（バカ！　な、何を言っとるんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5816,
            "text_len": 68,
            "text_offset": 5826,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "で、ですが、野生動物の危険性を\\nまだ生徒達に伝えていませんし……。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5918,
            "text_len": 76,
            "text_offset": 5928,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "でしたら、早くしてもらうよう伝えてください。\\nそうじゃないと生徒達が……！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6026,
            "text_len": 46,
            "text_offset": 6036,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "わ、わかってますよ！\\nちゃんと伝えますから！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6108,
            "text_len": 62,
            "text_offset": 6118,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（さて、どうするか？\\n　演説は長引かせなくてはならんし……）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6290,
            "text_len": 66,
            "text_offset": 6300,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うおっ！？　今度は中之条が倒れた！\\n弌弌あっちで立花も倒れてる！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6386,
            "text_len": 46,
            "text_offset": 6396,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "教頭先生、お願いします！\\n早くしてください！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6558,
            "text_len": 30,
            "text_offset": 6568,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（ええい、こうなったら弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6658,
            "text_len": 20,
            "text_offset": 6668,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ぐうううううっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6834,
            "text_len": 62,
            "text_offset": 6844,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "だ、大丈夫ですか！？\\nだ、誰か！　教頭先生まで倒れられたぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6926,
            "text_len": 60,
            "text_offset": 6936,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……ふふ、うまくいった。\\n　これで校長の話は長引く弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7026,
            "text_len": 74,
            "text_offset": 7036,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "保健室に運びましょう！　それから病院に！\\n高崎先生、担架をお願いします！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7126,
            "text_len": 64,
            "text_offset": 7136,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "だったら俺、背負います！\\n教頭先生、少しだけ我慢してください！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7306,
            "text_len": 38,
            "text_offset": 7316,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌え？\\nいや、別にそこまでは……。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7382,
            "text_len": 50,
            "text_offset": 7392,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "何を言ってるんですか！\\n高崎先生、お願いします！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7470,
            "text_len": 38,
            "text_offset": 7480,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "わかりました！　それじゃ行きますよ！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7630,
            "text_len": 54,
            "text_offset": 7640,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "いや、その、待ってください。\\n私は校長の最期を弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7832,
            "text_len": 58,
            "text_offset": 7840,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……ねえねえ、なのちゃん。\\n教頭先生まで倒れたんだって。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7922,
            "text_len": 80,
            "text_offset": 7928,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "校長先生の話、長いですからね……と言うか、\\n教頭先生も集会に出ていたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8030,
            "text_len": 86,
            "text_offset": 8038,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "うん、気付かなかったけどね。それにしても、\\n先生たちまでなんて、すごい被害だよね―。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8154,
            "text_len": 68,
            "text_offset": 8160,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……貧血でも、ここまで被害がでたら、\\n責任を取らされても仕方ない。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8260,
            "text_len": 70,
            "text_offset": 8266,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "う―ん、もしそうなったら、\\n次の校長先生には教頭先生がなるんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8366,
            "text_len": 82,
            "text_offset": 8374,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ムリムリ、日陰にいたのに倒れたんだよ？\\n校長先生なんて大役、体が持たないって―。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8478,
            "text_len": 76,
            "text_offset": 8484,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ、確かにそうですよね。お歳だそうですし、\\n無理させたらいけませんよね―。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8590,
            "text_len": 18,
            "text_offset": 8596,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……残念でした。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8718,
            "text_len": 16,
            "text_offset": 8726,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "残念？　何が？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8758,
            "text_len": 18,
            "text_offset": 8764,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……何でもない。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8844,
            "text_len": 42,
            "text_offset": 8854,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "わ、わかりました！　すぐに伝えますから！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9018,
            "text_len": 38,
            "text_offset": 9028,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（だがしかし、どうすればいいんだ？）\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9092,
            "text_len": 76,
            "text_offset": 9102,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（話を終わらせるような感じで、\\n　それでいて、話を長引かせる指示とは弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9200,
            "text_len": 26,
            "text_offset": 9210,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "うう、考えろ……考えろ！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9262,
            "text_len": 62,
            "text_offset": 9272,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（そう、私が校長になるため！\\n　考えろ！　考えるんだ……！）\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9450,
            "text_len": 14,
            "text_offset": 9460,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ぬぐっ……！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9538,
            "text_len": 6,
            "text_offset": 9548,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "う。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9666,
            "text_len": 32,
            "text_offset": 9676,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きゃ―っ！？　教頭先生が―！！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9738,
            "text_len": 56,
            "text_offset": 9748,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あ、頭が……\\nす、すいませんが、ちょっと保健室に……。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9828,
            "text_len": 66,
            "text_offset": 9838,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "教頭先生しっかりしてください！\\nわかりました、すぐに保健室に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9926,
            "text_len": 6,
            "text_offset": 9936,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "う。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10004,
            "text_len": 40,
            "text_offset": 10014,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "うわわっ！？\\nこ、今度は高崎先生が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10076,
            "text_len": 6,
            "text_offset": 10086,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10148,
            "text_len": 20,
            "text_offset": 10158,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10282,
            "text_len": 40,
            "text_offset": 10292,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "だ、誰か……早く……保健室……に……。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10454,
            "text_len": 56,
            "text_offset": 10464,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "え―……と言うわけで本日なのですが……\\nおや、皆さん？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10646,
            "text_len": 20,
            "text_offset": 10656,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10692,
            "text_len": 14,
            "text_offset": 10702,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……全滅！？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10736,
            "text_len": 16,
            "text_offset": 10746,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10866,
            "text_len": 24,
            "text_offset": 10876,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "以上で話を終わります。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10968,
            "text_len": 56,
            "text_offset": 10978,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "申し訳ないですが……それはできません！\\nできんのです！\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11070,
            "text_len": 16,
            "text_offset": 11080,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11128,
            "text_len": 46,
            "text_offset": 11138,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "な、なぜです！？　みんなが危ないんですよ！？\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11210,
            "text_len": 34,
            "text_offset": 11220,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "しかし、校長先生の邪魔はできん！\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11286,
            "text_len": 70,
            "text_offset": 11296,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ど、どうしてもというのなら……\\nそうだ、高崎先生、君がやりたまえっ！\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11392,
            "text_len": 26,
            "text_offset": 11402,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あ、はい、わかりました。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11466,
            "text_len": 30,
            "text_offset": 11476,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌え？\\nそんな簡単に……。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11540,
            "text_len": 60,
            "text_offset": 11550,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "校長せんせ―い、終わりで―す！\\nタイムオ―バ―ですから―！\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11732,
            "text_len": 52,
            "text_offset": 11742,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "え？　あ……そうなんですか。\\nえ―と、じゃあ……。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11816,
            "text_len": 68,
            "text_offset": 11826,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "校内に野生動物がうろついてます。\\n皆さん、注意してください、以上。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12034,
            "text_len": 74,
            "text_offset": 12044,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "そ、そんな……せっかくのチャンスを……\\nいとも簡単に……あっさりと……。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12150,
            "text_len": 42,
            "text_offset": 12160,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "教頭先生、これで臨時集会は終わりですか？\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12224,
            "text_len": 16,
            "text_offset": 12234,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あ、ああ……。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12354,
            "text_len": 54,
            "text_offset": 12364,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "弌弌以上で臨時集会を終わります。\\nそして教頭先生弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12434,
            "text_len": 48,
            "text_offset": 12444,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "本日までありがとうございました。\\n一同、礼弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12522,
            "text_len": 26,
            "text_offset": 12532,
            "warnings": 0
        },
        "speaker": "生徒たち",
        "text": "ありがとうございました！\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12674,
            "text_len": 16,
            "text_offset": 12684,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 12808,
            "text_len": 30,
            "text_offset": 12818,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……ありがとうございました。\\n",
        "type": "dialog"
    }
]