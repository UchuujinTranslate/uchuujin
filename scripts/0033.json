[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 38,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よし、必要なメンバ―が揃ったわけだ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2210,
            "text_len": 66,
            "text_offset": 2216,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "んぐ……ムシャムシャ……\\nこれで思う存分練習が…んぐっ、ゴクッ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2306,
            "text_len": 8,
            "text_offset": 2312,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2344,
            "text_len": 10,
            "text_offset": 2350,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "んん―？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2382,
            "text_len": 38,
            "text_offset": 2388,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "おやつ食べながら喋らないでください。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2456,
            "text_len": 52,
            "text_offset": 2466,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "まあまあ関口、いいじゃないか。\\n先生も１つ貰うぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2540,
            "text_len": 18,
            "text_offset": 2548,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、じゃあ僕も。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2588,
            "text_len": 46,
            "text_offset": 2598,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うん、うまい。\\n部室で食うおやつは最高だな！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2666,
            "text_len": 12,
            "text_offset": 2672,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "でしょう？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2710,
            "text_len": 12,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2752,
            "text_len": 40,
            "text_offset": 2762,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "しかし囲碁サッカ―も変わったもんだな。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2826,
            "text_len": 24,
            "text_offset": 2834,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "んぐ、そうなんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2880,
            "text_len": 66,
            "text_offset": 2890,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "オレが現役の頃は部室にこんなに\\n色んなもん置いてなかったもんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2980,
            "text_len": 22,
            "text_offset": 2986,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、先生それは弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3040,
            "text_len": 40,
            "text_offset": 3050,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ほら、あそこには犬まで居るじゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3114,
            "text_len": 38,
            "text_offset": 3120,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いやいや、部室に犬は居ないですよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3180,
            "text_len": 42,
            "text_offset": 3190,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何言ってるんだ、あそこに居るじゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3270,
            "text_len": 12,
            "text_offset": 3278,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3316,
            "text_len": 24,
            "text_offset": 3322,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "先生、あれ野良犬です。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3372,
            "text_len": 46,
            "text_offset": 3382,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何ぃっ！\\nちょっと待ってろ、追い出してくる！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3506,
            "text_len": 56,
            "text_offset": 3510,
            "warnings": 1
        },
        "speaker": "",
        "text": "しまった、捕まった！\\nこのままじゃ、地球人に作戦が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3590,
            "text_len": 36,
            "text_offset": 3594,
            "warnings": 1
        },
        "speaker": "",
        "text": "仕方ない、秘密兵器を使うとするか。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3652,
            "text_len": 16,
            "text_offset": 3656,
            "warnings": 1
        },
        "speaker": "",
        "text": "弌弌秘密兵器？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3694,
            "text_len": 64,
            "text_offset": 3698,
            "warnings": 1
        },
        "speaker": "",
        "text": "あれには証拠隠滅の装置がついてるんだ。\\nそれ、スイッチオ―ン！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3806,
            "text_len": 30,
            "text_offset": 3810,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ、おい！　画面が消えたぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3856,
            "text_len": 60,
            "text_offset": 3860,
            "warnings": 1
        },
        "speaker": "",
        "text": "そりゃそうだ。証拠隠滅のために\\nアンテナを破壊したからな。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3936,
            "text_len": 18,
            "text_offset": 3940,
            "warnings": 1
        },
        "speaker": "",
        "text": "ってことは……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4004,
            "text_len": 12,
            "text_offset": 4008,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4036,
            "text_len": 18,
            "text_offset": 4040,
            "warnings": 1
        },
        "speaker": "",
        "text": "……ご、ごめん。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4150,
            "text_len": 68,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "おお、ボ―ドゲ―ムまであるのか！\\nなあ……ちょっとだけやらないか？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4252,
            "text_len": 18,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お、いいですね！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4300,
            "text_len": 42,
            "text_offset": 4308,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの、囲碁サッカ―の練習はいいんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4372,
            "text_len": 56,
            "text_offset": 4382,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "おやつの時間が終わるまでだって！\\nほら、桜井もやろう！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4460,
            "text_len": 36,
            "text_offset": 4468,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "う、う―ん……まあ、ちょっとなら。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4536,
            "text_len": 16,
            "text_offset": 4542,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口もどうだ？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4582,
            "text_len": 16,
            "text_offset": 4588,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "私はいいです。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4626,
            "text_len": 38,
            "text_offset": 4632,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……そっか、じゃあ三人でやりますか。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4796,
            "text_len": 50,
            "text_offset": 4806,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うおおおぉぉっ！\\nどうして教師になれないんだ！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4880,
            "text_len": 22,
            "text_offset": 4886,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お、宝くじ当たった。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4930,
            "text_len": 26,
            "text_offset": 4938,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……こ、子供四人目って。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4986,
            "text_len": 44,
            "text_offset": 4996,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "くそっ、まだだ！\\nまだ逆転の可能性はある！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5062,
            "text_len": 40,
            "text_offset": 5070,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "僕だってまだ負けたわけじゃないですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5134,
            "text_len": 8,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5172,
            "text_len": 38,
            "text_offset": 5182,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ん、どうした関口。\\nお前も混じるか？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5246,
            "text_len": 32,
            "text_offset": 5252,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ここ、囲碁サッカ―部ですよね。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5310,
            "text_len": 12,
            "text_offset": 5320,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5370,
            "text_len": 12,
            "text_offset": 5378,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5448,
            "text_len": 54,
            "text_offset": 5454,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "何言ってんだ関口、\\nこれはすごく大切なことなんだぞ？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5530,
            "text_len": 8,
            "text_offset": 5536,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5566,
            "text_len": 78,
            "text_offset": 5572,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "まだお互いのことを何にも知らないんだ。\\nレクリエ―ションで親睦を深めないと！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5690,
            "text_len": 72,
            "text_offset": 5700,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そ、そうだ！　生徒とコミュニケ―ションを\\n取るのも……その、教育者だ！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5806,
            "text_len": 68,
            "text_offset": 5814,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そ、そうですよね！　今ので先生のこと\\nもっと理解出来た気がします！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5906,
            "text_len": 36,
            "text_offset": 5912,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "でも、試合まで時間無いんですよね。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5982,
            "text_len": 12,
            "text_offset": 5988,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6028,
            "text_len": 12,
            "text_offset": 6036,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6080,
            "text_len": 64,
            "text_offset": 6090,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだ！　お前たち！\\nこんな風に遊んでる暇なんてないんだぞ！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6188,
            "text_len": 6,
            "text_offset": 6194,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6230,
            "text_len": 14,
            "text_offset": 6238,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの、先生？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6278,
            "text_len": 46,
            "text_offset": 6288,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "さあ行くぞ！\\nまずはランニングと柔軟からだ！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6358,
            "text_len": 26,
            "text_offset": 6364,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "はい！　わかりました！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6420,
            "text_len": 14,
            "text_offset": 6428,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長まで！？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6464,
            "text_len": 16,
            "text_offset": 6474,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "よし行くぞ！！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6512,
            "text_len": 24,
            "text_offset": 6520,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、待ってくださいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6652,
            "text_len": 12,
            "text_offset": 6658,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6730,
            "text_len": 56,
            "text_offset": 6736,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "確かに、関口の言うとおりだ。\\nオレたちが間違っていた。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6816,
            "text_len": 52,
            "text_offset": 6824,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そうですね……\\nうっかり夢中になってしまうなんて。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6900,
            "text_len": 80,
            "text_offset": 6910,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "顧問を引き受けておいて遊んでしまうとは、\\n教育者としての自分を見失っていたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7012,
            "text_len": 58,
            "text_offset": 7018,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ありがとうな関口、\\nお前のおかげでオレたち目が覚めたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7100,
            "text_len": 12,
            "text_offset": 7106,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……いえ。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7140,
            "text_len": 38,
            "text_offset": 7146,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よし、それじゃ早速練習しましょうか。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7208,
            "text_len": 64,
            "text_offset": 7218,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだな、まずはランニングからだ。\\nそれが終わったら柔軟だぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7306,
            "text_len": 10,
            "text_offset": 7314,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はいっ。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7356,
            "text_len": 38,
            "text_offset": 7362,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ほら、関口。\\nお前も早く着替えろよ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7444,
            "text_len": 6,
            "text_offset": 7450,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7482,
            "text_len": 28,
            "text_offset": 7488,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの、私は参加しませんよ？\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7538,
            "text_len": 58,
            "text_offset": 7544,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "何言ってるんだ関口、\\nお前も囲碁サッカ―部員じゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7624,
            "text_len": 32,
            "text_offset": 7630,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あ、そうか。着替えのことだな？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7684,
            "text_len": 22,
            "text_offset": 7690,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、そうじゃ……。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7734,
            "text_len": 48,
            "text_offset": 7740,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "心配すんなって、\\nオレたちが外で着替えるから。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7810,
            "text_len": 14,
            "text_offset": 7816,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "だから……。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7852,
            "text_len": 40,
            "text_offset": 7858,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それじゃ、先に走ってるから早くこいよ？\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7936,
            "text_len": 12,
            "text_offset": 7942,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8058,
            "text_len": 18,
            "text_offset": 8064,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……どうしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8144,
            "text_len": 42,
            "text_offset": 8150,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口、それはそれ、これはこれってやつだ。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8224,
            "text_len": 12,
            "text_offset": 8230,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8266,
            "text_len": 28,
            "text_offset": 8276,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "おい、大工。お前の番だぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8326,
            "text_len": 62,
            "text_offset": 8332,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おっと、すいません。\\nよ―し、次は何が……げっ、家が火事！？\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8434,
            "text_len": 12,
            "text_offset": 8440,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8488,
            "text_len": 22,
            "text_offset": 8496,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "宝くじ帳消しですね！\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8552,
            "text_len": 12,
            "text_offset": 8558,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8594,
            "text_len": 36,
            "text_offset": 8604,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ぐあぁぁっ！\\nまた借金が増えた―！\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8662,
            "text_len": 16,
            "text_offset": 8670,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "先生ご愁傷様。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8720,
            "text_len": 12,
            "text_offset": 8726,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8762,
            "text_len": 40,
            "text_offset": 8770,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "さ、さてと、それじゃあ次はオレの……。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8838,
            "text_len": 12,
            "text_offset": 8844,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8888,
            "text_len": 12,
            "text_offset": 8896,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8934,
            "text_len": 12,
            "text_offset": 8940,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8980,
            "text_len": 12,
            "text_offset": 8990,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9030,
            "text_len": 12,
            "text_offset": 9036,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9076,
            "text_len": 24,
            "text_offset": 9084,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、あの、先生、部長。\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9136,
            "text_len": 16,
            "text_offset": 9146,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あ、ああ……。\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9190,
            "text_len": 18,
            "text_offset": 9196,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "練習、しようか。\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9318,
            "text_len": 12,
            "text_offset": 9324,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……ふぅ。\\n",
        "type": "dialog"
    }
]