[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2096,
            "text_len": 40,
            "text_offset": 2104,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はぁ、はぁ……\\nやっと逃げ切れた……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2166,
            "text_len": 64,
            "text_offset": 2174,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "だけどかなり時間を無駄にした……\\nこんなはずじゃなかったのに。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2260,
            "text_len": 54,
            "text_offset": 2268,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "明日１日で、\\nこの髪の毛を元に戻せるのだろうか……？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2396,
            "text_len": 12,
            "text_offset": 2408,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "いたっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2454,
            "text_len": 30,
            "text_offset": 2462,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ん？　あ、父さん。ただいま。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2522,
            "text_len": 24,
            "text_offset": 2534,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "これはどういうことだ！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2580,
            "text_len": 10,
            "text_offset": 2588,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2634,
            "text_len": 6,
            "text_offset": 2642,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2670,
            "text_len": 66,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "この色といい、長さといい……\\nどう見てもお前のモヒカンだろう！！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2770,
            "text_len": 56,
            "text_offset": 2778,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ、ああ……\\nぼ、僕の髪の毛が、こんなところに……！！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2862,
            "text_len": 46,
            "text_offset": 2874,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "大福くんとモヒカンで遊ぶとはどういうことだ！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2942,
            "text_len": 36,
            "text_offset": 2950,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ち、違う！　僕だって何がなんだか！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3012,
            "text_len": 68,
            "text_offset": 3020,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（弌弌ハッ！　そう言えば……\\n　イタコさん、呪いをかけるって……）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3114,
            "text_len": 60,
            "text_offset": 3122,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "父さん、１万円ちょうだい！\\n治せる人に心当たりがあるんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3204,
            "text_len": 50,
            "text_offset": 3216,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "本当か！！　なら今すぐその人を\\n呼んできなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3288,
            "text_len": 12,
            "text_offset": 3296,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "わかった！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3430,
            "text_len": 68,
            "text_offset": 3438,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "イタコさんはどこだ……？\\nまだこの辺りにいてくれるといいけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3532,
            "text_len": 60,
            "text_offset": 3540,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（まさか僕がイタコさんを\\n　必要とする時が来るなんて……）\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3626,
            "text_len": 60,
            "text_offset": 3634,
            "warnings": 1
        },
        "speaker": "中之条",
        "text": "（ああ、元僕がお金を払っていれば\\n　こんなことには……）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3720,
            "text_len": 12,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3762,
            "text_len": 22,
            "text_offset": 3770,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あっ！　イタコさん！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3818,
            "text_len": 12,
            "text_offset": 3826,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3860,
            "text_len": 56,
            "text_offset": 3868,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ごめんなさい！\\nお金はあります。だから呪いを解いて弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3954,
            "text_len": 12,
            "text_offset": 3962,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4082,
            "text_len": 6,
            "text_offset": 4090,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4118,
            "text_len": 60,
            "text_offset": 4126,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "えっ、イ、イタコさん！？\\nお金はあるから……呪いを……！！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4300,
            "text_len": 12,
            "text_offset": 4308,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4424,
            "text_len": 12,
            "text_offset": 4432,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "くっ……！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4470,
            "text_len": 20,
            "text_offset": 4478,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "イタコさあああん！\\n",
        "type": "dialog"
    }
]