[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 26,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あれ？　あの後姿は……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2194,
            "text_len": 54,
            "text_offset": 2202,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原じゃない。\\nあいつ、こんなところで何してんのよ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2278,
            "text_len": 46,
            "text_offset": 2286,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "まさか、\\nまたはた迷惑なことしてるんじゃ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2436,
            "text_len": 40,
            "text_offset": 2444,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "外で食事！！\\nしかもあれは……カニ！？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2506,
            "text_len": 66,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "バリッ！　ベキッ！　ホジホジ……\\nパクリ……モグモグ……ゴクン。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2690,
            "text_len": 14,
            "text_offset": 2696,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、美味。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2790,
            "text_len": 52,
            "text_offset": 2798,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原！　あんた、\\nこんなところで何やってんのよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2884,
            "text_len": 44,
            "text_offset": 2890,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "？　私は一人静かに食事をしていただけだが。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2956,
            "text_len": 58,
            "text_offset": 2964,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なにが静かによ！\\nカニ解体する音がまわりに響いてるわよ！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3044,
            "text_len": 50,
            "text_offset": 3050,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……そうであったか。\\nそれはすまないことをした。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3126,
            "text_len": 62,
            "text_offset": 3132,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "（カニ三昧フルコ―スが\\n　もれなく焼きガニになってしまった）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3234,
            "text_len": 62,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "お、カニか！　美味そうだな。\\n一度食ってみたかったんだよな！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3326,
            "text_len": 10,
            "text_offset": 3334,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3360,
            "text_len": 8,
            "text_offset": 3366,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "むっ？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3404,
            "text_len": 46,
            "text_offset": 3414,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "（し、しまった！\\n　つい匂いにつられて……）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3486,
            "text_len": 14,
            "text_offset": 3492,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "こやつ……。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3528,
            "text_len": 14,
            "text_offset": 3538,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "に、にゃ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3576,
            "text_len": 26,
            "text_offset": 3584,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ちょ、ちょっと笹原……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3632,
            "text_len": 40,
            "text_offset": 3638,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "猫の分際で大儀である。\\n一杯進ぜよう。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3706,
            "text_len": 62,
            "text_offset": 3716,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "（え、喋ったことはスル―！？\\n　……ま、いいや、ラッキ―！）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3802,
            "text_len": 42,
            "text_offset": 3810,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "やめなさいよ！\\nあんた、何考えてんのよ！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3874,
            "text_len": 62,
            "text_offset": 3882,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "猫にカニは良くないのよ！\\nビタミンＡ１分解酵素がどうとかで！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3972,
            "text_len": 24,
            "text_offset": 3982,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "（え、そうなのか！？）\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4034,
            "text_len": 76,
            "text_offset": 4042,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "だから、ごめんね。ところで笹原弌弌\\nあ、死にたくなかったらあっち行ってね。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4150,
            "text_len": 68,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "（ぬおおおおお〜〜〜〜っ！？\\n　警告してすぐ撃つんじゃねえ〜〜！）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4304,
            "text_len": 52,
            "text_offset": 4308,
            "warnings": 1
        },
        "speaker": "",
        "text": "ふう、危ないなぁ。\\nぎりぎり避けられてよかったぜ。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4388,
            "text_len": 12,
            "text_offset": 4392,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あっ。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4428,
            "text_len": 18,
            "text_offset": 4432,
            "warnings": 1
        },
        "speaker": "",
        "text": "ん？　どうした？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4492,
            "text_len": 28,
            "text_offset": 4496,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナに……命中してた。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4616,
            "text_len": 56,
            "text_offset": 4622,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ところで立花みさとよ。\\n１つ聞きたいことがあるのだが。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4700,
            "text_len": 24,
            "text_offset": 4708,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え？　な、なによ……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4754,
            "text_len": 54,
            "text_offset": 4760,
            "warnings": 2
        },
        "speaker": "笹原",
        "text": "俗世にはカニカマなる庶民の食品が\\nあるらしいが。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4836,
            "text_len": 48,
            "text_offset": 4842,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "あれは何なのだ？\\nカニなのか、カマボコなのか。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4912,
            "text_len": 74,
            "text_offset": 4918,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "一体どんな味がするのものなのか……\\nまったく想像できんのだ。教えてくれ。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5014,
            "text_len": 62,
            "text_offset": 5022,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ど、どんなって言われても……\\nカニっぽい味のするカマボコよ。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5106,
            "text_len": 58,
            "text_offset": 5112,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "カニっぽい味のカマボコと言われても……\\nどうも解せんな。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5194,
            "text_len": 58,
            "text_offset": 5200,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "カニの味を楽しみたいなら\\nカニを食べれば良いではないか。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5280,
            "text_len": 56,
            "text_offset": 5286,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "カマボコを食べたいなら\\nカマボコを食べれば良いはずだ。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5374,
            "text_len": 80,
            "text_offset": 5380,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "あえてカニカマを食すのは、カニカマ独自の\\n味わいを求めているからではないのか。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5482,
            "text_len": 66,
            "text_offset": 5488,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "私はそれが知りたい。\\nカニカマとは、いかなる味がするものなのか。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5616,
            "text_len": 52,
            "text_offset": 5624,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ってか、気になるなら\\n実際食べてみればいいでしょ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5698,
            "text_len": 86,
            "text_offset": 5704,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "何を馬鹿な。笹原家の長男ともあろう者が、\\nあのような庶民の食品を買うなど考えられん。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5862,
            "text_len": 12,
            "text_offset": 5868,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5988,
            "text_len": 52,
            "text_offset": 5996,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "何が庶民の食品よ！\\nカニカマ、馬鹿にしないでよね！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6068,
            "text_len": 50,
            "text_offset": 6076,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "っていうか、\\nあんたん家だってただの農家でしょ！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6142,
            "text_len": 68,
            "text_offset": 6148,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "農家だからこそだ。漁師ならば\\nカニカマも食す機会があるのだろうが。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6236,
            "text_len": 60,
            "text_offset": 6244,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "関係ないでしょ！\\n漁師が水揚げしてるわけじゃないんだから！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6330,
            "text_len": 24,
            "text_offset": 6336,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "漁師が……水揚げ……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6474,
            "text_len": 36,
            "text_offset": 6480,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……ぷっ、はははは。これは愉快だ。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6538,
            "text_len": 76,
            "text_offset": 6544,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "立花みさとにも可愛いところがあるのだな。\\nカニカマが海で泳いでいる、とは！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6656,
            "text_len": 42,
            "text_offset": 6664,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なっ！？\\nな、な、な、何言ってるのよ！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6728,
            "text_len": 32,
            "text_offset": 6734,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "なに、恥ずかしがることはない。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6788,
            "text_len": 62,
            "text_offset": 6794,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "女人であれば、メルヘンチックな空想も\\n許されようというもの。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6878,
            "text_len": 68,
            "text_offset": 6884,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "しかし……立花みさとがな……\\nそうか……人には意外な一面がある弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7108,
            "text_len": 12,
            "text_offset": 7114,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7242,
            "text_len": 60,
            "text_offset": 7250,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なに馬鹿なこと言ってるのよ！\\nいい加減なこと言わないで！！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7332,
            "text_len": 64,
            "text_offset": 7340,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "私が可愛いだなんて、\\nそんな、そんな、そんな〜〜〜〜〜〜っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7536,
            "text_len": 12,
            "text_offset": 7542,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7612,
            "text_len": 58,
            "text_offset": 7620,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "公共の公園で下らないこと考えないでよ！\\n近所迷惑なのよ！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7700,
            "text_len": 68,
            "text_offset": 7706,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "いやしかし、疑問は解決するに如かず。\\n気になるものは気になるのだ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7796,
            "text_len": 60,
            "text_offset": 7804,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ―もうめんどくさいわね！\\nちょっとそこで待ってなさいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7912,
            "text_len": 4,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8058,
            "text_len": 48,
            "text_offset": 8066,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ほら、買ってきてあげたわ。\\nこれがカニカマよ。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8136,
            "text_len": 66,
            "text_offset": 8142,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そうか、これが夢にまで見た……\\n立花みさとよ、わざわざすまぬな。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8230,
            "text_len": 40,
            "text_offset": 8238,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "い、いいから！\\nさっさと食べなさいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8300,
            "text_len": 36,
            "text_offset": 8306,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、ではさっそく。馳走になろう。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8374,
            "text_len": 32,
            "text_offset": 8382,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（あ、あれ！？　しまった！！）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8440,
            "text_len": 54,
            "text_offset": 8448,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（間違えて笹かま\\n　買って来ちゃった〜〜〜〜っ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8540,
            "text_len": 66,
            "text_offset": 8546,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、これがカニカマか。\\nしかし、これの一体どこがカニなのやら。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8634,
            "text_len": 64,
            "text_offset": 8642,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ち、違うの！　そ、その……\\nそれはカニカマじゃなくて笹かまよ！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8734,
            "text_len": 58,
            "text_offset": 8742,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原のこと考えてたら……\\n笹ってのが頭にあったから……。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8822,
            "text_len": 28,
            "text_offset": 8828,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "なるほど、これは笹かまか。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8878,
            "text_len": 80,
            "text_offset": 8884,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "カニカマがカニのようなカマボコなら、\\n笹かまは私のようなカマボコなのだろうな。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8986,
            "text_len": 6,
            "text_offset": 8994,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "は？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9022,
            "text_len": 58,
            "text_offset": 9028,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "気に入った！\\n今日からこの笹かまの名は笹原カマジロ―だ！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9112,
            "text_len": 12,
            "text_offset": 9120,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9154,
            "text_len": 70,
            "text_offset": 9160,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ。と言うわけで、今日の記念にカマジロ―を\\n立花みさとに進ぜよう。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9262,
            "text_len": 26,
            "text_offset": 9270,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え？　わ、私にくれるの？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9318,
            "text_len": 54,
            "text_offset": 9324,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ。せいぜい可愛がってやってくれ。\\n……では失礼。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9406,
            "text_len": 24,
            "text_offset": 9414,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原の……笹かま……。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9542,
            "text_len": 10,
            "text_offset": 9550,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ぱくっ。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9620,
            "text_len": 12,
            "text_offset": 9626,
            "warnings": 0
        },
        "speaker": "執事",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9678,
            "text_len": 56,
            "text_offset": 9686,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（あ、カニカマ……\\n　あるならさっさと出しなさいよね）\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9764,
            "text_len": 66,
            "text_offset": 9770,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、これがカニカマというものか。\\nではさっそく食してみるかな。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9858,
            "text_len": 50,
            "text_offset": 9864,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "赤い部分が甲羅か。\\nなるほど、良くできているな。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9956,
            "text_len": 6,
            "text_offset": 9964,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10014,
            "text_len": 6,
            "text_offset": 10020,
            "warnings": 0
        },
        "speaker": "執事",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10056,
            "text_len": 64,
            "text_offset": 10064,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（あ、赤いところを避けて食べてる……\\n　甲羅だと思って……？）\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10158,
            "text_len": 36,
            "text_offset": 10164,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、これは……なんというか……。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10226,
            "text_len": 12,
            "text_offset": 10234,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10350,
            "text_len": 26,
            "text_offset": 10356,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……なんとも、言えんな。\\n",
        "type": "dialog"
    }
]