[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 60,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はぁ、はぁ、うっかりしてた。\\n花壇に水をあげれてなかった。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2234,
            "text_len": 70,
            "text_offset": 2242,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "もうすぐ花が咲くくらいまで育ったのに\\nこれで枯れたら後悔しちゃうよ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2334,
            "text_len": 32,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "さぁて、早速水をあげないと弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2396,
            "text_len": 10,
            "text_offset": 2404,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……ん？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2444,
            "text_len": 12,
            "text_offset": 2450,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2488,
            "text_len": 12,
            "text_offset": 2492,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2530,
            "text_len": 6,
            "text_offset": 2538,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2590,
            "text_len": 12,
            "text_offset": 2598,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2630,
            "text_len": 6,
            "text_offset": 2638,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2674,
            "text_len": 72,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（ヤギ、鹿、犬の３匹が从みあっている……\\n　一体何が始まるんだ……？）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2780,
            "text_len": 70,
            "text_offset": 2788,
            "warnings": 1
        },
        "speaker": "中之条",
        "text": "（もしかして各の誇りをかけた戦いが\\n　これから始まるとでも……？）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2940,
            "text_len": 12,
            "text_offset": 2944,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2984,
            "text_len": 12,
            "text_offset": 2988,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3022,
            "text_len": 46,
            "text_offset": 3026,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんだこれ？\\nすんげ―从み合ってるけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3094,
            "text_len": 52,
            "text_offset": 3098,
            "warnings": 1
        },
        "speaker": "",
        "text": "もしかして……バトル？\\nバトル始まっちゃうのかな？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3172,
            "text_len": 30,
            "text_offset": 3176,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ、じゃあ俺……鹿に賭ける！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3228,
            "text_len": 44,
            "text_offset": 3232,
            "warnings": 1
        },
        "speaker": "",
        "text": "おお、なら俺は犬！\\n大穴狙いで行ってやる！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3298,
            "text_len": 26,
            "text_offset": 3302,
            "warnings": 1
        },
        "speaker": "",
        "text": "じゃあ俺はヤギでいくぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3354,
            "text_len": 18,
            "text_offset": 3358,
            "warnings": 1
        },
        "speaker": "",
        "text": "おっ、始まった！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3398,
            "text_len": 52,
            "text_offset": 3402,
            "warnings": 1
        },
        "speaker": "",
        "text": "いけいけ！　スピ―ドを武器に\\n相手を翻弄するんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3476,
            "text_len": 20,
            "text_offset": 3480,
            "warnings": 1
        },
        "speaker": "",
        "text": "負けるなよヤギ―！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3546,
            "text_len": 6,
            "text_offset": 3558,
            "warnings": 3
        },
        "speaker": "＆＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3580,
            "text_len": 22,
            "text_offset": 3584,
            "warnings": 1
        },
        "speaker": "",
        "text": "犬がリタイア――！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3706,
            "text_len": 36,
            "text_offset": 3714,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（な、何故ここに鹿とヤギが……？）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3776,
            "text_len": 60,
            "text_offset": 3784,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（い、いや、それよりもあの２匹、\\n　从みあっていないか？）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3870,
            "text_len": 60,
            "text_offset": 3878,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（まさか、これからケンカでも\\n　おっぱじめようとでも……）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3966,
            "text_len": 12,
            "text_offset": 3972,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4012,
            "text_len": 12,
            "text_offset": 4016,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4048,
            "text_len": 72,
            "text_offset": 4056,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（この張り詰めた空気……間違いない。\\n　これから戦いが始まるんだ……）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4148,
            "text_len": 76,
            "text_offset": 4156,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（この現代社会で獣同士の戦いを見られるなんて\\n　なんてついているんだ……）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4266,
            "text_len": 12,
            "text_offset": 4274,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4316,
            "text_len": 26,
            "text_offset": 4324,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（あっ！？　マズイ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4378,
            "text_len": 64,
            "text_offset": 4386,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（あの２匹の中心には……\\n　僕が世話をしている花壇が……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4476,
            "text_len": 68,
            "text_offset": 4484,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（戦いが始まったら、あの花壇は\\n　決して無事では済まない……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4612,
            "text_len": 22,
            "text_offset": 4620,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "待て待て待て――！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4682,
            "text_len": 6,
            "text_offset": 4692,
            "warnings": 0
        },
        "speaker": "鹿＆ヤギ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4742,
            "text_len": 58,
            "text_offset": 4750,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "僕の目が黒いうちは花壇の花たちに\\n指一本ふれさせない！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4852,
            "text_len": 6,
            "text_offset": 4862,
            "warnings": 0
        },
        "speaker": "鹿＆ヤギ",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4904,
            "text_len": 6,
            "text_offset": 4908,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4938,
            "text_len": 6,
            "text_offset": 4946,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5060,
            "text_len": 10,
            "text_offset": 5068,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "合気道！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5098,
            "text_len": 6,
            "text_offset": 5104,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5226,
            "text_len": 6,
            "text_offset": 5232,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5262,
            "text_len": 6,
            "text_offset": 5270,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5384,
            "text_len": 10,
            "text_offset": 5392,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "合気道！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5516,
            "text_len": 18,
            "text_offset": 5524,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "まだまだ――！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5664,
            "text_len": 34,
            "text_offset": 5676,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "おおお……動物相手にも容赦ね―。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5732,
            "text_len": 66,
            "text_offset": 5744,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "こんな凶暴な一面も持ってたなんて、\\nやっぱりモヒカンはパンクだ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5882,
            "text_len": 60,
            "text_offset": 5890,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（いや、しかし、僕には\\n　２匹の戦いを止める力は無い……）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5976,
            "text_len": 82,
            "text_offset": 5984,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（そもそも動物同士の戦いに人間の都合で\\n　水を差してしまってもいいのだろうか？）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6092,
            "text_len": 52,
            "text_offset": 6100,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（いや、ダメだ。\\n　僕には手を出すことはできない）\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6178,
            "text_len": 66,
            "text_offset": 6186,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（この世は弱肉強食。この２匹には\\n　もう戦う以外の選択肢は無い）\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6278,
            "text_len": 58,
            "text_offset": 6286,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（強い方が勝ち、弱い方が負ける。\\n　それが自然の摂理だ）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6370,
            "text_len": 58,
            "text_offset": 6378,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（なんてこった……\\n　僕はあの花たちを助けられない……）\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6462,
            "text_len": 12,
            "text_offset": 6470,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6504,
            "text_len": 44,
            "text_offset": 6512,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "去ろう……\\n僕にできることは何もない……。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6636,
            "text_len": 12,
            "text_offset": 6640,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6712,
            "text_len": 12,
            "text_offset": 6718,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 6868,
            "text_len": 32,
            "text_offset": 6888,
            "warnings": 0
        },
        "speaker": "ナレ―タ―お姉さん",
        "text": "彼らも気まずくなったようです。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6990,
            "text_len": 12,
            "text_offset": 6996,
            "warnings": 0
        },
        "speaker": "ヤギ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 7036,
            "text_len": 12,
            "text_offset": 7040,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7068,
            "text_len": 14,
            "text_offset": 7076,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7126,
            "text_len": 12,
            "text_offset": 7134,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7172,
            "text_len": 12,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7296,
            "text_len": 24,
            "text_offset": 7304,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "教室、戻らないと……。\\n",
        "type": "dialog"
    }
]