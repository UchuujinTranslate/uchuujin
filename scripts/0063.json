            "speaker_offset": 6970,
            "text_len": 20,
            "text_offset": 6980,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "効かない……かな？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7038,
            "text_len": 68,
            "text_offset": 7048,
            "warnings": 0
        },
