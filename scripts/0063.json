            "speaker_len": 8,
            "speaker_offset": 5020,
            "text_len": 6,
            "text_offset": 5030,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5062,
            "text_len": 48,
            "text_offset": 5072,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あの鹿はカボチャ……\\nカボチャ、カボチャ……。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5140,
            "text_len": 12,
            "text_offset": 5144,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5176,
            "text_len": 12,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5308,
            "text_len": 26,
            "text_offset": 5318,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "よしっ！　これで大丈夫！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5370,
            "text_len": 30,
            "text_offset": 5380,
            "warnings": 0
        },
