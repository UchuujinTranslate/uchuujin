[
    {
        "id": 0,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 2138,
            "text_len": 30,
            "text_offset": 2162,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒１）",
        "text": "んじゃ、オレ部活あるから―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2214,
            "text_len": 22,
            "text_offset": 2220,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "お―、また明日な―。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2262,
            "text_len": 12,
            "text_offset": 2268,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2402,
            "text_len": 34,
            "text_offset": 2408,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……よし、ここなら誰も居ないな。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2546,
            "text_len": 70,
            "text_offset": 2552,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "いつまでもアフロに知らない物が\\n入っているのは落ち着かないからな―。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2638,
            "text_len": 48,
            "text_offset": 2644,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "さ―て、\\nそんなわけでアフロの大掃除といくか！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2812,
            "text_len": 24,
            "text_offset": 2818,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "え―っと、まずは……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2868,
            "text_len": 8,
            "text_offset": 2874,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！！？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2908,
            "text_len": 42,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "うわっ、うわぁっ！？\\n何か手握られた！！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2976,
            "text_len": 50,
            "text_offset": 2982,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "うそっ、何か居る！？\\nう……うわあぁぁぁぁっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3080,
            "text_len": 12,
            "text_offset": 3086,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3120,
            "text_len": 50,
            "text_offset": 3126,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "な―んだ、マネキンの腕か。\\nびっくりした―……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3218,
            "text_len": 44,
            "text_offset": 3224,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……なんだ？\\n腕から音が……ええ――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3290,
            "text_len": 22,
            "text_offset": 3296,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……あ、危な―いっ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3364,
            "text_len": 6,
            "text_offset": 3374,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3460,
            "text_len": 34,
            "text_offset": 3464,
            "warnings": 1
        },
        "speaker": "",
        "text": "地球人の攻撃によりアンテナ破損！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3540,
            "text_len": 60,
            "text_offset": 3544,
            "warnings": 1
        },
        "speaker": "",
        "text": "まさか急に動き出すとは……\\n予想外の攻撃に反応が遅れたぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3620,
            "text_len": 84,
            "text_offset": 3624,
            "warnings": 1
        },
        "speaker": "",
        "text": "ロケットパンチはアニメやゲ―ムの中だけだと\\n思っていたのが、仇になっちまった……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3724,
            "text_len": 88,
            "text_offset": 3728,
            "warnings": 1
        },
        "speaker": "",
        "text": "アニメやゲ―ムみたいなことが起きるなら、俺、\\n最後ぐらい女の子と仲良くなりたかったよ！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4092,
            "text_len": 48,
            "text_offset": 4098,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "え―っと……\\nこれはさっきのコケシっぽいのか。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4162,
            "text_len": 66,
            "text_offset": 4168,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "こんな物、いつ入ったんだ？\\n名前も書いてないしなあ……ま、次だ。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4440,
            "text_len": 44,
            "text_offset": 4446,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "次は……どら焼きか。\\nこれはオレのだよな。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4696,
            "text_len": 76,
            "text_offset": 4702,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "雑誌に……大工ストアのポイントカ―ド……\\nタケノコ！？　何でなまものまで！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4984,
            "text_len": 44,
            "text_offset": 4990,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "何だこりゃ、炊飯器？\\n……わけわかんね―。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5068,
            "text_len": 50,
            "text_offset": 5074,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "って、この量！\\n明らかに髪に入りきらないだろ！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5172,
            "text_len": 32,
            "text_offset": 5178,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……ん？\\nどこからか視線が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5244,
            "text_len": 32,
            "text_offset": 5250,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（うわ、アレ中村先生じゃん！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5306,
            "text_len": 72,
            "text_offset": 5312,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（こっち見てるっていうか見られてた！？\\n　ヤベッ、どうしようかな……）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5410,
            "text_len": 76,
            "text_offset": 5416,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（ち、近づいてきたぞ！　っていうか\\n　手に持ってる小さなビスは何だよ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5518,
            "text_len": 34,
            "text_offset": 5524,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（あ―もう、わけわかんね―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5624,
            "text_len": 22,
            "text_offset": 5630,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（に、逃げろっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5686,
            "text_len": 14,
            "text_offset": 5696,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "あっ、待て！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5836,
            "text_len": 52,
            "text_offset": 5842,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（どんなもんだ！\\n　こう見えて走りには自信が弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6018,
            "text_len": 50,
            "text_offset": 6024,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ぬおおおおっ！？\\nな、なんだコレ！　落とし穴！？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6100,
            "text_len": 38,
            "text_offset": 6110,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "よし、これで追いつけ弌弌しまった！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6192,
            "text_len": 6,
            "text_offset": 6198,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6224,
            "text_len": 40,
            "text_offset": 6234,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "こら、田中！\\n私まで落ちてしまったぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6316,
            "text_len": 34,
            "text_offset": 6322,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ええっ！？　オレが怒られるの！？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6372,
            "text_len": 66,
            "text_offset": 6382,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ああ、お前が逃げなければ、\\n自分で掘った穴になんて落ちなかった。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6474,
            "text_len": 18,
            "text_offset": 6480,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "自分で掘った！？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6514,
            "text_len": 16,
            "text_offset": 6524,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "弌弌あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6580,
            "text_len": 12,
            "text_offset": 6586,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6620,
            "text_len": 60,
            "text_offset": 6626,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ところでセンセ―、\\nこれ、どうやって抜ければいいんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6702,
            "text_len": 28,
            "text_offset": 6712,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……どうやればいいか、か。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6760,
            "text_len": 12,
            "text_offset": 6770,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6886,
            "text_len": 24,
            "text_offset": 6896,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "誰か―、助けてくれ―！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6940,
            "text_len": 20,
            "text_offset": 6946,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ええええええっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7028,
            "text_len": 38,
            "text_offset": 7034,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（よし、ここはまた誤魔化すしか……）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7106,
            "text_len": 12,
            "text_offset": 7112,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7152,
            "text_len": 12,
            "text_offset": 7162,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7200,
            "text_len": 24,
            "text_offset": 7206,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ど、どら焼き食べる！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7262,
            "text_len": 6,
            "text_offset": 7272,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7310,
            "text_len": 28,
            "text_offset": 7320,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "コ、コ―ヒ―を飲まないか？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7380,
            "text_len": 6,
            "text_offset": 7386,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7428,
            "text_len": 42,
            "text_offset": 7434,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……あ、えっと……\\nはい、どら焼きです。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7502,
            "text_len": 50,
            "text_offset": 7508,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（……あっれ―？\\n　何でこんな事になってんだろ）\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7580,
            "text_len": 36,
            "text_offset": 7590,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "あ、ああ、頂こう。\\nお前も、ほら。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7652,
            "text_len": 20,
            "text_offset": 7658,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（……まあいいや）\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7700,
            "text_len": 76,
            "text_offset": 7706,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あちっ、これまだちょっと熱いな。\\n……このコ―ヒ―ってセンセ―が亞れたの？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7804,
            "text_len": 70,
            "text_offset": 7814,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ああ、今日は間違えないように、\\n別の水筒に入れて持ち歩いていたんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7906,
            "text_len": 76,
            "text_offset": 7916,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "しかし、コ―ヒ―とどら焼きというのは\\nなかなか不思議な組み合わ……せ……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8020,
            "text_len": 16,
            "text_offset": 8026,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "え？　先生！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8062,
            "text_len": 30,
            "text_offset": 8072,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "しまった……私が飲んで……。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8164,
            "text_len": 14,
            "text_offset": 8170,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "何ぃぃっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8210,
            "text_len": 12,
            "text_offset": 8216,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8250,
            "text_len": 22,
            "text_offset": 8256,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あ―、え―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8386,
            "text_len": 34,
            "text_offset": 8392,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "と、とりあえず秘密は守れたぜ！！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8484,
            "text_len": 12,
            "text_offset": 8494,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8534,
            "text_len": 48,
            "text_offset": 8540,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "セ、センセ―？\\n今、もしかして見てました……？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8614,
            "text_len": 12,
            "text_offset": 8624,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8664,
            "text_len": 20,
            "text_offset": 8670,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あの―、センセ―？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8712,
            "text_len": 24,
            "text_offset": 8722,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "見た？　なんのことだ？\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8768,
            "text_len": 26,
            "text_offset": 8774,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "いや、別に何でもないよ！\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8830,
            "text_len": 30,
            "text_offset": 8836,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（ラッキ―！　助かったぜ！）\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8888,
            "text_len": 42,
            "text_offset": 8898,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "弌弌よし。\\nやはり、これのビスだったか。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8962,
            "text_len": 38,
            "text_offset": 8968,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あ、その炊飯器、ネジが抜けてたんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9028,
            "text_len": 34,
            "text_offset": 9038,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "炊飯器？　これは遠心分離機だぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9110,
            "text_len": 56,
            "text_offset": 9120,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "いつの間にか理科実験室から\\n無くなっていたのだが……。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9198,
            "text_len": 60,
            "text_offset": 9204,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "へ―見つかって良かったね。\\nそれ、かなり高そうだもんな―。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9286,
            "text_len": 56,
            "text_offset": 9296,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ああ、凄く高い。\\n無くしたのがバレて減俸されたからな。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9378,
            "text_len": 10,
            "text_offset": 9384,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "え……？\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9436,
            "text_len": 68,
            "text_offset": 9446,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "しかし、灯台下暗しだな。\\nまさかこんな場所に持ち出されていたとは。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9540,
            "text_len": 48,
            "text_offset": 9546,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ええっ！？\\nいや、確かに持ってましたけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9616,
            "text_len": 68,
            "text_offset": 9626,
            "warnings": 1
        },
        "speaker": "中村先生",
        "text": "……さて校舎に戻ろうか。\\nこの件について色と話を聞きたいからな。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9720,
            "text_len": 12,
            "text_offset": 9726,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9846,
            "text_len": 38,
            "text_offset": 9852,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（ぜんぜんラッキ―じゃなかった……）\\n",
        "type": "dialog"
    }
]