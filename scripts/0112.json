[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 78,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そういえば、次の授業は化学であったか。\\nトイレが済んだら移動せねばならんな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2250,
            "text_len": 42,
            "text_offset": 2256,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……む、課題が出ていたのを失念していた。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2320,
            "text_len": 56,
            "text_offset": 2326,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "私とした事が迂佛な。\\n仕方が無い、こうなれば誰かに弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2406,
            "text_len": 22,
            "text_offset": 2412,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ……あわわわっ！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2492,
            "text_len": 14,
            "text_offset": 2498,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "むう……っ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2548,
            "text_len": 44,
            "text_offset": 2554,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "……何とか止まれたか。\\n危ない所であった。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2620,
            "text_len": 34,
            "text_offset": 2626,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はぁ……ぶつからなくて良かった。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2682,
            "text_len": 40,
            "text_offset": 2688,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "済まなかったな、こちらの前方不注意だ。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2750,
            "text_len": 30,
            "text_offset": 2756,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、いえっ！　こちらこそ！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2808,
            "text_len": 34,
            "text_offset": 2814,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "しかしお互い大事無くて一安心弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2956,
            "text_len": 56,
            "text_offset": 2964,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原ァ――――！！\\nあんた下級生に何やってんのよ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3084,
            "text_len": 6,
            "text_offset": 3090,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3216,
            "text_len": 6,
            "text_offset": 3224,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3246,
            "text_len": 10,
            "text_offset": 3252,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "なっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3278,
            "text_len": 18,
            "text_offset": 3284,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ワンちゃんが！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3318,
            "text_len": 24,
            "text_offset": 3326,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "よ、よけて――――っ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3430,
            "text_len": 12,
            "text_offset": 3434,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3478,
            "text_len": 28,
            "text_offset": 3482,
            "warnings": 1
        },
        "speaker": "",
        "text": "……バリア、間に合ったか。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3534,
            "text_len": 20,
            "text_offset": 3538,
            "warnings": 1
        },
        "speaker": "",
        "text": "いいや、手遅れさ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3580,
            "text_len": 80,
            "text_offset": 3584,
            "warnings": 1
        },
        "speaker": "",
        "text": "そ、そんな……あんなにかわいい犬を\\n犠牲にしてしまったなんて……俺は……俺は！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3686,
            "text_len": 76,
            "text_offset": 3690,
            "warnings": 1
        },
        "speaker": "",
        "text": "は、何言ってんだ、犬は無事だぜ？\\nアンテナのバリアが間に合わなかったんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3788,
            "text_len": 28,
            "text_offset": 3792,
            "warnings": 1
        },
        "speaker": "",
        "text": "なるほど、そりゃ手遅れだ！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3862,
            "text_len": 32,
            "text_offset": 3870,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "でも、犬は助かって良かったぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3992,
            "text_len": 20,
            "text_offset": 3998,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "きゃぁぁぁぁっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4034,
            "text_len": 6,
            "text_offset": 4040,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4150,
            "text_len": 18,
            "text_offset": 4156,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あいたたた……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4196,
            "text_len": 38,
            "text_offset": 4204,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あっちゃ〜、下級生巻き込んじゃっ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4356,
            "text_len": 18,
            "text_offset": 4364,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え……ええ……？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4400,
            "text_len": 10,
            "text_offset": 4406,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4436,
            "text_len": 24,
            "text_offset": 4444,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、あわわわ……っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4484,
            "text_len": 18,
            "text_offset": 4490,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ……ああっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4618,
            "text_len": 12,
            "text_offset": 4624,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4698,
            "text_len": 16,
            "text_offset": 4704,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "素晴らしい！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4744,
            "text_len": 6,
            "text_offset": 4750,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4782,
            "text_len": 80,
            "text_offset": 4788,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "まさかここで爆発にあわせて手を飛ばすとは！\\nなんたる胆力、なんたる傾きぶり！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4890,
            "text_len": 14,
            "text_offset": 4896,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "は、はい！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4932,
            "text_len": 36,
            "text_offset": 4938,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "気に入った！\\nこれが噂のロボッ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5130,
            "text_len": 52,
            "text_offset": 5138,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原ァ！\\nアンタそれより先にやる事あるでしょうが！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5206,
            "text_len": 46,
            "text_offset": 5212,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "やる事……？\\nなるほど、確かにそうだったな。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5370,
            "text_len": 62,
            "text_offset": 5376,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "すまんな立花みさと。\\n礼を言うぞ、危うく己を見失う所だった。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5462,
            "text_len": 44,
            "text_offset": 5470,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "べ、別に礼なんていいから早くやりなさいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5618,
            "text_len": 68,
            "text_offset": 5624,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、ではトイレへ馳せ参じてくる。\\n急いでいたのを忘れておったわ。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5712,
            "text_len": 20,
            "text_offset": 5720,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そっちじゃない！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5808,
            "text_len": 26,
            "text_offset": 5816,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "手、手がっ！　手が！！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5866,
            "text_len": 54,
            "text_offset": 5872,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "落ち着け立花みさと。\\nそれはそこの娘がやった手品だ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5964,
            "text_len": 20,
            "text_offset": 5972,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え、そ、そうなの？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6016,
            "text_len": 52,
            "text_offset": 6022,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ……はっ、はい！\\nそうです！　そうなんですよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6098,
            "text_len": 60,
            "text_offset": 6104,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "立花みさとが驚かせるから、\\nとっさに驚かせ返したらしいな。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6186,
            "text_len": 66,
            "text_offset": 6192,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "いやはや、何とも機転の利く娘だ。\\n天晴れとしか言いようが無いな。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6280,
            "text_len": 38,
            "text_offset": 6288,
            "warnings": 1
        },
        "speaker": "みさと",
        "text": "う、うん……？\\n…何か違和感が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6350,
            "text_len": 18,
            "text_offset": 6356,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ホントですよ！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6396,
            "text_len": 60,
            "text_offset": 6402,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "だって、そんな、人間の手が\\n飛ぶわけないじゃないですか〜。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6484,
            "text_len": 30,
            "text_offset": 6492,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そうだよね……あははははは。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6546,
            "text_len": 58,
            "text_offset": 6552,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そういうわけだ。\\n決して噂のロボット生徒ではないからな。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6718,
            "text_len": 12,
            "text_offset": 6724,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ぎゃっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6800,
            "text_len": 54,
            "text_offset": 6806,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "おっと、こんな場合ではなかった。\\nトイレへ急がねば。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6884,
            "text_len": 48,
            "text_offset": 6890,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ、ええ〜っ！？\\nあの、私のこの状態は……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6960,
            "text_len": 46,
            "text_offset": 6966,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "トイレの前には全て些事。\\nそれでは失礼する。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7056,
            "text_len": 30,
            "text_offset": 7064,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ちょ、ちょっと待ちなさい！！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7120,
            "text_len": 62,
            "text_offset": 7128,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "アンタのせいでしょアンタの！！\\n責任とって何とかしなさいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7212,
            "text_len": 58,
            "text_offset": 7218,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "落ち着け立花みさと、\\nどう考えてもお前のせいではないか。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7298,
            "text_len": 50,
            "text_offset": 7306,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "アンタが下級生に\\n変なことしてたせいでしょうが！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7378,
            "text_len": 40,
            "text_offset": 7384,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ただぶつかりそうになって避けただけだ。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7448,
            "text_len": 28,
            "text_offset": 7454,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ、そ、そうなんですよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7504,
