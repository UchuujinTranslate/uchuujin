[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2138,
            "text_len": 70,
            "text_offset": 2148,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "よ―し、授業はここまで。\\n残り時間でこの間の小テストを返却するぞ―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2240,
            "text_len": 16,
            "text_offset": 2250,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "まずは相生―。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2288,
            "text_len": 10,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "は―い。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2332,
            "text_len": 42,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（……相変わらず相生のテストはひどいな）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2410,
            "text_len": 72,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（真面目にやろうとした形跡はあるんだが、\\n　……いや、そうでもないか）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2518,
            "text_len": 68,
            "text_offset": 2528,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（特に漢字の読み問題。\\n　何で途中から詩にすりかわっているんだ？）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2622,
            "text_len": 72,
            "text_offset": 2632,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（文章問題はオヤジギャグになってるし……。\\n　本当にひどいありさまだ）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2746,
            "text_len": 18,
            "text_offset": 2756,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……むっ！　猫？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2806,
            "text_len": 46,
            "text_offset": 2816,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "迷い込んだのか？\\n何にせよ、授業の邪魔だな。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2940,
            "text_len": 28,
            "text_offset": 2944,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ、おい、見つかったぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2996,
            "text_len": 60,
            "text_offset": 3000,
            "warnings": 1
        },
        "speaker": "",
        "text": "安心しろって、こんな時のために\\n姿を隠す装置を搭載してる。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3082,
            "text_len": 28,
            "text_offset": 3086,
            "warnings": 1
        },
        "speaker": "",
        "text": "おおっ、で、どのボタンだ？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3136,
            "text_len": 30,
            "text_offset": 3140,
            "warnings": 1
        },
        "speaker": "",
        "text": "そこの赤くてデカいボタン弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3204,
            "text_len": 10,
            "text_offset": 3208,
            "warnings": 1
        },
        "speaker": "",
        "text": "これか。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3234,
            "text_len": 30,
            "text_offset": 3238,
            "warnings": 1
        },
        "speaker": "",
        "text": "弌弌の隣にある小さいボタン。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3314,
            "text_len": 20,
            "text_offset": 3318,
            "warnings": 1
        },
        "speaker": "",
        "text": "……紛らわしいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3434,
            "text_len": 62,
            "text_offset": 3444,
            "warnings": 1
        },
        "speaker": "高崎先生",
        "text": "（しかし……少し視点を変えると、\\n　中面白いかもしれない）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3534,
            "text_len": 86,
            "text_offset": 3544,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（真面目にやってこの回答が出せるというのは\\n　思考の幅が広い証拠じゃないだろうか？）\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3656,
            "text_len": 82,
            "text_offset": 3666,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（頭ごなしに何でも叱るのが教育じゃない。\\n　時には褒めてやるのも大切なはず……）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3774,
            "text_len": 36,
            "text_offset": 3784,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（しかし、テストの点を思えば……）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3880,
            "text_len": 40,
            "text_offset": 3890,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（いや、ここはきちんと叱るべきだ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3956,
            "text_len": 30,
            "text_offset": 3964,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "せんせ―、取りに来ましたっ！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4026,
            "text_len": 30,
            "text_offset": 4036,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "相生っ！\\nこの回答は何だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4088,
            "text_len": 60,
            "text_offset": 4096,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ええっ、私なんか変な……あ……。\\nいや、そのポエムは……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4178,
            "text_len": 16,
            "text_offset": 4188,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "特にこの部分！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4230,
            "text_len": 34,
            "text_offset": 4240,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "衝撃\\n一目見て受けたインパクト！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4310,
            "text_len": 18,
            "text_offset": 4318,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ノォォォ―ウ！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4358,
            "text_len": 12,
            "text_offset": 4368,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "他にもだ！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4406,
            "text_len": 30,
            "text_offset": 4416,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "永久\\n私の想いはエタ―ナル！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4482,
            "text_len": 68,
            "text_offset": 4490,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "せ、せせせせせんせぇ―っ！？\\nよ、よ、読み上げるのはちょっと……！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4580,
            "text_len": 28,
            "text_offset": 4590,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そんな事を言ってる場合か！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4726,
            "text_len": 62,
            "text_offset": 4736,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "いいか相生ッ！！\\n国語のテストでカタカナ語を使うんじゃない！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4818,
            "text_len": 32,
            "text_offset": 4826,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ツッコむトコそこ―――――！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4924,
            "text_len": 64,
            "text_offset": 4934,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（そうだ、きちんと褒めてやろう。\\n　この詩はいい詩じゃないか）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5024,
            "text_len": 30,
            "text_offset": 5032,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "せんせ―、取りに来ましたっ！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5084,
            "text_len": 20,
            "text_offset": 5094,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あのな、相生……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5136,
            "text_len": 56,
            "text_offset": 5144,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "何ですか？\\nあ、もしかしていつもより点数良かったとか？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5232,
            "text_len": 18,
            "text_offset": 5242,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ナイスポエム！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5298,
            "text_len": 6,
            "text_offset": 5306,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5344,
            "text_len": 64,
            "text_offset": 5354,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "出題した漢字を全て使った上で\\nしっかりとしたスト―リ―がある！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5440,
            "text_len": 20,
            "text_offset": 5448,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え、ええええっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5494,
            "text_len": 70,
            "text_offset": 5504,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "これだけのポエムが作れるなら、\\n詩人としてやっていけるかもしれん！！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5602,
            "text_len": 46,
            "text_offset": 5610,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "マ、マジですか！\\nそんなに良かったですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5688,
            "text_len": 28,
            "text_offset": 5698,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ああ、すばらしいポエムだ！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5758,
            "text_len": 16,
            "text_offset": 5766,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "高崎先生……。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5886,
            "text_len": 32,
            "text_offset": 5896,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "たとえテストは再試験でもな！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5948,
            "text_len": 16,
            "text_offset": 5956,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6034,
            "text_len": 78,
            "text_offset": 6044,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（いや、今回は何も言わないでやろう。\\n　結果が結果だ、本人も見て反省するさ）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6154,
            "text_len": 32,
            "text_offset": 6162,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "せんせ―、取りに来ましたよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6216,
            "text_len": 40,
            "text_offset": 6224,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや―、今回は結構自信あるんですよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6294,
            "text_len": 18,
            "text_offset": 6304,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "じ、自信が……？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6358,
            "text_len": 52,
            "text_offset": 6366,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "特に漢字の問題なんて\\nいいカンジにできてますし！！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6454,
            "text_len": 12,
            "text_offset": 6464,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6498,
            "text_len": 36,
            "text_offset": 6506,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、でも動詞の活用の所はダメかも。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6564,
            "text_len": 66,
            "text_offset": 6572,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "なんせ考えてる間ず―っと\\nドウシよう、なんて思ってましたから！！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6674,
            "text_len": 12,
            "text_offset": 6684,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6722,
            "text_len": 6,
            "text_offset": 6730,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6768,
            "text_len": 32,
            "text_offset": 6776,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え―っと、え―っと……そうだ！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6830,
            "text_len": 74,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "文章問題もちょっと不安なんですよね。\\nでもそっちはあきらめてるんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6934,
            "text_len": 52,
            "text_offset": 6942,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "文章だけに、たぶん、しょ―がない！\\nな―んて……。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7034,
            "text_len": 12,
            "text_offset": 7044,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7160,
            "text_len": 20,
            "text_offset": 7170,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "相生―、１８点―。\\n",
        "type": "dialog"
    }
]