[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 30,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "１３６、１３７、１３８……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2204,
            "text_len": 54,
            "text_offset": 2212,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "何とか落とさず教室まで行けそう……っと。\\n……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2288,
            "text_len": 74,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "向こうから来るのって関口さん？\\n本読んでるから顔は見えないけど……多分。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2392,
            "text_len": 28,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "すみませ―ん、関口さ―ん。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2460,
            "text_len": 28,
            "text_offset": 2466,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……あ、桜井くん。どうも。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2516,
            "text_len": 66,
            "text_offset": 2524,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こんにちは。歩きながら\\n本読んでる人が居るからびっくりしました。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2612,
            "text_len": 36,
            "text_offset": 2620,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ほんとにいつも本読んでるんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2678,
            "text_len": 62,
            "text_offset": 2684,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "私もリフティングしながら\\n歩いてくる人がいるから驚きました。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2768,
            "text_len": 40,
            "text_offset": 2774,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ほんとにいつもボ―ル蹴ってるんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2836,
            "text_len": 50,
            "text_offset": 2844,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "もちろんですよ。\\n囲碁サッカ―、大好きですから。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2916,
            "text_len": 12,
            "text_offset": 2922,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なるほど。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2970,
            "text_len": 40,
            "text_offset": 2976,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "でも、少しは前を見たほうがいいですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3038,
            "text_len": 6,
            "text_offset": 3046,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3076,
            "text_len": 14,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ほら、足元。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3126,
            "text_len": 6,
            "text_offset": 3134,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3172,
            "text_len": 70,
            "text_offset": 3180,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "まさかオレ、踏みかけてた！？\\n……ってこの猫、何で逃げないんだろう？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3328,
            "text_len": 48,
            "text_offset": 3332,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそっ、こいつ！\\nアンテナが折れちまっただろ！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3404,
            "text_len": 82,
            "text_offset": 3408,
            "warnings": 1
        },
        "speaker": "",
        "text": "バリアで猫は平気だが、万一のことがあったら\\nどうするんだよ！　とっちめてやる！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3514,
            "text_len": 30,
            "text_offset": 3518,
            "warnings": 1
        },
        "speaker": "",
        "text": "……これから？　どうやって？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3570,
            "text_len": 68,
            "text_offset": 3574,
            "warnings": 1
        },
        "speaker": "",
        "text": "どうやってて、そりゃお前、\\n今、アンテナが壊れたんだから弌弌あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3688,
            "text_len": 12,
            "text_offset": 3692,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3720,
            "text_len": 18,
            "text_offset": 3724,
            "warnings": 1
        },
        "speaker": "",
        "text": "やるせねぇ……。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3838,
            "text_len": 12,
            "text_offset": 3846,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3886,
            "text_len": 12,
            "text_offset": 3892,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3938,
            "text_len": 32,
            "text_offset": 3946,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（あれ……？　会話終わった？）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4018,
            "text_len": 58,
            "text_offset": 4026,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（そういえば、\\n　いつもは関口さんと何を話してたっけ？）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4110,
            "text_len": 50,
            "text_offset": 4118,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……そうか！\\n　いつもは間に部長が居るんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4194,
            "text_len": 80,
            "text_offset": 4202,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（で、原因がわかった所でどうしよう。\\n　挨拶しておいて何も話さないのも変だし）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4344,
            "text_len": 78,
            "text_offset": 4352,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（ここは一番得意なジャンルで勝負！\\n　とりあえずさっきの話を膨らませて……）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4456,
            "text_len": 84,
            "text_offset": 4464,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そ、そう！　囲碁サッカ―が大好きだから\\nいつだって技を出す準備も出来てるんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4570,
            "text_len": 8,
            "text_offset": 4576,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4606,
            "text_len": 44,
            "text_offset": 4614,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "例えばこうやってボ―ルを高く浮かせて……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4680,
            "text_len": 14,
            "text_offset": 4688,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ここで碁石！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4724,
            "text_len": 6,
            "text_offset": 4730,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4758,
            "text_len": 30,
            "text_offset": 4766,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そして碁石と高さをあわせて！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4818,
            "text_len": 68,
            "text_offset": 4824,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの、まさかとは思いますけど\\nこんな所でシュ―トなんてしたら……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4924,
            "text_len": 20,
            "text_offset": 4932,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "シュ――――ト！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4980,
            "text_len": 6,
            "text_offset": 4986,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5024,
            "text_len": 74,
            "text_offset": 5032,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "碁石も蹴ったように見せかけて\\n実はボ―ルしか蹴らない！　これが必殺の弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5164,
            "text_len": 10,
            "text_offset": 5172,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5214,
            "text_len": 30,
            "text_offset": 5224,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "text": "あ、赤城先生大丈夫ですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5274,
            "text_len": 28,
            "text_offset": 5284,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "text": "ダメだ、白目剥いてる……。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5336,
            "text_len": 78,
            "text_offset": 5346,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "text": "誰だ！　こんな所でボ―ルを蹴ったのは！！\\nさては、わざと赤城先生を狙ったな！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5446,
            "text_len": 82,
            "text_offset": 5454,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ど、どうしよう、わざとじゃないのに……\\nあの関口さん、一緒に誤解を弌弌居ない！？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5654,
            "text_len": 46,
            "text_offset": 5664,
            "warnings": 0
        },
        "speaker": "富岡先生",
        "text": "あっ、犯人はそこのお前か！？\\nちょっと来い！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5726,
            "text_len": 12,
            "text_offset": 5734,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……はい。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5802,
            "text_len": 18,
            "text_offset": 5810,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（あ、そうだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5860,
            "text_len": 18,
            "text_offset": 5868,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの、関口さん。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5908,
            "text_len": 14,
            "text_offset": 5914,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5950,
            "text_len": 58,
            "text_offset": 5958,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "良かったらその本、\\nちょっと読ませて貰ってもいいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6038,
            "text_len": 14,
            "text_offset": 6044,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いいですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6080,
            "text_len": 48,
            "text_offset": 6088,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ありがとう。\\nそれじゃ、代わりにボ―ルどうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6158,
            "text_len": 6,
            "text_offset": 6164,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6192,
            "text_len": 34,
            "text_offset": 6200,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "オレだけ借りてたら不公平でしょ？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6256,
            "text_len": 22,
            "text_offset": 6262,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、私は別に……。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6306,
            "text_len": 74,
            "text_offset": 6314,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "使いすぎてボロボロですけど、\\n初めて買ったボ―ルなんで許してくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6414,
            "text_len": 6,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6464,
            "text_len": 54,
            "text_offset": 6470,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そんな大事なもの、私が使うわけには。\\nそもそも……。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6546,
            "text_len": 52,
            "text_offset": 6554,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いえ、使ってください！\\nむしろ使って欲しいんです。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6632,
            "text_len": 52,
            "text_offset": 6640,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "関口さんは大切な囲碁サッカ―部の\\n仲間ですから！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6718,
            "text_len": 80,
            "text_offset": 6726,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（そうだ、話しにくいだなんて失礼な。\\n　仲間なんだからもっと親しくしないと！）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6832,
            "text_len": 6,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6870,
            "text_len": 12,
            "text_offset": 6876,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6992,
            "text_len": 24,
            "text_offset": 6998,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ありがとうございます。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7074,
            "text_len": 74,
            "text_offset": 7082,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（そうだ、囲碁サッカ―に興味を持ってもらう\\n　いいチャンスじゃないか！）\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7192,
            "text_len": 38,
            "text_offset": 7200,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（よ―し、そうと決まればその路線で）\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7260,
            "text_len": 84,
            "text_offset": 7268,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いや―、思わず廊下でリフティングするぐらい\\n囲碁サッカ―って本当に面白いんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7374,
            "text_len": 88,
            "text_offset": 7382,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "学校に囲碁サッカ―部があったのが嬉しくて、\\n最近は特にテンションあがってるんですよね！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7492,
            "text_len": 12,
            "text_offset": 7498,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7532,
            "text_len": 84,
            "text_offset": 7540,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そ、それにプロリ―グも盛り上がってて、\\nオリンピック競技になるかもしれないですし！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7646,
            "text_len": 18,
            "text_offset": 7652,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そうなんですか。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7700,
            "text_len": 12,
            "text_offset": 7708,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7756,
            "text_len": 52,
            "text_offset": 7764,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（あ、あれ〜？\\n　何かリアクション薄いんだけど？）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7852,
            "text_len": 36,
            "text_offset": 7860,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（よし、こうなったら単刀直入に！）\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7918,
            "text_len": 74,
            "text_offset": 7926,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの……もしかして関口さんって、\\n囲碁サッカ―あんまり興味ないんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8022,
            "text_len": 14,
            "text_offset": 8028,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "無いですね。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8078,
            "text_len": 12,
            "text_offset": 8086,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8134,
            "text_len": 46,
            "text_offset": 8140,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そろそろ授業なので行きますね。\\nまた部室で。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8212,
            "text_len": 12,
            "text_offset": 8220,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8336,
            "text_len": 24,
            "text_offset": 8344,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うちの部……ダメかも。\\n",
        "type": "dialog"
    }
]