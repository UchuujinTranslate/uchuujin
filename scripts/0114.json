        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6806,
            "text_len": 6,
            "text_offset": 6816,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
