            "speaker_offset": 6710,
            "text_len": 26,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（あれっ？　いない！？）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6768,
            "text_len": 12,
            "text_offset": 6774,
            "warnings": 0
        },
