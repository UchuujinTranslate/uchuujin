[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 58,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "この英文は、教科書に書かれている方法で\\n簡単に訳せます。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2234,
            "text_len": 58,
            "text_offset": 2244,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "まずは５分だけ自分で考えてみてください。\\nはいスタ―ト。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2328,
            "text_len": 12,
            "text_offset": 2338,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2374,
            "text_len": 36,
            "text_offset": 2380,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっと……後ろから順番に訳す……。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2444,
            "text_len": 58,
            "text_offset": 2454,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（そうです。英文は後ろから訳すと\\n　わかりやすいですね）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2552,
            "text_len": 10,
            "text_offset": 2558,
            "warnings": 1
        },
        "speaker": "みお",
        "text": "〜〜〜\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2596,
            "text_len": 64,
            "text_offset": 2606,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（あっ、もう答え書いてる。\\n　長野原さんには簡単すぎたかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2710,
            "text_len": 12,
            "text_offset": 2718,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2758,
            "text_len": 38,
            "text_offset": 2768,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（相生さんには……難しかったかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2842,
            "text_len": 64,
            "text_offset": 2852,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（でも教科書を見れば解ける問題なので\\n　がんばってくださいっ）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2956,
            "text_len": 12,
            "text_offset": 2966,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3006,
            "text_len": 68,
            "text_offset": 3016,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（校長先生は……かなり苦戦してる……\\n　やっぱり難しいのかな……）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3114,
            "text_len": 12,
            "text_offset": 3124,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3178,
            "text_len": 18,
            "text_offset": 3188,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……校長先生！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3232,
            "text_len": 12,
            "text_offset": 3242,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……う！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3322,
            "text_len": 20,
            "text_offset": 3332,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（えええ――！？）\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3434,
            "text_len": 46,
            "text_offset": 3438,
            "warnings": 1
        },
        "speaker": "",
        "text": "えええっ！？\\nなんでオッサンが倒れたんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3508,
            "text_len": 38,
            "text_offset": 3512,
            "warnings": 1
        },
        "speaker": "",
        "text": "う、うう……\\nお、俺にはわかる……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3572,
            "text_len": 28,
            "text_offset": 3576,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわ、こっちでも倒れてる！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3626,
            "text_len": 54,
            "text_offset": 3630,
            "warnings": 1
        },
        "speaker": "",
        "text": "って、おい！\\nあのオッサンが倒れた理由、わかるのか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3706,
            "text_len": 20,
            "text_offset": 3710,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ。お、俺……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3752,
            "text_len": 30,
            "text_offset": 3756,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、どうした、何があった？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3808,
            "text_len": 66,
            "text_offset": 3812,
            "warnings": 1
        },
        "speaker": "",
        "text": "俺……英語だけは駄目なんだ……\\n見ただけでお腹が痛く……ううっ！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3904,
            "text_len": 12,
            "text_offset": 3908,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3966,
            "text_len": 24,
            "text_offset": 3970,
            "warnings": 1
        },
        "speaker": "",
        "text": "そんな理由で―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4096,
            "text_len": 12,
            "text_offset": 4102,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4136,
            "text_len": 48,
            "text_offset": 4146,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（水上さんも\\n　教科書を見ながら頑張って弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4232,
            "text_len": 12,
            "text_offset": 4238,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4294,
            "text_len": 6,
            "text_offset": 4304,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4340,
            "text_len": 34,
            "text_offset": 4350,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（こ、この本、教科書じゃない！）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4440,
            "text_len": 70,
            "text_offset": 4450,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（授業に関係ない本を見てるんだから、\\n　注意しなきゃダメだよね……）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4544,
            "text_len": 40,
            "text_offset": 4554,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "み、水上さん……今は授業中だから……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4618,
            "text_len": 16,
            "text_offset": 4624,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……から……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4672,
            "text_len": 8,
            "text_offset": 4682,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4722,
            "text_len": 88,
            "text_offset": 4728,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……という英文を訳して太郎の心境を答えよ。\\nここで大切なのは伉かれそうになった彼が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4852,
            "text_len": 12,
            "text_offset": 4862,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4906,
            "text_len": 64,
            "text_offset": 4916,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（……え？　なにを朗読してるんだろう？\\n　あの本の……内容？）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5002,
            "text_len": 72,
            "text_offset": 5008,
            "warnings": 2
        },
        "speaker": "麻衣",
        "text": "注目するのはここのＲｇｈｓ！です。\\n太郎の感情が強く出ていて……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5106,
            "text_len": 46,
            "text_offset": 5116,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（あっ！　これ、教科書に書いてある文章だ！）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5188,
            "text_len": 66,
            "text_offset": 5198,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（で、でも手に持ってるのは教科書じゃ……\\n　だけど朗読して……）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5290,
            "text_len": 38,
            "text_offset": 5300,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（……えっ、うそ、でも、まさか……）\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5456,
            "text_len": 12,
            "text_offset": 5466,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "丸暗記！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5600,
            "text_len": 12,
            "text_offset": 5606,
            "warnings": 0
        },
