[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2142,
            "text_len": 12,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2186,
            "text_len": 72,
            "text_offset": 2196,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……もうみんな帰ったみたいだね。\\nそろそろ見回りは終わりにしようかな。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2290,
            "text_len": 12,
            "text_offset": 2300,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2334,
            "text_len": 60,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "う―ん……バディとコバディ、\\n一体どこに行ったんだよ……。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2422,
            "text_len": 68,
            "text_offset": 2432,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（あの子、どうしたんだろう……？\\n　ここの生徒……じゃないよね？）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2524,
            "text_len": 30,
            "text_offset": 2534,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの―、スイマセン……。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2588,
            "text_len": 8,
            "text_offset": 2596,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "はい？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2626,
            "text_len": 72,
            "text_offset": 2636,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ここの生徒じゃないですよね？\\n敷地内には入っちゃダメなんですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2730,
            "text_len": 24,
            "text_offset": 2738,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あ、ご、ごめんなさい。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2784,
            "text_len": 56,
            "text_offset": 2794,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "誰か捜してるんですか？\\nお兄さん？　それともお姉さん？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2872,
            "text_len": 80,
            "text_offset": 2880,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ううん、うちで飼ってる犬を探してて……\\nバディとコバディって言うんだけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2982,
            "text_len": 38,
            "text_offset": 2992,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "犬……？\\nあ……もしかしてあの……？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3052,
            "text_len": 44,
            "text_offset": 3060,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "知ってるの？\\nバディとコバディは今どこに？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3126,
            "text_len": 72,
            "text_offset": 3136,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ス、スイマセン、知ってはいるんですけど、\\n場所まではわからなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3230,
            "text_len": 56,
            "text_offset": 3238,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "そうですか……\\nはぁ。あいつら、どこに行ったんだろう。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3338,
            "text_len": 12,
            "text_offset": 3346,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3374,
            "text_len": 10,
            "text_offset": 3384,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3426,
            "text_len": 54,
            "text_offset": 3434,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あっ！　バディ！\\n捜したんだぞ！　どこにいたんだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3514,
            "text_len": 12,
            "text_offset": 3522,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3564,
            "text_len": 28,
            "text_offset": 3574,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "見つかってよかったですね。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3634,
            "text_len": 56,
            "text_offset": 3642,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "うん、ありがとう！\\nほらバディ、お前もお礼を言うんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3774,
            "text_len": 50,
            "text_offset": 3778,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわっ、やめろ！\\nそ、そんなに頭を押さえたら弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3874,
            "text_len": 30,
            "text_offset": 3878,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわあああ、壊れたぁぁっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4000,
            "text_len": 58,
            "text_offset": 4010,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そ、そんなに落ち込まないで。\\n私も一緒に捜すから。ねっ？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4090,
            "text_len": 26,
            "text_offset": 4098,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "えっ？　捜してくれるの？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4156,
            "text_len": 64,
            "text_offset": 4166,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はい。私と一緒なら校内も捜せるから……\\nきっと見つかりますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4252,
            "text_len": 54,
            "text_offset": 4260,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ありがとうお姉ちゃん！\\nじゃあ、早く捜しに行こうよ。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4336,
            "text_len": 24,
            "text_offset": 4346,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はい、それじゃ弌弌え？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4414,
            "text_len": 12,
            "text_offset": 4418,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 16,
            "speaker_offset": 4446,
            "text_len": 6,
            "text_offset": 4464,
            "warnings": 0
        },
        "speaker": "桜井先生＆きよし",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4506,
            "text_len": 12,
            "text_offset": 4510,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4554,
            "text_len": 28,
            "text_offset": 4564,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "こ、この鹿もキミのペット？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4622,
            "text_len": 38,
            "text_offset": 4630,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ち、違うよ。鹿なんて飼ってないもん。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4690,
            "text_len": 48,
            "text_offset": 4700,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "だよね……\\nもしかして迷い込んじゃったのかな？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4778,
            "text_len": 6,
            "text_offset": 4782,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4808,
            "text_len": 36,
            "text_offset": 4816,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "わっ！？\\nあ、あの鹿、怒ってるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4914,
            "text_len": 72,
            "text_offset": 4924,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（うぅ、怖い……。でも、大人として、\\n　この子だけでも守らないと……）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5022,
            "text_len": 58,
            "text_offset": 5030,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "お姉ちゃん、どうしよう！？\\nあの鹿、こっちを狙ってるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5114,
            "text_len": 42,
            "text_offset": 5124,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "〜〜〜〜っ！\\nこ、こら！　止まりなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5202,
            "text_len": 6,
            "text_offset": 5206,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5234,
            "text_len": 56,
            "text_offset": 5244,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きゃっ！　ど、ど、ど―ど―。\\n落ち着いてください……。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5330,
            "text_len": 12,
            "text_offset": 5334,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5378,
            "text_len": 48,
            "text_offset": 5386,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あっ、すごい、\\nお姉ちゃんの言うこと聞いたよ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5476,
            "text_len": 62,
            "text_offset": 5486,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "わぁ……ありがとう鹿さん。\\nもしかしたら言葉がわかるのかな？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5574,
            "text_len": 12,
            "text_offset": 5578,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5612,
            "text_len": 58,
            "text_offset": 5622,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ここには入ってきちゃダメだよ？\\nあと乱暴もダメだからね？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5706,
            "text_len": 12,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5748,
            "text_len": 48,
            "text_offset": 5756,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ね―、そろそろ\\nバディたちを捜しに行こうよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5826,
            "text_len": 12,
            "text_offset": 5836,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、うん。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5882,
            "text_len": 12,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5924,
            "text_len": 54,
            "text_offset": 5934,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "いい？　誰かに見つかる前に\\nちゃんと家に帰るんだよ？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6036,
            "text_len": 12,
            "text_offset": 6040,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6080,
            "text_len": 12,
            "text_offset": 6090,
            "warnings": 1
        },
        "speaker": "校長先生",
        "text": "〜〜〜〜\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6138,
            "text_len": 6,
            "text_offset": 6142,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6272,
            "text_len": 6,
            "text_offset": 6282,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6350,
            "text_len": 38,
            "text_offset": 6360,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ、えっ、どうして怒ってるの……？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6506,
            "text_len": 6,
            "text_offset": 6510,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6536,
            "text_len": 10,
            "text_offset": 6546,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きゃっ！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6706,
            "text_len": 12,
            "text_offset": 6710,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6754,
            "text_len": 14,
            "text_offset": 6764,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6800,
            "text_len": 12,
            "text_offset": 6810,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6862,
            "text_len": 20,
            "text_offset": 6870,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "と、止まった……。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6932,
            "text_len": 34,
            "text_offset": 6942,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？　わっ、くすぐったいよぉ。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7020,
            "text_len": 52,
            "text_offset": 7028,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あはは、お姉ちゃん、\\nこの鹿に懐かれたみたいだね。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7102,
