[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 12,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……ふう。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2188,
            "text_len": 66,
            "text_offset": 2198,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "なんだかんだで、気分転換どころか\\n逆に気疲れしてしまったな……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2286,
            "text_len": 14,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "まあいいか。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2332,
            "text_len": 60,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "持ち帰りの大福もある。\\nあとは理科室で、ゆっくり楽しもう。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2424,
            "text_len": 28,
            "text_offset": 2434,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの〜、中村先生……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2488,
            "text_len": 50,
            "text_offset": 2498,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "はい？　……ああ、桜井先生。\\nどうかしましたか？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2570,
            "text_len": 28,
            "text_offset": 2580,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、あの……えっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2630,
            "text_len": 12,
            "text_offset": 2640,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2678,
            "text_len": 12,
            "text_offset": 2688,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2726,
            "text_len": 64,
            "text_offset": 2736,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（……なんだ？　用があるなら\\n　早く済ませてほしいんだが……）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2822,
            "text_len": 56,
            "text_offset": 2832,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あの……ああいうことは……\\n風紀が乱れるので…………。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2910,
            "text_len": 66,
            "text_offset": 2920,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………？　すみません、\\n言ってることがよくわからないのですが。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3018,
            "text_len": 18,
            "text_offset": 3028,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、えっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3090,
            "text_len": 12,
            "text_offset": 3098,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3136,
            "text_len": 18,
            "text_offset": 3146,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、また……。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3190,
            "text_len": 26,
            "text_offset": 3200,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（また？　……あ、犬だ）\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3252,
            "text_len": 52,
            "text_offset": 3262,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……ああ、もしかして風紀とは\\nこの犬のことですか？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3336,
            "text_len": 8,
            "text_offset": 3346,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3376,
            "text_len": 58,
            "text_offset": 3386,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "偶然私の近くにいるだけで、\\n私の飼い犬ではありませんよ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3466,
            "text_len": 24,
            "text_offset": 3476,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの、えっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3526,
            "text_len": 52,
            "text_offset": 3536,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ひとまずこの犬は私が外に出します。\\nほら、行くぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3630,
            "text_len": 38,
            "text_offset": 3640,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……ん？\\n大福の袋に何か当たったか？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3754,
            "text_len": 52,
            "text_offset": 3758,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ？　今ので折れたのか！？\\n脆すぎるだろっ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3852,
            "text_len": 70,
            "text_offset": 3856,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそ、アンテナ作ったやつ呼び出せ！\\nいくらなんでも手抜きすぎだろ！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3942,
            "text_len": 60,
            "text_offset": 3946,
            "warnings": 1
        },
        "speaker": "",
        "text": "本部、応答せよ！　本部！\\n技術班に繋げてくれ！　対策を弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4022,
            "text_len": 80,
            "text_offset": 4030,
            "warnings": 0
        },
        "speaker": "留守電",
        "text": "ただいま都合により通話をお受けできません。\\n発信音のあとにメッセ―ジをどうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4130,
            "text_len": 18,
            "text_offset": 4138,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "えええ――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4248,
            "text_len": 66,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "その……偶然なんですけど……\\n放課後に理科室の前を通ったら……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4346,
            "text_len": 8,
            "text_offset": 4356,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4386,
            "text_len": 52,
            "text_offset": 4396,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "中村先生の……\\nさ、触らせてくれっていう声が……。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4484,
            "text_len": 6,
            "text_offset": 4494,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4540,
            "text_len": 68,
            "text_offset": 4550,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "り、理科室の中に……東雲さんがいて……\\n中村先生が迫っていて……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4642,
            "text_len": 36,
            "text_offset": 4652,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "い、いや！　誤解です！　それは弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4726,
            "text_len": 8,
            "text_offset": 4736,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ひっ！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4774,
            "text_len": 6,
            "text_offset": 4784,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4822,
            "text_len": 30,
            "text_offset": 4832,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ……ス、スイマセン……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4950,
            "text_len": 14,
            "text_offset": 4960,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5000,
            "text_len": 52,
            "text_offset": 5010,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（これはマズイ！\\n　桜井先生は間違いなく誤解を！）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5118,
            "text_len": 12,
            "text_offset": 5128,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "桜井先生！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5162,
            "text_len": 12,
            "text_offset": 5172,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ひっ……！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5228,
            "text_len": 6,
            "text_offset": 5238,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5286,
            "text_len": 12,
            "text_offset": 5296,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5342,
            "text_len": 48,
            "text_offset": 5352,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "せ、先生、今、\\n何故一歩下がったのでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5422,
            "text_len": 30,
            "text_offset": 5432,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ……ス、スイマセン……。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5510,
            "text_len": 12,
            "text_offset": 5520,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5564,
            "text_len": 12,
            "text_offset": 5574,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5618,
            "text_len": 12,
            "text_offset": 5628,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5666,
            "text_len": 12,
            "text_offset": 5676,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5744,
            "text_len": 6,
            "text_offset": 5754,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5790,
            "text_len": 34,
            "text_offset": 5800,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "桜井先生！　逃げないでください！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5880,
            "text_len": 12,
            "text_offset": 5890,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "わぁっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5970,
            "text_len": 14,
            "text_offset": 5980,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "捕まえた！！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6016,
            "text_len": 14,
            "text_offset": 6026,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ううぅ……。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6062,
            "text_len": 40,
            "text_offset": 6072,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "さあ、ちゃんと話を聞いてもらいますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6138,
            "text_len": 50,
            "text_offset": 6148,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "うぅ……私を食べても\\n美味しくないですよぉ……。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6226,
            "text_len": 6,
            "text_offset": 6236,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6274,
            "text_len": 14,
            "text_offset": 6284,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ううぅ……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6412,
            "text_len": 12,
            "text_offset": 6422,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6502,
            "text_len": 76,
            "text_offset": 6512,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "い、いいですか？　アレはただマッサ―ジの\\n練習台を頼んだだけであって……。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6610,
            "text_len": 24,
            "text_offset": 6620,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "マッサ―ジ……ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6666,
            "text_len": 84,
            "text_offset": 6676,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "そうです。だから桜井先生が考えているような\\n風紀が乱れるようなことではありません。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6782,
            "text_len": 18,
            "text_offset": 6792,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ほ、本当ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6832,
            "text_len": 62,
            "text_offset": 6842,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "本当です。なんなら桜井先生にも\\nマッサ―ジをしてあげますが？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6926,
            "text_len": 42,
            "text_offset": 6936,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ―……ちょっと興味はありますけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7000,
            "text_len": 52,
            "text_offset": 7010,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "なら、理科室へ行きましょう。\\nさあ、早くこっちへ。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7088,
            "text_len": 12,
            "text_offset": 7098,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7132,
            "text_len": 16,
            "text_offset": 7142,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "どうしました？\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7184,
            "text_len": 32,
            "text_offset": 7194,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、その……目が……怖いです。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7258,
            "text_len": 52,
            "text_offset": 7268,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "こ、怖くないです。\\nそれよりも、マッサ―ジを……。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7342,
            "text_len": 30,
            "text_offset": 7352,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "うぅ……目が血走ってる……。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7404,
            "text_len": 32,
            "text_offset": 7414,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ス、スイマセン！　失礼します！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7502,
            "text_len": 24,
            "text_offset": 7512,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "あっ！？　桜井先生！！\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7562,
            "text_len": 12,
            "text_offset": 7572,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7688,
            "text_len": 28,
            "text_offset": 7698,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "終わった……何もかも……。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7794,
            "text_len": 78,
            "text_offset": 7804,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（ここでムキになるとさらに誤解を生む。\\n　冷静に、冷静になれ！　中村かな！）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7908,
            "text_len": 12,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7962,
            "text_len": 38,
            "text_offset": 7972,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……桜井先生、大福でも食べませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8032,
            "text_len": 26,
            "text_offset": 8042,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？　だ、大福ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8090,
            "text_len": 50,
            "text_offset": 8100,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ええ。まずはこれでも食べて\\n落ち着いてください。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8172,
            "text_len": 28,
            "text_offset": 8182,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、は、はい……頂きます。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8238,
            "text_len": 10,
            "text_offset": 8248,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8280,
            "text_len": 14,
            "text_offset": 8290,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はむっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8326,
            "text_len": 22,
            "text_offset": 8336,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "わぁ、おいしい……！\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8390,
            "text_len": 68,
            "text_offset": 8400,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "商店街の大福屋のものですよ。\\nあそこで飲める緑茶もうまかったです。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8490,
            "text_len": 64,
            "text_offset": 8500,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そうなんですか―。中村先生は\\nこの辺りのお店に詳しいんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8586,
            "text_len": 32,
            "text_offset": 8596,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "はは、そうでもないですけどね。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8654,
            "text_len": 54,
            "text_offset": 8664,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（よし、良い感じに桜井先生の\\n　警戒が解けてきたぞ）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8740,
            "text_len": 70,
            "text_offset": 8750,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うまい店なら何件か知ってますよ。\\nこの後、一緒に食事でもどうですか？\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8842,
            "text_len": 22,
            "text_offset": 8852,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？　食事ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8896,
            "text_len": 60,
            "text_offset": 8906,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ええ。たまには教師というものを忘れて\\nハメを外しませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8988,
            "text_len": 12,
            "text_offset": 8998,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9032,
            "text_len": 30,
            "text_offset": 9042,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "女二人で朝まで飲みましょう。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9104,
            "text_len": 14,
            "text_offset": 9114,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "朝まで……。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9154,
            "text_len": 70,
            "text_offset": 9164,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（朝まで中村先生と二人っきり……\\n　お酒も入って酔っちゃうから……）\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9264,
            "text_len": 6,
            "text_offset": 9274,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9302,
            "text_len": 32,
            "text_offset": 9312,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ス、スイマセン！　失礼します！\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9402,
            "text_len": 20,
            "text_offset": 9412,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "えっ？　桜井先生？\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9464,
            "text_len": 12,
            "text_offset": 9474,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9512,
            "text_len": 52,
            "text_offset": 9522,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "あっ！？　ち、違う！\\nそういう意味じゃな―――い！\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9682,
            "text_len": 22,
            "text_offset": 9692,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "桜井先生――――！！\\n",
        "type": "dialog"
    }
]