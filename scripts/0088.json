[
    {
        "id": 0,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2138,
            "text_len": 22,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "店員（玉村）",
        "text": "いらっしゃいませ―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2200,
            "text_len": 12,
            "text_offset": 2208,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2246,
            "text_len": 26,
            "text_offset": 2254,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（また来てしまった……）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2306,
            "text_len": 84,
            "text_offset": 2314,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ちょっと喉が渇いただけだから自販機か\\n　それとも少し我慢して家で何か飲めばいい）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2424,
            "text_len": 34,
            "text_offset": 2432,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（それなのに入ってしまった……）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2492,
            "text_len": 60,
            "text_offset": 2500,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（……わかってる。\\n　本当は理由なんかとっくにわかってる）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2586,
            "text_len": 62,
            "text_offset": 2594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（悔しかったんだ。だからリベンジしたくて\\n　やってきたんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2692,
            "text_len": 66,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（注文もろくに出来なかったことが\\n　今も私の胸の中に残っている）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2792,
            "text_len": 46,
            "text_offset": 2800,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（今こそリベンジを果たす時！\\n　頑張れ私！）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2882,
            "text_len": 32,
            "text_offset": 2888,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "いらっしゃいませ。ご注文は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2946,
            "text_len": 12,
            "text_offset": 2952,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3000,
            "text_len": 36,
            "text_offset": 3008,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（うっ！？　あの時の店員さん！？）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3074,
            "text_len": 74,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（だ、大丈夫。気圧されちゃ駄目。\\n　あの時の私と違うところを見せないと）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3188,
            "text_len": 20,
            "text_offset": 3194,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、お客様……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3236,
            "text_len": 42,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ、は、はい！\\nえ、えっと今日は……。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3308,
            "text_len": 70,
            "text_offset": 3314,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、当店はペットを連れての入店は\\nお断りさせて頂いていまして……。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3406,
            "text_len": 18,
            "text_offset": 3414,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "へっ？　ペット？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3460,
            "text_len": 12,
            "text_offset": 3468,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3502,
            "text_len": 62,
            "text_offset": 3510,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ！？　あの、この犬は違うんです！\\nほらほら、あっち行け。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3648,
            "text_len": 68,
            "text_offset": 3652,
            "warnings": 1
        },
        "speaker": "",
        "text": "やっぱり駄目か。ペットを装えば\\n正面からでも行けると思ったんだが。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3742,
            "text_len": 74,
            "text_offset": 3746,
            "warnings": 1
        },
        "speaker": "",
        "text": "動物のときは店に入られると困るよな。\\nこっそり入っても気付かれやすいし。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3842,
            "text_len": 42,
            "text_offset": 3846,
            "warnings": 1
        },
        "speaker": "",
        "text": "仕方ない。\\n出てくるまで店の前で待つか。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3910,
            "text_len": 50,
            "text_offset": 3914,
            "warnings": 1
        },
        "speaker": "",
        "text": "だな。\\n……おっ？　なんだか人が集まってきたぞ？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3986,
            "text_len": 60,
            "text_offset": 3990,
            "warnings": 1
        },
        "speaker": "",
        "text": "おお、撫でられてる。\\n店のマスコットとでも思われてるのか？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4072,
            "text_len": 58,
            "text_offset": 4076,
            "warnings": 1
        },
        "speaker": "",
        "text": "ちょっ！　も、もっと優しく！\\nそんな乱暴に撫でたら弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4174,
            "text_len": 22,
            "text_offset": 4182,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "あっ……折れた……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4254,
            "text_len": 24,
            "text_offset": 4262,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "うおおおお――――！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4378,
            "text_len": 16,
            "text_offset": 4384,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ご注文は……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4424,
            "text_len": 18,
            "text_offset": 4432,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、えっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4482,
            "text_len": 54,
            "text_offset": 4490,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（うっ……いざメニュ―を前にすると\\n　緊張する……）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4570,
            "text_len": 60,
            "text_offset": 4578,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（謎の文字の羅列と店員さんの\\n　無言のプレッシャ―……！）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4664,
            "text_len": 50,
            "text_offset": 4672,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（頑張れ私！\\n　この雰囲気に呑まれちゃ駄目よ！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4790,
            "text_len": 24,
            "text_offset": 4798,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "う―ん、え―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4848,
            "text_len": 12,
            "text_offset": 4854,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4892,
            "text_len": 72,
            "text_offset": 4900,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あ、悩んだらもっと駄目だ！\\n　時間をかけたら、その分焦りが生じる！）\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4994,
            "text_len": 22,
            "text_offset": 5000,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、ご注文は……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5048,
            "text_len": 58,
            "text_offset": 5056,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（もう何でもいい！\\n　パッと目についたものを適当に……）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5152,
            "text_len": 28,
            "text_offset": 5160,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "こ、これ！　一番大きいの！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5214,
            "text_len": 12,
            "text_offset": 5220,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5262,
            "text_len": 78,
            "text_offset": 5270,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（し、しまった！　これじゃメニュ―を\\n　読めないと言っているのと同じだ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5370,
            "text_len": 52,
            "text_offset": 5376,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "え―……エスプレッソフラペチ―ノの\\n一番大きな弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5450,
            "text_len": 46,
            "text_offset": 5458,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ド、ドッピオ！\\nドド、ド、ドッピオください！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5540,
            "text_len": 12,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5580,
            "text_len": 12,
            "text_offset": 5588,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5628,
            "text_len": 58,
            "text_offset": 5634,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの……一番大きなサイズだと\\nベンティになりますが……。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5732,
            "text_len": 34,
            "text_offset": 5740,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ベベベヴェ！！　ベンティ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5804,
            "text_len": 38,
            "text_offset": 5812,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（べべべべべべべべべべベンティ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5872,
            "text_len": 48,
            "text_offset": 5878,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、サイズはベンティで\\nよろしいでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5952,
            "text_len": 14,
            "text_offset": 5960,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6004,
            "text_len": 16,
            "text_offset": 6012,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "もう知らん！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6080,
            "text_len": 28,
            "text_offset": 6086,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "えええ―！？　お客様―！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6222,
            "text_len": 66,
            "text_offset": 6230,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ベンティって、ベンティって……\\n　なんなんだ―――――！！！）\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6356,
            "text_len": 12,
            "text_offset": 6364,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6398,
            "text_len": 58,
            "text_offset": 6406,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ……キ、キミ、先にどうぞ。\\n私まだ決まってないから。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6486,
            "text_len": 40,
            "text_offset": 6494,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あっ、は―い。\\nありがとうございます。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6560,
            "text_len": 72,
            "text_offset": 6568,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（よし、これでインタ―バルを置いて\\n　仕切り直しよ。焦るな、私……！）\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6670,
            "text_len": 16,
            "text_offset": 6678,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "え―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6732,
            "text_len": 66,
            "text_offset": 6740,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ト―ルのオレンジクリ―ムフラペチ―ノに\\nエスプレットショットを。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6856,
            "text_len": 6,
            "text_offset": 6864,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6900,
            "text_len": 70,
            "text_offset": 6906,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい。ト―ルのオレンジクリ―ム\\nフラペチ―ノにエスプレットショット。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7014,
            "text_len": 28,
            "text_offset": 7022,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ノ―ミスだとおおお！？）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7076,
            "text_len": 48,
            "text_offset": 7084,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "それと、キノコとベ―コンの\\nパンデサルを２つ。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7154,
            "text_len": 46,
            "text_offset": 7160,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい。キノコとベ―コンの\\nパンデサルを２つ。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7244,
            "text_len": 32,
            "text_offset": 7252,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（またもやノ―ミッスィ―ズ！）\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7310,
            "text_len": 48,
            "text_offset": 7318,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（何この子……！\\n　この歳でパンデサル……！）\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7392,
            "text_len": 24,
            "text_offset": 7398,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "以上でよろしいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7444,
            "text_len": 26,
            "text_offset": 7452,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あっ、えっと最後に……。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7500,
            "text_len": 76,
            "text_offset": 7508,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ヘ―ゼルナッツキャラメルカフェミスト\\nキャラメルマキア―トのグランデ１つ。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7624,
            "text_len": 6,
            "text_offset": 7632,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7664,
            "text_len": 34,
            "text_offset": 7670,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい。以上１７５０円になります。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7726,
            "text_len": 10,
            "text_offset": 7734,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "は―い。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7778,
            "text_len": 12,
            "text_offset": 7786,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7820,
            "text_len": 16,
            "text_offset": 7826,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "次の方どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7868,
            "text_len": 44,
            "text_offset": 7876,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あっ、お姉ちゃん、ありがとう。\\n次どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7950,
            "text_len": 10,
            "text_offset": 7958,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "死ね！！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8106,
            "text_len": 14,
            "text_offset": 8120,
            "warnings": 0
        },
        "speaker": "きよし＆玉村",
        "text": "えええっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8192,
            "text_len": 46,
            "text_offset": 8200,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（そ、そうだ！\\n　前にみおちゃんは確か……）\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8278,
            "text_len": 56,
            "text_offset": 8286,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ホ、ホワイトチョコレ―ト\\nモカフラペチ―ノ、ト―ルで！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8368,
            "text_len": 8,
            "text_offset": 8374,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "！！？\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8404,
            "text_len": 64,
            "text_offset": 8410,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい！　ホワイトチョコレ―ト\\nモカフラペチ―ノのト―ルですね。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8514,
            "text_len": 16,
            "text_offset": 8522,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ＸＤＲ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8564,
            "text_len": 60,
            "text_offset": 8572,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（やった、やったよゆっこ！\\n　ちゃんと注文できたじゃん！）\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8658,
            "text_len": 78,
            "text_offset": 8666,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "text": "（もう私は前までの私じゃないみたいね。\\n　ふふふ、レベルアップ―って感じ）\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8766,
            "text_len": 34,
            "text_offset": 8772,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "注文は以上でよろしいでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8828,
            "text_len": 18,
            "text_offset": 8836,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はい、それで弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8880,
            "text_len": 70,
            "text_offset": 8888,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（待って、待つのよゆっこ！\\n　これは、みおちゃんの真似をしただけ！）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8984,
            "text_len": 44,
            "text_offset": 8992,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（注文ができたとは言えないんじゃないの？）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9058,
            "text_len": 16,
            "text_offset": 9064,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あ、あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9106,
            "text_len": 78,
            "text_offset": 9114,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（どうすれば……どうすれば……ん？\\n　追加の……カスタマイズメニュ―…！？）\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9226,
            "text_len": 36,
            "text_offset": 9234,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（レベルアップ、もらった――っ！）\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9292,
            "text_len": 20,
            "text_offset": 9298,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、ご注文は弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9340,
            "text_len": 84,
            "text_offset": 9348,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ追加で！　エスプレッソのショ―ト。\\nエスプレッソのショ―トをしょ―っとだけ！\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9464,
            "text_len": 76,
            "text_offset": 9472,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "text": "ショ―トでも、サイズはト―ルですよ？\\nちゃんと聞いと―る？　なんちゃって\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9584,
            "text_len": 84,
            "text_offset": 9590,
            "warnings": 2
        },
        "speaker": "玉村",
        "text": "ホワイトチョコレ―トモカフラペチ―ノの\\nト―ルに…エスプレットショットですね？\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9700,
            "text_len": 44,
            "text_offset": 9708,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えっ？\\nあ、あの……もう一回お願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9774,
            "text_len": 88,
            "text_offset": 9780,
            "warnings": 2
        },
        "speaker": "玉村",
        "text": "ホ、ホワイトチョコレ―トモカフラペチ―ノの\\nト―ルにエスプレットショットですよね？\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9890,
            "text_len": 32,
            "text_offset": 9898,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "エ、エスプレット……ショット？\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9952,
            "text_len": 16,
            "text_offset": 9958,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "……ショット。\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10086,
            "text_len": 20,
            "text_offset": 10094,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（レベルダウン！）\\n",
        "type": "dialog"
    }
]