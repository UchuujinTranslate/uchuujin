[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2146,
            "text_len": 46,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ひゃ―、間に合った〜。\\nギリギリセ―フだね。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2232,
            "text_len": 28,
            "text_offset": 2238,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "誰のせいよ、まったく……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2288,
            "text_len": 46,
            "text_offset": 2296,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "事故だよ事故。\\n犬に噛まれたと思って諦めよ？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2364,
            "text_len": 16,
            "text_offset": 2370,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はいはい……。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2412,
            "text_len": 64,
            "text_offset": 2418,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（まあ怪我したわけでも\\n　授業に遅れたわけでもないからいいか）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2602,
            "text_len": 60,
            "text_offset": 2608,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（さて、そんじゃ授業の準備っと。\\n　最初は国語だっけ……）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2694,
            "text_len": 12,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2740,
            "text_len": 14,
            "text_offset": 2746,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2794,
            "text_len": 42,
            "text_offset": 2800,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（無い！？　カバンの中に原稿が無い！！）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2878,
            "text_len": 86,
            "text_offset": 2884,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（うちに忘れた？　ううん、そんなはず無い。\\n　何度も確認してから、うちを出たし……）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2996,
            "text_len": 62,
            "text_offset": 3002,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あっ、そっか！\\n　さっき、ゆっことぶつかった時に……っ！）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3090,
            "text_len": 62,
            "text_offset": 3096,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（な、なんてこった……！\\n　アレを、誰かに見られたら……！）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3184,
            "text_len": 64,
            "text_offset": 3190,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（笹原先輩と中之条くんの掛け算を\\n　誰かに見られたら……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3280,
            "text_len": 48,
            "text_offset": 3286,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（私は……！\\n　私は、死ぬしか……ないっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3366,
            "text_len": 14,
            "text_offset": 3372,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………ん？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3412,
            "text_len": 12,
            "text_offset": 3420,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3454,
            "text_len": 24,
            "text_offset": 3460,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あれ、何かくわえて弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3514,
            "text_len": 20,
            "text_offset": 3520,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？　私の原稿！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3616,
            "text_len": 28,
            "text_offset": 3620,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おい……この漫画……。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3670,
            "text_len": 18,
            "text_offset": 3674,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわぁ……\\nん？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3726,
            "text_len": 12,
            "text_offset": 3738,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3784,
            "text_len": 6,
            "text_offset": 3792,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3832,
            "text_len": 12,
            "text_offset": 3844,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3916,
            "text_len": 16,
            "text_offset": 3924,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "えええ――！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4036,
            "text_len": 80,
            "text_offset": 4042,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ぶつかったのは少し前。だったらブツは\\n　まだ現場に残っている可能性が高い！）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4148,
            "text_len": 64,
            "text_offset": 4154,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（今からなら間に合う！\\n　授業を抜けてでも取りに行かないと！）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4342,
            "text_len": 48,
            "text_offset": 4350,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ、みおちゃん、ちょっと待って。\\nあのね弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4426,
            "text_len": 60,
            "text_offset": 4432,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "止めないでゆっこ！！\\n仕方がないの！　仕方なかったのよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4514,
            "text_len": 20,
            "text_offset": 4522,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "えっ？　でも……。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4568,
            "text_len": 60,
            "text_offset": 4574,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "お願い行かせて！！\\n私のためを思うなら、黙って行かせて！！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4660,
            "text_len": 48,
            "text_offset": 4668,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや、だけど……\\nこれ、みおちゃんのじゃ……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4738,
            "text_len": 6,
            "text_offset": 4744,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4790,
            "text_len": 22,
            "text_offset": 4796,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ、そ、それは！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4860,
            "text_len": 74,
            "text_offset": 4868,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "さっきカバンの中身拾ってる時に\\n見つけたんだけどさ、みおちゃんのだよね？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4964,
            "text_len": 38,
            "text_offset": 4970,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そ、そうなんだ。\\nありがとうゆっこ！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5030,
            "text_len": 22,
            "text_offset": 5038,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、ちょいストップ。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5084,
            "text_len": 8,
            "text_offset": 5090,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5120,
            "text_len": 78,
            "text_offset": 5128,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや、ほら、拾ったのは私なんだからさ、\\nお礼として見せて欲しいな―、なんて。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5238,
            "text_len": 6,
            "text_offset": 5244,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5316,
            "text_len": 54,
            "text_offset": 5324,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "これ、新しいマンガでしょ？\\n今度はどんなの描いたの？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5400,
            "text_len": 14,
            "text_offset": 5406,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5456,
            "text_len": 72,
            "text_offset": 5462,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（言えない！　先輩とクラスメイトが\\n　モデルのマンガなんて言えない！）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5556,
            "text_len": 64,
            "text_offset": 5564,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……あ、大丈夫だって。どういうのかは、\\n私は知ってるわけだし。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5658,
            "text_len": 62,
            "text_offset": 5664,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（それでも今回の内容は……！\\n　見せられるわけが無いっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5748,
            "text_len": 36,
            "text_offset": 5756,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "すぐ返すから、ちょっとだけ見るね？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5820,
            "text_len": 6,
            "text_offset": 5826,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5940,
            "text_len": 14,
            "text_offset": 5946,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "セイヤッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5990,
            "text_len": 6,
            "text_offset": 5998,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6112,
            "text_len": 12,
            "text_offset": 6120,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6154,
            "text_len": 28,
            "text_offset": 6160,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "これでいい……これで……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6296,
            "text_len": 52,
            "text_offset": 6302,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ごめん、ごめんね……\\n　せめて……安らかに……）\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6406,
            "text_len": 40,
            "text_offset": 6412,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ま、待って、ゆっこ！\\nそれは……っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6474,
            "text_len": 64,
            "text_offset": 6482,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "どれどれ〜？　お、マンガの新作なんだ。\\nちょっと読ませてね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6572,
            "text_len": 12,
            "text_offset": 6580,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6634,
            "text_len": 6,
            "text_offset": 6642,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6692,
            "text_len": 6,
            "text_offset": 6698,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6756,
            "text_len": 12,
            "text_offset": 6764,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6800,
            "text_len": 12,
            "text_offset": 6808,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6846,
            "text_len": 18,
            "text_offset": 6854,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………ひっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6904,
            "text_len": 18,
            "text_offset": 6910,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ……ああ……。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6956,
            "text_len": 12,
            "text_offset": 6964,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7084,
            "text_len": 76,
            "text_offset": 7092,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（……相生さん。なんで僕を見て、\\n　あんなに複雑な顔をしていたんだろう？）\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7226,
            "text_len": 46,
            "text_offset": 7232,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ダ、ダメだよ、\\n落とし物の中身勝手に見ちゃ！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7300,
            "text_len": 48,
            "text_offset": 7308,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……？　うん。\\nでもコレ、みおちゃんのでしょ？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7378,
            "text_len": 50,
            "text_offset": 7384,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そ、そうだけど……\\n私のじゃないかもしれないし！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7456,
            "text_len": 36,
            "text_offset": 7464,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ、確認の為にも尚更見なきゃ。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7522,
            "text_len": 42,
            "text_offset": 7528,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "で、でも人の物を勝手に見たらダメだって。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7592,
            "text_len": 62,
            "text_offset": 7600,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ、みおちゃんが確認して？\\nみおちゃんのだったら見せて。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7688,
            "text_len": 6,
            "text_offset": 7694,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7732,
            "text_len": 28,
            "text_offset": 7740,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はい。\\n中身確認してみて。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7790,
            "text_len": 14,
            "text_offset": 7796,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7842,
            "text_len": 84,
            "text_offset": 7848,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（うっ……この原稿は、紛れも無く私の……\\n　しかしそれを言えばコレを見られる……）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7960,
            "text_len": 40,
            "text_offset": 7968,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ねえねえ、どう？　みおちゃんのだった？\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8030,
            "text_len": 14,
            "text_offset": 8036,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8086,
            "text_len": 16,
            "text_offset": 8092,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………違う！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8142,
            "text_len": 32,
            "text_offset": 8148,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（はっ！？　私は一体何を！？）\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8202,
            "text_len": 40,
            "text_offset": 8210,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そっか―。じゃ、後は先生に任せますか。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8296,
            "text_len": 6,
            "text_offset": 8302,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8428,
            "text_len": 26,
            "text_offset": 8438,
            "warnings": 0
        },
        "speaker": "校内放送",
        "text": "落とし物のお知らせです。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8484,
            "text_len": 84,
            "text_offset": 8494,
            "warnings": 0
        },
        "speaker": "校内放送",
        "text": "１年Ｐ組の中之条くんと２年Ｏ組の笹原くんに\\nよく似た絵のマンガ原稿が届いています。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8598,
            "text_len": 54,
            "text_offset": 8608,
            "warnings": 0
        },
        "speaker": "校内放送",
        "text": "心当たりのある人は、\\n職員室まで取りに来てください。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8770,
            "text_len": 12,
            "text_offset": 8776,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    }
