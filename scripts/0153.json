[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 42,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "廊下も異常なし。\\n事件や不審者もなしと。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2218,
            "text_len": 38,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あとは体育館裏で見回りは終わりだな。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2388,
            "text_len": 40,
            "text_offset": 2398,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ここも異常な……こんな所にお婆さんが？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2464,
            "text_len": 74,
            "text_offset": 2474,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（……そういえば生徒たちが、\\n　体育館裏に老婆の霊が出るとか話してたな）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2588,
            "text_len": 56,
            "text_offset": 2598,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（つまり、目の前に居るのは幽霊？\\n　……ゆ、幽霊！？）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2694,
            "text_len": 72,
            "text_offset": 2704,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ちょっと待て高崎学。ここはひとまず\\n　声をかけてみようじゃないか！）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2800,
            "text_len": 22,
            "text_offset": 2810,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あの……すみません。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2856,
            "text_len": 16,
            "text_offset": 2864,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "なんでしょう？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2902,
            "text_len": 54,
            "text_offset": 2912,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "いや、ここは学校の敷地なんですが、\\nこんな所で何を？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2988,
            "text_len": 46,
            "text_offset": 2996,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……この学校に潜む悪霊の声を聞いていました。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3074,
            "text_len": 12,
            "text_offset": 3084,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あ、悪霊？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3122,
            "text_len": 26,
            "text_offset": 3130,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ええ、今まさにそこに！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3214,
            "text_len": 14,
            "text_offset": 3224,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "（……む？）\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3274,
            "text_len": 30,
            "text_offset": 3284,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……どう見ても猫なんですが。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3344,
            "text_len": 44,
            "text_offset": 3352,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あの猫こそ！　邪悪な宇宙からの来訪者です！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3418,
            "text_len": 12,
            "text_offset": 3428,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3466,
            "text_len": 28,
            "text_offset": 3476,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（悪霊じゃなかったのか？）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3612,
            "text_len": 50,
            "text_offset": 3620,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "今すぐに成敗してやりましょう！\\nキエエエエエ！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3720,
            "text_len": 64,
            "text_offset": 3730,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "うおおっ！？　あぶねぇ―っ！\\nなに投げた、このババア――ッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3872,
            "text_len": 48,
            "text_offset": 3876,
            "warnings": 1
        },
        "speaker": "",
        "text": "なぜだ！\\nなぜ、あのバアさんに正体がバレた！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3948,
            "text_len": 34,
            "text_offset": 3952,
            "warnings": 1
        },
        "speaker": "",
        "text": "落ち着け、ただの当てずっぽうだ！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4008,
            "text_len": 66,
            "text_offset": 4012,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ？　あ……そ、そうだよな、\\nこの星の人間にバレるはずがない。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4100,
            "text_len": 56,
            "text_offset": 4104,
            "warnings": 1
        },
        "speaker": "",
        "text": "はははは、すまんすまん。\\nお前のおかげで落ち着けたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4182,
            "text_len": 66,
            "text_offset": 4186,
            "warnings": 1
        },
        "speaker": "",
        "text": "なに、リラックスまでしてんだよ！？\\n逃げないとアンテナに石が弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4298,
            "text_len": 20,
            "text_offset": 4306,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "当たった――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4418,
            "text_len": 56,
            "text_offset": 4428,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "困りますよ！\\n勝手に入り込んで霊がどうとか言われちゃ！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4508,
            "text_len": 20,
            "text_offset": 4516,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ですが悪霊が……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4558,
            "text_len": 60,
            "text_offset": 4568,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "とにかく、そういうのはいいので\\n今すぐ出ていってください！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4650,
            "text_len": 20,
            "text_offset": 4658,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……わかりました。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4704,
            "text_len": 58,
            "text_offset": 4714,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（おや、もっと長引くかと思ったら\\n　意外と話が通じたな）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4794,
            "text_len": 26,
            "text_offset": 4802,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ああ、その前にお兄さん。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4850,
            "text_len": 16,
            "text_offset": 4860,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……その手は？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4902,
            "text_len": 20,
            "text_offset": 4910,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "５千円になります。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5006,
            "text_len": 24,
            "text_offset": 5016,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ふざけるんじゃない！！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5062,
            "text_len": 54,
            "text_offset": 5072,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "これ以上バカなこと言ってると\\n警察呼びますよ警察！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5158,
            "text_len": 18,
            "text_offset": 5166,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "でも悪霊が……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5206,
            "text_len": 46,
            "text_offset": 5216,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "霊なんて居ない！\\nほら、早く行った行った！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5284,
            "text_len": 52,
            "text_offset": 5294,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "次見たらとっ捕まえて\\n警察に突き出しますからね！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5378,
            "text_len": 52,
            "text_offset": 5388,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "まったく、何が悪霊だ。\\nそんなもん怖くなんて……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5470,
            "text_len": 32,
            "text_offset": 5480,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（本当はちょっと怖ぇ―っ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5548,
            "text_len": 62,
            "text_offset": 5558,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（だが頑張ったぞ高崎学。\\n　怖いのを我慢してよく追い出した）\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5646,
            "text_len": 52,
            "text_offset": 5656,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（それでこそ教師の鑑。\\n　生徒のために頑張ったな）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5766,
            "text_len": 20,
            "text_offset": 5774,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あ、後ろに悪霊が。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5846,
            "text_len": 6,
            "text_offset": 5856,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5970,
            "text_len": 26,
            "text_offset": 5980,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………出ていけ―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6068,
            "text_len": 46,
            "text_offset": 6078,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……わかりました。\\n５千円お出ししましょう。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6146,
            "text_len": 64,
            "text_offset": 6156,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ただし、ここに居付いているという\\n悪霊をどうにかしてください！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6246,
            "text_len": 44,
            "text_offset": 6256,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（自腹をきるのは痛いが、これも生徒のため）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6336,
            "text_len": 80,
            "text_offset": 6346,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（悪霊さえ居なくなればうわさも消えるし、\\n　お婆さんも来なくなるに違いない！）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6452,
            "text_len": 12,
            "text_offset": 6460,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6500,
            "text_len": 56,
            "text_offset": 6510,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "どうしました？\\nさあ、早く悪霊をどうにかしてください！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6588,
            "text_len": 28,
            "text_offset": 6596,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あ……え―っと、無理です。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6656,
            "text_len": 10,
            "text_offset": 6666,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……は？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6712,
            "text_len": 78,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "イタコなので悪霊と交信はできますが、\\n仍ったりとかそういうのはちょっと……。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6828,
            "text_len": 24,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ええ――――っ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6894,
            "text_len": 38,
            "text_offset": 6904,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そ、それじゃあ何のために学校に……？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6968,
            "text_len": 12,
            "text_offset": 6976,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7092,
            "text_len": 14,
            "text_offset": 7100,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "失礼します。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7138,
            "text_len": 28,
            "text_offset": 7148,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ええ――――っ！！！？）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7244,
            "text_len": 14,
            "text_offset": 7254,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……あの―。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7290,
            "text_len": 20,
            "text_offset": 7298,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "５千円になります。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7340,
            "text_len": 36,
            "text_offset": 7350,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "いや、それは一旦置いといてですね。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7408,
            "text_len": 38,
            "text_offset": 7418,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "校内で悪霊のうわさが出てたんですよ。\\n",
        "type": "dialog"
