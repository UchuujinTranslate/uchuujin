[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2138,
            "text_len": 70,
            "text_offset": 2144,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はぁ……はぁ……た、確かこの辺だよね。\\nゆっことぶつかったのは……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2236,
            "text_len": 50,
            "text_offset": 2242,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なんとしても……\\nなんとしても原稿見つけないと！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2314,
            "text_len": 14,
            "text_offset": 2320,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………ん？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2360,
            "text_len": 12,
            "text_offset": 2364,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2396,
            "text_len": 22,
            "text_offset": 2402,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "うわっ！？　鹿だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 2450,
            "text_len": 12,
            "text_offset": 2454,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2508,
            "text_len": 20,
            "text_offset": 2514,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "びっくりした……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2556,
            "text_len": 76,
            "text_offset": 2562,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "つか、あの鹿って朝のやつだよね？\\nくっそ―、あいつのせいで私の原稿が……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2668,
            "text_len": 12,
            "text_offset": 2674,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2790,
            "text_len": 6,
            "text_offset": 2796,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2926,
            "text_len": 32,
            "text_offset": 2932,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "鹿―――！！　待て――――！！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3024,
            "text_len": 6,
            "text_offset": 3034,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3116,
            "text_len": 50,
            "text_offset": 3120,
            "warnings": 1
        },
        "speaker": "",
        "text": "あの子、どうしたんだ！\\nいきなり走り出したぞ！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3192,
            "text_len": 54,
            "text_offset": 3196,
            "warnings": 1
        },
        "speaker": "",
        "text": "と、とにかく追うんだ！\\n逃げられたら姫に怒られるぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3272,
            "text_len": 8,
            "text_offset": 3276,
            "warnings": 1
        },
        "speaker": "",
        "text": "おう！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3306,
            "text_len": 56,
            "text_offset": 3310,
            "warnings": 1
        },
        "speaker": "",
        "text": "……？　どうした、もっと急がせろよ。\\n見失っちまうぞ？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3388,
            "text_len": 30,
            "text_offset": 3392,
            "warnings": 1
        },
        "speaker": "",
        "text": "ご老体なんだ、無理言うなよ。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3444,
            "text_len": 26,
            "text_offset": 3448,
            "warnings": 1
        },
        "speaker": "",
        "text": "……そうか、そうだよな。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3496,
            "text_len": 16,
            "text_offset": 3500,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ、バテた。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3538,
            "text_len": 16,
            "text_offset": 3542,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ、転んだ。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3600,
            "text_len": 16,
            "text_offset": 3608,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "あっ、折れた！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3816,
            "text_len": 44,
            "text_offset": 3822,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はぁ、はぁ……！\\nあっ、いた！　鹿――！！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3894,
            "text_len": 12,
            "text_offset": 3898,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3940,
            "text_len": 42,
            "text_offset": 3946,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "取り返す……何が何でも……私の原稿……！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4010,
            "text_len": 6,
            "text_offset": 4014,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4202,
            "text_len": 12,
            "text_offset": 4208,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ぐは―！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4376,
            "text_len": 38,
            "text_offset": 4382,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（な、なに！？　なんなのこいつ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4440,
            "text_len": 68,
            "text_offset": 4446,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（こんなところで……こんなところで\\n　私の人生は終わっちゃうの？）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4534,
            "text_len": 38,
            "text_offset": 4540,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（こんな……鹿１匹のために……っ！）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4602,
            "text_len": 30,
            "text_offset": 4608,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（いや……まだ……まだよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4664,
            "text_len": 22,
            "text_offset": 4670,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "命を、燃やせ―――！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4728,
            "text_len": 6,
            "text_offset": 4732,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4846,
            "text_len": 14,
            "text_offset": 4852,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "へや―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4886,
            "text_len": 8,
            "text_offset": 4890,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5010,
            "text_len": 10,
            "text_offset": 5016,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ぃよし！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5048,
            "text_len": 16,
            "text_offset": 5054,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "勝った……！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5184,
            "text_len": 32,
            "text_offset": 5190,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ――――！？　原稿―――！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5286,
            "text_len": 50,
            "text_offset": 5292,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "つ、強そう……\\n何か……何かいい手はないの……？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5370,
            "text_len": 52,
            "text_offset": 5376,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "カバンの中は……\\nダメだ。ろくなものが無いよ……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5450,
            "text_len": 72,
            "text_offset": 5456,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "くぅ、そりゃそうだ、対鹿用アイテムなんて\\n日頃から持ってるわけが……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5554,
            "text_len": 34,
            "text_offset": 5560,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ！　ポケットに鹿せんべい！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5634,
            "text_len": 42,
            "text_offset": 5640,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（なんで！？\\n　でも神さまありがと―！）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5712,
            "text_len": 56,
            "text_offset": 5718,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "よ―しよしよし。こっちおいで〜。\\n良いものあげるよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5808,
            "text_len": 12,
            "text_offset": 5812,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5850,
            "text_len": 40,
            "text_offset": 5856,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（よし、食べた！　今の内に原稿を……）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5918,
            "text_len": 30,
            "text_offset": 5924,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "やった―！！　ゲット――！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5980,
            "text_len": 12,
            "text_offset": 5984,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6018,
            "text_len": 64,
            "text_offset": 6024,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、なに？　もっと食べたい？\\nわかったわかった。あと１枚……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6114,
            "text_len": 26,
            "text_offset": 6120,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………あ、もうないや。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6260,
            "text_len": 6,
            "text_offset": 6264,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6388,
            "text_len": 12,
            "text_offset": 6394,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ぐは―！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6468,
            "text_len": 50,
            "text_offset": 6474,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ダメ……こいつの目……\\n　ただ者じゃないっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6554,
            "text_len": 66,
            "text_offset": 6560,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（一歩も動けない……先に動いたら……\\n　確実に殺られる……っ！）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6646,
            "text_len": 42,
            "text_offset": 6652,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "ああいたいた。\\nったく、世話が焼けるな。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6716,
            "text_len": 6,
            "text_offset": 6722,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6764,
            "text_len": 56,
            "text_offset": 6770,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "ん？　なんだ君は。\\nひょっとして君がこの鹿の飼い主か？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6848,
            "text_len": 18,
            "text_offset": 6854,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "い、いいえ……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6888,
            "text_len": 72,
            "text_offset": 6894,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "そうか。いや―、近隣住民から\\n鹿が暴れ回ってるって苦情が殺到してね―。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 7000,
            "text_len": 12,
            "text_offset": 7004,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "！！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7032,
            "text_len": 28,
            "text_offset": 7038,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "おっ！？　こら、暴れるな！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7090,
            "text_len": 36,
            "text_offset": 7096,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "それじゃ、この鹿は連れて行くから。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7162,
            "text_len": 20,
            "text_offset": 7168,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "た、助かった……。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7214,
            "text_len": 12,
            "text_offset": 7220,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7342,
            "text_len": 30,
            "text_offset": 7348,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、原稿取ってないじゃん！！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7486,
            "text_len": 38,
            "text_offset": 7492,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "すみません！\\nその鹿、私のですっ！！\\n",
        "type": "dialog"
    }
]