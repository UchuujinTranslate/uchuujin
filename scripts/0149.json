[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 40,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "さ―て、今日の部活は何をしようかなと。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2212,
            "text_len": 60,
            "text_offset": 2218,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "上毛カルタあたり？\\nいや、今日こそは世界一周ゲ―ムを……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2334,
            "text_len": 6,
            "text_offset": 2340,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2386,
            "text_len": 40,
            "text_offset": 2392,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "今チラッと見えたの、\\n中村先生だよな。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2454,
            "text_len": 26,
            "text_offset": 2460,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "え―っと……どこ行った？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2508,
            "text_len": 50,
            "text_offset": 2514,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……あ、居た居た。\\nあんなトコで何してるんだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2586,
            "text_len": 46,
            "text_offset": 2592,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん―……。\\nちょっとここからじゃ見辛いなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2660,
            "text_len": 28,
            "text_offset": 2666,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "弌弌ちょっと覗いてみるか！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2716,
            "text_len": 60,
            "text_offset": 2722,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "見つからないように……\\nお、あそこの木なんか良さそうだな。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2918,
            "text_len": 40,
            "text_offset": 2924,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（さ―て、中村先生は何してんのかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2994,
            "text_len": 14,
            "text_offset": 3000,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（あっ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3044,
            "text_len": 70,
            "text_offset": 3054,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "こら、暴れるんじゃない。\\n少しその頭のものを調べさせてもらうだけだ。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3152,
            "text_len": 68,
            "text_offset": 3162,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ええい、だから暴れるなと！\\nしかたない、少し大人しくしてもらうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3250,
            "text_len": 44,
            "text_offset": 3256,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（うおおぉっ！？\\n　先生それはまさか……）\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3384,
            "text_len": 22,
            "text_offset": 3390,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（動物虐待だ―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3496,
            "text_len": 20,
            "text_offset": 3500,
            "warnings": 7
        },
        "speaker": "",
        "text": "＆！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3542,
            "text_len": 26,
            "text_offset": 3546,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おいっ、大丈夫か！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3594,
            "text_len": 38,
            "text_offset": 3598,
            "warnings": 1
        },
        "speaker": "",
        "text": "……うわ、駄目だ、白目剥いてやがる。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3658,
            "text_len": 50,
            "text_offset": 3662,
            "warnings": 1
        },
        "speaker": "",
        "text": "しかし、何だってこいつがダメ―ジを。\\n……っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3734,
            "text_len": 46,
            "text_offset": 3738,
            "warnings": 1
        },
        "speaker": "",
        "text": "こ、これは……\\nダメ―ジ身代わりスイッチ！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3806,
            "text_len": 52,
            "text_offset": 3810,
            "warnings": 1
        },
        "speaker": "",
        "text": "……そうか。\\nこいつ、あの猫をまもってくれたのか。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3884,
            "text_len": 22,
            "text_offset": 3888,
            "warnings": 1
        },
        "speaker": "",
        "text": "でも、どうせなら弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3952,
            "text_len": 36,
            "text_offset": 3956,
            "warnings": 1
        },
        "speaker": "",
        "text": "弌弌アンテナも守ってほしかったぜ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4088,
            "text_len": 60,
            "text_offset": 4094,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（まだちょっと見辛いな。\\n　何かやってるのはわかるが……）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4262,
            "text_len": 56,
            "text_offset": 4268,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（お、今チラッと見えたのって\\n　シャベルじゃないか？）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4344,
            "text_len": 48,
            "text_offset": 4350,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（間違いない、シャベルだ。\\n　ってことは……）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4418,
            "text_len": 46,
            "text_offset": 4424,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（わかったぞ！\\n　中村先生は穴を掘ってる！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4494,
            "text_len": 20,
            "text_offset": 4500,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（何のために！？）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4550,
            "text_len": 20,
            "text_offset": 4556,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（……落とし穴？）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4600,
            "text_len": 34,
            "text_offset": 4606,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（いや、だから何のためにだよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4760,
            "text_len": 18,
            "text_offset": 4766,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あっ、やべっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4822,
            "text_len": 36,
            "text_offset": 4828,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（先生がこっち来るぞ！　隠れろ！）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4936,
            "text_len": 38,
            "text_offset": 4946,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "私としたことがアレを忘れるとは……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5080,
            "text_len": 36,
            "text_offset": 5086,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……ふぅ、やり過ごしたみたいだな。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5144,
            "text_len": 70,
            "text_offset": 5150,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "しっかし……アレって何だ？\\n弌弌とりあえず、穴の様子でも見てみるか。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5278,
            "text_len": 58,
            "text_offset": 5284,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "う―ん……このサイズの穴って、\\n明らかに人間用だよなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5364,
            "text_len": 36,
            "text_offset": 5370,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……これまでの情報をまとめるとだ。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5428,
            "text_len": 80,
            "text_offset": 5434,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "中村先生は落とし穴に人間を落とし、\\n更に仕上げのアレでどうにかしようとしてる。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5542,
            "text_len": 38,
            "text_offset": 5548,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……犯罪の匂いがすげぇするんだけど。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5622,
            "text_len": 72,
            "text_offset": 5628,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（よし、丁度シャベルもあるし、\\n　先生のためにも埋め戻しとくとしよう）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5722,
            "text_len": 16,
            "text_offset": 5728,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よっ……っと。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5766,
            "text_len": 66,
            "text_offset": 5772,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "意外とキツいなこれ……\\n中村先生よくこんな深くまで掘ったもんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5860,
            "text_len": 16,
            "text_offset": 5866,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よいせ……っ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5912,
            "text_len": 14,
            "text_offset": 5920,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "見つけた！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5956,
            "text_len": 6,
            "text_offset": 5962,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6002,
            "text_len": 56,
            "text_offset": 6010,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "僕たちを落とし穴に落としたのは\\n先輩だったんですか！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6102,
            "text_len": 40,
            "text_offset": 6108,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "えっ……あ！\\nい、いやこれは違うんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6180,
            "text_len": 36,
            "text_offset": 6188,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "じゃあそのシャベルは何なんです！！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6246,
            "text_len": 34,
            "text_offset": 6252,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、だから……あっ、中村先生！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6314,
            "text_len": 12,
            "text_offset": 6324,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6358,
            "text_len": 70,
            "text_offset": 6364,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "良かった。怒りに我を忘れているみたいで……\\nここは中村先生が説明を！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6482,
            "text_len": 22,
            "text_offset": 6488,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（何――――っ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6628,
            "text_len": 66,
            "text_offset": 6636,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "僕以外にも落ちているんですよ！\\nさあ、一緒に謝りに行きましょう！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6722,
            "text_len": 16,
            "text_offset": 6728,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "濡れ衣だ―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6802,
            "text_len": 46,
            "text_offset": 6808,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "う―む……。\\n見れば見るほどこの深さは……。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6886,
            "text_len": 30,
            "text_offset": 6892,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（人間用に見えてくるよなぁ）\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6944,
            "text_len": 72,
            "text_offset": 6950,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ハハハ、何考えてんだよオレ。\\n先生が学校でそんなことするわけないだろ。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7044,
            "text_len": 80,
            "text_offset": 7050,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……念のため、何か目印でもつけとくか。\\n悪いことする気だったら気づくだろうし。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7152,
            "text_len": 38,
            "text_offset": 7158,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "って言っても、今手元にあるのは……。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7318,
            "text_len": 68,
            "text_offset": 7328,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "探すのに手間取ってしまった。\\nまさか引き出しにしまっていたとはな。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7418,
            "text_len": 40,
            "text_offset": 7428,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "しかし今度こそ、これで東雲なのを……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7498,
            "text_len": 6,
            "text_offset": 7508,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7540,
            "text_len": 44,
            "text_offset": 7550,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "どうして穴の前に碁石とサッカ―ボ―ルが！？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7622,
            "text_len": 40,
            "text_offset": 7632,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "な、なんだ、どういう意味なんだコレは。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7694,
            "text_len": 56,
            "text_offset": 7704,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……そうか、これは\\n見ているぞというメッセ―ジなのか。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7782,
            "text_len": 62,
            "text_offset": 7792,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "計画は中止しよう。\\n弌弌次からはもう少し慎重にやらないとな。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7884,
            "text_len": 14,
            "text_offset": 7890,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（よしっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8012,
            "text_len": 56,
            "text_offset": 8018,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（いや―、囲碁サッカ―も\\n　たまには役に立つもんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8122,
            "text_len": 26,
            "text_offset": 8128,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うっわ―、すげえ深い穴。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8176,
            "text_len": 66,
            "text_offset": 8182,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "人間落とすのに丁度いいサイズ……\\nってそんなわけないか、ははっ！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8274,
            "text_len": 12,
            "text_offset": 8280,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8326,
            "text_len": 28,
            "text_offset": 8332,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（すごく落ちてみたいっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8406,
            "text_len": 60,
            "text_offset": 8412,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（落ち着けオレ。\\n　確かに気になるけど落ちちゃ駄目だろ！）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8498,
            "text_len": 12,
            "text_offset": 8504,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8542,
            "text_len": 20,
            "text_offset": 8548,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8690,
            "text_len": 52,
            "text_offset": 8700,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "どこに置いたかと思えば\\nあんな所に入れてたとはな。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8774,
            "text_len": 68,
            "text_offset": 8784,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "何にせよ見つかってよかった。\\nこれで今度こそ、東雲なのを……うん？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8880,
            "text_len": 12,
            "text_offset": 8886,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "助けて―！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8938,
            "text_len": 12,
            "text_offset": 8948,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8976,
            "text_len": 18,
            "text_offset": 8982,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "誰か助けて―！！\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9118,
            "text_len": 18,
            "text_offset": 9128,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（逃げろっ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9250,
            "text_len": 30,
            "text_offset": 9256,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "誰か―……誰か助けてくれ―。\\n",
        "type": "dialog"
    }
]