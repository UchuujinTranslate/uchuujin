[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2138,
            "text_len": 28,
            "text_offset": 2148,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "え―っと、こんなものかな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2202,
            "text_len": 74,
            "text_offset": 2212,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（しかし、楽とはいえ夕食をコンビニ弁当で\\n　済ませていると寂しくなるな）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2312,
            "text_len": 22,
            "text_offset": 2322,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（はぁ――――あっ）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2370,
            "text_len": 42,
            "text_offset": 2380,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（桜井先生に漬物を漬けてもらいてぇ―！）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2458,
            "text_len": 80,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（桜井先生はどの漬物が得意だろうか。\\n　沢庵漬けなんて得意そうに思えるが……）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2580,
            "text_len": 28,
            "text_offset": 2590,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "はっ、あれは……桜井先生！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2726,
            "text_len": 40,
            "text_offset": 2736,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（大工カフェの前でどうしたんだろう？）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2796,
            "text_len": 12,
            "text_offset": 2806,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2834,
            "text_len": 12,
            "text_offset": 2844,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……ふぅ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2876,
            "text_len": 58,
            "text_offset": 2886,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（……ためいき？\\n　良くわからないが困っているみたいだ）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3054,
            "text_len": 32,
            "text_offset": 3064,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "弌弌これはチャンスじゃないか。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3118,
            "text_len": 22,
            "text_offset": 3128,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……よし、行くぞ！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3314,
            "text_len": 38,
            "text_offset": 3324,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "って、おぉっとぉ！\\nこ、校長先生！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3394,
            "text_len": 48,
            "text_offset": 3404,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ど、どうも！\\nこんな所で会うなんて奇遇ですね！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3530,
            "text_len": 52,
            "text_offset": 3534,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おい。こういう時は、\\n頭を下げ返すもんだよな？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3608,
            "text_len": 78,
            "text_offset": 3612,
            "warnings": 1
        },
        "speaker": "",
        "text": "おう、それが礼儀ってヤツだ。……いや、\\nちょっと待て！　この距離で下げたら！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3712,
            "text_len": 10,
            "text_offset": 3716,
            "warnings": 1
        },
        "speaker": "",
        "text": "……ん？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3772,
            "text_len": 36,
            "text_offset": 3776,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ――っ、そういうことか――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3908,
            "text_len": 38,
            "text_offset": 3918,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（しかし、走り出してみたものの……）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3984,
            "text_len": 34,
            "text_offset": 3994,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（何がどうチャンスなんだオレ？）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4064,
            "text_len": 80,
            "text_offset": 4074,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（しかも、桜井先生が何を困ってるかも\\n　わからないのに走り出してどうすんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4180,
            "text_len": 40,
            "text_offset": 4190,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（……いや、違う！　違うぞ高崎学！！）\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4256,
            "text_len": 58,
            "text_offset": 4266,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（誰であろうと目の前に\\n　困っている人が居るなら助ける）\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4350,
            "text_len": 30,
            "text_offset": 4360,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（それが教師ってもんだろ？）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4452,
            "text_len": 16,
            "text_offset": 4462,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井先生！！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4502,
            "text_len": 20,
            "text_offset": 4512,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、高崎先生っ。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4554,
            "text_len": 20,
            "text_offset": 4564,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "どうされましたか！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4606,
            "text_len": 56,
            "text_offset": 4616,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、えっと、\\nちょっと注文の仕方がわからなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4694,
            "text_len": 34,
            "text_offset": 4704,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "なるほど、オレに任せてください！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4770,
            "text_len": 44,
            "text_offset": 4780,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、は、はい。\\nよろしくおねがいします。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4846,
            "text_len": 28,
            "text_offset": 4856,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "え―っと、メニュ―は……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4920,
            "text_len": 12,
            "text_offset": 4930,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4972,
            "text_len": 30,
            "text_offset": 4982,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（さ、さっぱりわからん……）\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5050,
            "text_len": 24,
            "text_offset": 5060,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの……高崎先生？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5110,
            "text_len": 64,
            "text_offset": 5120,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（し、しかしわからないと言うのは。\\n　それだけはできない！！）\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5214,
            "text_len": 52,
            "text_offset": 5224,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……き、気合です！！\\n勢いで言えば何とかなります！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5302,
            "text_len": 6,
            "text_offset": 5312,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5350,
            "text_len": 30,
            "text_offset": 5360,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何を注文するつもりでしたか？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5412,
            "text_len": 28,
            "text_offset": 5422,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、普通にコ―ヒ―を……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5476,
            "text_len": 54,
            "text_offset": 5486,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "コ―ヒ―！　ホット！　中ぐらい！\\nこれで通じます！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5572,
            "text_len": 12,
            "text_offset": 5582,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！！！！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5626,
            "text_len": 66,
            "text_offset": 5636,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "は、はいっ、やってみますっ。\\nその、ありがとうございました……。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5730,
            "text_len": 22,
            "text_offset": 5736,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "いらっしゃいませ―。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5780,
            "text_len": 12,
            "text_offset": 5790,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……ふぅ。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5910,
            "text_len": 82,
            "text_offset": 5920,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（いい事をした後は気持ちがいいもんだ。\\n　弌弌それが教師の醍醐味ってやつかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6064,
            "text_len": 16,
            "text_offset": 6074,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井先生！！！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6116,
            "text_len": 20,
            "text_offset": 6126,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、高崎先生っ。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6168,
            "text_len": 30,
            "text_offset": 6178,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "何かお困りのようですが……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6230,
            "text_len": 82,
            "text_offset": 6240,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、実は少し寄っていこうと思ったんですが、\\n注文の仕方がよくわからなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6344,
            "text_len": 26,
            "text_offset": 6354,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "なるほど、どれどれ……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6410,
            "text_len": 12,
            "text_offset": 6420,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6464,
            "text_len": 64,
            "text_offset": 6474,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（さ、さっぱりわからん……。\\n　だが、ここで正直に言うのはっ）\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6582,
            "text_len": 36,
            "text_offset": 6590,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あれ、先生たち何やってるんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6642,
            "text_len": 18,
            "text_offset": 6652,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、相生さん。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6696,
            "text_len": 36,
            "text_offset": 6704,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、もしかして注文で困ってるとか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6762,
            "text_len": 24,
            "text_offset": 6772,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "実はそうなんですよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6818,
            "text_len": 68,
            "text_offset": 6826,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、じゃあ私教えましょうか？\\nこないだ注文のしかた覚えたんですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6926,
            "text_len": 46,
            "text_offset": 6936,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、ほんと？\\nそれじゃお願いしていいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7004,
            "text_len": 54,
            "text_offset": 7012,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "まっかせてくださいよ―！\\n高崎先生と二人分ですよね？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7090,
            "text_len": 24,
            "text_offset": 7100,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あ……いや、オレは弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7150,
            "text_len": 64,
            "text_offset": 7160,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（待てよ、これは桜井先生と\\n　お茶をするチャンスだぞ高崎学！）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7252,
            "text_len": 20,
            "text_offset": 7262,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "あ、ああ！　頼む！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7308,
            "text_len": 64,
            "text_offset": 7318,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ありがとう相生！\\n　今度の宿題は少し簡単にしてやるからな！）\\n",
