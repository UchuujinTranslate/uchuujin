[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2096,
            "text_len": 22,
            "text_offset": 2102,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "いらっしゃいませ―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2146,
            "text_len": 12,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2194,
            "text_len": 28,
            "text_offset": 2200,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ご注文は何になさいますか。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2250,
            "text_len": 28,
            "text_offset": 2260,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ホットコ―ヒ―。ト―ルで。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2310,
            "text_len": 20,
            "text_offset": 2316,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "かしこまりました。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2364,
            "text_len": 12,
            "text_offset": 2374,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2412,
            "text_len": 68,
            "text_offset": 2422,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（結局、桜井先生は誤解したままだ。\\n　次に会うのが気まずいな……）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2516,
            "text_len": 38,
            "text_offset": 2526,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（はぁ。今日はひどい１日だった……）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2588,
            "text_len": 24,
            "text_offset": 2594,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お待たせいたしました。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2640,
            "text_len": 38,
            "text_offset": 2650,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ん？　このスコ―ンは頼んでいないが？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2710,
            "text_len": 74,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "これはコ―ヒ―を頼んだお客様全員に\\nサ―ビスさせていただいているんです。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2812,
            "text_len": 46,
            "text_offset": 2818,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あっ、カップが熱いので\\nお気をつけください。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2890,
            "text_len": 12,
            "text_offset": 2900,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2934,
            "text_len": 14,
            "text_offset": 2940,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "……お客様？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2976,
            "text_len": 12,
            "text_offset": 2986,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "おぉ……。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3024,
            "text_len": 40,
            "text_offset": 3034,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（な、なぜだ、胸の中があたたかい……）\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3100,
            "text_len": 76,
            "text_offset": 3110,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（そうか……今日、初めて触れる人の優しさに\\n　私は感動しているんだ……！）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3208,
            "text_len": 16,
            "text_offset": 3214,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの、お客様？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3334,
            "text_len": 26,
            "text_offset": 3344,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "店員さん！　ありがとう！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3390,
            "text_len": 10,
            "text_offset": 3396,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ひっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3432,
            "text_len": 6,
            "text_offset": 3442,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3560,
            "text_len": 28,
            "text_offset": 3568,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あれ……理科の中村じゃん。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3620,
            "text_len": 36,
            "text_offset": 3626,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "だよね……\\n何やってるんだろ……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3770,
            "text_len": 18,
            "text_offset": 3780,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（……なんだ？）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3910,
            "text_len": 74,
            "text_offset": 3920,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（あれは……うちの学校の生徒か？\\n　……おや、後ろにも、もう一人いるな）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4032,
            "text_len": 12,
            "text_offset": 4038,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4170,
            "text_len": 10,
            "text_offset": 4180,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "東雲！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4206,
            "text_len": 30,
            "text_offset": 4212,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "中村先生……それは一体……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4262,
            "text_len": 10,
            "text_offset": 4268,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4298,
            "text_len": 6,
            "text_offset": 4308,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4330,
            "text_len": 26,
            "text_offset": 4340,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ち、違う！　私は別に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4470,
            "text_len": 54,
            "text_offset": 4476,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "白衣の不審者を見たと聞いてきたが、\\nさてはお前だな？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4552,
            "text_len": 32,
            "text_offset": 4562,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？　それは違う、私は無実だ！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4610,
            "text_len": 34,
            "text_offset": 4616,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "なら、まず署まで来てもらおうか。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4672,
            "text_len": 6,
            "text_offset": 4682,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4796,
            "text_len": 20,
            "text_offset": 4806,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うわあああああ！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4842,
            "text_len": 18,
            "text_offset": 4848,
            "warnings": 0
        },
        "speaker": "警察",
        "text": "あっ！　待て！！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4986,
            "text_len": 20,
            "text_offset": 4996,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うわあああああ！！\\n",
        "type": "dialog"
    }
]