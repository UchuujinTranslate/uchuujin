[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 32,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "結局、学校に来てしまった……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2204,
            "text_len": 76,
            "text_offset": 2212,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（髪型は元のモヒカンに戻せたから良いが、\\n　相変わらず幽霊になったままだ）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2324,
            "text_len": 70,
            "text_offset": 2332,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（こんなことになってるなんて、\\n　誰にも気付かれるわけにはいかない）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2428,
            "text_len": 72,
            "text_offset": 2436,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（なあに、今日は運良く半日だ。\\n　午前の授業を耐えれば、すぐに終わり）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2534,
            "text_len": 74,
            "text_offset": 2542,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（それまでの間、誰にもモヒカンのことを\\n　気付かれなければ良いだけさ！）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2652,
            "text_len": 12,
            "text_offset": 2660,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2704,
            "text_len": 32,
            "text_offset": 2712,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ん？　あれ、犬が入り込んで弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2770,
            "text_len": 12,
            "text_offset": 2778,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2826,
            "text_len": 6,
            "text_offset": 2834,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2870,
            "text_len": 82,
            "text_offset": 2878,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（こ、この犬……モヒカンを見ている！？\\n　もしかしたら動物にはわかるのかも……）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2986,
            "text_len": 90,
            "text_offset": 2994,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（！？　まずい、このまま見つめられていたら、\\n　他のみんなにも気付かれてしまう……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3166,
            "text_len": 58,
            "text_offset": 3170,
            "warnings": 1
        },
        "speaker": "",
        "text": "うおっ！？　いきなりなんだ！？\\nこっちに向かってきたぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3252,
            "text_len": 48,
            "text_offset": 3256,
            "warnings": 1
        },
        "speaker": "",
        "text": "に、逃げろ！\\nこのままでは犬が捕まってしまう！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3326,
            "text_len": 46,
            "text_offset": 3330,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああっ、ダメだ！\\n逃げても追いかけてくるぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3398,
            "text_len": 22,
            "text_offset": 3402,
            "warnings": 1
        },
        "speaker": "",
        "text": "あああっ、捕まった！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3470,
            "text_len": 26,
            "text_offset": 3474,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナも壊れた――っ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3586,
            "text_len": 28,
            "text_offset": 3596,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの、中之条くん……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3656,
            "text_len": 44,
            "text_offset": 3664,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……あっ、桜井先生。\\nおはようございます。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3730,
            "text_len": 34,
            "text_offset": 3740,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、はい、おはようございます。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3796,
            "text_len": 20,
            "text_offset": 3804,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "どうかしましたか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3846,
            "text_len": 50,
            "text_offset": 3856,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、えっと……\\nその髪型のことなんですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3936,
            "text_len": 6,
            "text_offset": 3944,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3986,
            "text_len": 58,
            "text_offset": 3996,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "前に、髪についての事情は\\n聞かせてもらいましたけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4076,
            "text_len": 58,
            "text_offset": 4086,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "で、でも、やっぱり今のままでは\\n風紀的にアレなので……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4166,
            "text_len": 64,
            "text_offset": 4176,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "その……髪の毛は立たせないように\\nしてもらえると……その……。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4266,
            "text_len": 84,
            "text_offset": 4274,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（くっ……桜井先生の意見はもっともだ……\\n　理由はどうあれ、僕は風紀を乱している）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4390,
            "text_len": 68,
            "text_offset": 4398,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（しかし！　しかし今は！\\n　下ろしたくても触ることすらできない！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4498,
            "text_len": 60,
            "text_offset": 4508,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、そうです。\\nどうせなら、かわいい髪型にしてみませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4638,
            "text_len": 28,
            "text_offset": 4646,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "チュ―リップとかですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4706,
            "text_len": 6,
            "text_offset": 4716,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4768,
            "text_len": 38,
            "text_offset": 4776,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "チュ―リップだけは勘弁してください！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4836,
            "text_len": 66,
            "text_offset": 4846,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、あ、スイマセン！\\nそれなら、チュ―リップはなしでいいですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4938,
            "text_len": 36,
            "text_offset": 4946,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "他の花も嫌です！\\n助けてください！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5014,
            "text_len": 38,
            "text_offset": 5024,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ハ、ハイ！\\n花全般、なしでいいです！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5088,
            "text_len": 36,
            "text_offset": 5096,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "じゃあ、この髪型でもいいですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5158,
            "text_len": 72,
            "text_offset": 5166,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（そう、冷静に考えてみると、\\n　別のどんな髪型でも今はダメなんだ……）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5264,
            "text_len": 66,
            "text_offset": 5272,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（なんとかして……なんとかして、\\n　この状況を回避しなければ！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5360,
            "text_len": 46,
            "text_offset": 5370,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\nこの髪型でもって……そ、それは……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5442,
            "text_len": 16,
            "text_offset": 5450,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "いいですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5492,
            "text_len": 8,
            "text_offset": 5502,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ひ！？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5536,
            "text_len": 20,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きょ、許可します！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5604,
            "text_len": 24,
            "text_offset": 5612,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "よかった！\\n……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5740,
            "text_len": 10,
            "text_offset": 5750,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あれ！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5814,
            "text_len": 30,
            "text_offset": 5822,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "か、かわいい髪型……ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5874,
            "text_len": 10,
            "text_offset": 5884,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はいっ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5916,
            "text_len": 24,
            "text_offset": 5924,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "た、例えばどんな……。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5970,
            "text_len": 82,
            "text_offset": 5980,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そうですね……あ、あの、長さを知りたいので\\nとりあえず、一度崩してもらえますか？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6088,
            "text_len": 68,
            "text_offset": 6096,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（くっ、まずい、今の僕の髪が\\n　さわれないことを知られては……！）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6196,
            "text_len": 14,
            "text_offset": 6204,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6244,
            "text_len": 28,
            "text_offset": 6254,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……あ、あの、中之条くん？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6308,
            "text_len": 36,
            "text_offset": 6316,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……じ、実は、僕、５円ハゲが……！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6384,
            "text_len": 6,
            "text_offset": 6394,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6426,
            "text_len": 22,
            "text_offset": 6434,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "６円でもいいんです！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6488,
            "text_len": 18,
            "text_offset": 6498,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……あがった！！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6538,
            "text_len": 32,
            "text_offset": 6546,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "だから、下ろしたくないんです。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6604,
            "text_len": 12,
            "text_offset": 6614,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6648,
            "text_len": 36,
            "text_offset": 6658,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……６円なら、しかたありませんね。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6726,
            "text_len": 32,
            "text_offset": 6734,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ありがとうございます、先生っ！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6894,
            "text_len": 12,
            "text_offset": 6902,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7018,
            "text_len": 16,
            "text_offset": 7026,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……どうして？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7100,
            "text_len": 20,
            "text_offset": 7108,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "かわいい髪型……？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7150,
            "text_len": 70,
            "text_offset": 7160,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はいっ！　かわいい髪型なら、\\nきっと風紀を乱すことはないと思います。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7268,
            "text_len": 22,
            "text_offset": 7276,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……でも、桜井先生。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7320,
            "text_len": 12,
            "text_offset": 7330,
