[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 38,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "いらっしゃいませ〜\\nご注文をどうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2210,
            "text_len": 24,
            "text_offset": 2218,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "う〜ん……そうね……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2268,
            "text_len": 66,
            "text_offset": 2276,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "カフェ・ラテのグランデを。\\nあ、それにア―モンドシロップ追加で。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2364,
            "text_len": 20,
            "text_offset": 2370,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "かしこまりました。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2412,
            "text_len": 32,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "さて、どこに座ろうかなっと弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2482,
            "text_len": 6,
            "text_offset": 2490,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2526,
            "text_len": 52,
            "text_offset": 2532,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む、奇遇だな。\\n立花みさともコ―ヒ―ブレイクとは。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2652,
            "text_len": 26,
            "text_offset": 2660,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "何がコ―ヒ―ブレイクよ！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2712,
            "text_len": 62,
            "text_offset": 2720,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そんなもの被って！\\nちょっとはお店の迷惑考えたらどうなの！！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2844,
            "text_len": 66,
            "text_offset": 2850,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、やはりそうか。\\nでは今後カフェでの役作りは控えるとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2948,
            "text_len": 30,
            "text_offset": 2956,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え！？　あ、あれ？　え―！？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3016,
            "text_len": 38,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うう……な、なぜ僕がこんな目に……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3102,
            "text_len": 22,
            "text_offset": 3110,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え！？　べ、別人！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3158,
            "text_len": 42,
            "text_offset": 3166,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なに？　なんなのこれ？　どういうこと！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3230,
            "text_len": 64,
            "text_offset": 3236,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "自分の役を客観的に見るということは\\n演技において非常に重要だ。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3322,
            "text_len": 20,
            "text_offset": 3330,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……だから、何よ。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3372,
            "text_len": 62,
            "text_offset": 3378,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "だから、通りがかりの下級生にあれを被せて\\n観察していたのだ。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3466,
            "text_len": 12,
            "text_offset": 3474,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3518,
            "text_len": 56,
            "text_offset": 3528,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "通りすがりの下級生……\\nということは、彼も私の生徒！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3606,
            "text_len": 54,
            "text_offset": 3616,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……おや？\\nこれは一体、私はなぜこんなところに……？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3692,
            "text_len": 78,
            "text_offset": 3702,
            "warnings": 1
        },
        "speaker": "校長先生",
        "text": "いえ、そんなことより！　君は……\\n１Ｐの中之条くんだね！　しっかりなさい！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3888,
            "text_len": 12,
            "text_offset": 3896,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3928,
            "text_len": 36,
            "text_offset": 3938,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "む、これは！\\n呼吸をしていない！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3990,
            "text_len": 32,
            "text_offset": 4000,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "では、さっそく人工呼吸を……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4074,
            "text_len": 60,
            "text_offset": 4084,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……悪化した！？\\nならば、急いで病院へ連れて行きましょう！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4170,
            "text_len": 52,
            "text_offset": 4180,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "死なせはしません！\\n私たちの生徒は、誰一人として！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4314,
            "text_len": 66,
            "text_offset": 4318,
            "warnings": 1
        },
        "speaker": "",
        "text": "ダ、ダメだ、本人の意思が強すぎて、\\nまったく操作を受け付けない！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4410,
            "text_len": 54,
            "text_offset": 4414,
            "warnings": 1
        },
        "speaker": "",
        "text": "まずい！\\nこのままだと操作電波の圏外に出てしまうぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4490,
            "text_len": 60,
            "text_offset": 4494,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんだと、圏外に！？\\n……で、圏外になったらどうなるんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4576,
            "text_len": 16,
            "text_offset": 4580,
            "warnings": 1
        },
        "speaker": "",
        "text": "そりゃあ……。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4618,
            "text_len": 16,
            "text_offset": 4622,
            "warnings": 1
        },
        "speaker": "",
        "text": "やっぱり……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4680,
            "text_len": 20,
            "text_offset": 4684,
            "warnings": 1
        },
        "speaker": "",
        "text": "……なるほどね―。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4804,
            "text_len": 46,
            "text_offset": 4810,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あの……すみません。\\n次の方、ご注文を……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4878,
            "text_len": 34,
            "text_offset": 4884,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "さ、笹原先輩！　先輩の番ですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4944,
            "text_len": 14,
            "text_offset": 4950,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む、そうか。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4998,
            "text_len": 58,
            "text_offset": 5006,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（どうなのよ……\\n　笹原先輩は普通に注文できるの……？）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5090,
            "text_len": 80,
            "text_offset": 5098,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（いやいや、そんなはずない。笹原先輩だよ？\\n　きっと盛大にボケてくれるはず！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5200,
            "text_len": 22,
            "text_offset": 5206,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、そうだな……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5248,
            "text_len": 26,
            "text_offset": 5256,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（ドキドキドキドキ……）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5312,
            "text_len": 18,
            "text_offset": 5318,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "よきにはからえ。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5368,
            "text_len": 36,
            "text_offset": 5374,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………？\\nか、かしこまりました。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5448,
            "text_len": 6,
            "text_offset": 5456,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5488,
            "text_len": 70,
            "text_offset": 5496,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ちょっと笹原！　店員さん困ってるじゃない！\\nちゃんと注文しなさいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5680,
            "text_len": 12,
            "text_offset": 5688,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5810,
            "text_len": 38,
            "text_offset": 5818,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、あれ！？\\nもう、なんでよ―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5882,
            "text_len": 46,
            "text_offset": 5888,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "立花みさとよ、\\nなにやら困っているようだな。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5962,
            "text_len": 60,
            "text_offset": 5970,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "な、なによ！　あんたには関係ないでしょ！\\n放っといてよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6052,
            "text_len": 36,
            "text_offset": 6058,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "いやいや、そう言うわけにはいかん。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6116,
            "text_len": 64,
            "text_offset": 6122,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "私と立花みさとの仲ではないか。\\n何事も遠慮なく相談するがいい。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6212,
            "text_len": 6,
            "text_offset": 6220,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6294,
            "text_len": 14,
            "text_offset": 6302,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "た…………。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6338,
            "text_len": 6,
            "text_offset": 6344,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "た？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6372,
            "text_len": 22,
            "text_offset": 6380,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……弾が、出ないの。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6424,
            "text_len": 42,
            "text_offset": 6430,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そのようだな。\\nよし、私が見て進ぜよう。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6508,
            "text_len": 62,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いやいやいやいや！　それってどうなの？\\n撃たれかけたのに！！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6608,
            "text_len": 12,
            "text_offset": 6614,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6648,
            "text_len": 22,
            "text_offset": 6656,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "どう？　何かわかる？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6700,
            "text_len": 38,
            "text_offset": 6706,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "立花みさとよ、弾が出ないのは当然だ。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6766,
            "text_len": 60,
            "text_offset": 6772,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "この兵器の装填弾数は一発。\\n先程、すでに一度撃っただろう。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6854,
            "text_len": 46,
            "text_offset": 6860,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ならば弾を込めてやらねば\\n撃てるわけがない。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6944,
            "text_len": 34,
            "text_offset": 6952,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、そっか。\\nうっかりしてたわ。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7008,
            "text_len": 50,
            "text_offset": 7014,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "こうして弾をこめれば……よし、\\nこれで問題ない。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7094,
            "text_len": 34,
            "text_offset": 7100,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "すご―い！　さすが笹原先輩です！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7204,
            "text_len": 36,
            "text_offset": 7212,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "お、お礼なんて言わないんだからね！\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7274,
            "text_len": 54,
            "text_offset": 7282,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "さ、笹原になんか、\\n絶対、感謝なんかしないんだから！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7450,
            "text_len": 12,
            "text_offset": 7456,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7526,
            "text_len": 30,
            "text_offset": 7534,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "わ、私と笹原の……仲……！？\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7600,
            "text_len": 6,
            "text_offset": 7606,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7648,
            "text_len": 70,
            "text_offset": 7654,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "さ、笹原先輩……あ、あ、あの……\\nお、お二人は一体……どういう……。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7746,
            "text_len": 46,
            "text_offset": 7754,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ど、ど、ど、どういう関係って言われても……。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7826,
            "text_len": 52,
            "text_offset": 7834,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ちょっと笹原！\\n誤解されるようなこと言わないでよ！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7912,
            "text_len": 24,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "？　誤解などではない。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7964,
            "text_len": 66,
            "text_offset": 7970,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "私と立花みさとは切っても切れぬ縁。\\n今さら何を隠すことがあろう。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8086,
            "text_len": 28,
            "text_offset": 8092,
            "warnings": 9
        },
        "speaker": "みお",
        "text": "９〜！？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8146,
            "text_len": 34,
            "text_offset": 8154,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "わ―！？　みおちゃんしっかり！！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8210,
            "text_len": 30,
            "text_offset": 8218,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "〜〜〜〜〜〜〜〜〜〜〜〜！！\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8362,
            "text_len": 12,
            "text_offset": 8370,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8492,
            "text_len": 76,
            "text_offset": 8498,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お待たせ致しました。カフェ・ラテのグランデ、\\nア―モンドシロップ入りです。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8600,
            "text_len": 24,
            "text_offset": 8608,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え、あ、ど、どうも弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8656,
            "text_len": 38,
            "text_offset": 8662,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ、ご苦労。\\n……うむ、美味なり。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8736,
            "text_len": 62,
            "text_offset": 8744,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ……さ、笹原！　それ、私のよ！！\\nなに勝手に飲んでるのよ！\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8828,
            "text_len": 38,
            "text_offset": 8834,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "む、それは失敬。\\nでは返すとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8894,
            "text_len": 18,
            "text_offset": 8902,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え……あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8942,
            "text_len": 26,
            "text_offset": 8948,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "どうした、早く飲まぬか。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8996,
            "text_len": 26,
            "text_offset": 9004,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、うん……ごくり……。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9052,
            "text_len": 24,
            "text_offset": 9058,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "どうだ、立花みさとよ。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9104,
            "text_len": 58,
            "text_offset": 9112,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "うん、おいし……はっ！？\\nそ、そんなわけないでしょっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9196,
            "text_len": 64,
            "text_offset": 9204,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "これ、もういらないわよ！\\n笹原、責任とって全部飲みなさいよね！\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9290,
            "text_len": 54,
            "text_offset": 9296,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そうか、では遠慮なく頂くとしよう。\\n……うむ、美味。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9372,
            "text_len": 16,
            "text_offset": 9380,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "か、間接……。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9514,
            "text_len": 38,
            "text_offset": 9520,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "きえ〜〜〜〜〜〜〜〜〜〜〜〜っ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9614,
            "text_len": 44,
            "text_offset": 9622,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "い、意外〜。\\n笹原先輩が良い人っぽいよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9696,
            "text_len": 28,
            "text_offset": 9702,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "笹原先輩は良い人だってば！\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9768,
            "text_len": 12,
            "text_offset": 9776,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9832,
            "text_len": 30,
            "text_offset": 9840,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、あれ？　行っちゃったよ。\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9886,
            "text_len": 16,
            "text_offset": 9892,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なんなんだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9930,
            "text_len": 52,
            "text_offset": 9936,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "手助けはいらぬということか。\\nさすがは立花みさと。\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10018,
            "text_len": 24,
            "text_offset": 10024,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お待たせ致しました〜。\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10080,
            "text_len": 34,
            "text_offset": 10086,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、笹原先輩、注文がきましたよ！\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10144,
            "text_len": 38,
            "text_offset": 10150,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、では外のベンチで頂くとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10320,
            "text_len": 78,
            "text_offset": 10326,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（し、信じられない！　学校の外で\\n　笹原先輩とこ〜ひ〜タイムできるなんて！）\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10432,
            "text_len": 24,
            "text_offset": 10440,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（そんなことより……）\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10582,
            "text_len": 58,
            "text_offset": 10590,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（コ―ヒ―飲んでる場合じゃないんじゃない？\\n　私ら弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10672,
            "text_len": 72,
            "text_offset": 10678,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ああ……笹原先輩と……！\\n　わ、わたし、もう死んでも良い〜〜っ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10908,
            "text_len": 12,
            "text_offset": 10916,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 105,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10948,
            "text_len": 24,
            "text_offset": 10954,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "な、なぜ、私だけ……。\\n",
        "type": "dialog"
    }
]