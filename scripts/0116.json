[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 26,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ふあ……あ―、眠い……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2194,
            "text_len": 72,
            "text_offset": 2202,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あんな深夜に囲碁サッカ―の試合\\nやってるんだもんな、そりゃ見ちゃうよ。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2296,
            "text_len": 50,
            "text_offset": 2304,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "とりあえず学校行って、\\n授業始まるまで寝よっと。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2382,
            "text_len": 52,
            "text_offset": 2392,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "マコちゃ―――ん！\\n待ってよ、マコちゃんってば―！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2460,
            "text_len": 10,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "げ……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2502,
            "text_len": 76,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はぁはぁ……も―、マコちゃんってば、\\n一緒に学校行こうって言ったでしょ―？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2612,
            "text_len": 30,
            "text_offset": 2620,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "行きたくないって言っただろ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2672,
            "text_len": 52,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "いいでしょ、どうせ同じ道なんだから。\\nほら、行こ？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2756,
            "text_len": 12,
            "text_offset": 2764,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2906,
            "text_len": 84,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（も―、姉ちゃんって何でこうなんだろ。\\n　もうちょっと弟のこと気遣ったりとか……）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3018,
            "text_len": 26,
            "text_offset": 3028,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、マコちゃん前っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3164,
            "text_len": 18,
            "text_offset": 3172,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え？　うわっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3234,
            "text_len": 68,
            "text_offset": 3244,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "こ、校長先生！　大丈夫ですか？\\nほらっ、マコちゃんも早く謝って！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3344,
            "text_len": 18,
            "text_offset": 3352,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "す、すいません。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3446,
            "text_len": 56,
            "text_offset": 3450,
            "warnings": 1
        },
        "speaker": "",
        "text": "マコちゃんだってさ！\\n高校生で、ちゃんはきついよな―。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3532,
            "text_len": 12,
            "text_offset": 3536,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3580,
            "text_len": 54,
            "text_offset": 3584,
            "warnings": 1
        },
        "speaker": "",
        "text": "なあ、お前もそう思うだろ？\\nちゃん、マコちゃんだぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3664,
            "text_len": 12,
            "text_offset": 3668,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3702,
            "text_len": 58,
            "text_offset": 3706,
            "warnings": 1
        },
        "speaker": "",
        "text": "おい、聞いてる？\\n今、地球人がマコちゃんって呼ばれて弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3790,
            "text_len": 22,
            "text_offset": 3794,
            "warnings": 1
        },
        "speaker": "",
        "text": "う、う、うるさ―い！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3838,
            "text_len": 62,
            "text_offset": 3842,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわっ、おいやめろ！\\nいきなり何を弌弌おい、そのスイッチは！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3966,
            "text_len": 70,
            "text_offset": 3970,
            "warnings": 1
        },
        "speaker": "",
        "text": "うるせえ！　未だに母ちゃんから\\nミっちゃんって呼ばれてるオレに謝れ！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4066,
            "text_len": 6,
            "text_offset": 4070,
            "warnings": 1
        },
        "speaker": "",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4122,
            "text_len": 44,
            "text_offset": 4126,
            "warnings": 1
        },
        "speaker": "",
        "text": "うっ、うう……\\n俺だって……俺だって……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4186,
            "text_len": 34,
            "text_offset": 4190,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんていうか……その、ごめんな？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 4410,
            "text_len": 56,
            "text_offset": 4434,
            "warnings": 0
        },
        "speaker": "女子生徒（女子生徒１）",
        "text": "おはようございま―す。\\n先生、弟さんと一緒なんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 4506,
            "text_len": 46,
            "text_offset": 4530,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒２）",
        "text": "桜井おはよ！\\n今日は姉ちゃんと一緒なんだな！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4688,
            "text_len": 34,
            "text_offset": 4698,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ふふっ、みんな声かけてくれるね。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4756,
            "text_len": 32,
            "text_offset": 4764,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ―も―……だから嫌なんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4830,
            "text_len": 30,
            "text_offset": 4840,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、マコちゃん今何か言った？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4892,
            "text_len": 30,
            "text_offset": 4900,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "先に学校行ってろって言った。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4952,
            "text_len": 60,
            "text_offset": 4962,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "何でよ、間に合う時間なんだから\\n一緒に行けばいいでしょ―？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5044,
            "text_len": 12,
            "text_offset": 5052,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5130,
            "text_len": 42,
            "text_offset": 5138,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ―もう！\\nだからうっとうしいんだって！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5202,
            "text_len": 56,
            "text_offset": 5210,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "学校じゃからむなって言っただろ！？\\n恥ずかしいんだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5288,
            "text_len": 20,
            "text_offset": 5298,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、でも、だって。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5344,
            "text_len": 40,
            "text_offset": 5352,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "家でもそんな仲いいわけでもないくせに！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5428,
            "text_len": 68,
            "text_offset": 5438,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そんなことないでしょ！？\\nお姉ちゃんはマコちゃんのこと好きだもん！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5542,
            "text_len": 6,
            "text_offset": 5550,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5588,
            "text_len": 38,
            "text_offset": 5596,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "だ、だからそういう問題じゃないって！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5656,
            "text_len": 52,
            "text_offset": 5666,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "じゃあどういう問題なの！？\\n私たち仲良しでしょ！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5740,
            "text_len": 42,
            "text_offset": 5750,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "それともマコちゃんは\\n私のこと嫌いなの？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5818,
            "text_len": 22,
            "text_offset": 5826,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "う……いや、それは。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5870,
            "text_len": 46,
            "text_offset": 5880,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "どうなの？\\nもしかしてお姉ちゃんのこと嫌い？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5948,
            "text_len": 38,
            "text_offset": 5956,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いや、それは……そうじゃないけどさ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6098,
            "text_len": 50,
            "text_offset": 6108,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "弌弌良かった！\\nやっぱりマコちゃんはいい子だね。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6284,
            "text_len": 12,
            "text_offset": 6292,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 6320,
            "text_len": 66,
            "text_offset": 6344,
            "warnings": 0
        },
        "speaker": "女子生徒（女子生徒２）",
        "text": "おはよ―ございます。\\n朝から姉弟ケンカなんて仲良しなんですね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6432,
            "text_len": 26,
            "text_offset": 6442,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、おはようございます。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 6484,
            "text_len": 62,
            "text_offset": 6508,
            "warnings": 0
        },
        "speaker": "女子生徒（女子生徒２）",
        "text": "桜井くん、お姉ちゃんいじめちゃダメだよ？\\nそれじゃ、後でね！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6592,
            "text_len": 34,
            "text_offset": 6602,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "マコちゃんのクラスの人だったね。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6740,
            "text_len": 24,
            "text_offset": 6748,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……み、見られた……。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6830,
            "text_len": 30,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "だったらオレが先行ってるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6914,
            "text_len": 28,
            "text_offset": 6924,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、待ってよマコちゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7008,
            "text_len": 10,
            "text_offset": 7016,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え……？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7054,
            "text_len": 18,
            "text_offset": 7064,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あいたたた……。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7110,
            "text_len": 30,
            "text_offset": 7118,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "まったく、なんで転ぶんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7170,
            "text_len": 50,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "マコちゃんを\\n追いかけようと思ったら弌弌痛っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7256,
            "text_len": 30,
            "text_offset": 7266,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "足、ひねっちゃったかも……。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7318,
            "text_len": 46,
            "text_offset": 7326,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "なら無理に立つなって！\\nほら、見せてみろよ。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7394,
            "text_len": 16,
            "text_offset": 7404,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "いたたっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7442,
            "text_len": 12,
            "text_offset": 7450,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……はあ。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 7578,
            "text_len": 62,
            "text_offset": 7602,
            "warnings": 0
        },
        "speaker": "女子生徒（女子生徒２）",
        "text": "うわっ、先生おんぶですか？\\nすっごい仲良しじゃないですか〜。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 7680,
            "text_len": 68,
            "text_offset": 7704,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒１）",
        "text": "桜井〜、お前すっげえ目立ってるぞ。\\nいいね―、姉弟仲よさそうでさ。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7788,
            "text_len": 30,
            "text_offset": 7798,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ご、ごめんねマコちゃん……。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7848,
            "text_len": 74,
            "text_offset": 7856,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……余計恥ずかしくなった。\\n　誰か助け……っていうか、むしろ死にたい）\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7992,
            "text_len": 80,
            "text_offset": 8000,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……しょうがない、リフティングでもして、\\n　姉ちゃんのことは見ないでおこう）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8108,
            "text_len": 82,
            "text_offset": 8118,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "マコちゃんまたボ―ル蹴りながら歩いてる。\\nも―、危ないからダメって言ったでしょ！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8222,
            "text_len": 14,
            "text_offset": 8230,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よっ……と。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8266,
            "text_len": 24,
            "text_offset": 8276,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "もうっ、ほら、貸して。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8326,
            "text_len": 22,
            "text_offset": 8334,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわっ、何すんだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8382,
            "text_len": 36,
            "text_offset": 8392,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "よそ見しながら歩いてたら危ないよ！\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8450,
            "text_len": 54,
            "text_offset": 8458,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ボ―ル横取りしようとする\\n姉ちゃんの方が危ないって！\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8538,
            "text_len": 36,
            "text_offset": 8546,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ほら、そんな事したら……あぁっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8714,
            "text_len": 14,
            "text_offset": 8724,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あわわ……。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8754,
            "text_len": 42,
            "text_offset": 8762,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわ……\\nこれ、歴史の富岡先生じゃ……。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8824,
            "text_len": 40,
            "text_offset": 8834,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "富岡先生、大丈夫ですか？\\n富岡先生っ！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8998,
            "text_len": 12,
            "text_offset": 9006,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9048,
            "text_len": 16,
            "text_offset": 9056,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（逃げろっ！）\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9186,
            "text_len": 42,
            "text_offset": 9196,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ええっ！？\\nま、待ってよマコちゃ―ん！！\\n",
        "type": "dialog"
    }
]