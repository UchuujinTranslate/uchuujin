[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 30,
            "text_offset": 2146,
            "warnings": 1
        },
        "speaker": "中之条",
        "text": "ふ〜ん、ふふん、ふふふ〜ん\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2192,
            "text_len": 50,
            "text_offset": 2200,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "よ―し、出来上がり。\\nそれじゃ、行ってきま―す！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2266,
            "text_len": 26,
            "text_offset": 2278,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ああ、行ってらっしゃい。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2420,
            "text_len": 18,
            "text_offset": 2432,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……懐かしいな。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2472,
            "text_len": 62,
            "text_offset": 2484,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "私も昔はあんな風に髪のセットから\\n１日が始まっていたもんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2572,
            "text_len": 12,
            "text_offset": 2584,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2618,
            "text_len": 26,
            "text_offset": 2630,
            "warnings": 1
        },
        "speaker": "中之条さん",
        "text": "久に少しやってみるか。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2688,
            "text_len": 32,
            "text_offset": 2700,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうと決まればクシを弌弌っと！\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2776,
            "text_len": 32,
            "text_offset": 2788,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "しまった、どこに落としたかな。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2836,
            "text_len": 62,
            "text_offset": 2848,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……ああ、あったあった。\\nあんな隙間に落としてしまったのか。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2926,
            "text_len": 68,
            "text_offset": 2938,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "しかし、クシにしては形が妙な気が……\\nとりあえず引っ張ってみよう。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3022,
            "text_len": 70,
            "text_offset": 3034,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "よっ……むっ、なかなか手が……よし届いた！\\nこのまま……よいしょ！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3134,
            "text_len": 40,
            "text_offset": 3146,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……クシじゃないな。\\n何だろうこれは。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3266,
            "text_len": 24,
            "text_offset": 3270,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナがもげた―！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3320,
            "text_len": 52,
            "text_offset": 3324,
            "warnings": 1
        },
        "speaker": "",
        "text": "誰だ！　手でもげるようにしたやつは！\\n呼んでこい！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3408,
            "text_len": 42,
            "text_offset": 3412,
            "warnings": 1
        },
        "speaker": "",
        "text": "無理です。\\n……もう、あいつはいません。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3480,
            "text_len": 10,
            "text_offset": 3484,
            "warnings": 1
        },
        "speaker": "",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3520,
            "text_len": 34,
            "text_offset": 3524,
            "warnings": 1
        },
        "speaker": "",
        "text": "塩と砂糖を間違えて……姫に……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3580,
            "text_len": 22,
            "text_offset": 3584,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうか、だったら弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3632,
            "text_len": 30,
            "text_offset": 3636,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっちで文句言ってやろうぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3712,
            "text_len": 26,
            "text_offset": 3716,
            "warnings": 1
        },
        "speaker": "",
        "text": "あきらめないで――っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3916,
            "text_len": 54,
            "text_offset": 3928,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……こうして髪を整えていると\\n若い頃を思い出すなあ。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3998,
            "text_len": 44,
            "text_offset": 4010,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "あの頃は髪型のことでずいぶん荒れたもんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4080,
            "text_len": 68,
            "text_offset": 4092,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "どうせならモヒカンらしく\\n不良になってやろうと暴れたりもしたっけ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4182,
            "text_len": 52,
            "text_offset": 4194,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そう……そんな自分を救ってくれたのは\\n大福だった。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4264,
            "text_len": 66,
            "text_offset": 4276,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "こうして今、まっとうな生活が出来ているのも\\n大福のおかげか……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4446,
            "text_len": 40,
            "text_offset": 4458,
            "warnings": 1
        },
        "speaker": "中之条さん",
        "text": "よし、完成。\\nうむ、中悪くない……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4522,
            "text_len": 20,
            "text_offset": 4532,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "ギャ――――ッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4584,
            "text_len": 6,
            "text_offset": 4596,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4752,
            "text_len": 38,
            "text_offset": 4764,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "な、何もそこまで驚く事は無いだろう！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4826,
            "text_len": 80,
            "text_offset": 4836,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あなた……もうモヒカンは止めるって\\nあの子が生まれる前に誓ったじゃないですか！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4942,
            "text_len": 6,
            "text_offset": 4954,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4980,
            "text_len": 40,
            "text_offset": 4990,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あの子はあんなに気にしているのに……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5052,
            "text_len": 18,
            "text_offset": 5064,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そ、そうか……。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5200,
            "text_len": 52,
            "text_offset": 5212,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "……すまなかった。\\nもう二度とモヒカンにはしない！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5294,
            "text_len": 36,
            "text_offset": 5304,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あなた……わかってくれたんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5362,
            "text_len": 58,
            "text_offset": 5374,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ああ、すまなかった。\\n私は何をバカなことをしていたんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5462,
            "text_len": 22,
            "text_offset": 5470,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "忘れ物忘れ物―っと。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5600,
            "text_len": 34,
            "text_offset": 5612,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "モヒカンなんて格好悪いのにな！！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5666,
            "text_len": 14,
            "text_offset": 5676,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あ、剛っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5814,
            "text_len": 12,
            "text_offset": 5826,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "な……っ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5874,
            "text_len": 18,
            "text_offset": 5882,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "と、父さん……。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5940,
            "text_len": 10,
            "text_offset": 5948,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "くっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6066,
            "text_len": 70,
            "text_offset": 6078,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "つよし！　違うんだ、今のは違う！！\\n違うんだ、つよし――――――！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6278,
            "text_len": 36,
            "text_offset": 6290,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "おいおい、そんなに驚かないでくれ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6356,
            "text_len": 22,
            "text_offset": 6366,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あ、あら……あなた。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6404,
            "text_len": 48,
            "text_offset": 6416,
            "warnings": 1
        },
        "speaker": "中之条さん",
        "text": "若い頃を思い出してな。\\n久にやってみたんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6484,
            "text_len": 22,
            "text_offset": 6494,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "まあ、そうでしたの。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6532,
            "text_len": 60,
            "text_offset": 6544,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "こうしていると\\n母さんと出会った頃のことも思い出すな……。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6620,
            "text_len": 58,
            "text_offset": 6632,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "そうだ、若い頃のように、\\n今からデ―トでもしてみないか？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6710,
            "text_len": 64,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あらあら、嬉しいお誘いだこと。\\nそれじゃ、お化粧してきますね。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6800,
            "text_len": 36,
            "text_offset": 6812,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ははは、ゆっくり準備してきなさい。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6868,
            "text_len": 40,
            "text_offset": 6878,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "ええ、めいっぱいおめかししてきますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7026,
            "text_len": 76,
            "text_offset": 7036,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "あ、ですけどその髪型はやめてくださいね。\\nいい歳して、似合ってませんから。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7138,
            "text_len": 6,
            "text_offset": 7150,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7300,
            "text_len": 24,
            "text_offset": 7312,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "待て、母さん！　私だ！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7356,
            "text_len": 32,
            "text_offset": 7366,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "……あ、あら、あなたでしたか。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7414,
            "text_len": 22,
            "text_offset": 7426,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "ああ、驚かせたかな？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7468,
            "text_len": 22,
            "text_offset": 7478,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "そりゃあ驚きますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7520,
            "text_len": 62,
            "text_offset": 7530,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "まさかあの頃みたいな\\nダサい格好をしてるなんて思いませんし。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7630,
            "text_len": 10,
            "text_offset": 7642,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "え……？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7672,
            "text_len": 50,
            "text_offset": 7682,
            "warnings": 0
        },
        "speaker": "中之条母",
        "text": "嫌ですよみっともない。\\nすぐに直してくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7758,
            "text_len": 42,
            "text_offset": 7770,
            "warnings": 0
        },
        "speaker": "中之条さん",
        "text": "いや……そこまで悪くは無いと思うんだが。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7832,
            "text_len": 64,
            "text_offset": 7842,
            "warnings": 0
        },
        "speaker": "中之条母",
