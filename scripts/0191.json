[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2096,
            "text_len": 62,
            "text_offset": 2106,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "昨日に続いていい天気だ。\\n今日も何だか良い事が起きそうだぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2190,
            "text_len": 46,
            "text_offset": 2200,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "……おや？\\n廊下の先に居るのは……桜井先生！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2276,
            "text_len": 30,
            "text_offset": 2286,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（早速いい事きたこれ―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2342,
            "text_len": 74,
            "text_offset": 2352,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（よし、早速声を……いや待て。\\n　どんな風に声をかけるか考えてからだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2452,
            "text_len": 60,
            "text_offset": 2462,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（どうすれば昨日のように\\n　自然に声がかけられるだろう？）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2548,
            "text_len": 22,
            "text_offset": 2558,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（ん？　昨日……？）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2616,
            "text_len": 12,
            "text_offset": 2626,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうか！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2672,
            "text_len": 66,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（昨日上手くいったのなら、\\n　昨日と同じ事を全てやればいい！！）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2774,
            "text_len": 86,
            "text_offset": 2784,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（そうすれば自然な挨拶から自然な会話へ。\\n　更には自然と好感度もアップするはず！！）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2892,
            "text_len": 28,
            "text_offset": 2902,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "よし、まずは挨拶から……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2950,
            "text_len": 12,
            "text_offset": 2956,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "高崎先生！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2994,
            "text_len": 50,
            "text_offset": 3000,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "また、\\n中之条くんが埋まっちゃってるんですけど！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3072,
            "text_len": 20,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3128,
            "text_len": 28,
            "text_offset": 3134,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "スグ来て欲しいんですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3188,
            "text_len": 12,
            "text_offset": 3198,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3232,
            "text_len": 22,
            "text_offset": 3242,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "いいか……よく聞け。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3286,
            "text_len": 8,
            "text_offset": 3292,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "はい！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3326,
            "text_len": 38,
            "text_offset": 3336,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "中之条は……埋まってなどいない！！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3400,
            "text_len": 20,
            "text_offset": 3406,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3452,
            "text_len": 34,
            "text_offset": 3462,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだ！！　落書きかもしれん！！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3522,
            "text_len": 20,
            "text_offset": 3528,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3570,
            "text_len": 68,
            "text_offset": 3576,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "でも助けようとした桜井先生も\\n落とし穴に落ちちゃってるんですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3670,
            "text_len": 20,
            "text_offset": 3680,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3722,
            "text_len": 24,
            "text_offset": 3732,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "中之条だけ塗りつぶせ。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3782,
            "text_len": 20,
            "text_offset": 3788,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3830,
            "text_len": 16,
            "text_offset": 3836,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "やってみます。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3878,
            "text_len": 20,
            "text_offset": 3888,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3934,
            "text_len": 18,
            "text_offset": 3944,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ちょっと待て！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3988,
            "text_len": 20,
            "text_offset": 3994,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "ええ―――っ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4048,
            "text_len": 28,
            "text_offset": 4058,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "だったらあれは誰なんだ！！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4102,
            "text_len": 8,
            "text_offset": 4108,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "！！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4132,
            "text_len": 72,
            "text_offset": 4142,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "同僚の後姿を見間違える教育者が……。\\nなんだ！！　その……教育者だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4230,
            "text_len": 58,
            "text_offset": 4240,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……私には桜井先生のような\\n可愛らしい系は似合わないな。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4314,
            "text_len": 56,
            "text_offset": 4324,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "これは無理にイメチェンしない方が\\nいいかもしれないな。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4412,
            "text_len": 12,
            "text_offset": 4422,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "行くぞ！！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4456,
            "text_len": 10,
            "text_offset": 4462,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "はいっ。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4580,
            "text_len": 18,
            "text_offset": 4590,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（急いで行くぜ）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4634,
            "text_len": 30,
            "text_offset": 4644,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "（それが教師ってもんだろ？）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4778,
            "text_len": 24,
            "text_offset": 4784,
            "warnings": 0
        },
        "speaker": "安中",
        "text": "先生！　こっちです！！\\n",
        "type": "dialog"
    }
]