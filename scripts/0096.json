[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2144,
            "text_len": 60,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "う―む。東雲なのの捕獲……\\n一体どうしたらできるのだろう。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2236,
            "text_len": 50,
            "text_offset": 2246,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "今まで試した方法はダメだ。\\nそれ以外で何か……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2324,
            "text_len": 34,
            "text_offset": 2334,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "おっと。ポケットから５円が……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2394,
            "text_len": 12,
            "text_offset": 2404,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2442,
            "text_len": 22,
            "text_offset": 2452,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "そうだ！　催眠術だ！\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2506,
            "text_len": 24,
            "text_offset": 2516,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "５円に紐を通して……。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2562,
            "text_len": 66,
            "text_offset": 2572,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "よし。後は５円を揺らしながら、\\nあなたはだんだん眠くなる……か？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2674,
            "text_len": 16,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "失礼しま―す。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2722,
            "text_len": 48,
            "text_offset": 2728,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "せんせ―。\\n授業で使った道具、返しに来たよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2806,
            "text_len": 28,
            "text_offset": 2816,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ああ、その辺に置いてくれ。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2874,
            "text_len": 10,
            "text_offset": 2888,
            "warnings": 0
        },
        "speaker": "中之条＆田中",
        "text": "は―い。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2942,
            "text_len": 54,
            "text_offset": 2952,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "（そうだ、東雲に試す前に\\n　こいつらにやってみるか）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3034,
            "text_len": 60,
            "text_offset": 3044,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "二人とも、すまない。\\n少しだけ実験に付き合ってもらえるか？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3128,
            "text_len": 14,
            "text_offset": 3136,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "実験ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3174,
            "text_len": 22,
            "text_offset": 3180,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "いいよ―。何やるの？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3224,
            "text_len": 52,
            "text_offset": 3234,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "催眠術だ。行くぞ。\\nこの５円玉をじっくり見ていろ。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3390,
            "text_len": 48,
            "text_offset": 3400,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "あなたはだんだん眠くな―る―。\\n眠くな―る―。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3464,
            "text_len": 10,
            "text_offset": 3472,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あはは。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3498,
            "text_len": 30,
            "text_offset": 3504,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "よくテレビで見るやつだね―。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3560,
            "text_len": 24,
            "text_offset": 3570,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "だんだん眠くな―る―。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3696,
            "text_len": 12,
            "text_offset": 3704,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あれ……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3732,
            "text_len": 20,
            "text_offset": 3738,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "なんか、眠く……。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3774,
            "text_len": 14,
            "text_offset": 3784,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "眠くな―る！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3902,
            "text_len": 12,
            "text_offset": 3910,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……ｙｙＹ\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3938,
            "text_len": 12,
            "text_offset": 3944,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……ｙｙＹ\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3982,
            "text_len": 20,
            "text_offset": 3992,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "！？　や、やった！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4058,
            "text_len": 12,
            "text_offset": 4068,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4134,
            "text_len": 28,
            "text_offset": 4144,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うわっ！　こ、校長先生！？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4200,
            "text_len": 12,
            "text_offset": 4210,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4244,
            "text_len": 12,
            "text_offset": 4254,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……校長？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4300,
            "text_len": 16,
            "text_offset": 4310,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……眠ってる。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4402,
            "text_len": 12,
            "text_offset": 4406,
            "warnings": 1
        },
        "speaker": "",
        "text": "なにぃ！？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4460,
            "text_len": 32,
            "text_offset": 4464,
            "warnings": 1
        },
        "speaker": "",
        "text": "……一体、何がおこったんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4512,
            "text_len": 72,
            "text_offset": 4516,
            "warnings": 1
        },
        "speaker": "",
        "text": "わからん！　急にオッサンが倒れて……\\nその衝撃でアンテナも壊れたんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4604,
            "text_len": 42,
            "text_offset": 4608,
            "warnings": 1
        },
        "speaker": "",
        "text": "しかし、なぜ急に？\\nま、まさか……魔法？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4666,
            "text_len": 46,
            "text_offset": 4670,
            "warnings": 1
        },
        "speaker": "",
        "text": "バ、バカ言うな！\\nそんなものがあるわけ……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4732,
            "text_len": 32,
            "text_offset": 4736,
            "warnings": 1
        },
        "speaker": "",
        "text": "だが、実際にオッサンは倒れた。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4784,
            "text_len": 18,
            "text_offset": 4788,
            "warnings": 1
        },
        "speaker": "",
        "text": "ｎｇ、マジカル！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4898,
            "text_len": 72,
            "text_offset": 4908,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "１発で成功するとは思わなかったが……\\nこの分なら本番も大丈夫そうだな。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5004,
            "text_len": 12,
            "text_offset": 5018,
            "warnings": 0
        },
        "speaker": "中之条＆田中",
        "text": "……ｙｙＹ\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5056,
            "text_len": 12,
            "text_offset": 5066,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5100,
            "text_len": 40,
            "text_offset": 5110,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "ちょっと遊んでみるか。\\nえ―っと……。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5176,
            "text_len": 46,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "二人の髪型が入れ替わ―る―。\\n……なんてな。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5346,
            "text_len": 14,
            "text_offset": 5356,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うおおっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5402,
            "text_len": 12,
            "text_offset": 5416,
            "warnings": 0
        },
        "speaker": "中之条＆田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5444,
            "text_len": 30,
            "text_offset": 5454,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "か、髪型が入れ替わった……。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5594,
            "text_len": 68,
            "text_offset": 5604,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "催眠術だからそういう風に思い込むくらいだと\\n思っていたのだが……。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5694,
            "text_len": 52,
            "text_offset": 5704,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "いや、それより……\\nもしかして私、催眠術の能力が？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5778,
            "text_len": 28,
            "text_offset": 5788,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "フ、フフフ……すごいぞ私。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5832,
            "text_len": 12,
            "text_offset": 5846,
            "warnings": 0
        },
        "speaker": "中之条＆田中",
        "text": "……ｙｙＹ\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5880,
            "text_len": 74,
            "text_offset": 5890,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……すごいが、喜ぶのは後だな。\\nまずはこの二人の髪を何とかしないと……。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6016,
            "text_len": 50,
            "text_offset": 6026,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "まずはこいつから。\\n田中の髪がアフロにな―る―。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6098,
            "text_len": 46,
            "text_offset": 6108,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "よし。次は\\n中之条の髪がモヒカンにな―る―。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6274,
            "text_len": 12,
            "text_offset": 6284,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うわっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6322,
            "text_len": 32,
            "text_offset": 6332,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "えっ？　アフロがデカくなった？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6484,
            "text_len": 24,
            "text_offset": 6494,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うぅ……やってみたい。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6544,
            "text_len": 52,
            "text_offset": 6554,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……いや、ダメだ。\\nやったら戻れなくなるかも……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6628,
            "text_len": 30,
            "text_offset": 6638,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "だ、だが、やってみたい……。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6700,
            "text_len": 12,
            "text_offset": 6710,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6744,
            "text_len": 40,
            "text_offset": 6754,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……た、田中のアフロがデカくな―る―。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6908,
            "text_len": 8,
            "text_offset": 6918,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "おお！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6952,
            "text_len": 42,
            "text_offset": 6962,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "田中のアフロが\\n更に更にデカくな―る―！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7114,
            "text_len": 12,
            "text_offset": 7124,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "うわっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7166,
            "text_len": 12,
            "text_offset": 7176,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7292,
            "text_len": 14,
            "text_offset": 7302,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "……キモっ。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
