    },
    {
      "magic": 65520,
      "id": 60,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "（あれっ？　いない！？）\n",
      "text_len": 26,
      "ctrl_code": "fbffc1ffca001000ebfe000065ff28000700"
    },
    {
      "magic": 65520,
