      "ctrl_code": "fdffedfffcfffcff66ff220000001aff0300000000fe00001e001aff0400000000fe00003c0016ffffffeefe0f000400e8fe0200ebfe000065ff2a001700"
    },
    {
      "magic": 65520,
      "id": 64,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "あっ、バディ！　コバディ！\nどこに行くんだ！\n",
      "text_len": 46,
      "ctrl_code": "fbffedffc1ffffff1000edffc8ff8b0166011000c1ff66011000ebfe000065ff2b001700"
    },
    {
      "magic": 65520,
      "id": 65,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "行っちゃった……。\nごめんね、みんなで散歩に行けなくて。\n",
      "text_len": 58,
      "ctrl_code": "fbffc1ff65011000ebfe000065ff2c001700"
    },
    {
      "magic": 65520,
      "id": 66,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "仕方ないから、僕とおじいちゃんと\n一緒に散歩に行こうか。\n",
