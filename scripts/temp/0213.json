        {
          "text": "ゲスト地球人の人気",
          "ctrl_code": "ffff00000500"
        },
        {
          "text": "アンテナ装着者の個性",
          "ctrl_code": "ffff00000600"
        },
        {
          "text": "わかるので大丈夫",
          "ctrl_code": "ffff040005000700"
        },
        {
          "text": "もう大丈夫",
