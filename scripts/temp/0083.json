{
  "UNK": "7324",
  "id": 16,
  "total": 96,
  "ptrs": 400,
  "extra": 0,
  "ptr2": {
    "size": 32,
    "total": 7,
    "offsets": [
      708,
      1794,
      1798,
      2558,
      2584,
