    "ctrl_code": "fcffedfe01000200060004ff03ff0300c1ffffff1000bcff8a01ff0067ff0900fcfff9ff0800eefe0000050066ff13000000daffc1fffd001000ebfe0100adffffff64ffffff0a00"
  },
  "contents": [
    {
      "magic": 65520,
      "id": 0,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（の、乗り切った……！）\n",
      "text_len": 26,
      "ctrl_code": "fbffc1fff7001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 1,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（モヒカンが幽霊になっていることは\n　誰にも気付かれていない）\n",
      "text_len": 64,
      "ctrl_code": "fbffc1ffff001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 2,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（とは言え油断はできない。\n　安心するのはうちに帰ってからだ）\n",
      "text_len": 64,
      "ctrl_code": "fbffc1fff6001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 3,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（このままダラダラとするより、\n　早いとこうちに帰ろう）\n",
      "text_len": 58,
      "ctrl_code": "fbffc1ff52011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 4,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "あっ、おーい中之条。\n",
      "text_len": 22,
      "ctrl_code": "fbffc1ffff001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 5,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbffedffc8fff60052011000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 6,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "……な、何？\n僕、急いでるんだけど……。\n",
      "text_len": 42,
      "ctrl_code": "fbffc1ff53011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 7,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "今日の夜、みんなで心霊スポットに\n行くことになったんだー。\n",
      "text_len": 60,
      "ctrl_code": "98ff0000effe070097fffbffc1fffb001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 8,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "心霊スポット？\n",
      "text_len": 16,
      "ctrl_code": "fbffc1ff52011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 9,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "おう。だからお前も来ないかー？\n",
      "text_len": 32,
      "ctrl_code": "fbffc1fffa001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 10,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（何かと思ったらそんなこと……\n　まあ気付かれるよりはいいか）\n",
      "text_len": 64,
      "ctrl_code": "fbfffefe010001007fff0200c1ff54011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 11,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "保護者同伴だから\n親にも怒られないぞ。\n",
      "text_len": 40,
      "ctrl_code": "fbffc1fff6001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 12,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "保護者？\n田中君のお父さん？\n",
      "text_len": 30,
      "ctrl_code": "fbffc1ff53011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 13,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "いーや。校長。\n",
      "text_len": 16,
      "ctrl_code": "fbffedffc1ffffff1000c1ffb3001000ebfe0000adffffff64ffffff0500"
    },
    {
      "magic": 65520,
      "id": 14,
      "speaker": "校長先生",
      "speaker_len": 8,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffe8fe0200ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 15,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbffedfffcfffaff01000000f8ff0800ecfec1ffffff100067fffffff4ff1e00f2fef0febcffdb01100067ff1000faff01000000f9ff0800c0ff03002e021000ebfe000065ff00002a00"
    },
    {
      "magic": 65520,
      "id": 16,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "お、おい、いいのかよ、\nそんな安請け合いしちまって……。\n",
      "text_len": 58,
      "ctrl_code": "fbffc0ff040009021000ebfe000065ff01002800"
    },
    {
      "magic": 65520,
      "id": 17,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "いいって。\nどうせ霊なんか出ないし。\n",
      "text_len": 38,
      "ctrl_code": "fbffc1ff2f021000ebfe000065ff02002a00"
    },
    {
      "magic": 65520,
      "id": 18,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "そ、そうかな……\nま、まあ、わかってるけどさ……。\n",
      "text_len": 52,
      "ctrl_code": "fbffc1ff07021000ebfe0000adffffff65ff03002800"
    },
    {
      "magic": 65520,
      "id": 19,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff09021000ebfe000065ff04002800"
    },
    {
      "magic": 65520,
      "id": 20,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "ふーん？\n",
      "text_len": 10,
      "ctrl_code": "fbffc1ff30021000ebfe000065ff05002a00"
    },
    {
      "magic": 65520,
      "id": 21,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "な、なんだよ？\n",
      "text_len": 16,
      "ctrl_code": "fbffc1ff07021000ebfe000065ff06002800"
    },
    {
      "magic": 65520,
      "id": 22,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "別に？　……あ、そうだ！\nこれも中継してやろうぜ。\n",
      "text_len": 52,
      "ctrl_code": "fbffc1ff32021000ebfe000065ff07002a00"
    },
    {
      "magic": 65520,
      "id": 23,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "えっ！？\n",
      "text_len": 10,
      "ctrl_code": "fbffc1ff09021000ebfe000065ff08002800"
    },
    {
      "magic": 65520,
      "id": 24,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "怪談とか苦手なやつも、強制参加！\n面白そうじゃね？\n",
      "text_len": 52,
      "ctrl_code": "fbffc1ff2e021000ebfe000065ff09002a00"
    },
    {
      "magic": 65520,
      "id": 25,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "あ、ああ……。\n",
      "text_len": 16,
      "ctrl_code": "fbff67ffffff66ff0b000000e8fe0200edffc8ff11022c021000adfffeffebfe000065ff0a002800"
    },
    {
      "magic": 65520,
      "id": 26,
      "speaker": "[{FF0D}]＆[{FF0F}]",
      "speaker_len": 6,
      "text": "えっ！？\n",
      "text_len": 10,
      "ctrl_code": "fdffedfffcfffcffedffc1ffffff100066ff00000000c1ff7402100067ff1800ebfe0000adffffff65ff0b001000"
    },
    {
      "magic": 65520,
      "id": 27,
      "speaker": "スターラ姫",
      "speaker_len": 10,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffedffaaff0200ff0063ff0000c1ffffffff00fdfefcfeadffffffe8fe0200ebfe000065ff0c002800"
    },
    {
      "magic": 65520,
      "id": 28,
      "speaker": "[{FF0D}]＆[{FF0F}]",
      "speaker_len": 6,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbffedfffcfff5ff040067ffffff63ffffffaaff00000000fdfebdffffff1000c1ffffff1000c2ffffff1000bcff6201ff00dafff6ff0800eafee9fef7ff0800fcffffff7fff0300c1fffa001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 29,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "悪いけど、興味ないよ。\n僕は幽霊って信じてないんだ。\n",
      "text_len": 54,
      "ctrl_code": "fbffc1ff54011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 30,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "何だよ、もしかしたら\n本当にいるかもしれないだろ？\n",
      "text_len": 52,
      "ctrl_code": "fbffc1fffb001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 31,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "いないよ。この世のものは全て\n科学で説明できるんだからね。\n",
      "text_len": 60,
      "ctrl_code": "98ff0000effe070097fffbffc1fffd001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 32,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（とは言え、今の僕の髪の毛を\n　科学で説明できるのだろうか……？）\n",
      "text_len": 68,
      "ctrl_code": "fbffc1fffb001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 33,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（ダ、ダメだ。科学を疑うんじゃない。\n　科学の力なら何でも解明できる）\n",
      "text_len": 72,
      "ctrl_code": "fbffc1fff6001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 34,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "そういうわけだから、僕は遠慮しておくよ。\n",
      "text_len": 42,
      "ctrl_code": "fbff05ffc1fffa001000ebfe0100adffffff64ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 35,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（それに、実は……ちょっと怖いんだよね。\n　いないんだけど、雰囲気的なものが……）\n",
      "text_len": 84,
      "ctrl_code": "fbff67ffffff66ff7e000000e8fe0300c1ff00011000ebfe010064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 36,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（僕の肩に手が！？）\n",
      "text_len": 22,
      "ctrl_code": "98ff0000effe040097fffdff7fff040002ff01ff05003200fbfffa000000960000fffffe05000600070063ff0300eefe15000400e8fe0300c1fffb001000ebfe0100adffffff65ff0d000a00"
    },
    {
      "magic": 65520,
      "id": 37,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（だ、誰だ！？　ま、まさか幽霊！？）\n",
      "text_len": 38,
      "ctrl_code": "fbffe8fe0300c1fffd001000ebfe0100adffffff65ff0e000a00"
    },
    {
      "magic": 65520,
      "id": 38,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "（振り返るな！　こういうとき、\n　振り返ったら呪われる！）\n",
      "text_len": 60,
      "ctrl_code": "fbff63ffffff67ff1800e8fe0200c1ff00011000ebfe000065ff0f000a00"
    },
    {
      "magic": 65520,
      "id": 39,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "うおおおおおお！！\n",
      "text_len": 20,
      "ctrl_code": "fbffedfffcff66ff1e000000c0ff0300ffff1000daffe8fe0200c1ff55011000ebfe000065ff10001300"
    },
    {
      "magic": 65520,
      "id": 40,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbffeefe030004001fff0300000000fe0000c0ff03000c01ff001aff0300000078ff00007800ebfe000065ff11000b00"
    },
    {
      "magic": 65520,
      "id": 41,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "…………！？\n",
      "text_len": 14,
      "ctrl_code": "fbff16ffffffeefe04000400c1ff52011000ebfe0000adffffff65ff12001300"
    },
    {
      "magic": 65520,
      "id": 42,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff11011000ebfe000065ff13000b00"
    },
    {
      "magic": 65520,
      "id": 43,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "え……えっ……？\n",
      "text_len": 18,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcfff701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe000065ff14000b00"
    },
    {
      "magic": 65520,
      "id": 44,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "？？？？？\n",
      "text_len": 12,
      "ctrl_code": "98ff0000effe030097fffdff67ffffffe7fe0000fdff67ffffffeafee9fef7ff0800fcffffffeefe03000400e8fe020066ff7c000000c1ff00011000ebfe000065ff15000a00"
    },
    {
      "magic": 65520,
      "id": 45,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "うわああ！？\n",
      "text_len": 14,
      "ctrl_code": "fbffc1ff55011000ebfe0000adffffff65ff16001300"
    },
    {
      "magic": 65520,
      "id": 46,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffedffc1ffffff1000c1ff0c011000ebfe000065ff17000b00"
    },
    {
      "magic": 65520,
      "id": 47,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "えっ？　えっ？\n",
      "text_len": 16,
      "ctrl_code": "fbff67ff1000c1fffa001000ebfe000065ff18000a00"
    },
    {
      "magic": 65520,
      "id": 48,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "あ……桜井先生……？\n",
      "text_len": 22,
      "ctrl_code": "fbffeefe04000400edffc8fffa0007011000ebfe000065ff19000b00"
    },
    {
      "magic": 65520,
      "id": 49,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "あ、あの、中之条くんのノート……\n返し忘れたので……。\n",
      "text_len": 56,
      "ctrl_code": "fbffc1fff7001000ebfe0000adffffff65ff1a000a00"
    },
    {
      "magic": 65520,
      "id": 50,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "98ff0000effe020097fffbffedffc1ffffff1000eefe07000400ebfe000065ff1b001300"
    },
    {
      "magic": 65520,
      "id": 51,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "…………プッ。\n",
      "text_len": 16,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcfff701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800e8fe0200ebfe000065ff1c000a00"
    },
    {
      "magic": 65520,
      "id": 52,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "98ff0000effe030097fffdff67ffffffe7fe0000fdff67ffffffeafee9fef7ff0800fcffffffeefe0300040066ff8a000000afff03000100c1ff00011000ebfe0000adffffff65ff1d000a00"
    },
    {
      "magic": 65520,
      "id": 53,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbff67ff1400c0ff040007011000ebfe000065ff1e000b00"
    },
    {
      "magic": 65520,
      "id": 54,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "中之条くん、ちょっといいですか？\n",
      "text_len": 34,
      "ctrl_code": "fbffeefe01000400c1ff00011000ebfe0000adffffff65ff1f000a00"
    },
    {
      "magic": 65520,
      "id": 55,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "98ff0000effe060097fffbffeefe0f000400c1ff11011000ebfe000065ff20000b00"
    },
    {
      "magic": 65520,
      "id": 56,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "……中之条くん？\n",
      "text_len": 18,
      "ctrl_code": "fbffc0ff040052011000ebfe0000adffffff65ff21001300"
    },
    {
      "magic": 65520,
      "id": 57,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff00011000ebfe0000adffffff65ff22000a00"
    },
    {
      "magic": 65520,
      "id": 58,
      "speaker": "中之条",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffafff03000000edffc8ff520111011000ebfe000065ff23001300"
    },
    {
      "magic": 65520,
      "id": 59,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "センセー。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff0b011000ebfe000065ff24000b00"
    },
    {
      "magic": 65520,
      "id": 60,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "あ、は、はい。何ですか田中君。\n",
      "text_len": 32,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcfff701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe000065ff25001300"
    },
    {
      "magic": 65520,
      "id": 61,
      "speaker": "田中",
      "speaker_len": 4,
      "text": "中之条、気絶してまーす。\n",
      "text_len": 26,
      "ctrl_code": "fbffe8fe0200ebfe000065ff26000b00"
    },
    {
      "magic": 65520,
      "id": 62,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "98ff0000effe030097fffdff67ffffffe7fe0000fdff67ffffffeafee9fef7ff0800fcffffff"
    }
  ]
}