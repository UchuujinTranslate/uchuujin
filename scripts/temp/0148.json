{
  "UNK": "c014",
  "id": 16,
  "total": 82,
  "ptrs": 352,
  "extra": 0,
  "ptr2": {
    "size": 32,
    "total": 7,
    "offsets": [
      742,
      1694,
      1698,
      2810,
      2836,
      4218,
