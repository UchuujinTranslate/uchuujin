  "extra": 0,
  "ptr2": {
    "size": 4,
    "total": 0,
    "offsets": [],
    "ctrl_code": "fcffc1ffffff1000bcffd5d1ff0067ff0b00fcfff9ff0800ebfe000065ff00001700"
  },
  "contents": [
    {
      "magic": 65520,
      "id": 0,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "ただいま〜……。\n",
      "text_len": 18,
      "ctrl_code": "fbffc1ff66011000ebfe000065ff01001700"
    },
    {
      "magic": 65520,
      "id": 1,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "はぁ……疲れた……\nバイトってこんなに大変なんだ。\n",
      "text_len": 52,
      "ctrl_code": "fbffc1ff64011000ebfe000065ff02001700"
    },
    {
      "magic": 65520,
      "id": 2,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "もう動けないよ……\nおじいちゃんたちを捜しにも行けない……。\n",
      "text_len": 62,
      "ctrl_code": "fbffedffc8ff7e0164011000ebfe000065ff03001b00"
    },
    {
      "magic": 65520,
      "id": 3,
      "speaker": "ジェントルマン",
      "speaker_len": 14,
      "text": "おお、おかえり。\n",
      "text_len": 18,
      "ctrl_code": "fbffc1ff65011000ebfe000065ff04001700"
    },
    {
      "magic": 65520,
      "id": 4,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "うん、おじいちゃん。\nただいまー。\n",
      "text_len": 36,
      "ctrl_code": "fbffc1ff64011000ebfe0000adffffff65ff05001700"
    },
    {
      "magic": 65520,
      "id": 5,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffe8fe0200c1ff67011000ebfe000065ff06001700"
    },
    {
      "magic": 65520,
      "id": 6,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "あれ！？　おじいちゃん！？\n",
      "text_len": 28,
      "ctrl_code": "fbffedffc8ff300132011000ebfe0000adffffff65ff07000e00"
    },
    {
      "magic": 65520,
      "id": 7,
      "speaker": "バディ",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff32011000ebfe0000adffffff65ff08000f00"
    },
    {
      "magic": 65520,
      "id": 8,
      "speaker": "コバディ",
      "speaker_len": 8,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffe8fe0200ebfe000065ff09001700"
    },
    {
      "magic": 65520,
      "id": 9,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "バディ！　コバディ！　お前たちも！\n帰ってたのか！？\n",
      "text_len": 54,
      "ctrl_code": "fbffedffc8ff7e0167011000ebfe000065ff0a001b00"
    },
    {
      "magic": 65520,
      "id": 10,
      "speaker": "ジェントルマン",
      "speaker_len": 14,
      "text": "夕方には帰っておったよ。\n",
      "text_len": 26,
      "ctrl_code": "fbffc1ff66011000ebfe000065ff0b001700"
    },
    {
      "magic": 65520,
      "id": 11,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "そんな……。\n",
      "text_len": 14,
      "ctrl_code": "fbffc1ff64011000ebfe000065ff0c001700"
    },
    {
      "magic": 65520,
      "id": 12,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "僕、おじいちゃんたちを捜しに行って、\nこんなに疲れるまで働いて……。\n",
      "text_len": 70,
      "ctrl_code": "fbffc1ff7f011000ebfe000065ff0d001b00"
    },
    {
      "magic": 65520,
      "id": 13,
      "speaker": "ジェントルマン",
      "speaker_len": 14,
      "text": "…………？\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff67011000ebfe0100adffffff65ff0e001700"
    },
    {
      "magic": 65520,
      "id": 14,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（あっ、バイトのことを言ったら\n　怒られるかも）\n",
      "text_len": 50,
      "ctrl_code": "fbffc1ff65011000ebfe000065ff0f001700"
    },
    {
      "magic": 65520,
      "id": 15,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "えっと、なんでもないよ。\nそれよりみんなが無事で良かった。\n",
      "text_len": 60,
      "ctrl_code": "fbffc1ff64011000ebfe0100adffffff65ff10001700"
    },
    {
      "magic": 65520,
      "id": 16,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（いっぱい心配して、街を走り回って、\n　バイトでくたくたになるまで働いて……）\n",
      "text_len": 80,
      "ctrl_code": "fbffc1ff65011000ebfe0100adffffff65ff11001700"
    },
    {
      "magic": 65520,
      "id": 17,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（色々あったけど、でも、みんな無事だから\n　結果的にこれで良かっ[{0D80}][{0D80}]）\n",
      "text_len": 72,
      "ctrl_code": "fbffc1ff67011000ebfe0000adffffff65ff12001700"
    },
    {
      "magic": 65520,
      "id": 18,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff64011000ebfe0100adffffff65ff13001700"
    },
    {
      "magic": 65520,
      "id": 19,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（良かったんだ。良かったんだけど……）\n",
      "text_len": 40,
      "ctrl_code": "fbffc1ff66011000ebfe0100adffffff65ff14001700"
    },
    {
      "magic": 65520,
      "id": 20,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（バディたちの心配をして、おじいちゃんの\n　心配もして、バイトすることになって……）\n",
      "text_len": 86,
      "ctrl_code": "fbffc1ff64011000ebfe0100adffffff65ff15001700"
    },
    {
      "magic": 65520,
      "id": 21,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（何だか、今日一番損しているのは\n　僕だった気がする……）\n",
      "text_len": 60,
      "ctrl_code": "fbffc1ff66011000ebfe0100adffffff65ff16001700"
    },
    {
      "magic": 65520,
      "id": 22,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "（バディたちはおじいちゃんに任せて\n　遊びに行けばよかったんじゃ……！）\n",
      "text_len": 74,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcffead1ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe000065ff17001b00"
    },
    {
      "magic": 65520,
      "id": 23,
      "speaker": "ジェントルマン",
      "speaker_len": 14,
      "text": "……どうしたんじゃ？　かずし？\n",
      "text_len": 32,
      "ctrl_code": "fbffe8fe0200ebfe000065ff18001700"
    },
    {
      "magic": 65520,
      "id": 24,
      "speaker": "きよし",
      "speaker_len": 6,
      "text": "きよしだよ！！\n",
      "text_len": 16,
      "ctrl_code": "fbffedfffcfff4ff780063ffffffbeff5401080067ff0d00f4ff7800fdfff8ff080067fffffff7ff0800fcffffff"
    }
  ]
}