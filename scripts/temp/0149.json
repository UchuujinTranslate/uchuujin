      "ctrl_code": "fbffebfe000065ff34000900"
    },
    {
      "magic": 65520,
      "id": 79,
      "speaker": "大工",
      "speaker_len": 4,
      "text": "誰か助けてー！！\n",
      "text_len": 18,
      "ctrl_code": "98ff0000effe030097fffbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbeff4600ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe0100adffffff65ff35000600"
    },
    {
      "magic": 65520,
      "id": 80,
      "speaker": "中村先生",
      "speaker_len": 8,
      "text": "（逃げろっ！！）\n",
