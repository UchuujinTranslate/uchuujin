{
  "UNK": "3a1c",
  "id": 16,
  "total": 82,
  "ptrs": 352,
  "extra": 0,
  "ptr2": {
    "size": 32,
    "total": 7,
    "offsets": [
      1004,
      1930,
      1934,
      2538,
