{
  "UNK": "021c",
  "id": 16,
  "total": 93,
  "ptrs": 400,
  "extra": 0,
  "ptr2": {
    "size": 32,
    "total": 7,
    "offsets": [
      1008,
      1804,
      1808,
      2852,
