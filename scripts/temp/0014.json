    "ctrl_code": "fcffedfe01000200060004ff03ff0300c1ffffff1000bcff00f0ff0067ff0900fcfff9ff0800eefe0000050066ff13000000daffc1ff66001000ebfe000064ffffff0300"
  },
  "contents": [
    {
      "magic": 65520,
      "id": 0,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "はぁ……やっと帰れる……。\n",
      "text_len": 28,
      "ctrl_code": "fbffc1ff44021000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 1,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（すぐに郵便局行って\n　この原稿を送らなきゃ……）\n",
      "text_len": 52,
      "ctrl_code": "fbffc1ff64001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 2,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（……っと、その前に、\n　念のため中を確認しとこっと）\n",
      "text_len": 56,
      "ctrl_code": "fbffc1ff5e001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 3,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（…………よし。周りに誰もいない。\n　今の内に……１……４……８……）\n",
      "text_len": 72,
      "ctrl_code": "fbffc1ff6a001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 4,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "……ん？\n数え間違えたかな。もう一回……。\n",
      "text_len": 44,
      "ctrl_code": "fbff67ffffffc1ff63001000ebfe0000adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 5,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbff66ff7c000000c1ff65001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 6,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "…………！？\n",
      "text_len": 14,
      "ctrl_code": "fbff67ff1100e8fe0300c1ff68001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 7,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（なーーーーーーいっ！！）\n",
      "text_len": 28,
      "ctrl_code": "98ff0000effe030097fffbffc1ff65001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 8,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（いや違う、正確には、\n　足りなーーーーーーーいっ！！）\n",
      "text_len": 58,
      "ctrl_code": "fbffc1ff68001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 9,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（やっぱりあれだ！\n　ゆっことぶつかった時！！）\n",
      "text_len": 50,
      "ctrl_code": "fbffc1ff65001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 10,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（くっ、タイムリミットは！？\n　ううっ！？　あと数時間しかない！）\n",
      "text_len": 68,
      "ctrl_code": "fbffc1ff69001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 11,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（ええい、悔やんでいる間も惜しい！\n　今からでも探し出してやる！）\n",
      "text_len": 68,
      "ctrl_code": "fbfffaff03000000f8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcff8701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfffaff04000000f9ff0800c1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 12,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "あっ、長野原さん。\nちょっと相談があるんですけど[{0D80}][{0D80}]\n",
      "text_len": 54,
      "ctrl_code": "fbffedffc8ff65000d011000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 13,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "えーっ！？　すみません、\nいま急いでるんで今度にしてください！！\n",
      "text_len": 66,
      "ctrl_code": "fbffc1ff07011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 14,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "あの、お時間は取らせませんから……\nちょっとだけお願いします。\n",
      "text_len": 64,
      "ctrl_code": "fbffc1ff66001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 15,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "うぅ〜……な、なんですか？\n",
      "text_len": 28,
      "ctrl_code": "fbffc1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 16,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "はい、実は……あれのことなんですけど。\n",
      "text_len": 40,
      "ctrl_code": "fbffc1ff64001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 17,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "あれ？\n",
      "text_len": 8,
      "ctrl_code": "fbfff8ff0800daff90ffffffdeff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbeff8a00ff00c1fffeffff0090ffffff90ffffff90ffffff67ffffff71ff000063ffffff90ffffffedfff9ff0800e8fe020066ff7b000000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 18,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "！！！！？\n",
      "text_len": 12,
      "ctrl_code": "98ff0000effe080097fffbff67ff1400ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 19,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "長野原さんの絵がとても素敵だったので、\n生活指導の垂れ幕に使わせてもらったんです。\n",
      "text_len": 84,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcff8701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800e8fe0400ebfe0000adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 20,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc0ff04000d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 21,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "美術部の子が描いてくれたんです。\n元の絵そっくりで良くできてますよね。\n",
      "text_len": 72,
      "ctrl_code": "fbffc0ff030066001000ebfe0000adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 22,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "…………。\n",
      "text_len": 12,
      "ctrl_code": "fbffc1ff11011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 23,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "？？？\n",
      "text_len": 8,
      "ctrl_code": "fbffedffc1ffffff1000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 24,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（終わった……\n　私の平穏な学園生活が崩れていく……）\n",
      "text_len": 56,
      "ctrl_code": "fbffc1ff6c001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 25,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "…………ん？\n",
      "text_len": 14,
      "ctrl_code": "fbfff8ff0800daff90ffffffdeff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbeff8b00ff00c1fffeffff0090ffffff90ffffff90ffffff67ff110071ff000063ffffff90ffffffedfff9ff0800e8fe0200ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 26,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "98ff0000effe060097fffbffebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 27,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（あそこに引っかかってるの、\n　私の原稿だ！！）\n",
      "text_len": 50,
      "ctrl_code": "fbfffefe010001007fff020066ffc7000000f8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcff8701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800c1ff61001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 28,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "あっ！？　こっちに落ちてくる！！\nやった、そのまま[{0D80}][{0D80}]\n",
      "text_len": 56,
      "ctrl_code": "fbffc1ffb3001000ebfe000064ffffff0500"
    },
    {
      "magic": 65520,
      "id": 29,
      "speaker": "校長先生",
      "speaker_len": 8,
      "text": "…………？\n",
      "text_len": 12,
      "ctrl_code": "fbffe8fe0200c1ff68001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 30,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbff67ff1800e8fe0200c1ff69001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 31,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "見るなーーーーーー！！\n",
      "text_len": 24,
      "ctrl_code": "fdffedfffcff1aff0300000000fe00000a0016ffffffedffc1ffffff1000c1ffb3001000edfffcff66ff3e000000c1ffffff1000b3ff0300f4ff0400daffb2ffe8fe0200c1ffb2001000ebfe000064ffffff0500"
    },
    {
      "magic": 65520,
      "id": 32,
      "speaker": "校長先生",
      "speaker_len": 8,
      "text": "！？\n",
      "text_len": 6,
      "ctrl_code": "fbffedfffcfffaff01000000f8ff0800ecfec1ffffff100067fffffff4ff1e00f2fef0febcffdb01100067fffcfffaff01000000f9ff0800e8fe0200c0ff030027021000ebfe000065ff00002a00"
    },
    {
      "magic": 65520,
      "id": 33,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "見えた！\n",
      "text_len": 10,
      "ctrl_code": "fbffc0ff040014021000ebfe000065ff01002800"
    },
    {
      "magic": 65520,
      "id": 34,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "えっ、嘘！？\n",
      "text_len": 14,
      "ctrl_code": "fbffc1ff27021000ebfe000065ff02002a00"
    },
    {
      "magic": 65520,
      "id": 35,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "マジだ！\nほんの一瞬だけどな。\n",
      "text_len": 32,
      "ctrl_code": "fbffc1ff13021000ebfe000065ff03002800"
    },
    {
      "magic": 65520,
      "id": 36,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "くっそおおおお！\n俺、全く見えなかったぁああ！\n",
      "text_len": 48,
      "ctrl_code": "fbffc1ff0c021000ebfe000065ff04002800"
    },
    {
      "magic": 65520,
      "id": 37,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "お、おい、教えてくれよ。\nどんなのが見えたんだ？\n",
      "text_len": 50,
      "ctrl_code": "fbffc1ff27021000ebfe000065ff05002a00"
    },
    {
      "magic": 65520,
      "id": 38,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "それが……なんかモヒカンのやつが\n服を脱いでる絵だったんだけどさ。\n",
      "text_len": 68,
      "ctrl_code": "fbffc1ff13021000ebfe000065ff06002800"
    },
    {
      "magic": 65520,
      "id": 39,
      "speaker": "[{FF0D}]",
      "speaker_len": 2,
      "text": "…………えっ？\n",
      "text_len": 16,
      "ctrl_code": "fbffc1ff26021000ebfe000065ff07002a00"
    },
    {
      "magic": 65520,
      "id": 40,
      "speaker": "[{FF0F}]",
      "speaker_len": 2,
      "text": "えっ？\n",
      "text_len": 8,
      "ctrl_code": "fbffedffaaff0200ff0063ff0000c1ffffffff00fdfefcfeadffffffebfe0000adffffff65ff08002800"
    },
    {
      "magic": 65520,
      "id": 41,
      "speaker": "[{FF0D}]＆[{FF0F}]",
      "speaker_len": 6,
      "text": "………………。\n",
      "text_len": 16,
      "ctrl_code": "fbffedfffcfff5ff040067ffffff63ffffffaaff00000000fdfebdffffff1000c1ffffff1000c2ffffff1000bcff6201ff00dafff6ff0800eafee9fef7ff0800fcffffff7fff030066ffc7000000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 42,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "ああっ！！　飛ぶ！！\n",
      "text_len": 22,
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcff8701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800edffc8ff65000b011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 43,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "えっ？　な、何がですか？\n",
      "text_len": 26,
      "ctrl_code": "fbffc1ff68001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 44,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（あっ、堪えた！　まだ飛ばされずに\n　ポスターにくっついてる！）\n",
      "text_len": 66,
      "ctrl_code": "fbffc1ff6a001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 45,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "あれなら……屋上に行けば……。\n",
      "text_len": 32,
      "ctrl_code": "fbffc1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 46,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "屋上？　屋上がどうかしたんですか？\n",
      "text_len": 36,
      "ctrl_code": "fbffc1ff5f001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 47,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "ごめんなさい！！\n私、行きます！\n",
      "text_len": 34,
      "ctrl_code": "fbffedfffcff66ff22000000c0ff0300ffff1000daffc1ff0c011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 48,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "きゃ！　長野原さん！？\n",
      "text_len": 24,
      "ctrl_code": "fbffc1ff0b011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 49,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "飛ぶ……屋上……逝く……。\n",
      "text_len": 28,
      "ctrl_code": "fbff67ffffff66ff7b000000e8fe0200c1ff0c011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 50,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "…………！？\n",
      "text_len": 14,
      "ctrl_code": "fbfffaff03000000f8ff0800daff90ffffffdeff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcffa201ff00c1fffeffff0090ffffff90ffffff90ffffff67ff090071ff000063ffffff90ffffffedfffaff04000000f9ff0800ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 51,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "はぁ、はぁ……！\nげ、原稿は…………！？\n",
      "text_len": 42,
      "ctrl_code": "fbffc1ff61001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 52,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "あっ！！　良かった〜、\nまだ風に飛ばされてなかった！\n",
      "text_len": 54,
      "ctrl_code": "fbff05ffc1ff63001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 53,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "回収回収っと。……あっ！\nこのシーン一番重要なとこじゃん！\n",
      "text_len": 60,
      "ctrl_code": "fbffc1ff61001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 54,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "見られたら一番ヤバいシーンだよ……\nホントに危なかった……。\n",
      "text_len": 62,
      "ctrl_code": "fbffc1ff5e001000ebfe000064ffffff0300"
    },
    {
      "magic": 65520,
      "id": 55,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "さて、誰かに見られる前に\nさっさと退散を[{0D80}][{0D80}]\n",
      "text_len": 46,
      "ctrl_code": "fbffedffc1ffffff1000edfffcff67ffffff66ff1e000000daffe8fe0200c1ff0c011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 56,
      "speaker": "桜井先生",
      "speaker_len": 8,
      "text": "長野原さん！　待って！　早まらないで！\n",
      "text_len": 40,
      "ctrl_code": "fbffe8fe0200c1ff1a011000ebfe000064ffffff0c00"
    },
    {
      "magic": 65520,
      "id": 57,
      "speaker": "高崎先生",
      "speaker_len": 8,
      "text": "こら、長野原！　馬鹿な真似はやめろ！\n",
      "text_len": 38,
      "ctrl_code": "fbffe8fe0300c1ff68001000ebfe0100adffffff64ffffff0300"
    },
    {
      "magic": 65520,
      "id": 58,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（ワオーーーー！？）\n",
      "text_len": 22,
      "ctrl_code": "fdff7fff040002ff01ff05003200fbfffa000000960000fffffe050006000700eefe0900040067ff0f00e8fe0200c1ff1a011000ebfe000065ff09000c00"
    },
    {
      "magic": 65520,
      "id": 59,
      "speaker": "高崎先生",
      "speaker_len": 8,
      "text": "飛び降りなんてバカなこと考えるな！\n今すぐこっちに来るんだ！\n",
      "text_len": 62,
      "ctrl_code": "fbffedffc8ff68001a011000ebfe0100adffffff65ff0a000300"
    },
    {
      "magic": 65520,
      "id": 60,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "（えええっ！？　飛び降り！？\n　何でそんなことになってるの！？）\n",
      "text_len": 66,
      "ctrl_code": "fbffc1ff14011000ebfe000065ff0b000c00"
    },
    {
      "magic": 65520,
      "id": 61,
      "speaker": "高崎先生",
      "speaker_len": 8,
      "text": "その遺書も先生に渡すんだ！　さあ早く！\n",
      "text_len": 40,
      "ctrl_code": "fbffc1ff65001000ebfe000065ff0c000300"
    },
    {
      "magic": 65520,
      "id": 62,
      "speaker": "みお",
      "speaker_len": 4,
      "text": "！？\nい、いや、これは遺書じゃ[{0D80}][{0D80}]\n",
      "text_len": 36,
      "ctrl_code": "fbffe8fe0200c1ff15011000ebfe000065ff0d000c00"
    },
    {
      "magic": 65520,
      "id": 63,
      "speaker": "高崎先生",
      "speaker_len": 8,
      "text": "わかってる！　お前の気持ちはわかっている！\nだから早まるんじゃない！\n",
