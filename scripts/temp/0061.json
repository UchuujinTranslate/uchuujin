  "ctrl_code": "200000003c080000b00b0000b40b0000a80d0000c20d00009411000090140000fcffedfe01000200060004ff03ff0300c1ffffff1000bcff8701ff0067ff0900fcfff9ff0800eefe00000500c0ff030007011000ebfe0000adffffff64ffffff0b00",
  "contents": [
    {
      "magic": 65520,
      "id": 0,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffedffc0ff0400ffff1000ebfe000064ffffff3200"
    },
    {
      "magic": 65520,
      "id": 1,
      "speaker": "男子生徒（男子生徒１）",
      "text": "桜井先生〜、さようなら〜。\n",
      "ctrl_code": "fbffc0ff03000d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 2,
      "speaker": "桜井先生",
      "text": "あっ、ハイ。さようなら。\n",
      "ctrl_code": "fbffc1ff07011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 3,
      "speaker": "桜井先生",
      "text": "（あっ、ボタンの一番上、外れてる……\n　あ、でももう放課後だし、いいのかな？）\n",
      "ctrl_code": "fbffc1ff08011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 4,
      "speaker": "桜井先生",
      "text": "（……ううん、放課後だからって見逃してたら\n　ますます風紀が乱れていっちゃう）\n",
      "ctrl_code": "fbffc1ff0a011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 5,
      "speaker": "桜井先生",
      "text": "（私は生活指導担当なんだから。\n　例え放課後でも厳しく注意しないと！）\n",
      "ctrl_code": "98ff0000effe070097fffbffedffc8ff0a0101001000ebfe000064ffffff0000"
    },
    {
      "magic": 65520,
      "id": 6,
      "speaker": "なの",
      "text": "あっ、桜井先生。さようなら〜。\n",
      "ctrl_code": "fbffc1ff08011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 7,
      "speaker": "桜井先生",
      "text": "あっ、東雲さん。\n",
      "ctrl_code": "fbffc1ff00001000ebfe000064ffffff0000"
    },
    {
      "magic": 65520,
      "id": 8,
      "speaker": "なの",
      "text": "はい？　なんですか？\n",
      "ctrl_code": "fbffc1ff11011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 9,
      "speaker": "桜井先生",
      "text": "えっと……。\n",
      "ctrl_code": "fbff67ff1000c1ff08011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 10,
      "speaker": "桜井先生",
      "text": "（髪型や服装は大丈夫だけど……\n　ネジが……）\n",
      "ctrl_code": "fbffc1ff0b011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 11,
      "speaker": "桜井先生",
      "text": "（あ、でも、東雲さんはロボットだから\n　いいのかな……）\n",
      "ctrl_code": "98ff0000effe010097fffbffc1ff08001000ebfe000064ffffff0000"
    },
    {
      "magic": 65520,
      "id": 12,
      "speaker": "なの",
      "text": "……桜井先生？\n",
      "ctrl_code": "fbffc1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 13,
      "speaker": "桜井先生",
      "text": "……え、えっと、\n気をつけて帰ってくださいね。\n",
      "ctrl_code": "fbffc1ff01001000ebfe000064ffffff0000"
    },
    {
      "magic": 65520,
      "id": 14,
      "speaker": "なの",
      "text": "はい。さようなら。\n",
      "ctrl_code": "fbffedffc0ff0400ffff1000c1ff11011000ebfe0000adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 15,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc0ff0400f7001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 16,
      "speaker": "中之条",
      "text": "あっ、桜井先生。さようなら。\n",
      "ctrl_code": "fbffc1ff0b011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 17,
      "speaker": "桜井先生",
      "text": "はい。……あっ、中之条くん。\n",
      "ctrl_code": "fbffc1fff6001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 18,
      "speaker": "中之条",
      "text": "はい。なんですか？\n",
      "ctrl_code": "fbffc1ff11011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 19,
      "speaker": "桜井先生",
      "text": "えっと……。\n",
      "ctrl_code": "fbffc1ff08011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 20,
      "speaker": "桜井先生",
      "text": "（この髪型は風紀的にダメだよね……\n　注意しないと……）\n",
      "ctrl_code": "98ff0000effe070097fffbffc1ff0b011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 21,
      "speaker": "桜井先生",
      "text": "（あっ、でも、横から生えてこないから\n　この髪型にしてるみたいだし……）\n",
      "ctrl_code": "fbffc1ff10011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 22,
      "speaker": "桜井先生",
      "text": "（髪のことを言ったら傷つけちゃうかも……）\n",
      "ctrl_code": "98ff0000effe000097fffbffc1fffb001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 23,
      "speaker": "中之条",
      "text": "桜井先生？　どうかしたんですか？\n",
      "ctrl_code": "fbffc1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 24,
      "speaker": "桜井先生",
      "text": "え、えっと……\n気をつけて帰ってくださいね。\n",
      "ctrl_code": "fbffc1fff7001000ebfe000064ffffff0a00"
    },
    {
      "magic": 65520,
      "id": 25,
      "speaker": "中之条",
      "text": "はい。さようなら先生。\n",
      "ctrl_code": "fbffbfff0a001000c1ff11011000ebfe0000adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 26,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff08011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 27,
      "speaker": "桜井先生",
      "text": "（全然注意できないけど……いいのかな？）\n",
      "ctrl_code": "fbfffefe000001007fff0200c1ff30011000ebfe0000adffffff64ffffff0e00"
    },
    {
      "magic": 65520,
      "id": 28,
      "speaker": "バディ",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff0b011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 29,
      "speaker": "桜井先生",
      "text": "あっ。\n",
      "ctrl_code": "fbffc1ff30011000ebfe0000adffffff64ffffff0e00"
    },
    {
      "magic": 65520,
      "id": 30,
      "speaker": "バディ",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff08011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 31,
      "speaker": "桜井先生",
      "text": "こ、こら！\n勝手に入ってきちゃダメでしょ。えい。\n",
      "ctrl_code": "fbff66ff3a000000c1ff30011000ebfe000064ffffff0e00"
    },
    {
      "magic": 65520,
      "id": 32,
      "speaker": "バディ",
      "text": "ワン！\n",
      "ctrl_code": "fbffedfffcfffaff01000000f8ff0800ecfec1ffffff100067fffffff4ff1e00f2fef0febcffdb01100067ff0f00faff01000000f9ff0800e8fe0200c1fff7011000ebfe000065ff00002600"
    },
    {
      "magic": 65520,
      "id": 33,
      "speaker": "[{FF0B}]",
      "text": "来るぞ！\n",
      "ctrl_code": "fbffedffaaff0200ff0063ff0000c1ffffffff00fdfefcfeadffffffebfe000065ff01002600"
    },
    {
      "magic": 65520,
      "id": 34,
      "speaker": "[{FF0B}]",
      "text": "うわあああ！！\n",
      "ctrl_code": "fbffebfe000065ff02002700"
    },
    {
      "magic": 65520,
      "id": 35,
      "speaker": "[{FF0C}]",
      "text": "くっ、あんな簡単に破損しやがって！\n",
      "ctrl_code": "fbffebfe000065ff03002600"
    },
    {
      "magic": 65520,
      "id": 36,
      "speaker": "[{FF0B}]",
      "text": "しかし、あのアンテナは強度アップの実験で\nできた試作品だったはずなんだが……。\n",
      "ctrl_code": "fbffebfe000065ff04002700"
    },
    {
      "magic": 65520,
      "id": 37,
      "speaker": "[{FF0C}]",
      "text": "何？　ということは……。\n",
      "ctrl_code": "fbffebfe000065ff05002600"
    },
    {
      "magic": 65520,
      "id": 38,
      "speaker": "[{FF0B}]",
      "text": "軽い一撃に見えて、\n実はかなりの破壊力があったんだ！\n",
      "ctrl_code": "fbffebfe000065ff06002700"
    },
    {
      "magic": 65520,
      "id": 39,
      "speaker": "[{FF0C}]",
      "text": "うおーー！　達人だーー！！\n",
      "ctrl_code": "fbffebfe000065ff07002500"
    },
    {
      "magic": 65520,
      "id": 40,
      "speaker": "[{FF0A}]",
      "text": "……いや、強度アップの実験が、\n失敗だっただけだと思うぞ。\n",
      "ctrl_code": "fbffedfffcfff5ff040067ffffff63ffffffaaff00000000fdfebdffffff1000c1ffffff1000c2ffffff1000bcff6201ff00dafff6ff0800eafee9fef7ff0800fcffffff7fff0300c1ff10011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 41,
      "speaker": "桜井先生",
      "text": "（でもあの二人の場合は仕方ないよね。\n　厳しく言って傷つけちゃダメだし……）\n",
      "ctrl_code": "fbffc0ff040053011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 42,
      "speaker": "田中",
      "text": "あっ、センセー、ばいばーい。\n",
      "ctrl_code": "fbffc1ff0b011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 43,
      "speaker": "桜井先生",
      "text": "（あっ、田中君……）\n",
      "ctrl_code": "fbffc1ff08011000ebfe0100adffffff64ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 44,
      "speaker": "桜井先生",
      "text": "（そうだ、田中君なら注意しても\n　傷付いたりしないだろうし……）\n",
      "ctrl_code": "98ff0000effe020097fffbffc1ff0d011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 45,
      "speaker": "桜井先生",
      "text": "あの、田中君。ちょっと……。\n",
      "ctrl_code": "fbff05ffc1ff52011000ebfe000064ffffff1300"
    },
    {
      "magic": 65520,
      "id": 46,
      "speaker": "田中",
      "text": "ん？　なーに？\n",
      "ctrl_code": "fbffc1ff11011000ebfe000064ffffff0b00"
    },
    {
      "magic": 65520,
      "id": 47,
      "speaker": "桜井先生",
      "text": "あの……。\n",
      "ctrl_code": "fdff7fff040002ff01fffbfffa00000096000500320000fffffe050006000700c1ff08011000ebfe000065ff08000b00"
    },
    {
      "magic": 65520,
      "id": 48,
      "speaker": "桜井先生",
      "text": "そ、その髪型、\n変えることはできませんか？\n",
      "ctrl_code": "fbffc1ff09011000ebfe0100adffffff65ff09000b00"
    },
    {
      "magic": 65520,
      "id": 49,
      "speaker": "桜井先生",
      "text": "（や、やった、言えた！）\n",
      "ctrl_code": "fbff67ff1200c1ff54011000ebfe000065ff0a001300"
    },
    {
      "magic": 65520,
      "id": 50,
      "speaker": "田中",
      "text": "この髪型を？\n気に入ってるんだけど、変かなー？\n",
      "ctrl_code": "fbffc1ff11011000ebfe000065ff0b000b00"
    },
    {
      "magic": 65520,
      "id": 51,
      "speaker": "桜井先生",
      "text": "あっ、い、いえ……。\n",
      "ctrl_code": "fbffeefe0b000400c1ff10011000ebfe0100adffffff65ff0c000b00"
    },
    {
      "magic": 65520,
      "id": 52,
      "speaker": "桜井先生",
      "text": "（えっ？　ちょっとショック受けてる？\n　もしかして厳しく言い過ぎた……？）\n",
      "ctrl_code": "fbffc1ff11011000ebfe000065ff0d000b00"
    },
    {
      "magic": 65520,
      "id": 53,
      "speaker": "桜井先生",
      "text": "あっ、そ、その……\n変じゃないと思いますよ。\n",
      "ctrl_code": "98ff0000effe0a0097fffbffc1ff52011000ebfe000065ff0e001300"
    },
    {
      "magic": 65520,
      "id": 54,
      "speaker": "田中",
      "text": "ホント？\n",
      "ctrl_code": "fbffc1ff09011000ebfe000065ff0f000b00"
    },
    {
      "magic": 65520,
      "id": 55,
      "speaker": "桜井先生",
      "text": "は、はい。\nよく似合ってると思います。\n",
      "ctrl_code": "fbffc1ff53011000ebfe000065ff10001300"
    },
    {
      "magic": 65520,
      "id": 56,
      "speaker": "田中",
      "text": "あはは、ありがとセンセー。\nじゃあまたねー。\n",
      "ctrl_code": "fbffc1ff0d011000ebfe000065ff11000b00"
    },
    {
      "magic": 65520,
      "id": 57,
      "speaker": "桜井先生",
      "text": "はい、さようなら。\n",
      "ctrl_code": "fbffbfff13001000c1ff0d011000ebfe0000adffffff65ff12000b00"
    },
    {
      "magic": 65520,
      "id": 58,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff0d011000ebfe0000adffffff65ff13000b00"
    },
    {
      "magic": 65520,
      "id": 59,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcfff701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe000065ff14000b00"
    },
    {
      "magic": 65520,
      "id": 60,
      "speaker": "桜井先生",
      "text": "……あれ？\n",
      "ctrl_code": "98ff0000effe030097fffdff67ffffffe7fe0000fdff67ffffffeafee9fef7ff0800fcffffff67ff1000c1ff11011000ebfe000065ff15000b00"
    },
    {
      "magic": 65520,
      "id": 61,
      "speaker": "桜井先生",
      "text": "その髪型なんですけど……\n風紀的に……その……。\n",
      "ctrl_code": "fbffc1ff52011000ebfe0000adffffff65ff16001300"
    },
    {
      "magic": 65520,
      "id": 62,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff10011000ebfe000065ff17000b00"
    },
    {
      "magic": 65520,
      "id": 63,
      "speaker": "桜井先生",
      "text": "えっと……だから……\n生徒指導担当としては……その……。\n",
      "ctrl_code": "fbffc1ff53011000ebfe000065ff18001300"
    },
    {
      "magic": 65520,
      "id": 64,
      "speaker": "田中",
      "text": "センセー。\n",
      "ctrl_code": "fbffc1ff0b011000ebfe000065ff19000b00"
    },
    {
      "magic": 65520,
      "id": 65,
      "speaker": "桜井先生",
      "text": "は、はいっ。\n",
      "ctrl_code": "fbffeefe03000400c9ff52021000c1ff54011000ebfe000065ff1a001300"
    },
    {
      "magic": 65520,
      "id": 66,
      "speaker": "田中",
      "text": "どら焼き食べる？\n",
      "ctrl_code": "fbffeefe04000400c1ff0c011000ebfe0000adffffff65ff1b000b00"
    },
    {
      "magic": 65520,
      "id": 67,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "98ff0000effe060097fffbffc9ffffff1000c1ff52011000ebfe0000adffffff65ff1c001300"
    },
    {
      "magic": 65520,
      "id": 68,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff11011000ebfe0000adffffff65ff1d000b00"
    },
    {
      "magic": 65520,
      "id": 69,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff53011000ebfe0000adffffff65ff1e001300"
    },
    {
      "magic": 65520,
      "id": 70,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fbfff8ff0800daff73ff0000deff0000003003000100ffff67ffffffdbff000063ffffff66ffffff0000daffbcfff701ff00c1fffeffff0090ffffff90ffffff90ffffff67fffcff71ff000063ffffff90ffffffedfff9ff0800ebfe0000adffffff65ff1f000b00"
    },
    {
      "magic": 65520,
      "id": 71,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffebfe0000adffffff65ff20001300"
    },
    {
      "magic": 65520,
      "id": 72,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fdff67ffffffe7fe0000fdff67ffffffeafee9fef7ff0800fcffffff67ff1000c1ff0c011000ebfe0100adffffff65ff21000b00"
    },
    {
      "magic": 65520,
      "id": 73,
      "speaker": "桜井先生",
      "text": "（あっ、でも、もし田中君も\n　髪型を注意されて傷付いたら……）\n",
      "ctrl_code": "fbffc1ff52011000ebfe0000adffffff65ff22001300"
    },
    {
      "magic": 65520,
      "id": 74,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff10011000ebfe0100adffffff65ff23000b00"
    },
    {
      "magic": 65520,
      "id": 75,
      "speaker": "桜井先生",
      "text": "（実はすごい天然で髪が勝手に\n　アフロになっちゃってるだけとか……）\n",
      "ctrl_code": "fbffc1ff11011000ebfe0100adffffff65ff24000b00"
    },
    {
      "magic": 65520,
      "id": 76,
      "speaker": "桜井先生",
      "text": "（中之条くんと同じで\n　仕方なくそうしてるとしたら……）\n",
      "ctrl_code": "fbffc1ff54011000ebfe000065ff25001300"
    },
    {
      "magic": 65520,
      "id": 77,
      "speaker": "田中",
      "text": "……センセー？\n",
      "ctrl_code": "fbffc1ff08011000ebfe0000adffffff65ff26000b00"
    },
    {
      "magic": 65520,
      "id": 78,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff52011000ebfe0000adffffff65ff27001300"
    },
    {
      "magic": 65520,
      "id": 79,
      "speaker": "田中",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff10011000ebfe0000adffffff65ff28000b00"
    },
    {
      "magic": 65520,
      "id": 80,
      "speaker": "桜井先生",
      "text": "…………。\n",
      "ctrl_code": "fbffc1ff11011000ebfe000065ff29000b00"
    },
    {
      "magic": 65520,
      "id": 81,
      "speaker": "桜井先生",
      "text": "……き、気をつけて帰ってくださいね。\n",
      "ctrl_code": "fbffc1ff53011000ebfe000065ff2a001300"
    },
    {
      "magic": 65520,
      "id": 82,
      "speaker": "田中",
      "text": "あっ、うん。\nじゃあね、センセー。\n",
      "ctrl_code": "fbffc1ff0d011000ebfe000065ff2b000b00"
    },
    {
      "magic": 65520,
      "id": 83,
      "speaker": "桜井先生",
      "text": "はい。さようなら。\n",
      "ctrl_code": "fbffedffc0ff0400ffff1000c1ff11011000ebfe0000adffffff65ff2c000b00"
    },
