[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2146,
            "text_len": 16,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こんにちは―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2194,
            "text_len": 36,
            "text_offset": 2200,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おう桜井くん。\\n今日は遅かったな。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2258,
            "text_len": 42,
            "text_offset": 2266,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いや―、ちょっと取りに行くものがあって。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2332,
            "text_len": 28,
            "text_offset": 2338,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "その、手にあるものですか？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2388,
            "text_len": 70,
            "text_offset": 2396,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はい、予約してた欧州囲碁サッカ―のＣＵＣ、\\n今日発売だったんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2504,
            "text_len": 32,
            "text_offset": 2510,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "欧州！？\\nそんなのもあるの！？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2564,
            "text_len": 58,
            "text_offset": 2572,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "やだな―。欧州リ―グの試合、\\nすごくカッコイイんですよ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2652,
            "text_len": 64,
            "text_offset": 2660,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "それで、せっかくだから\\nみんなで見ようかな―って思ったんです。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2746,
            "text_len": 22,
            "text_offset": 2752,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "マジで？　見る見る！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2796,
            "text_len": 14,
            "text_offset": 2804,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "じゃあ早速。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2840,
            "text_len": 68,
            "text_offset": 2846,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あ―、ちょっと待っててくれよ、\\nジュ―スとポップコ―ン買って来る！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2936,
            "text_len": 6,
            "text_offset": 2944,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2972,
            "text_len": 66,
            "text_offset": 2978,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや―、面白そうなもの持ってきてくれたよな。\\nすぐ買ってくるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3074,
            "text_len": 22,
            "text_offset": 3082,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ！！　部長っ、前！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3134,
            "text_len": 6,
            "text_offset": 3140,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3172,
            "text_len": 20,
            "text_offset": 3178,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うおおおお―っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3230,
            "text_len": 24,
            "text_offset": 3236,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……大丈夫だったかな？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3284,
            "text_len": 58,
            "text_offset": 3292,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "ぎりぎり避けられたようですから、\\n大丈夫だと思いますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3374,
            "text_len": 12,
            "text_offset": 3380,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ホントか？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3416,
            "text_len": 40,
            "text_offset": 3422,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "でも、頭のアンテナは折れてますけどね。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3486,
            "text_len": 14,
            "text_offset": 3492,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……まじで？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3532,
            "text_len": 16,
            "text_offset": 3538,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3584,
            "text_len": 22,
            "text_offset": 3590,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ドンマイドンマイ！！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3692,
            "text_len": 20,
            "text_offset": 3696,
            "warnings": 1
        },
        "speaker": "",
        "text": "お前が言うなよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3762,
            "text_len": 18,
            "text_offset": 3766,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわあああああ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3896,
            "text_len": 14,
            "text_offset": 3904,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……う―ん。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3944,
            "text_len": 74,
            "text_offset": 3952,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（興味持ってくれてるっぽいけど、\\n　どうも本気っぽくないんだよなぁ……）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4048,
            "text_len": 18,
            "text_offset": 4056,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの、関口さん。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4098,
            "text_len": 12,
            "text_offset": 4104,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "何ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4138,
            "text_len": 58,
            "text_offset": 4146,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "部長って、囲碁サッカ―のこと\\nどう思ってるか知ってます？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4226,
            "text_len": 22,
            "text_offset": 4232,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "どう、と言われても。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4276,
            "text_len": 38,
            "text_offset": 4284,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ―、その、興味とかありそうですか？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4344,
            "text_len": 58,
            "text_offset": 4350,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あると思いますよ。\\n面白くなってきたって言ってましたし。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4430,
            "text_len": 74,
            "text_offset": 4436,
            "warnings": 1
        },
        "speaker": "関口",
        "text": "元存在すら知らない競技だったから、\\n戸惑っているとも言ってましたけど。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4532,
            "text_len": 46,
            "text_offset": 4540,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……なるほど。\\n関口さんに聞いて正解でした。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4608,
            "text_len": 38,
            "text_offset": 4616,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "やっぱり部長のことよく見てますよね。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4678,
            "text_len": 26,
            "text_offset": 4684,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そ、そんなことないです。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4732,
            "text_len": 68,
            "text_offset": 4738,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "桜井くんより長く一緒に居るだけで、\\n別にいつも見てるわけでは……。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4832,
            "text_len": 44,
            "text_offset": 4840,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……あれ？　この反応ってもしかして……）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4950,
            "text_len": 28,
            "text_offset": 4958,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "関口さん、もしかして……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5012,
            "text_len": 6,
            "text_offset": 5018,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5054,
            "text_len": 38,
            "text_offset": 5062,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "囲碁サッカ―に興味でてきましたか！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5128,
            "text_len": 6,
            "text_offset": 5134,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5172,
            "text_len": 70,
            "text_offset": 5180,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "見てないっていうのに、部長が囲碁サッカ―を\\nどう思ってるか知ってる。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5272,
            "text_len": 62,
            "text_offset": 5280,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "つまり関口さん、囲碁サッカ―に\\n興味があるってことですよね！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5364,
            "text_len": 26,
            "text_offset": 5370,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、そういうわけでは。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5424,
            "text_len": 54,
            "text_offset": 5432,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "またまた―、わかってますよ！\\n照れくさいんですよね！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5508,
            "text_len": 56,
            "text_offset": 5516,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "大丈夫ですよ！\\n女子囲碁サッカ―も盛り上がってますし！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5604,
            "text_len": 26,
            "text_offset": 5610,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、本当に違うんです。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5658,
            "text_len": 78,
            "text_offset": 5666,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "だから隠さなくっていいですって。\\nそうだ！　コレ部長と二人で見ててください。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5766,
            "text_len": 40,
            "text_offset": 5774,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "女子囲碁サッカ―のＣＵＣ買ってきます！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5856,
            "text_len": 28,
            "text_offset": 5862,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "っ！？　いや、待って弌弌。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5910,
            "text_len": 22,
            "text_offset": 5918,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "すぐ戻りますんで！！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6050,
            "text_len": 18,
            "text_offset": 6056,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……どうしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6136,
            "text_len": 26,
            "text_offset": 6144,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ―……そういうことか。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6196,
            "text_len": 6,
            "text_offset": 6202,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6230,
            "text_len": 16,
            "text_offset": 6236,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6274,
            "text_len": 36,
            "text_offset": 6282,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "大丈夫ですよ、オレ言いませんから。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6340,
            "text_len": 6,
            "text_offset": 6346,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6374,
            "text_len": 60,
            "text_offset": 6382,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "誰にも言いませんよ、\\n関口さんが部長のこと……。もごっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6464,
            "text_len": 42,
            "text_offset": 6470,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "弌弌部長が戻ってきたらどうするんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6538,
            "text_len": 22,
            "text_offset": 6546,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……す、すいません。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6590,
            "text_len": 84,
            "text_offset": 6598,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "とにかく、言う気は無いですし、ついでに\\nこれを理由にして協力を求めたりもしません。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6714,
            "text_len": 30,
            "text_offset": 6720,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "桜井くん、いい人なんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6772,
            "text_len": 74,
            "text_offset": 6780,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "いや―、同じ部活の仲間ですし、\\nそんな事で仲悪くなるの嫌じゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6876,
            "text_len": 80,
            "text_offset": 6884,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "それに、コレばらしてたら高崎先生が\\nうちの姉ちゃんを好きな事なんて何回……あ。\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6992,
            "text_len": 12,
            "text_offset": 6998,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "へぇ……。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7046,
            "text_len": 30,
            "text_offset": 7054,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、いや、その……え―っと。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7106,
            "text_len": 54,
            "text_offset": 7112,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "大丈夫、誰にも言いません。\\nこれでおあいこですから。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7188,
            "text_len": 28,
            "text_offset": 7196,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、ありがとうございます！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7328,
            "text_len": 32,
            "text_offset": 7334,
            "warnings": 1
        },
        "speaker": "関口",
        "text": "それに、元有名な話ですから。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7386,
            "text_len": 10,
            "text_offset": 7394,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7466,
            "text_len": 40,
            "text_offset": 7474,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（……これを上手く使えばもしかしたら）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7546,
            "text_len": 30,
            "text_offset": 7554,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あの、オレ協力しましょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7610,
            "text_len": 6,
            "text_offset": 7616,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7644,
            "text_len": 60,
            "text_offset": 7650,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "な、何の話ですか？\\n私、今何も困ったことなんて無いですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7732,
            "text_len": 70,
            "text_offset": 7740,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そういうことにしといてもいいですよ。\\nとにかく、いい案があるんです。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7832,
            "text_len": 12,
            "text_offset": 7838,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7872,
            "text_len": 40,
            "text_offset": 7880,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "関口さん、囲碁サッカ―やってみません？\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7952,
            "text_len": 76,
            "text_offset": 7960,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "多分部長の性格からして、二人ともやってれば\\n食いついてくるはずなんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8074,
            "text_len": 22,
            "text_offset": 8080,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "否定はできませんね。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8124,
            "text_len": 74,
            "text_offset": 8132,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "つまり共通の話題もできるし、\\n一緒に居る時間も増えるわけです、どうです？\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8228,
            "text_len": 48,
            "text_offset": 8234,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……わかりました。\\nその手に乗ってみましょう。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8304,
            "text_len": 38,
            "text_offset": 8312,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よ―し、部長が戻ってきたら早速……。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8472,
            "text_len": 14,
            "text_offset": 8478,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ただいま―！\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8516,
            "text_len": 26,
            "text_offset": 8524,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "関口さん、お願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8574,
            "text_len": 12,
            "text_offset": 8580,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……はい。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8614,
            "text_len": 14,
            "text_offset": 8620,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの、部長。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8658,
            "text_len": 8,
            "text_offset": 8664,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん―？\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8694,
            "text_len": 54,
            "text_offset": 8700,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ＣＵＣ見てから、\\n囲碁サッカ―の練習してみませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8786,
            "text_len": 44,
            "text_offset": 8792,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "え、いいよ。\\n今日は体育で疲れたからパス。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8944,
            "text_len": 12,
            "text_offset": 8950,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8982,
            "text_len": 12,
            "text_offset": 8990,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………。\\n",
        "type": "dialog"
    }
]