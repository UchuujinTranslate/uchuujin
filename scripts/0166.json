[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 50,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "今日の部活、何やるのかな？\\n……相変わらずかな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2222,
            "text_len": 18,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、あの人……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2272,
            "text_len": 32,
            "text_offset": 2278,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（違うクラスのゼンマイの人だ）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2336,
            "text_len": 54,
            "text_offset": 2342,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（確かロボなんだっけ。\\n　でも、なんでゼンマイが？）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2422,
            "text_len": 60,
            "text_offset": 2428,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（気にしてるって噂だし……\\n　あんまり見ないようにしよう）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2536,
            "text_len": 14,
            "text_offset": 2542,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はうあっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2576,
            "text_len": 6,
            "text_offset": 2582,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2636,
            "text_len": 14,
            "text_offset": 2642,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "う、腕が！？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2676,
            "text_len": 32,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ダ、ダメ！\\n避けてください！！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2818,
            "text_len": 6,
            "text_offset": 2824,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2870,
            "text_len": 6,
            "text_offset": 2876,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2924,
            "text_len": 28,
            "text_offset": 2930,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……び、びっくりしました。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2982,
            "text_len": 34,
            "text_offset": 2988,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "よ、良かったぁ……当たらなくて。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3102,
            "text_len": 32,
            "text_offset": 3106,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナに当たったっつ―の！！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3170,
            "text_len": 58,
            "text_offset": 3174,
            "warnings": 1
        },
        "speaker": "",
        "text": "聞こえてないんだから、よせっ！\\nそれより、今は弌弌ほら！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3278,
            "text_len": 76,
            "text_offset": 3282,
            "warnings": 1
        },
        "speaker": "",
        "text": "バカな！　あんな豆鉄砲で壊れるのか！？\\n今回の責任者はどこに居るんだっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3374,
            "text_len": 20,
            "text_offset": 3378,
            "warnings": 1
        },
        "speaker": "",
        "text": "……ご、ごめ―ん。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3414,
            "text_len": 76,
            "text_offset": 3418,
            "warnings": 1
        },
        "speaker": "",
        "text": "え？　えっと……お前だったの？\\nいやね、別にそんなに怒ってるわけじゃな弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3598,
            "text_len": 24,
            "text_offset": 3604,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（腕がもげた……！？）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3654,
            "text_len": 42,
            "text_offset": 3660,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あぁ―っ！？\\n違うんですっ、これはその！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3728,
            "text_len": 14,
            "text_offset": 3734,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "右手が……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3852,
            "text_len": 60,
            "text_offset": 3858,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ち、違うんです！\\nこれは……手品、そう！　手品なんです！！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3934,
            "text_len": 18,
            "text_offset": 3940,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "で、でも銃口が。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3984,
            "text_len": 6,
            "text_offset": 3990,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4022,
            "text_len": 66,
            "text_offset": 4028,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "こ、これは違うんです！\\nヒ―ロ―ごっこ！　ヒ―ロ―ごっこでして！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4112,
            "text_len": 74,
            "text_offset": 4118,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "年頃の女の子がヒ―ロ―ごっこなんて\\n恥ずかしいですよね！　あははは……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4218,
            "text_len": 72,
            "text_offset": 4224,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（左腕からロ―ルケ―キが出てるんだけど、\\n　これは言うべきなのかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4316,
            "text_len": 20,
            "text_offset": 4322,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ……どひ―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4362,
            "text_len": 24,
            "text_offset": 4368,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（気づいたみたい……）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4552,
            "text_len": 30,
            "text_offset": 4558,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの、私は何も見てませんよ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4630,
            "text_len": 18,
            "text_offset": 4636,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ、で、でも。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4676,
            "text_len": 14,
            "text_offset": 4682,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "見てません。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4722,
            "text_len": 18,
            "text_offset": 4728,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "で、でもでも！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4768,
            "text_len": 18,
            "text_offset": 4774,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "見てませんから。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4818,
            "text_len": 6,
            "text_offset": 4824,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4852,
            "text_len": 32,
            "text_offset": 4858,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "で、でも目の前で私の右手が！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4912,
            "text_len": 60,
            "text_offset": 4918,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "右手なんてもげてません。\\n右手がもげるなんてありえません。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5000,
            "text_len": 56,
            "text_offset": 5006,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "左手も外れてませんし、\\nロ―ルケ―キなんて出てません。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5088,
            "text_len": 20,
            "text_offset": 5094,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ほ、本当ですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5136,
            "text_len": 12,
            "text_offset": 5142,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "本当です。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5176,
            "text_len": 18,
            "text_offset": 5182,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "良かったぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5222,
            "text_len": 34,
            "text_offset": 5228,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "では、何も見ていない私はこれで。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5284,
            "text_len": 14,
            "text_offset": 5290,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "は、はいっ。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5338,
            "text_len": 24,
            "text_offset": 5344,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "右手、私が拾いますね。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5390,
            "text_len": 26,
            "text_offset": 5396,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ありがとうございます〜。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5450,
            "text_len": 20,
            "text_offset": 5456,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "どうぞ。それでは。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5594,
            "text_len": 26,
            "text_offset": 5600,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……やっぱり見てる―！！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5776,
            "text_len": 26,
            "text_offset": 5782,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "うわっ、うわわわわっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5832,
            "text_len": 14,
            "text_offset": 5838,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "こ、これで！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5880,
            "text_len": 62,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ロ―ルケ―キをあげますから\\n見なかったことにしてください！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5976,
            "text_len": 6,
            "text_offset": 5982,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6026,
            "text_len": 26,
            "text_offset": 6032,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "お、おねがいですから！！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6080,
            "text_len": 12,
            "text_offset": 6086,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "でも……。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6124,
            "text_len": 36,
            "text_offset": 6130,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ささっ、さもなくば、撃ちますっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6198,
            "text_len": 6,
            "text_offset": 6204,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6246,
            "text_len": 50,
            "text_offset": 6252,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ、いえっ、嘘です撃ちません！\\nゴメンナサイ！！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6324,
            "text_len": 56,
            "text_offset": 6330,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あれ、でも忘れて貰わないと、\\nえっと……あ、あれ―！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6412,
            "text_len": 12,
            "text_offset": 6418,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6544,
            "text_len": 30,
            "text_offset": 6550,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長、ロ―ルケ―キ好きかな？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6720,
            "text_len": 24,
            "text_offset": 6726,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あわ、あわわわわっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6772,
            "text_len": 10,
            "text_offset": 6778,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6816,
            "text_len": 32,
            "text_offset": 6822,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "わ、わたしの目を見てください！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6884,
            "text_len": 12,
            "text_offset": 6890,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "は、はい？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6924,
            "text_len": 70,
            "text_offset": 6930,
            "warnings": 1
        },
        "speaker": "なの",
        "text": "ほ、ほ―ら、あなたは段眠くなる〜。\\n眠くなって全部忘れてしまう〜。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7028,
            "text_len": 16,
            "text_offset": 7034,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え……えっと。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7088,
            "text_len": 28,
            "text_offset": 7094,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ね、眠くなりませんか……？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7144,
            "text_len": 40,
            "text_offset": 7150,
            "warnings": 0
        },
        "speaker": "関口",
