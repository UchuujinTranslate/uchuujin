[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 54,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "さ―て、早く帰って\\n次回作のネ―ムでも考えよ―っと。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2230,
            "text_len": 58,
            "text_offset": 2236,
            "warnings": 1
        },
        "speaker": "みお",
        "text": "（今度はどんなカップリングがいいかな？\\n　むふふ……）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2320,
            "text_len": 62,
            "text_offset": 2326,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（次は今までとは\\n　違うベクトルの作品にしてみるのもいいな）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2414,
            "text_len": 32,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（違うベクトル……う―ん……）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2560,
            "text_len": 70,
            "text_offset": 2566,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（おっ、そうだ。たまには渋いオジサマとか\\n　出してみてもいいかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2656,
            "text_len": 58,
            "text_offset": 2662,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（うん、面白いな。次はその方向性で\\n　行ってみようかな）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2740,
            "text_len": 80,
            "text_offset": 2746,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（でもオジサマ系は資料がないんだよね。\\n　どこかにモデルでもいないかな……？）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2842,
            "text_len": 28,
            "text_offset": 2848,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "オジサマ……オジサマ……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2990,
            "text_len": 56,
            "text_offset": 2996,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "校長先生と教頭先生のカップリング！？\\n教頭先生が攻め！\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 3088,
            "text_len": 14,
            "text_offset": 3108,
            "warnings": 0
        },
        "speaker": "？？？（教頭先生）",
        "text": "責める、か！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3232,
            "text_len": 12,
            "text_offset": 3238,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3272,
            "text_len": 32,
            "text_offset": 3278,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "何だろ？\\n今、誰かの声が……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3332,
            "text_len": 18,
            "text_offset": 3338,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……ま、いいや。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3390,
            "text_len": 84,
            "text_offset": 3400,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あの生徒の話はよくわからなかったが……\\n校長を責める、か……それはぜひしてみたい。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3510,
            "text_len": 76,
            "text_offset": 3520,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（徹底的に校長の責任を……責めて責めて、\\n　校長の座から降ろしてみたい！）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3630,
            "text_len": 80,
            "text_offset": 3640,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "ひっ！？　きょ、教頭先生……？　そ、その……\\n私を攻めたいとは……ま、まさか？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3746,
            "text_len": 6,
            "text_offset": 3756,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3840,
            "text_len": 52,
            "text_offset": 3844,
            "warnings": 1
        },
        "speaker": "",
        "text": "……攻めたい、と言ったな。\\nまさか……本当に……。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3920,
            "text_len": 40,
            "text_offset": 3924,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ……\\n攻めと言ったら他にないだろ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3986,
            "text_len": 42,
            "text_offset": 3990,
            "warnings": 1
        },
        "speaker": "",
        "text": "まさか、\\nこのメガネの人があんな風に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4054,
            "text_len": 36,
            "text_offset": 4058,
            "warnings": 1
        },
        "speaker": "",
        "text": "……言うな。\\nみなまで言うな……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4116,
            "text_len": 16,
            "text_offset": 4120,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………ああ。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4178,
            "text_len": 40,
            "text_offset": 4182,
            "warnings": 1
        },
        "speaker": "",
        "text": "おまえたちの思考回路、腐りすぎだ―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4320,
            "text_len": 70,
            "text_offset": 4326,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（いやいやいやいやいやいや！\\n　あり得ないから！　絶対ナイから！！）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4520,
            "text_len": 72,
            "text_offset": 4526,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ったく、なんて想像してるんだか……\\n　これじゃもう末期症状だよ……）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4624,
            "text_len": 70,
            "text_offset": 4630,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（まあでも、教頭はメガネだし……\\n　美化して描けばそれなりに……？）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4726,
            "text_len": 52,
            "text_offset": 4732,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（いや〜……それもちょっと\\n　無理があるなぁ……）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4810,
            "text_len": 62,
            "text_offset": 4816,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……ん？　あそこにいるのって教頭？\\nこんなとこで何をして弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4992,
            "text_len": 18,
            "text_offset": 4998,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "えええ―――！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5054,
            "text_len": 68,
            "text_offset": 5060,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（なななな、なんで教頭が\\n　校長の上に馬乗りになってるの――！？）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5242,
            "text_len": 20,
            "text_offset": 5252,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "！？　だ、誰だ！？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5296,
            "text_len": 40,
            "text_offset": 5302,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あっ、しまった……み、見つかった……。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5368,
            "text_len": 64,
            "text_offset": 5378,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "そ、その制服、うちの生徒……\\nま、まさか、見ていたのか……！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5468,
            "text_len": 76,
            "text_offset": 5474,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（そんなに慌ててるってことは……\\n　見られちゃまずいことを校長に……！？）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5608,
            "text_len": 46,
            "text_offset": 5618,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "た、頼む……\\nこのことは内緒にしてくれ……。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5694,
            "text_len": 82,
            "text_offset": 5700,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（内緒に！？　ってことは校長と教頭って、\\n　やっぱりそういう関係なんだ〜〜！？）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5804,
            "text_len": 50,
            "text_offset": 5814,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……このことが知れたら……\\n私は……私は……っ！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5890,
            "text_len": 32,
            "text_offset": 5896,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（バレたら教頭と校長は解任！）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5950,
            "text_len": 50,
            "text_offset": 5960,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "このまま行けば……\\nせっかくのチャンスなんだっ！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6036,
            "text_len": 68,
            "text_offset": 6042,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（でも、弍のなくなった二人は\\n　今まで以上に強い仆で結ばれて弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6136,
            "text_len": 58,
            "text_offset": 6142,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（心も体も……\\n　文字通り、結ばれるチャンス――っ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6236,
            "text_len": 68,
            "text_offset": 6242,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（すごい、こんな身近に\\n　オジサン同士のカップリングが………！！）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6336,
            "text_len": 26,
            "text_offset": 6342,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（でも……でも……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6390,
            "text_len": 38,
            "text_offset": 6400,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "頼む！　絶対に誰にも言わないでくれ！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6460,
            "text_len": 40,
            "text_offset": 6466,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "い、言いませんっ！\\n誰にも言いません！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6528,
            "text_len": 24,
            "text_offset": 6538,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "約束……できるな……？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6584,
            "text_len": 26,
            "text_offset": 6590,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "は、はいっ！　もう絶対！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6638,
            "text_len": 58,
            "text_offset": 6644,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "校長と教頭がランデブ―だなんて、\\n死んでもいいませんっ！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6724,
            "text_len": 12,
            "text_offset": 6734,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……なに？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6768,
            "text_len": 30,
            "text_offset": 6774,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "じゃ、じゃあ、失礼しますっ！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6850,
            "text_len": 18,
            "text_offset": 6860,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……ランデブ―？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6908,
            "text_len": 50,
            "text_offset": 6918,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……はっ！？\\n教頭先生、私に心臓マッサ―ジを！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6998,
            "text_len": 46,
            "text_offset": 7008,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……し、しまった、せっかくのチャンスが！）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7176,
            "text_len": 50,
            "text_offset": 7182,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はぁ……はぁ……\\nあるんだ！　ホントにあるんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7254,
            "text_len": 34,
            "text_offset": 7260,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "でも……でも、なんていうか……！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7402,
            "text_len": 28,
            "text_offset": 7408,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "リアルだとキモ―――イ！！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7502,
            "text_len": 80,
            "text_offset": 7512,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "違う！　誤解だ！　今回は私じゃないぞ！\\nここに来たらすでに校長が倒れてたんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7614,
            "text_len": 34,
            "text_offset": 7620,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あ、そ、そうだったんですか……。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7680,
            "text_len": 58,
            "text_offset": 7686,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（でも、今回は、って……\\n　じゃあ普段はどうなのよ……）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7766,
            "text_len": 40,
            "text_offset": 7776,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あ―、ごほん。\\nそれでだね、キミ……。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7838,
            "text_len": 16,
            "text_offset": 7844,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "は、はい……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7882,
            "text_len": 32,
            "text_offset": 7892,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "校長が息をしとらんようなんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7946,
            "text_len": 78,
            "text_offset": 7956,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "心臓マッサ―ジは試してみたのだが……\\nすまんが、人工呼吸をしてもらえるかね。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8056,
            "text_len": 10,
            "text_offset": 8062,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8110,
            "text_len": 40,
            "text_offset": 8120,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ほれ、早くせんと校長が死んでしまうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8182,
            "text_len": 62,
            "text_offset": 8188,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "い、いや、そういうのはやっぱり、\\n経験豊富な教頭先生の方が！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8280,
            "text_len": 66,
            "text_offset": 8286,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（何が悲しくてこんなオッサンに\\n　人工呼吸しなきゃダメなのよ！）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8374,
            "text_len": 48,
            "text_offset": 8384,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "何を言う。\\n男同士ではどうも都合が悪いだろう。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8454,
            "text_len": 18,
            "text_offset": 8460,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "それはまぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8508,
            "text_len": 12,
            "text_offset": 8514,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8552,
            "text_len": 76,
            "text_offset": 8558,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "いや、男同士も、それはそれでアリ……\\nと言うか、私的には全然ＮＪですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8656,
            "text_len": 58,
            "text_offset": 8666,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "何をいってるんだ。\\nこういうのは最低限男女でやるべきだ。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8746,
            "text_len": 46,
            "text_offset": 8752,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "いやいや！\\n人命救助に男も女もないですから！\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8820,
            "text_len": 46,
            "text_offset": 8830,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "いやいや、\\n人命救助の経験も立派な教育だよ！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8898,
            "text_len": 38,
            "text_offset": 8904,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "いやいや、\\n謹んで辞退しますってば！\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9050,
            "text_len": 18,
            "text_offset": 9060,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "いやいやキミが！\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9108,
            "text_len": 22,
            "text_offset": 9114,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "いやいや教頭先生が！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9156,
            "text_len": 12,
            "text_offset": 9166,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9240,
            "text_len": 40,
            "text_offset": 9246,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "み、見てません！　何も見てませ――ん！\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9338,
            "text_len": 14,
            "text_offset": 9348,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "逃げた……。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9384,
            "text_len": 66,
            "text_offset": 9394,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "生徒が、私から……逃げた……\\n私が一体何をしたのと言うのだ……。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9486,
            "text_len": 12,
            "text_offset": 9496,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9534,
            "text_len": 40,
            "text_offset": 9544,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "逃げられたと言えば……\\n小学生の時弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9606,
            "text_len": 82,
            "text_offset": 9616,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "鬼ごっこの鬼……なかなかタッチできなくて\\n１ヶ月以上みんなに逃げられ続けた……。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9734,
            "text_len": 12,
            "text_offset": 9744,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9778,
            "text_len": 34,
            "text_offset": 9788,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "タッチと言えばあれは７年前……。\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9852,
            "text_len": 12,
            "text_offset": 9862,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 9986,
            "text_len": 86,
            "text_offset": 10006,
            "warnings": 0
        },
        "speaker": "ナレ―タ―お姉さん",
        "text": "その時、大きな川がはっきり見えました。後日、\\n校長先生はそう真剣に語ったと言います。\\n",
        "type": "dialog"
    }
]