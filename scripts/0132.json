[
    {
        "id": 0,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2144,
            "text_len": 32,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "あっ、そこの子、ちょっといい？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2204,
            "text_len": 8,
            "text_offset": 2210,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2244,
            "text_len": 48,
            "text_offset": 2256,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "キミ１年だよね？\\n東雲なのちゃんって知らない？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2326,
            "text_len": 30,
            "text_offset": 2332,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "え？　なのは私ですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2386,
            "text_len": 62,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "あなたがなのちゃんか〜。\\nえっとね、今度体育祭があるでしょ？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2484,
            "text_len": 14,
            "text_offset": 2490,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、はい。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2528,
            "text_len": 70,
            "text_offset": 2540,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "おめでとうっ。３学年混合の組み体操で、\\nピラミッド役に選ばれました。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2632,
            "text_len": 18,
            "text_offset": 2638,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ？　えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 2680,
            "text_len": 78,
            "text_offset": 2694,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "先生がくじで勝手に決めちゃったんだって。\\n学年ちがうけど、私たちも仲間だよ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2796,
            "text_len": 78,
            "text_offset": 2808,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "と言うわけでピラミッドのどの位置がいいか\\nなのちゃんの意見を聞きにきたんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2908,
            "text_len": 58,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "どの位置がいいかですか。\\nう―ん……私は別にどこでも弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3016,
            "text_len": 12,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3062,
            "text_len": 28,
            "text_offset": 3068,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あれ？　この犬、また……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3120,
            "text_len": 42,
            "text_offset": 3132,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "おっ！　犬だ！\\nよしよし、こっちおいで。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 3196,
            "text_len": 22,
            "text_offset": 3210,
            "warnings": 1
        },
        "speaker": "フェっちゃん",
        "text": "あははは、かわい―\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3308,
            "text_len": 56,
            "text_offset": 3312,
            "warnings": 1
        },
        "speaker": "",
        "text": "おい、タ―ゲットが見えないぞ？\\nちゃんと映せないのか？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3394,
            "text_len": 54,
            "text_offset": 3398,
            "warnings": 1
        },
        "speaker": "",
        "text": "ううむ、がっちり捕まってるから\\n身動きが取れないな。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3474,
            "text_len": 54,
            "text_offset": 3478,
            "warnings": 1
        },
        "speaker": "",
        "text": "全く……うらやましいやつめ。\\nとりあえず暴れてみろ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3554,
            "text_len": 12,
            "text_offset": 3558,
            "warnings": 1
        },
        "speaker": "",
        "text": "オッケ―。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3594,
            "text_len": 36,
            "text_offset": 3606,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "あっ、こいつ！\\n逃がさないぞ――！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3664,
            "text_len": 60,
            "text_offset": 3668,
            "warnings": 1
        },
        "speaker": "",
        "text": "……俺も女の子にハグされて、\\nこんなこと言われてみたいよ。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3754,
            "text_len": 54,
            "text_offset": 3758,
            "warnings": 1
        },
        "speaker": "",
        "text": "はは、まったくだ。\\nま、今は気分だけでも味わおうぜ。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3856,
            "text_len": 6,
            "text_offset": 3864,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3916,
            "text_len": 30,
            "text_offset": 3920,
            "warnings": 1
        },
        "speaker": "",
        "text": "ぎゃあああ！？　折れた――！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3966,
            "text_len": 42,
            "text_offset": 3970,
            "warnings": 1
        },
        "speaker": "",
        "text": "余計なこと、\\nするんじゃなかった――！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4116,
            "text_len": 24,
            "text_offset": 4122,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あっ！？　背中！！）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4176,
            "text_len": 60,
            "text_offset": 4182,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（組み体操でピラミッドになったら\\n　ネジが邪魔になる！！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4268,
            "text_len": 80,
            "text_offset": 4274,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（こんなところでもネジの弊害が……\\n　う―ん、一番上なら邪魔にならないかな？）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4376,
            "text_len": 38,
            "text_offset": 4382,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "それじゃあ私は一番上でお願いします。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 4444,
            "text_len": 42,
            "text_offset": 4458,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "一番上か―。\\nなのちゃんって体重軽い方？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4522,
            "text_len": 8,
            "text_offset": 4528,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4560,
            "text_len": 84,
            "text_offset": 4572,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "ピラミッドって下の人に負担かかるじゃん？\\nだからできるだけ軽い人を上にしたいんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4678,
            "text_len": 44,
            "text_offset": 4684,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、そうですね。\\nえ―、私の体重は……。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4758,
            "text_len": 24,
            "text_offset": 4764,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あっ！？　体重！！）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4820,
            "text_len": 74,
            "text_offset": 4826,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（私はロボットだから絶対に軽くない。\\n　なのに私が一番上になったら……）\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4938,
            "text_len": 34,
            "text_offset": 4944,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ピラミッドは崩壊を免れない！）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5020,
            "text_len": 76,
            "text_offset": 5026,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（これじゃ私は一番下になるしか……\\n　でもネジのせいで下にもなれない……）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5126,
            "text_len": 62,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "……あれ？　もしかしてなのちゃんって\\n言えないくらい重いの？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5258,
            "text_len": 60,
            "text_offset": 5264,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そ、そんなことないです！\\n他の人よりちょっと重いだけです！\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5348,
            "text_len": 56,
            "text_offset": 5360,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "ん―、それなら一番上は無理かな。\\n悪いけど下の方に弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5438,
            "text_len": 22,
            "text_offset": 5444,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いえ！　大丈夫です！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5494,
            "text_len": 8,
            "text_offset": 5506,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5540,
            "text_len": 26,
            "text_offset": 5546,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ダイエットしますから！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5606,
            "text_len": 66,
            "text_offset": 5620,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "で、でも、今からダイエットしても\\n一番軽くなるのは難しいし……。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5708,
            "text_len": 22,
            "text_offset": 5714,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いえ！　大丈夫です！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5758,
            "text_len": 8,
            "text_offset": 5772,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5806,
            "text_len": 26,
            "text_offset": 5812,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "軽量化なら一晩ですから！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 5870,
            "text_len": 8,
            "text_offset": 5884,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5914,
            "text_len": 10,
            "text_offset": 5920,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5964,
            "text_len": 10,
            "text_offset": 5976,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "軽量化？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6008,
            "text_len": 34,
            "text_offset": 6014,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "軽量化という名のダイエットです！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6072,
            "text_len": 48,
            "text_offset": 6086,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "じゃあやっぱり一晩じゃ\\n間に合わないんじゃ弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6156,
            "text_len": 22,
            "text_offset": 6162,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いえ！　大丈夫です！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6206,
            "text_len": 12,
            "text_offset": 6220,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "ふぇっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6258,
            "text_len": 32,
            "text_offset": 6264,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "腕の立つ人を知っていますから！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 24,
            "speaker_offset": 6342,
            "text_len": 6,
            "text_offset": 6368,
            "warnings": 0
        },
        "speaker": "ウェボシ―＆フェっちゃん",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6488,
            "text_len": 54,
            "text_offset": 6500,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "そ、そっか―。じゃあ、ダイエット、\\n頑張ってきてよ。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6570,
            "text_len": 22,
            "text_offset": 6576,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい！　頑張ります！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6672,
            "text_len": 48,
            "text_offset": 6678,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（重い！\\n　きっと普通の女の子の何倍も重い！）\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6756,
            "text_len": 76,
            "text_offset": 6762,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（それを認めると強制的に下の方に……\\n　でもネジ……上はみんなが怪我……）\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6868,
            "text_len": 56,
            "text_offset": 6882,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "あれ？　お―いなのちゃ―ん？\\n……聞こえてないみたい。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 6964,
            "text_len": 68,
            "text_offset": 6976,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "重いって言われてショックだったんだよ。\\nオブラ―トにつつみなって。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 7066,
            "text_len": 20,
            "text_offset": 7080,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "あっ、そっか……。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7130,
            "text_len": 40,
            "text_offset": 7136,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ、すみません。\\n何か言いましたか？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7202,
            "text_len": 70,
            "text_offset": 7214,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "い、いや、何も。それより、\\nごめんねなのちゃん。私たちそろそろ……。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 7308,
            "text_len": 50,
            "text_offset": 7322,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "ほ、他の人にも聞かないといけないし。\\nじゃあね！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7412,
            "text_len": 30,
            "text_offset": 7418,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ……\\n行っちゃった……。\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7552,
            "text_len": 58,
            "text_offset": 7558,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……結局、私はどのポジションになれば\\nいいんだろう……。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7674,
            "text_len": 48,
            "text_offset": 7680,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ど、どうでしょう。\\n最近体重を量ってませんし。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7752,
            "text_len": 44,
            "text_offset": 7764,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "前に量った時は\\n何キロだったか覚えてない？\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7830,
            "text_len": 66,
            "text_offset": 7836,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "う―ん……ちょっと覚えてないですね。\\nでも軽い方だったはずです。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 7940,
            "text_len": 32,
            "text_offset": 7952,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "へ〜。なら一番上になれるかも。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8008,
            "text_len": 52,
            "text_offset": 8022,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "あ、そうだ。\\n今から保健室に行って量ってみようよ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8096,
            "text_len": 74,
            "text_offset": 8102,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あ―……すみません、今日は急いでいて\\nすぐに帰らなくちゃいけなくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8200,
            "text_len": 40,
            "text_offset": 8212,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "あ、そうなんだ。\\nごめんね呼び止めて。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8274,
            "text_len": 36,
            "text_offset": 8280,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いえ。\\nまた体重量ってきますから。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8340,
            "text_len": 20,
            "text_offset": 8354,
            "warnings": 0
        },
        "speaker": "フェっちゃん",
        "text": "うん。じゃあね―。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8418,
            "text_len": 12,
            "text_offset": 8424,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8458,
            "text_len": 28,
            "text_offset": 8464,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "嘘をつき通しちゃった……。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8596,
            "text_len": 34,
            "text_offset": 8602,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ピラミッドの崩壊は免れない……。\\n",
        "type": "dialog"
    }
]