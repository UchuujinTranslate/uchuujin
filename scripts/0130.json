[
    {
        "id": 0,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 2138,
            "text_len": 54,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "女性客（女性客１）",
        "text": "えっと、チ―ズケ―キ１つと、\\nココアの……スモ―ル？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2232,
            "text_len": 12,
            "text_offset": 2238,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 2266,
            "text_len": 20,
            "text_offset": 2286,
            "warnings": 0
        },
        "speaker": "女性客（女性客１）",
        "text": "……あ、ショ―ト？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2322,
            "text_len": 36,
            "text_offset": 2328,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい！\\nココアのショ―ト入ります！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 2380,
            "text_len": 40,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "女性客（女性客１）",
        "text": "へ―、Ｒがショ―トでＳがト―ルなんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2466,
            "text_len": 34,
            "text_offset": 2472,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（やった、また気付いてくれた！）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2542,
            "text_len": 60,
            "text_offset": 2548,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（サイズを言い間違える人多いから\\n　気まずかったんだよね）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2634,
            "text_len": 86,
            "text_offset": 2640,
            "warnings": 1
        },
        "speaker": "玉村",
        "text": "（でも見えやすい位置にポップを置いてから\\n　間違う人が減ってきてる。作戦大成功―）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2770,
            "text_len": 28,
            "text_offset": 2776,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "あっ、いらっしゃいませ―。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2838,
            "text_len": 32,
            "text_offset": 2848,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "ホットコ―ヒ―を。\\nト―ルで。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2902,
            "text_len": 28,
            "text_offset": 2908,
            "warnings": 1
        },
        "speaker": "玉村",
        "text": "はい。少お待ちください。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2972,
            "text_len": 12,
            "text_offset": 2982,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3018,
            "text_len": 42,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お待たせしました。\\n３００円になります。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3088,
            "text_len": 38,
            "text_offset": 3098,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "どうもどうも。\\nそれでは、３００円。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3158,
            "text_len": 32,
            "text_offset": 3164,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい、ありがとうございました！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3220,
            "text_len": 12,
            "text_offset": 3230,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "さて……。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3264,
            "text_len": 36,
            "text_offset": 3274,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "お、あそこの席が空いて弌弌んっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3378,
            "text_len": 14,
            "text_offset": 3388,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "うわあっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3424,
            "text_len": 16,
            "text_offset": 3434,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "〜〜〜〜っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3520,
            "text_len": 66,
            "text_offset": 3524,
            "warnings": 1
        },
        "speaker": "",
        "text": "あはははは。でさ―、\\n俺、超能力が使えるようになる夢を見てさ―。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3606,
            "text_len": 32,
            "text_offset": 3610,
            "warnings": 1
        },
        "speaker": "",
        "text": "お―、それからどうなったんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3676,
            "text_len": 28,
            "text_offset": 3680,
            "warnings": 1
        },
        "speaker": "",
        "text": "うおっ！？　な、なんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3732,
            "text_len": 48,
            "text_offset": 3736,
            "warnings": 1
        },
        "speaker": "",
        "text": "あっ！　アンテナが壊れてる！\\nでも、何でだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3808,
            "text_len": 38,
            "text_offset": 3812,
            "warnings": 1
        },
        "speaker": "",
        "text": "オッサンが倒れてコ―ヒ―まみれに！！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3872,
            "text_len": 64,
            "text_offset": 3876,
            "warnings": 1
        },
        "speaker": "",
        "text": "あああ！？　サボってる間に、\\nオッサンの制御が解けていたんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3962,
            "text_len": 38,
            "text_offset": 3966,
            "warnings": 1
        },
        "speaker": "",
        "text": "……ど、どうする、どうすればいい！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4026,
            "text_len": 42,
            "text_offset": 4030,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうだ！\\nこんな時こそ、お前の超能力で！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4094,
            "text_len": 38,
            "text_offset": 4098,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうか！\\nいくぞ、はぁぁぁぁ弌弌！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4182,
            "text_len": 30,
            "text_offset": 4186,
            "warnings": 1
        },
        "speaker": "",
        "text": "って、使えるか――――っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4318,
            "text_len": 12,
            "text_offset": 4326,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4368,
            "text_len": 42,
            "text_offset": 4374,
            "warnings": 1
        },
        "speaker": "玉村",
        "text": "（こ、この子は……色と間違える子！？）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4456,
            "text_len": 12,
            "text_offset": 4464,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4502,
            "text_len": 84,
            "text_offset": 4508,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（……だ、大丈夫。今はポップに書いてあるし、\\n　前にＲとＳの読み方を学んだし……）\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4614,
            "text_len": 32,
            "text_offset": 4620,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ご、ご注文は何にしましょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4674,
            "text_len": 40,
            "text_offset": 4682,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え―……\\nモ、モカフラペチ―ノの……。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4752,
            "text_len": 70,
            "text_offset": 4760,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（Ｆ……！？　Ｆって何！？\\n　ギガ？　グレイト？　グラヴィティ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4866,
            "text_len": 58,
            "text_offset": 4872,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（……気付いて！\\n　あなたのすぐそこに、ポップが……！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4956,
            "text_len": 62,
            "text_offset": 4964,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（弌弌あっ、何これ。ポップ？\\n　Ｆ……あ、グランデなんだ！）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5052,
            "text_len": 44,
            "text_offset": 5058,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（あっ！　ポップに気付いてくれたみたい！）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5138,
            "text_len": 36,
            "text_offset": 5146,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "モカフラペチ―ノ、グ、グランデで！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5208,
            "text_len": 46,
            "text_offset": 5214,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はいっ！\\nモカフラペチ―ノのグランデですね！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5286,
            "text_len": 42,
            "text_offset": 5292,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（やった！　この子も間違えずに言えた！）\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5366,
            "text_len": 36,
            "text_offset": 5372,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ご注文は以上でよろしいでしょうか。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5432,
            "text_len": 32,
            "text_offset": 5440,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ、あ―、ついでに食べ物も！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5494,
            "text_len": 70,
            "text_offset": 5500,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい。あ、本日のオススメは\\n３種のキノコのバジルブレッドになります。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5592,
            "text_len": 62,
            "text_offset": 5600,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あっ、おいしそう。\\nじゃあその３種のキノコのバジブルッド弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5702,
            "text_len": 16,
            "text_offset": 5708,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（噛んだ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5756,
            "text_len": 14,
            "text_offset": 5764,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "弌弌くっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5838,
            "text_len": 68,
            "text_offset": 5844,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（すっかり油断してた……\\n　まさかこのタイミングで噛むなんて……）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5940,
            "text_len": 14,
            "text_offset": 5948,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5994,
            "text_len": 84,
            "text_offset": 6000,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（どうすれば……ここは訂正するべきなの？\\n　そのまま間違えた名前で言うべきなの？）\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6118,
            "text_len": 10,
            "text_offset": 6124,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ば……。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6160,
            "text_len": 42,
            "text_offset": 6166,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "バジブルッド、１つでよろしいでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6234,
            "text_len": 6,
            "text_offset": 6242,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6290,
            "text_len": 32,
            "text_offset": 6298,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ハ、ハイ！　バジブルッド１つ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6362,
            "text_len": 68,
            "text_offset": 6368,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "では、モカフラペチ―ノのグランデと\\nバジブルッドで計５３０円です。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6458,
            "text_len": 10,
            "text_offset": 6466,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "は―い！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6502,
            "text_len": 56,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "ゆっこ＆玉村",
        "text": "（……よかった。\\n　本当によかった、これで無事に弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6616,
            "text_len": 66,
            "text_offset": 6622,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あっはっは、バジブルッドって何だよ。\\nバジルブレッドだぞ、相生。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 6816,
            "text_len": 26,
            "text_offset": 6830,
            "warnings": 0
        },
        "speaker": "ゆっこ＆玉村",
        "text": "（……田中のヤロ―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6918,
            "text_len": 68,
            "text_offset": 6924,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（き、気まずい……店員として、\\n　ここはどう返すのがベストなの？）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7020,
            "text_len": 22,
            "text_offset": 7026,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ええと……その……。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7074,
            "text_len": 12,
            "text_offset": 7082,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7120,
            "text_len": 24,
            "text_offset": 7126,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "……で、では、ソレで。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7176,
            "text_len": 6,
            "text_offset": 7184,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7226,
            "text_len": 34,
            "text_offset": 7234,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "は、はい！　ソレとソレのソレで！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7290,
            "text_len": 40,
            "text_offset": 7296,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ではソレとソレのソレで計５３０円です！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7358,
            "text_len": 42,
            "text_offset": 7366,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "はいっ。\\nあっ、ソレ、少し熱いくらいで！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7430,
            "text_len": 28,
            "text_offset": 7436,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい。\\nではソレは熱めに！\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7496,
            "text_len": 68,
            "text_offset": 7502,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "それでは、出来ましたらお持ちしますので\\nテ―ブルでお待ちください！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7592,
            "text_len": 16,
            "text_offset": 7600,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "は――――い！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7652,
            "text_len": 32,
            "text_offset": 7658,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（……ふう。\\n　さて、と……）\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7798,
            "text_len": 24,
            "text_offset": 7804,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（……どれが、どれ？）\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7896,
            "text_len": 56,
            "text_offset": 7902,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（サイズをちゃんと言えた分、\\n　この空気がつらい……）\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7990,
            "text_len": 70,
            "text_offset": 7996,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（でも、これは訂正してもしなくても\\n　気まずいのは変わりないし……）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8098,
            "text_len": 54,
            "text_offset": 8104,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "バ……バジルブレッド、\\nお１つでよろしいでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8180,
            "text_len": 14,
            "text_offset": 8188,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8224,
            "text_len": 54,
            "text_offset": 8232,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "じゃあ……そのバジブルッドの\\nバジルブレッドで……！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8328,
            "text_len": 6,
            "text_offset": 8334,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8374,
            "text_len": 42,
            "text_offset": 8380,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（バジブルッドの弌弌バジルブレッド！？）\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8444,
            "text_len": 14,
            "text_offset": 8452,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8492,
            "text_len": 88,
            "text_offset": 8498,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（バジルブレッドがバジブルッドに……それは\\n　間違いだからバジルブッドに……あらら？）\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8608,
            "text_len": 16,
            "text_offset": 8616,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8654,
            "text_len": 54,
            "text_offset": 8660,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "は、はい！\\nバ、バジルブレッドのバジドレッドは弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8746,
            "text_len": 10,
            "text_offset": 8754,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "弌弌！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8820,
            "text_len": 16,
            "text_offset": 8834,
            "warnings": 0
        },
        "speaker": "ゆっこ＆玉村",
        "text": "………………。\\n",
