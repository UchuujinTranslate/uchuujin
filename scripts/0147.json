        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7426,
            "text_len": 20,
            "text_offset": 7432,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "どういたしまして。\\n",
        "type": "dialog"
    },
