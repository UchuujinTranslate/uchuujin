        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7370,
            "text_len": 28,
            "text_offset": 7376,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "（まさかまたアフロに……）\\n",
