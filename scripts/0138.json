[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2146,
            "text_len": 38,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ゆっこ、注文の仕方ちゃんと覚えたの？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2214,
            "text_len": 28,
            "text_offset": 2222,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "う、うん……たぶん大丈夫。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2272,
            "text_len": 70,
            "text_offset": 2280,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "Ｒはスモ―ルじゃなくてショ―ト……\\nＳはティ―じゃなくてト―ル……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2376,
            "text_len": 28,
            "text_offset": 2382,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ホントに大丈夫か……？）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2432,
            "text_len": 42,
            "text_offset": 2438,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……あれ？\\nあのアフロ、田中君じゃない？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2510,
            "text_len": 60,
            "text_offset": 2518,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ、ホントだ。ぷぷぷ、\\nあいつ、ちゃんと注文できるのかな？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2602,
            "text_len": 38,
            "text_offset": 2608,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ゆっこじゃないんだし、できるでしょ？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2670,
            "text_len": 54,
            "text_offset": 2678,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "わかんないよ？\\nもしかしたら私以上にひどいかも……。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2762,
            "text_len": 80,
            "text_offset": 2768,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ん―と……ホワイトチョコレ―トモカの\\nト―ル、ミルクを豆乳に変えて、温度熱め。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2870,
            "text_len": 56,
            "text_offset": 2876,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あとヘ―ゼルナッツシロップと\\nホイップクリ―ム追加で。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2964,
            "text_len": 26,
            "text_offset": 2970,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "はい、かしこまりました。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3106,
            "text_len": 24,
            "text_offset": 3114,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "田中のヤロ―――！！！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3260,
            "text_len": 32,
            "text_offset": 3266,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ほらね。ま、わかってたけどさ。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3322,
            "text_len": 54,
            "text_offset": 3330,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "生意気な……\\nみおちゃん、こっちも負けてられないよ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3406,
            "text_len": 48,
            "text_offset": 3412,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なに言ってんの。\\n別に勝負してるわけじゃ……。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3490,
            "text_len": 18,
            "text_offset": 3496,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………フフン。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3538,
            "text_len": 6,
            "text_offset": 3544,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "む？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3584,
            "text_len": 32,
            "text_offset": 3592,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ほら、ケンカ売ってるよあいつ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3650,
            "text_len": 40,
            "text_offset": 3656,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "よ、よ―し、いっちょやってやりますか！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3736,
            "text_len": 20,
            "text_offset": 3742,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "ご注文をどうぞ―。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3784,
            "text_len": 54,
            "text_offset": 3790,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ホワイトチョコレ―トモカフラペチ―ノの\\nグランデで。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3866,
            "text_len": 80,
            "text_offset": 3872,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "それに追加でア―モンドシロップ、それと\\nチョコレ―トソ―スにシナモンパウダ―。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3974,
            "text_len": 54,
            "text_offset": 3980,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "とどめにキャラメルソ―ス、\\nヘ―ゼルナッツシロップ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4056,
            "text_len": 48,
            "text_offset": 4062,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "チョコレ―トチップ、\\nエキストラホイップの弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4138,
            "text_len": 30,
            "text_offset": 4144,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "エスプレッソショット一杯で。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4206,
            "text_len": 30,
            "text_offset": 4212,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "は、はい、かしこまりました。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4272,
            "text_len": 22,
            "text_offset": 4278,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "（…………とどめ？）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4336,
            "text_len": 28,
            "text_offset": 4342,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "次の方、ご注文をどうぞ―。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4392,
            "text_len": 36,
            "text_offset": 4402,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "すまんが……コ―ヒ―を頂けるかな。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4460,
            "text_len": 56,
            "text_offset": 4466,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "え、え―っと……こちらのメニュ―から\\nお選びください。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4544,
            "text_len": 16,
            "text_offset": 4554,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "ふむふむ……。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4602,
            "text_len": 14,
            "text_offset": 4612,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……？？？？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4704,
            "text_len": 26,
            "text_offset": 4708,
            "warnings": 1
        },
        "speaker": "",
        "text": "……あ、アンテナ壊れた。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4758,
            "text_len": 54,
            "text_offset": 4762,
            "warnings": 1
        },
        "speaker": "",
        "text": "なぜだ！！\\n壊れるようなトラブルは何も起きてないぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4838,
            "text_len": 42,
            "text_offset": 4842,
            "warnings": 1
        },
        "speaker": "",
        "text": "いや―、これは壊れるって。\\n仕方ないさ。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4908,
            "text_len": 32,
            "text_offset": 4912,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうだな。\\nこれは壊れるよな。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4990,
            "text_len": 20,
            "text_offset": 4994,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんで――――！？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5106,
            "text_len": 18,
            "text_offset": 5112,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ふふん、どうよ？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5152,
            "text_len": 10,
            "text_offset": 5158,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ぐ……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5204,
            "text_len": 66,
            "text_offset": 5212,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "やった―！　みおちゃん偉い！\\nなんかよくわかんなかったけど偉い！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5300,
            "text_len": 28,
            "text_offset": 5306,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "次の方、ご注文をどうぞ―。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5356,
            "text_len": 48,
            "text_offset": 5364,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "抹茶クリ―ムフラペチ―ノ。\\nト―ルのホットで。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5456,
            "text_len": 6,
            "text_offset": 5468,
            "warnings": 0
        },
        "speaker": "みお＆田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5518,
            "text_len": 32,
            "text_offset": 5526,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……えっ？　なに？　どしたの？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5580,
            "text_len": 56,
            "text_offset": 5588,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "あと追加で、アッサムブラックティ―ラテ、\\nト―ルの弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5666,
            "text_len": 28,
            "text_offset": 5674,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ミルクは豆乳、温度ぬるめ。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5724,
            "text_len": 76,
            "text_offset": 5732,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ホイップクリ―ムとバニラシロップ、\\nそれにエスプレッソのショットを入れて。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 5848,
            "text_len": 6,
            "text_offset": 5860,
            "warnings": 0
        },
        "speaker": "みお＆田中",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5892,
            "text_len": 56,
            "text_offset": 5898,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あ、ありえない……\\n　なんてカッコイイ注文なの……）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5986,
            "text_len": 14,
            "text_offset": 5994,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "……にやり。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6030,
            "text_len": 14,
            "text_offset": 6036,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "うぐぐ……。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6106,
            "text_len": 74,
            "text_offset": 6114,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ねぇみおちゃん……よくわからないんだけど、\\n今の注文、そんなにスゴイの？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6214,
            "text_len": 40,
            "text_offset": 6220,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "すごいよ！　あり得ないくらいすごいよ！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6282,
            "text_len": 54,
            "text_offset": 6288,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "本来アイスしかない\\nフラペチ―ノを無理矢理ホットに！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6364,
            "text_len": 50,
            "text_offset": 6370,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "しかもティ―にエスプレットショット\\n入れるとか！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6446,
            "text_len": 58,
            "text_offset": 6452,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "それってもはやコ―ヒ―なのか\\n紅茶なのかわかんないよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6552,
            "text_len": 24,
            "text_offset": 6560,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "そうなのか……よ―し！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6612,
            "text_len": 22,
            "text_offset": 6618,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え？　ちょ、ゆっこ？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6670,
            "text_len": 24,
            "text_offset": 6676,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お次のお客様、どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6722,
            "text_len": 56,
            "text_offset": 6730,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "は〜い。え―っと、\\nエスプレッソのホット、ドッピオで。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6812,
            "text_len": 6,
            "text_offset": 6818,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6856,
            "text_len": 20,
            "text_offset": 6864,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "それに、追加で弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6906,
            "text_len": 22,
            "text_offset": 6914,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "カスタマイズ、全部。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6972,
            "text_len": 6,
            "text_offset": 6978,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7010,
            "text_len": 6,
            "text_offset": 7016,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7048,
            "text_len": 6,
            "text_offset": 7056,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7092,
            "text_len": 18,
            "text_offset": 7098,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（全部って！？）\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7138,
            "text_len": 46,
            "text_offset": 7144,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "え〜……あ、はい……\\nかしこまりました……。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7220,
            "text_len": 26,
            "text_offset": 7226,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（注文受けちゃった！？）\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7278,
            "text_len": 64,
            "text_offset": 7284,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ま、まあ、フラペチ―ノのホットが\\n　ＮＪなくらいだしね……）\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7378,
            "text_len": 42,
            "text_offset": 7386,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ど―よ、みおちゃん！\\n私、最強でしょ！！\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7450,
            "text_len": 60,
            "text_offset": 7456,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "え〜……あ〜……うん……\\n確かに、それ以上はないよね……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7546,
            "text_len": 52,
            "text_offset": 7552,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "お待たせ致しました。\\nエスプレッソの全部入りです。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7626,
            "text_len": 18,
            "text_offset": 7634,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "わ―い来た来た！\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7688,
            "text_len": 36,
            "text_offset": 7694,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（うぎゃ！？　なんじゃこりゃ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7756,
            "text_len": 38,
            "text_offset": 7764,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "んじゃ、いただきま―す。ごくっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7824,
            "text_len": 62,
            "text_offset": 7832,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "マズ―！！　なにこれ！？\\nこんな甘いのコ―ヒ―じゃないよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7930,
            "text_len": 12,
            "text_offset": 7936,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8052,
            "text_len": 26,
            "text_offset": 8058,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ゆっこはバカだなぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8140,
            "text_len": 70,
            "text_offset": 8146,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（こ、この子……すごい！\\n　とてもじゃないけど、勝てる気がしない！）\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8242,
            "text_len": 18,
            "text_offset": 8248,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………長野原。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8282,
            "text_len": 6,
            "text_offset": 8288,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8320,
            "text_len": 52,
            "text_offset": 8326,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（そうだ！\\n　ここは田中君と力を合わせて……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8404,
            "text_len": 20,
            "text_offset": 8410,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "金、貸してくんね？\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8462,
            "text_len": 6,
            "text_offset": 8468,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8500,
            "text_len": 68,
            "text_offset": 8506,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（そ、そっか、あの子に勝つには\\n　大量の注文をすることが前提……）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8600,
            "text_len": 54,
            "text_offset": 8606,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（でも、すでに注文した田中君は\\n　もう所持金が……）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8686,
            "text_len": 40,
            "text_offset": 8692,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あれ？　そういや私の所持金は……？）\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8758,
            "text_len": 6,
            "text_offset": 8764,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8812,
            "text_len": 36,
            "text_offset": 8818,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ゆっこ、この間貸したお金返して弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8890,
            "text_len": 10,
            "text_offset": 8898,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 8958,
            "text_len": 6,
            "text_offset": 8970,
            "warnings": 0
        },
        "speaker": "みお＆田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 9014,
            "text_len": 20,
            "text_offset": 9026,
            "warnings": 0
        },
        "speaker": "みお＆田中",
        "text": "待て―――――！！\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9154,
            "text_len": 12,
            "text_offset": 9160,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9230,
            "text_len": 36,
            "text_offset": 9236,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（あ、あれを超える注文なんて……）\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9298,
            "text_len": 52,
            "text_offset": 9304,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（もう、カスタマイズ全部入れ\\n　ぐらいしか……！）\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9382,
            "text_len": 58,
            "text_offset": 9388,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（やるの……？　やるの私？\\n　禁断の全部入れを……っ！）\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9468,
            "text_len": 16,
            "text_offset": 9476,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "ごくごく……。\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9522,
            "text_len": 12,
            "text_offset": 9530,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "マズ―！！\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9570,
            "text_len": 6,
            "text_offset": 9576,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9620,
            "text_len": 6,
            "text_offset": 9626,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9658,
            "text_len": 4,
            "text_offset": 9666,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "？\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9696,
            "text_len": 70,
            "text_offset": 9702,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（そりゃそうだ……あんなカスタマイズ……\\n　美味しいはずがない……）\\n",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9880,
            "text_len": 48,
            "text_offset": 9888,
            "warnings": 0
        },
        "speaker": "きよし",
        "text": "マズイマズイマズイ！！\\nマズ―――――イ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9956,
            "text_len": 28,
            "text_offset": 9962,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（普通が……一番だよ……）\\n",
        "type": "dialog"
    }
]