[
    {
        "id": 0,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 2146,
            "text_len": 26,
            "text_offset": 2170,
            "warnings": 0
        },
        "speaker": "男子生徒（男子生徒２）",
        "text": "教頭先生、さよ―なら―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2218,
            "text_len": 20,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "はい、さようなら。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 22,
            "speaker_offset": 2264,
            "text_len": 26,
            "text_offset": 2288,
            "warnings": 0
        },
        "speaker": "女子生徒（女子生徒２）",
        "text": "教頭先生、失礼しま―す。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2336,
            "text_len": 18,
            "text_offset": 2346,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "はい、お疲れ様。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2390,
            "text_len": 16,
            "text_offset": 2400,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2438,
            "text_len": 32,
            "text_offset": 2448,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "はあ……教頭。\\n……教頭、か。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2502,
            "text_len": 76,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "なぜみんな、私を校長と呼んでくれん？\\nなぜだ？　教頭だからとでも言うのか？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2624,
            "text_len": 16,
            "text_offset": 2634,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2672,
            "text_len": 60,
            "text_offset": 2682,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……それはそうか。\\n校長がいる限り私は今のまま……教頭だ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2764,
            "text_len": 46,
            "text_offset": 2774,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "どうすればいい？\\n校長をクビにするには……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2842,
            "text_len": 54,
            "text_offset": 2852,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "さっきの臨時集会でも失敗したし……\\n一体どうすれば？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2932,
            "text_len": 20,
            "text_offset": 2942,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2984,
            "text_len": 60,
            "text_offset": 2994,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……はあ…………わからん。\\n集めた落ち葉でも捨てに行くか。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3182,
            "text_len": 66,
            "text_offset": 3192,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "おお、おお、燃えとる燃えとる。\\n木の枝や枯葉はこうするに限るな。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3274,
            "text_len": 64,
            "text_offset": 3284,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……しかし、焚き火をすると\\n気分が落ち着くのはどうしてだろう？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3370,
            "text_len": 34,
            "text_offset": 3380,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……はあ…………何だっていいか。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3436,
            "text_len": 34,
            "text_offset": 3446,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……ん？\\nあそこにいるのは……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3598,
            "text_len": 12,
            "text_offset": 3602,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3640,
            "text_len": 36,
            "text_offset": 3650,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "し、鹿！？　なぜ私の学校に鹿が！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3804,
            "text_len": 88,
            "text_offset": 3814,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（あ、鹿といえば、町内を亂乘していると言う\\n　野生動物の話が……そうか、アイツか！？）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3928,
            "text_len": 62,
            "text_offset": 3938,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（ならば、アイツは人を襲うはず！\\n　は、早く逃げないと弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4046,
            "text_len": 46,
            "text_offset": 4056,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "あ―あ、逃げやがったか。\\n意外と根性ないな。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4142,
            "text_len": 16,
            "text_offset": 4152,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4200,
            "text_len": 22,
            "text_offset": 4204,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……ッ……ッ……ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4248,
            "text_len": 58,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "……お、おい、何だよ？\\n俺は猫だぞ！？　関係ないだろ！？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4348,
            "text_len": 24,
            "text_offset": 4352,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ！　ッ！　ッ！　ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4402,
            "text_len": 18,
            "text_offset": 4412,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "だから俺は弌弌！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4538,
            "text_len": 22,
            "text_offset": 4542,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッッッッッ！！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4612,
            "text_len": 24,
            "text_offset": 4622,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "ギャアアアアアアッ！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4724,
            "text_len": 12,
            "text_offset": 4728,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ―っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4764,
            "text_len": 30,
            "text_offset": 4768,
            "warnings": 1
        },
        "speaker": "",
        "text": "まずい！　システムに影響が！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4820,
            "text_len": 38,
            "text_offset": 4824,
            "warnings": 1
        },
        "speaker": "",
        "text": "は、早く対処しろっ！\\nじゃないと弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 4908,
            "text_len": 26,
            "text_offset": 4920,
            "warnings": 3
        },
        "speaker": "＆＆",
        "text": "うあああああああああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5038,
            "text_len": 40,
            "text_offset": 5048,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……おや？　あの鹿、どこか見ているな。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5112,
            "text_len": 38,
            "text_offset": 5122,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "あそこは確か……校長室のある辺りか？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5176,
            "text_len": 80,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "そういえば以前、校長が鹿を仕留めたはず……\\nもしかして仲間の仇討ちに来たのか？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5290,
            "text_len": 16,
            "text_offset": 5300,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5338,
            "text_len": 48,
            "text_offset": 5348,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……悪くない。\\nあの鹿……利用させてもらおう。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5428,
            "text_len": 68,
            "text_offset": 5438,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "いや、人聞きが悪いな。\\n互いの利益が合致したから手を組む、共闘だ。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5528,
            "text_len": 38,
            "text_offset": 5538,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ぷっ……教頭が共闘か。\\nふふふ……。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5612,
            "text_len": 16,
            "text_offset": 5622,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5660,
            "text_len": 66,
            "text_offset": 5670,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "そ、そんなことはどうでもいい！\\nとにかくアイツを手なずけないと。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5758,
            "text_len": 78,
            "text_offset": 5768,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……だが、鹿の手なずけ方なんて知らないし。\\nうむむむ……餌付けでもするのか？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 5920,
            "text_len": 16,
            "text_offset": 5924,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5966,
            "text_len": 6,
            "text_offset": 5976,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "む？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6008,
            "text_len": 28,
            "text_offset": 6012,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ……ッ……ッ……ッ……！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6066,
            "text_len": 70,
            "text_offset": 6076,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "な、なんだね、その雰囲気の悪い鼻息は？\\nま、まさか、私を襲う気か！？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6172,
            "text_len": 62,
            "text_offset": 6182,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（マズい！　非常にマズい！\\n　早く何とかしなければ……！！）\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6316,
            "text_len": 72,
            "text_offset": 6326,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（よ、よし、アイツを手なずけよう……！\\n　でないと……殺られる……！）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6424,
            "text_len": 74,
            "text_offset": 6434,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（だが、どうすればいいんだ？\\n　犬や猫と同じでいいのか？　いや待て……）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6530,
            "text_len": 36,
            "text_offset": 6540,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "え、え〜と確か牛のあやし方は……。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6598,
            "text_len": 64,
            "text_offset": 6608,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "べえ〜、べえべえべえべえ。\\n良い子だな〜。べえ〜べえべえべえ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6704,
            "text_len": 18,
            "text_offset": 6708,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………ッ……？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6752,
            "text_len": 42,
            "text_offset": 6762,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（おお、鹿にも効いてる！　これなら弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6826,
            "text_len": 66,
            "text_offset": 6836,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "べえ〜、べえべえべえべえ。\\n大丈夫ですよ、怖くないですからね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 6938,
            "text_len": 16,
            "text_offset": 6942,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6980,
            "text_len": 76,
            "text_offset": 6990,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "べえ〜べえ〜べえ〜べえべえべえ。\\n私は怖くないですよ〜、優しいんですよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7088,
            "text_len": 76,
            "text_offset": 7098,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "だからほら、べえ〜べえべえべえ。\\nこっちにおいで。撫で撫でしてあげるよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 7220,
            "text_len": 16,
            "text_offset": 7224,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7274,
            "text_len": 74,
            "text_offset": 7284,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（よし！　ぃよしっ！　大成功！\\n　後は家に連れて帰ってなつかせるだけ！）\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7380,
            "text_len": 54,
            "text_offset": 7390,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "さ、さあ、こっちにおいで。\\nここに隠れていれば……。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 7486,
            "text_len": 28,
            "text_offset": 7490,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ……ッ……ッ……ッ……！\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7544,
            "text_len": 58,
            "text_offset": 7554,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "え？　ま、またなぜその鼻息が！？\\nなんで怒ってるんだ！？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7640,
            "text_len": 22,
            "text_offset": 7650,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……ん？　この音は？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7716,
            "text_len": 68,
            "text_offset": 7726,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "しまった、火か！　動物は火を怖がる！\\nこ、こ、興奮状態になっとる！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7908,
            "text_len": 30,
            "text_offset": 7918,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（ええ―――――――っ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 7974,
            "text_len": 24,
            "text_offset": 7978,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ！　ッ！　ッ！　ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8022,
            "text_len": 52,
            "text_offset": 8032,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ま、待ってくれ！　違う！　誤解だ！\\n話せばわか弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8200,
            "text_len": 26,
            "text_offset": 8210,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "うあああああああっ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8304,
            "text_len": 76,
            "text_offset": 8314,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……殺気立ってはだめだ。動物は気配を読む。\\n　落ち着け……落ち着くんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8424,
            "text_len": 36,
            "text_offset": 8434,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "す―……は―……す―……は―……。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 8508,
            "text_len": 26,
            "text_offset": 8512,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……ッ……ッ……ッ……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8560,
            "text_len": 36,
            "text_offset": 8570,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "す―……は―……す―……は―……。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 8628,
            "text_len": 26,
            "text_offset": 8632,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "…………ッ………………？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8690,
            "text_len": 72,
            "text_offset": 8700,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……よし、向こうも落ち着いたぞ。\\n　これで後は……後は………………）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8798,
            "text_len": 26,
            "text_offset": 8808,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……どうしたらいいんだ？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8878,
            "text_len": 76,
            "text_offset": 8888,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（しまった！　ノ―アイデアだ！\\n　どうすればいい！　どうしたら助かる！？）\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9000,
            "text_len": 56,
            "text_offset": 9010,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "そ、そうだ！　ワラ人形だ！\\n鹿のワラ人形を作ればいい！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9098,
            "text_len": 88,
            "text_offset": 9108,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌だめだ！　材料は家だ！　うう……ああ！\\nどうする？　人形は何で作ればいいんだ―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 9228,
            "text_len": 26,
            "text_offset": 9232,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……ッ……ッ……ッ……。\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9288,
            "text_len": 50,
            "text_offset": 9298,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "はっ！？　しまった！\\n混乱して我を忘れて弌弌！！\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 9374,
            "text_len": 24,
            "text_offset": 9378,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッ！　ッ！　ッ！　ッ！\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9424,
            "text_len": 58,
            "text_offset": 9434,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "待て！　誤解だ！　今の無し！\\n落ち着け！　落ち着くんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 9518,
            "text_len": 28,
            "text_offset": 9522,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "ッッッッッッッッッッッ！！\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9666,
            "text_len": 22,
            "text_offset": 9676,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ぎゃああああ――！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9774,
            "text_len": 66,
            "text_offset": 9784,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……目を合わせない。\\n　そうしたら動物は襲ってこないはず……）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9886,
            "text_len": 70,
            "text_offset": 9896,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（き、気づいて無いフリをするんだ。\\n　冷静に、そしてごく自然に……）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9994,
            "text_len": 68,
            "text_offset": 10004,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "さ、さ―てゴミの片付けも終わったし、\\n職員室に戻らないといかんな！\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10094,
            "text_len": 82,
            "text_offset": 10104,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "特に怪しい者はいないし、絶対にいないし、\\n動物なんているはずもないし見えないし。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 10208,
            "text_len": 16,
            "text_offset": 10212,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "………………？\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10258,
            "text_len": 72,
            "text_offset": 10268,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（……止まった。それに鼻息も落ち着いたぞ。\\n　とりあえず今のうち弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10376,
            "text_len": 74,
            "text_offset": 10386,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "きょ、今日のおやつは何にしよう？\\nき、昨日は大福だったから、今日は……。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10482,
            "text_len": 58,
            "text_offset": 10492,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "よし、職員室に帰りながら決めよう。\\nそうだ、それがいい。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 10576,
            "text_len": 20,
            "text_offset": 10580,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10626,
            "text_len": 80,
            "text_offset": 10636,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（よし！　いける！　今なら逃げられる！\\n　ゆっくりと……けれど速やかに弌弌！）\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10744,
            "text_len": 60,
            "text_offset": 10754,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……おや？　教頭先生じゃないですか。\\n落ち葉の焼却ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10852,
            "text_len": 34,
            "text_offset": 10862,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "こ、校長……！？　なぜここに！？\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10932,
            "text_len": 62,
            "text_offset": 10942,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "私も落ち葉を焼きに来たんですよ。\\nこれもお願いできますかな？\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 11034,
            "text_len": 32,
            "text_offset": 11038,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……ッ……ッ……ッ……ッ……！\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11092,
            "text_len": 62,
            "text_offset": 11102,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "弌弌ん？　……おまえは！！\\nまさか、前に倒したアイツの……！\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 11188,
            "text_len": 34,
            "text_offset": 11198,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "（今までの苦労が台無しだ――！）\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 11258,
            "text_len": 32,
            "text_offset": 11262,
            "warnings": 0
        },
        "speaker": "鹿",
        "text": "……ッ……ッ……ッ……ッ……！\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 11420,
            "text_len": 26,
            "text_offset": 11440,
            "warnings": 0
        },
        "speaker": "校長先生＆教頭先生",
        "text": "ぎゃあああああああ！！！\\n",
        "type": "dialog"
    }
]