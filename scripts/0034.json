[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2146,
            "text_len": 40,
            "text_offset": 2152,
            "warnings": 1
        },
        "speaker": "大工",
        "text": "いや―、囲碁サッカ―って中楽しいな。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2216,
            "text_len": 60,
            "text_offset": 2224,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そうでしょう？\\nこれほど奥が深いスポ―ツ他にありませんよ！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2318,
            "text_len": 12,
            "text_offset": 2324,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2362,
            "text_len": 78,
            "text_offset": 2368,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや―、でも今日の練習は残念だった。\\n盛り上がってきた所で下校時間なんてな。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2468,
            "text_len": 68,
            "text_offset": 2476,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "オレも高崎先生に教わったこと、\\nもっと試したかったんですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2566,
            "text_len": 38,
            "text_offset": 2572,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お、桜井くん、あそこに公園があるぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2632,
            "text_len": 16,
            "text_offset": 2640,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……やります？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2678,
            "text_len": 26,
            "text_offset": 2684,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ！\\n関口もやるよな？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2734,
            "text_len": 16,
            "text_offset": 2740,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "私はいいです。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2788,
            "text_len": 42,
            "text_offset": 2794,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そっか……じゃあ関口はマネ―ジャ―だな！\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2858,
            "text_len": 16,
            "text_offset": 2864,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "それも嫌です。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2912,
            "text_len": 70,
            "text_offset": 2918,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "も―、そういうこと言うなって。\\nとりあえずオレたちは練習してくから！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3112,
            "text_len": 44,
            "text_offset": 3120,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "とりあえずリフティングからやりましょうか。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3284,
            "text_len": 42,
            "text_offset": 3290,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よっ、は……っ！\\nここで碁石を……っと！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3348,
            "text_len": 32,
            "text_offset": 3356,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "よしっ、ここで高く上げて……！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3408,
            "text_len": 12,
            "text_offset": 3414,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3446,
            "text_len": 20,
            "text_offset": 3452,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ここでシュ―ト！！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3522,
            "text_len": 6,
            "text_offset": 3528,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3554,
            "text_len": 18,
            "text_offset": 3562,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あっ、危ない！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3630,
            "text_len": 24,
            "text_offset": 3636,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "だ、誰か、当たった！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3770,
            "text_len": 28,
            "text_offset": 3778,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "うわっ、校長先生ですよ！？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3822,
            "text_len": 24,
            "text_offset": 3828,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "だ、大丈夫ですか……？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3934,
            "text_len": 24,
            "text_offset": 3938,
            "warnings": 1
        },
        "speaker": "",
        "text": "アンテナが折れた―！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3986,
            "text_len": 42,
            "text_offset": 3990,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそっ、あの球体はこの星の兵器なのか！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4054,
            "text_len": 62,
            "text_offset": 4058,
            "warnings": 1
        },
        "speaker": "",
        "text": "いや、誰がどう見てもサッカ―ボ―ルだろ！\\nただの遊び道具だ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4142,
            "text_len": 56,
            "text_offset": 4146,
            "warnings": 1
        },
        "speaker": "",
        "text": "そんな、じゃあアンテナの\\n強度に問題があったとでも！？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4224,
            "text_len": 22,
            "text_offset": 4228,
            "warnings": 1
        },
        "speaker": "",
        "text": "どう考えてもそうだ！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4272,
            "text_len": 58,
            "text_offset": 4276,
            "warnings": 1
        },
        "speaker": "",
        "text": "なんせ、ザ・ズボラと呼ばれる俺が\\n組み立てたんだからな！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4380,
            "text_len": 32,
            "text_offset": 4384,
            "warnings": 1
        },
        "speaker": "",
        "text": "（そんなヤツに任せるなよ……）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4710,
            "text_len": 34,
            "text_offset": 4716,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ふぅ……これくらいにしとこうか。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4774,
            "text_len": 14,
            "text_offset": 4782,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そうですね。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4826,
            "text_len": 20,
            "text_offset": 4832,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……お疲れ様です。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4868,
            "text_len": 32,
            "text_offset": 4874,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "何だ関口、待っててくれたのか？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4932,
            "text_len": 10,
            "text_offset": 4938,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "これを。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4972,
            "text_len": 50,
            "text_offset": 4980,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、タオルとジュ―スだ。\\nありがとうございます。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5062,
            "text_len": 18,
            "text_offset": 5068,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長も、どうぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5110,
            "text_len": 34,
            "text_offset": 5116,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "なんだ、用意いいじゃないか関口。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5172,
            "text_len": 28,
            "text_offset": 5178,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "待ってる間、暇だったので。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5232,
            "text_len": 12,
            "text_offset": 5238,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5272,
            "text_len": 14,
            "text_offset": 5278,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "なあ、関口。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5344,
            "text_len": 40,
            "text_offset": 5350,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "やっぱりお前、マネ―ジャ―向いてるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5412,
            "text_len": 24,
            "text_offset": 5418,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……言うと思いました。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5464,
            "text_len": 50,
            "text_offset": 5470,
            "warnings": 1
        },
        "speaker": "大工",
        "text": "ってことはつまり、\\n実はやる気満だったんだな？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5542,
            "text_len": 26,
            "text_offset": 5548,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、やる気は無いです。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5596,
            "text_len": 34,
            "text_offset": 5602,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "なんだよそれ―、変なヤツだな―。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5660,
            "text_len": 20,
            "text_offset": 5668,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……そうだ、部長！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5710,
            "text_len": 20,
            "text_offset": 5716,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "どうした桜井くん。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5758,
            "text_len": 50,
            "text_offset": 5766,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "だったら、これはどうですか？\\n……ごにょごにょ。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5842,
            "text_len": 24,
            "text_offset": 5848,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……なるほど、それだ！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5902,
            "text_len": 44,
            "text_offset": 5908,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "嫌な予感がするので先に断ってもいいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5974,
            "text_len": 52,
            "text_offset": 5980,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いいから聞け。\\n関口はマネ―ジャ―が嫌なんだよな？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6054,
            "text_len": 40,
            "text_offset": 6060,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いえ、マネ―ジャ―がと言うわけでは弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6130,
            "text_len": 52,
            "text_offset": 6136,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "今日からお前は\\n時定高校囲碁サッカ―部の監督だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6220,
            "text_len": 8,
            "text_offset": 6226,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "監督？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6256,
            "text_len": 76,
            "text_offset": 6262,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そう！　高崎先生はあくまで臨時顧問！\\nつまりうちの部には監督が居ないんだ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6362,
            "text_len": 62,
            "text_offset": 6370,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そこで関口さんが監督になってくれれば\\n全部解決するんですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6460,
            "text_len": 30,
            "text_offset": 6466,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "全然解決してないような……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6520,
            "text_len": 68,
            "text_offset": 6526,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おいおい何言ってんだよ監督！\\n明日、就任の挨拶をしてもらうからな！\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6618,
            "text_len": 38,
            "text_offset": 6626,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "これで必要な役職が全部揃いましたね！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6686,
            "text_len": 32,
            "text_offset": 6692,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、これでうちの部も安泰だ！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6832,
            "text_len": 12,
            "text_offset": 6838,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6908,
            "text_len": 34,
            "text_offset": 6914,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "やっぱマネ―ジャ―に向いてるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6970,
            "text_len": 18,
            "text_offset": 6976,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なりませんから。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7016,
            "text_len": 56,
            "text_offset": 7022,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "わかってる！\\nもうマネ―ジャ―になれとは言わないって。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7108,
            "text_len": 54,
            "text_offset": 7114,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "その代わり、\\n俺にマネ―ジャ―のやり方を教えてくれ！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7206,
            "text_len": 6,
            "text_offset": 7212,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7246,
            "text_len": 12,
            "text_offset": 7254,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "えぇっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7292,
            "text_len": 36,
            "text_offset": 7298,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "部長兼マネ―ジャ―とか凄くないか？\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7356,
            "text_len": 80,
            "text_offset": 7362,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ついでに囲碁サッカ―自体もちょっとやるから\\nなんとプレイングマネ―ジャ―だぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7476,
            "text_len": 70,
            "text_offset": 7484,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え、え―っと……プレイングマネ―ジャ―って\\nそういう意味でしたっけ？\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7580,
            "text_len": 12,
            "text_offset": 7586,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7624,
            "text_len": 52,
            "text_offset": 7630,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そうだろ？\\nだから関口、俺にマネ―ジャ―道を……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7704,
            "text_len": 16,
            "text_offset": 7710,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "私がやります。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7748,
            "text_len": 28,
            "text_offset": 7754,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、関口今やらないって。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7804,
            "text_len": 20,
            "text_offset": 7810,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "私がやりますから。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7858,
            "text_len": 30,
            "text_offset": 7864,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そうか、ならお前に任せるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7916,
            "text_len": 62,
            "text_offset": 7922,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ―し、マネ―ジャ―も決まったことだし、\\nそろそろ帰ろうぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8088,
            "text_len": 12,
            "text_offset": 8094,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……はぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8122,
            "text_len": 20,
            "text_offset": 8130,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……お疲れ様です。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8212,
            "text_len": 42,
            "text_offset": 8218,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "やっぱりマネ―ジャ―、やってくれないか？\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8288,
            "text_len": 14,
            "text_offset": 8294,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "無理ですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8330,
            "text_len": 20,
            "text_offset": 8336,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そう言わずにさ―。\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8378,
            "text_len": 22,
            "text_offset": 8384,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そう言われても……。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8428,
            "text_len": 40,
            "text_offset": 8434,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "めんどくさい仕事押し付けないからさ―。\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8496,
            "text_len": 64,
            "text_offset": 8502,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "めんどくさいからってわけでは\\nないんですが……やはりちょっと。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8588,
            "text_len": 14,
            "text_offset": 8594,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "どうしても？\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8630,
            "text_len": 18,
            "text_offset": 8636,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "どうしてもです。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8676,
            "text_len": 50,
            "text_offset": 8682,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "なってくれたら\\n言うこと何でも聞くって言っても？\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8758,
            "text_len": 12,
            "text_offset": 8764,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8808,
            "text_len": 14,
            "text_offset": 8814,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なりません。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8866,
            "text_len": 22,
            "text_offset": 8874,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（今一瞬悩んだ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9010,
            "text_len": 42,
            "text_offset": 9016,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それじゃしょうがないな！\\nさ、帰ろうぜ！\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9074,
            "text_len": 8,
            "text_offset": 9080,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9112,
            "text_len": 16,
            "text_offset": 9120,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "（いいの！？）\\n",
        "type": "dialog"
    }
]