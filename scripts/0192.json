[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2098,
            "text_len": 16,
            "text_offset": 2106,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こんにちは―。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2146,
            "text_len": 20,
            "text_offset": 2156,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "お―、桜井来たか。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2200,
            "text_len": 80,
            "text_offset": 2206,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "今片付けるからちょっと待っててくれよ。\\n丁度オレの勝ちで終わったとこなんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2308,
            "text_len": 24,
            "text_offset": 2316,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はいっ、わかりました！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2364,
            "text_len": 32,
            "text_offset": 2374,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "くそっ、次は勝ってやるからな！\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2430,
            "text_len": 44,
            "text_offset": 2436,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そういや先生、今日は何の練習するんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2502,
            "text_len": 66,
            "text_offset": 2512,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうだな……そろそろ大工にも\\n団体戦の簡単な技を教えないとな―。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2600,
            "text_len": 30,
            "text_offset": 2606,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "やっと技か……長かったなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2662,
            "text_len": 44,
            "text_offset": 2670,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……とうとう部長も技の練習に入るんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2736,
            "text_len": 80,
            "text_offset": 2744,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "囲碁サッカ―部がちゃんと動きだしたって\\n感じで……オレ、ちょっと感動しました。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2846,
            "text_len": 68,
            "text_offset": 2854,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "このまま行けば部長も試合に……\\nいや、全国出場も夢じゃないですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2946,
            "text_len": 42,
            "text_offset": 2956,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "そうなったらオレは名監督か……悪くない！\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3022,
            "text_len": 34,
            "text_offset": 3028,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "はははっ、まあ気楽に頑張るかな。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3084,
            "text_len": 28,
            "text_offset": 3092,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はいっ、がんばりましょう！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3146,
            "text_len": 22,
            "text_offset": 3152,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……あの、桜井くん。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3196,
            "text_len": 14,
            "text_offset": 3204,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3244,
            "text_len": 36,
            "text_offset": 3250,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "団体戦って、二人でいけるんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3314,
            "text_len": 28,
            "text_offset": 3322,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "…………あああぁぁぁっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3380,
            "text_len": 40,
            "text_offset": 3388,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "団体戦二人じゃ出られないじゃないか！！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3454,
            "text_len": 34,
            "text_offset": 3464,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "桜井、お前気づいてなかったのか？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3522,
            "text_len": 50,
            "text_offset": 3528,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "てっきり桜井くんが\\n部員を見つけてくるもんだと。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3600,
            "text_len": 32,
            "text_offset": 3608,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "……試合直前に部員探しなんて。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3748,
            "text_len": 60,
            "text_offset": 3756,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "でも、でもオレはこんなことで\\nくじけたりしませんからね！！\\n",
        "type": "dialog"
    }
]