[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2156,
            "text_len": 26,
            "text_offset": 2164,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（の、乗り切った……！）\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2216,
            "text_len": 64,
            "text_offset": 2224,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（モヒカンが幽霊になっていることは\\n　誰にも気付かれていない）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2314,
            "text_len": 64,
            "text_offset": 2322,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（とは言え油断はできない。\\n　安心するのはうちに帰ってからだ）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2412,
            "text_len": 58,
            "text_offset": 2420,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（このままダラダラとするより、\\n　早いとこうちに帰ろう）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2500,
            "text_len": 22,
            "text_offset": 2506,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あっ、お―い中之条。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2550,
            "text_len": 6,
            "text_offset": 2558,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2590,
            "text_len": 42,
            "text_offset": 2598,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "……な、何？\\n僕、急いでるんだけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2662,
            "text_len": 60,
            "text_offset": 2668,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "今日の夜、みんなで心霊スポットに\\n行くことになったんだ―。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2760,
            "text_len": 16,
            "text_offset": 2768,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "心霊スポット？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2806,
            "text_len": 32,
            "text_offset": 2812,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "おう。だからお前も来ないか―？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2870,
            "text_len": 64,
            "text_offset": 2878,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（何かと思ったらそんなこと……\\n　まあ気付かれるよりはいいか）\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2974,
            "text_len": 40,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "保護者同伴だから\\n親にも怒られないぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3042,
            "text_len": 30,
            "text_offset": 3050,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "保護者？\\n田中君のお父さん？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3102,
            "text_len": 16,
            "text_offset": 3108,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "い―や。校長。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3158,
            "text_len": 12,
            "text_offset": 3168,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3200,
            "text_len": 6,
            "text_offset": 3208,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3292,
            "text_len": 58,
            "text_offset": 3296,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、おい、いいのかよ、\\nそんな安請け合いしちまって……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3378,
            "text_len": 38,
            "text_offset": 3382,
            "warnings": 1
        },
        "speaker": "",
        "text": "いいって。\\nどうせ霊なんか出ないし。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3442,
            "text_len": 52,
            "text_offset": 3446,
            "warnings": 1
        },
        "speaker": "",
        "text": "そ、そうかな……\\nま、まあ、わかってるけどさ……。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3524,
            "text_len": 12,
            "text_offset": 3528,
            "warnings": 1
        },
        "speaker": "",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3562,
            "text_len": 10,
            "text_offset": 3566,
            "warnings": 1
        },
        "speaker": "",
        "text": "ふ―ん？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3598,
            "text_len": 16,
            "text_offset": 3602,
            "warnings": 1
        },
        "speaker": "",
        "text": "な、なんだよ？\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3640,
            "text_len": 52,
            "text_offset": 3644,
            "warnings": 1
        },
        "speaker": "",
        "text": "別に？　……あ、そうだ！\\nこれも中継してやろうぜ。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3718,
            "text_len": 10,
            "text_offset": 3722,
            "warnings": 1
        },
        "speaker": "",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3754,
            "text_len": 52,
            "text_offset": 3758,
            "warnings": 1
        },
        "speaker": "",
        "text": "怪談とか苦手なやつも、強制参加！\\n面白そうじゃね？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3832,
            "text_len": 16,
            "text_offset": 3836,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ、ああ……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3896,
            "text_len": 10,
            "text_offset": 3904,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "えっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3964,
            "text_len": 12,
            "text_offset": 3976,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4034,
            "text_len": 6,
            "text_offset": 4042,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4140,
            "text_len": 54,
            "text_offset": 4148,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "悪いけど、興味ないよ。\\n僕は幽霊って信じてないんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4224,
            "text_len": 52,
            "text_offset": 4230,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "何だよ、もしかしたら\\n本当にいるかもしれないだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4304,
            "text_len": 60,
            "text_offset": 4312,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "いないよ。この世のものは全て\\n科学で説明できるんだからね。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4408,
            "text_len": 68,
            "text_offset": 4416,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（とは言え、今の僕の髪の毛を\\n　科学で説明できるのだろうか……？）\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4510,
            "text_len": 72,
            "text_offset": 4518,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（ダ、ダメだ。科学を疑うんじゃない。\\n　科学の力なら何でも解明できる）\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4612,
            "text_len": 42,
            "text_offset": 4620,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "そういうわけだから、僕は遠慮しておくよ。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4690,
            "text_len": 84,
            "text_offset": 4698,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（それに、実は……ちょっと怖いんだよね。\\n　いないんだけど、雰囲気的なものが……）\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4818,
            "text_len": 22,
            "text_offset": 4826,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（僕の肩に手が！？）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4928,
            "text_len": 38,
            "text_offset": 4936,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（だ、誰だ！？　ま、まさか幽霊！？）\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5004,
            "text_len": 60,
            "text_offset": 5012,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（振り返るな！　こういうとき、\\n　振り返ったら呪われる！）\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5106,
            "text_len": 20,
            "text_offset": 5114,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うおおおおおお！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5180,
            "text_len": 6,
            "text_offset": 5186,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5244,
            "text_len": 14,
            "text_offset": 5254,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………！？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5304,
            "text_len": 12,
            "text_offset": 5310,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5344,
            "text_len": 18,
            "text_offset": 5354,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え……えっ……？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5476,
            "text_len": 12,
            "text_offset": 5486,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "？？？？？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5572,
            "text_len": 14,
            "text_offset": 5580,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "うわああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5620,
            "text_len": 12,
            "text_offset": 5626,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5668,
            "text_len": 16,
            "text_offset": 5678,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？　えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5720,
            "text_len": 22,
            "text_offset": 5728,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "あ……桜井先生……？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5782,
            "text_len": 56,
            "text_offset": 5792,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの、中之条くんのノ―ト……\\n返し忘れたので……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5874,
            "text_len": 12,
            "text_offset": 5882,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5934,
            "text_len": 16,
            "text_offset": 5940,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………プッ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6064,
            "text_len": 6,
            "text_offset": 6072,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6158,
            "text_len": 12,
            "text_offset": 6166,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6206,
            "text_len": 34,
            "text_offset": 6216,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "中之条くん、ちょっといいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6282,
            "text_len": 12,
            "text_offset": 6290,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6340,
            "text_len": 18,
            "text_offset": 6350,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……中之条くん？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6396,
            "text_len": 12,
            "text_offset": 6402,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6440,
            "text_len": 12,
            "text_offset": 6448,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6492,
            "text_len": 12,
            "text_offset": 6498,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "センセ―。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6532,
            "text_len": 32,
            "text_offset": 6542,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、は、はい。何ですか田中君。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6678,
            "text_len": 26,
            "text_offset": 6684,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "中之条、気絶してま―す。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6730,
            "text_len": 6,
            "text_offset": 6740,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    }
