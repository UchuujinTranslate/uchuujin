[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2148,
            "text_len": 48,
            "text_offset": 2154,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ―し、練習開始だ！\\nまずはリフティングから！\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2228,
            "text_len": 10,
            "text_offset": 2236,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2272,
            "text_len": 52,
            "text_offset": 2278,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（部長の方が初心者なのに。\\n　なんか不思議な感じ）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2454,
            "text_len": 40,
            "text_offset": 2460,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おっと！\\n悪い関口、ボ―ル取ってくれ。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2524,
            "text_len": 8,
            "text_offset": 2530,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2560,
            "text_len": 14,
            "text_offset": 2566,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "サンキュ―。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2688,
            "text_len": 86,
            "text_offset": 2694,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（ううん初心者なのによりも、むしろ部長が\\n　真面目に練習しているのが不思議かも……）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2796,
            "text_len": 32,
            "text_offset": 2802,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "なあ桜井くん、ちょっといいか？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2850,
            "text_len": 18,
            "text_offset": 2858,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はい、何ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2908,
            "text_len": 64,
            "text_offset": 2914,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "碁石を出すタイミングがつかめなくてさ。\\n何かコツとかあるかい？\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3000,
            "text_len": 24,
            "text_offset": 3008,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "あ、それはですね……。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3066,
            "text_len": 72,
            "text_offset": 3074,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "こうです。こうやって高さを保ちながら\\n胸ポケットから碁石を出して……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3250,
            "text_len": 22,
            "text_offset": 3256,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "こうして……こうか！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3294,
            "text_len": 30,
            "text_offset": 3302,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "そうです、そうしてそこから！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3372,
            "text_len": 20,
            "text_offset": 3382,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "一人ジャ―マン！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 3436,
            "text_len": 6,
            "text_offset": 3456,
            "warnings": 0
        },
        "speaker": "大工＆関口＆桜井誠",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3576,
            "text_len": 16,
            "text_offset": 3586,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3688,
            "text_len": 14,
            "text_offset": 3692,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ――っ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3748,
            "text_len": 62,
            "text_offset": 3752,
            "warnings": 1
        },
        "speaker": "",
        "text": "そりゃそうだよ！\\nあんな勢いで頭からじゃダメに決まってるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3830,
            "text_len": 68,
            "text_offset": 3834,
            "warnings": 1
        },
        "speaker": "",
        "text": "すまん、ついオレが囲碁サッカ―に\\nハマってしまったばっかりに……。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3922,
            "text_len": 56,
            "text_offset": 3926,
            "warnings": 1
        },
        "speaker": "",
        "text": "最近、オレの碁石が減っていたのは、\\nお前のせいか―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4086,
            "text_len": 60,
            "text_offset": 4092,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（こんな風に真面目な顔してると、\\n　格好いいんだけど……）\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4178,
            "text_len": 84,
            "text_offset": 4184,
            "warnings": 1
        },
        "speaker": "関口",
        "text": "（いつも無茶苦茶なことばかりするから。\\n　今日だって朝から色大変な目にあったし）\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4294,
            "text_len": 62,
            "text_offset": 4300,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（……そういえば、\\n　今日は授業が無い時はずっと一緒に居る）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4388,
            "text_len": 66,
            "text_offset": 4394,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（弌弌触れなかったり、\\n　お弁当褒められて嬉しかったりしたっけ）\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4486,
            "text_len": 38,
            "text_offset": 4492,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（うん。やっぱり私、部長のこと弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4566,
            "text_len": 14,
            "text_offset": 4572,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "お―い関口。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4612,
            "text_len": 10,
            "text_offset": 4618,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "っ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 12,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "大丈夫か？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4698,
            "text_len": 20,
            "text_offset": 4704,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ど、どうしました？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4748,
            "text_len": 74,
            "text_offset": 4754,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、ボ―ルがそっち行ったから\\n取ってくれって言おうと思ったんだけどさ。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4850,
            "text_len": 20,
            "text_offset": 4856,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ……いつの間に。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4898,
            "text_len": 56,
            "text_offset": 4904,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "結構前からだけど、大丈夫か？\\n何か顔も赤いみたいだし。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4982,
            "text_len": 20,
            "text_offset": 4988,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なんでもないです。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5030,
            "text_len": 12,
            "text_offset": 5036,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん―……。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5094,
            "text_len": 12,
            "text_offset": 5100,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！！！！！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5184,
            "text_len": 26,
            "text_offset": 5190,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（ぶ、部長の顔が……！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5266,
            "text_len": 34,
            "text_offset": 5272,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うおっ！？　凄い熱だぞ、関口！！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5328,
            "text_len": 10,
            "text_offset": 5334,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え……？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5366,
            "text_len": 80,
            "text_offset": 5372,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "何でこんなになるまでほっといたんだ！\\n桜井くん！　保健の先生呼んできてくれ！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5476,
            "text_len": 10,
            "text_offset": 5484,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5518,
            "text_len": 26,
            "text_offset": 5524,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、あの……私なんとも。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5576,
            "text_len": 20,
            "text_offset": 5582,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ないわけないだろ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5624,
            "text_len": 12,
            "text_offset": 5630,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "弌弌っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5668,
            "text_len": 56,
            "text_offset": 5674,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うおぉっ、また熱が！\\n保健の先生じゃない、救急車だ！！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5770,
            "text_len": 16,
            "text_offset": 5776,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（えぇっ！？）\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5820,
            "text_len": 34,
            "text_offset": 5826,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長、だから本当になんとも……。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5882,
            "text_len": 34,
            "text_offset": 5888,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いいか？　無理しなくていいんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5944,
            "text_len": 72,
            "text_offset": 5950,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ほら、そこで大人しくしとけ！\\n救急車呼ぶついでに冷やすもんもってくる！\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6126,
            "text_len": 18,
            "text_offset": 6132,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……どうしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6212,
            "text_len": 34,
            "text_offset": 6218,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "結構熱あるような……ないような？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6274,
            "text_len": 20,
            "text_offset": 6280,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ど、どっちですか？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6332,
            "text_len": 18,
            "text_offset": 6338,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（顔、近い……）\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6382,
            "text_len": 72,
            "text_offset": 6388,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いまいちわかんないんだよなぁ。\\n何か熱上がってきてるような気もするし。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6486,
            "text_len": 20,
            "text_offset": 6492,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "一応保健室行くか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6534,
            "text_len": 18,
            "text_offset": 6540,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "多分大丈夫です。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6584,
            "text_len": 10,
            "text_offset": 6590,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "本当に？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6622,
            "text_len": 14,
            "text_offset": 6628,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "大丈夫です。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6668,
            "text_len": 64,
            "text_offset": 6674,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや、一応熱計ったほうがいいな。\\n保健室から借りてきてやるよ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6770,
            "text_len": 28,
            "text_offset": 6776,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……部長があんなに近くに。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6830,
            "text_len": 22,
            "text_offset": 6836,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（ドキドキした……）\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6880,
            "text_len": 74,
            "text_offset": 6886,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "体温計取ってきてくれるらしいから、\\nこのまま桜井くんと待っておかないと。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6986,
            "text_len": 16,
            "text_offset": 6992,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……桜井くん？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7044,
            "text_len": 14,
            "text_offset": 7050,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……ッ！！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7172,
            "text_len": 46,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "え―っと、なんていうか……その。\\nごめんね？\\n",
