[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2138,
            "text_len": 14,
            "text_offset": 2146,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ｙｙＹ……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2188,
            "text_len": 26,
            "text_offset": 2196,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん…………ふぁあ〜……。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2254,
            "text_len": 28,
            "text_offset": 2262,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん〜……\\nよく寝た〜……。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2314,
            "text_len": 24,
            "text_offset": 2320,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はかせ―、ただいま―。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2366,
            "text_len": 54,
            "text_offset": 2374,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あれ？　なの帰ってきた。\\nもうそんな時間だったんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2450,
            "text_len": 48,
            "text_offset": 2456,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ただいまはかせ。\\nあっ、お昼寝してたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2534,
            "text_len": 12,
            "text_offset": 2542,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2576,
            "text_len": 30,
            "text_offset": 2582,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……ん？　どうかしましたか？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2634,
            "text_len": 46,
            "text_offset": 2642,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ううん、ネジがあったら\\n可愛いと思っただけ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2710,
            "text_len": 28,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "いや、それは無いですから。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2766,
            "text_len": 68,
            "text_offset": 2774,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ぷぷぷ。阪本と同じこと言ってる。\\nでもちゃんとデ―タ取ったもんね。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2864,
            "text_len": 28,
            "text_offset": 2870,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "デ―タ？　何のことですか？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2946,
            "text_len": 12,
            "text_offset": 2954,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2996,
            "text_len": 80,
            "text_offset": 3002,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ！　はかせ、犬拾ってきたんですか！？\\nダメですよ、うちには阪本さんが……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3110,
            "text_len": 50,
            "text_offset": 3118,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "わっ！？　い、犬だ！？\\nなの―！　助けて――！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3192,
            "text_len": 48,
            "text_offset": 3198,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ？　え？　あ、はい。\\n……違ったんですか。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3322,
            "text_len": 54,
            "text_offset": 3326,
            "warnings": 1
        },
        "speaker": "",
        "text": "あ―……抱きかかえられたら、\\nアンテナ折れちまった。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3422,
            "text_len": 56,
            "text_offset": 3426,
            "warnings": 1
        },
        "speaker": "",
        "text": "ま、こんなもんさ。\\n今回の選びかたは失敗だったんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3498,
            "text_len": 46,
            "text_offset": 3502,
            "warnings": 1
        },
        "speaker": "",
        "text": "……だな。\\nやっぱり、ル―レットは難しいな！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3564,
            "text_len": 8,
            "text_offset": 3568,
            "warnings": 1
        },
        "speaker": "",
        "text": "だな！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3668,
            "text_len": 48,
            "text_offset": 3676,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "なのはネジを可愛くないって\\n思ってるんだよね？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3746,
            "text_len": 72,
            "text_offset": 3752,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "当たり前です。こんなのがついていて\\n可愛いと思う人ははかせくらいです。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3846,
            "text_len": 66,
            "text_offset": 3854,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "残念でした―！　はかせ以外にも\\n可愛いって言った人いたもんね―！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3946,
            "text_len": 26,
            "text_offset": 3952,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっ！？　いたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4000,
            "text_len": 14,
            "text_offset": 4008,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "いたんです。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4044,
            "text_len": 42,
            "text_offset": 4050,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "と言うか、わざわざ聞きに行ったんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4114,
            "text_len": 60,
            "text_offset": 4122,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "行ったんです。\\nそしてはかせが正しいことが証明されました！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4204,
            "text_len": 18,
            "text_offset": 4210,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "う、う―ん……。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4250,
            "text_len": 44,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だからネジは可愛いから\\n自信持って、なの！\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4472,
            "text_len": 6,
            "text_offset": 4478,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4502,
            "text_len": 46,
            "text_offset": 4510,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははは！　いっぱい出てきた！\\nあはははは！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4572,
            "text_len": 46,
            "text_offset": 4578,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ちょっ！？\\n何してくれてるんですかはかせ！！\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4640,
            "text_len": 42,
            "text_offset": 4648,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははは！　可愛いでしょ！\\nあはははは！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4710,
            "text_len": 40,
            "text_offset": 4716,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "こんなの可愛いわけないじゃないですか！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4802,
            "text_len": 68,
            "text_offset": 4810,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ぷぷぷ！　だからそれはなののセンスが\\nはかせたちより悪いからだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4904,
            "text_len": 72,
            "text_offset": 4910,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ネジ１つならそうかもしれませんけど！\\nこれを可愛いと思うのは異常です！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4998,
            "text_len": 34,
            "text_offset": 5006,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "異常じゃないよ！\\n可愛いじゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5056,
            "text_len": 52,
            "text_offset": 5062,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "可愛い可愛くないの次元を\\n超えちゃってるんですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5130,
            "text_len": 52,
            "text_offset": 5136,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "こんな姿で外出たら通報されますし、\\nそもそも……。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5208,
            "text_len": 52,
            "text_offset": 5214,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そもそも、こんなにネジがあったら\\n気持ち悪いです！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5370,
            "text_len": 6,
            "text_offset": 5378,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5426,
            "text_len": 72,
            "text_offset": 5434,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "そんなこと言わないで。はかせ、\\nなののためにいっぱい用意したんだから。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5528,
            "text_len": 60,
            "text_offset": 5534,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "その気持ちは嬉しいですけど\\nなんでネジを用意するんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5610,
            "text_len": 40,
            "text_offset": 5618,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "可愛いからだよ？\\nそれなのにダメなの？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5674,
            "text_len": 50,
            "text_offset": 5680,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "日頃からネジは嫌だと\\n言っているじゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5750,
            "text_len": 62,
            "text_offset": 5756,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "なのにこの仕打ちは嫌がらせ以外の\\nなにものでもありませんよ！\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5936,
            "text_len": 12,
            "text_offset": 5944,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5972,
            "text_len": 12,
            "text_offset": 5978,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6018,
            "text_len": 66,
            "text_offset": 6026,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "嫌がらせなんてひどいよ。\\nはかせ、なのに喜んでもらいたくて……。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6108,
            "text_len": 14,
            "text_offset": 6114,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はかせ……。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6154,
            "text_len": 24,
            "text_offset": 6160,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……想像してください。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6206,
            "text_len": 10,
            "text_offset": 6214,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "………？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6246,
            "text_len": 58,
            "text_offset": 6252,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ある日突然、体中からたくさんの\\nネジが生えてくる光景を！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6332,
            "text_len": 6,
            "text_offset": 6340,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6368,
            "text_len": 18,
            "text_offset": 6374,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "怖くないですか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6414,
            "text_len": 62,
            "text_offset": 6422,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あははははははは！　面白い！\\nはかせの腕からにゅってネジが！\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6510,
            "text_len": 6,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6636,
            "text_len": 14,
            "text_offset": 6642,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……異常だ。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6796,
            "text_len": 52,
            "text_offset": 6804,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "だってはかせ以外の人も\\nネジが可愛いって言ったよ？\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6884,
            "text_len": 62,
            "text_offset": 6890,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そうかもしれませんけど、\\nそれを聞いたのは今日なんですよね？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6974,
            "text_len": 8,
            "text_offset": 6982,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7012,
            "text_len": 58,
            "text_offset": 7018,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "でも私はそれより前に\\n改造されちゃってるじゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7098,
            "text_len": 8,
            "text_offset": 7106,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "うん。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7136,
            "text_len": 44,
            "text_offset": 7142,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "何の確信も無く\\n改造してるじゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
