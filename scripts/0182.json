[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2096,
            "text_len": 58,
            "text_offset": 2104,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "いや―、悪いね―、みおちゃん。\\nおごってもらっちゃって。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2188,
            "text_len": 16,
            "text_offset": 2194,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "今度返してね。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2232,
            "text_len": 58,
            "text_offset": 2238,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……と言いたいけど、\\n今日なんか元気なかったし。乂るよ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2318,
            "text_len": 26,
            "text_offset": 2326,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ありがとう、みおちゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2376,
            "text_len": 30,
            "text_offset": 2382,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ゆっこ。クラブサンド食べる？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2434,
            "text_len": 32,
            "text_offset": 2442,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "うん、ありがとう、麻衣ちゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2500,
            "text_len": 70,
            "text_offset": 2508,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（二人が優しくしてくれるのは、\\n　もしかしたら悪霊が消えたからかな）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2604,
            "text_len": 70,
            "text_offset": 2612,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（高い買い物しちゃって財布の中は\\n　スッカラカン。……でも、いいや）\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2708,
            "text_len": 76,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（私にはみおちゃんと麻衣ちゃんという\\n　大切な親友が二人もいるんだから！）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2822,
            "text_len": 62,
            "text_offset": 2830,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あと、この２つのお守り。\\n　これから一生身につけていよう）\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2928,
            "text_len": 22,
            "text_offset": 2934,
            "warnings": 0
        },
        "speaker": "玉村",
        "text": "いらっしゃいませ―。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2982,
            "text_len": 40,
            "text_offset": 2990,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（あっ！　あれはさっきのおばあさん！）\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3052,
            "text_len": 12,
            "text_offset": 3060,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ふぅ……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3098,
            "text_len": 34,
            "text_offset": 3106,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（……私には気付いてないみたい）\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3166,
            "text_len": 58,
            "text_offset": 3174,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（にしし、後ろから声かけて\\n　驚かせてあげようかな―？）\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3348,
            "text_len": 42,
            "text_offset": 3356,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "……はい。もしもし？\\nあ―、久しぶり―。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3418,
            "text_len": 76,
            "text_offset": 3426,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（友達かな？　きっと私にとっての\\n　みおちゃんと麻衣ちゃんみたいな人かも）\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3518,
            "text_len": 62,
            "text_offset": 3526,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "あははは、そうそう、聞いて聞いて。\\n今日、すごい儲かったの。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3604,
            "text_len": 78,
            "text_offset": 3612,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "突然、私、悪霊に取り亅かれてるんです！\\nなんて言ってくる変な学生がいてさぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3720,
            "text_len": 6,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3754,
            "text_len": 60,
            "text_offset": 3762,
            "warnings": 0
        },
        "speaker": "イタコ",
        "text": "ちょうどいいから適当にお守り渡して\\n儲けさせてもらった―。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3842,
            "text_len": 12,
            "text_offset": 3850,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3980,
            "text_len": 12,
            "text_offset": 3988,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4022,
            "text_len": 22,
            "text_offset": 4028,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "あはは、でさ―……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4154,
            "text_len": 36,
            "text_offset": 4160,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……あれ？　ゆっこ？　どうしたの？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4216,
            "text_len": 12,
            "text_offset": 4224,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4252,
            "text_len": 10,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ゆっこ？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4292,
            "text_len": 12,
            "text_offset": 4300,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "…………。\\n",
        "type": "dialog"
    }
]