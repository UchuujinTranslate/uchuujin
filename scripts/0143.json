[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 12,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……ふう。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2190,
            "text_len": 32,
            "text_offset": 2198,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "よっ、みさと、頑張ってたね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2252,
            "text_len": 62,
            "text_offset": 2260,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え？　あ、先輩！　お久しぶりです。\\n今日はどうしたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2344,
            "text_len": 52,
            "text_offset": 2352,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "いや〜、近くまで来たんで、\\nちょっと顔出そうかと。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2430,
            "text_len": 22,
            "text_offset": 2436,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "こ、こんにちは……。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2480,
            "text_len": 38,
            "text_offset": 2488,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "私、剣道部の練習って初めてみたよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2542,
            "text_len": 28,
            "text_offset": 2550,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あれ？　この子たちは……。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2604,
            "text_len": 74,
            "text_offset": 2612,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "あ〜ごめんね。これ、うちの妹とその友達。\\nちょうどそこで会っちゃってね。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2708,
            "text_len": 34,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "えっ！？　そうだったんですか！？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2772,
            "text_len": 82,
            "text_offset": 2780,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "うん。でね、剣道部に行くって言ったら、\\nどうしても見学したいってきかなくてさ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2892,
            "text_len": 52,
            "text_offset": 2898,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "言ってないよ！\\nお姉が無理矢理連れてきたんじゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2972,
            "text_len": 58,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "でも、しっかり剣道部の練習、\\n見学してたんですけどね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3064,
            "text_len": 70,
            "text_offset": 3072,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "それにしてもみさと、ちょっと見ないうちに\\nまた腕を上げたみたいね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3164,
            "text_len": 56,
            "text_offset": 3172,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そ、そんな……まだまだです。\\n先輩に比べたら全然……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3250,
            "text_len": 56,
            "text_offset": 3258,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "いやいやご謙遜〜。\\n前見た時とはキレが違うよ、キレが。\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3346,
            "text_len": 44,
            "text_offset": 3354,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "これはアレだね。\\nみさと、彼氏ができたね？\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3434,
            "text_len": 26,
            "text_offset": 3442,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "なっ！？　な、な、な弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3494,
            "text_len": 46,
            "text_offset": 3502,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "へ―、剣道って、\\nそうやって強くなるんだ―。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3570,
            "text_len": 20,
            "text_offset": 3576,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "んなわけないって。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3628,
            "text_len": 58,
            "text_offset": 3636,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "でも、みおちゃん。\\n想いは人を強くするとか言いますよ〜？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3720,
            "text_len": 42,
            "text_offset": 3728,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "で、では、私は練習があるので、これでっ！\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3792,
            "text_len": 28,
            "text_offset": 3800,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "あ―、うん、頑張ってね―。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3854,
            "text_len": 36,
            "text_offset": 3860,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "……練習終わったんじゃなかったの？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3918,
            "text_len": 68,
            "text_offset": 3926,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ま、せっかくですから。\\nまたこっそりと見学させていただきますか〜。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 30,
            "speaker_offset": 4114,
            "text_len": 10,
            "text_offset": 4146,
            "warnings": 0
        },
        "speaker": "女子剣道部員（女子剣道部員１）",
        "text": "始めっ！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4172,
            "text_len": 22,
            "text_offset": 4180,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "おりゃあああああ！！\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 30,
            "speaker_offset": 4218,
            "text_len": 24,
            "text_offset": 4250,
            "warnings": 0
        },
        "speaker": "女子剣道部員（女子剣道部員２）",
        "text": "ぎゃあああああああ！！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4290,
            "text_len": 10,
            "text_offset": 4298,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "キャン！\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4328,
            "text_len": 30,
            "text_offset": 4334,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "なにか巻き込まれた――っ！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4442,
            "text_len": 18,
            "text_offset": 4446,
            "warnings": 1
        },
        "speaker": "",
        "text": "……強かったな。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4488,
            "text_len": 44,
            "text_offset": 4492,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、強かったな。\\nぽっきりと折れる程に。\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4558,
            "text_len": 60,
            "text_offset": 4562,
            "warnings": 1
        },
        "speaker": "",
        "text": "体育館へ忍び込む案は、\\n犬では危険だった……ということか。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4644,
            "text_len": 32,
            "text_offset": 4648,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、危険だった。\\nそして弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4722,
            "text_len": 34,
            "text_offset": 4726,
            "warnings": 1
        },
        "speaker": "",
        "text": "俺たちの命にも危険だったようだ。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4862,
            "text_len": 56,
            "text_offset": 4870,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "ね、ね、みさとの彼ってどんなの？\\nやっぱ、剣道部の子？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4948,
            "text_len": 52,
            "text_offset": 4956,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ち、ちちち違います！\\nそんなんじゃないですってば！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5032,
            "text_len": 46,
            "text_offset": 5038,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "もう、お姉ってば〜、\\n先輩困ってるじゃない。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5106,
            "text_len": 16,
            "text_offset": 5114,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "演劇部ですよ！\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5156,
            "text_len": 12,
            "text_offset": 5162,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そっち！？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5198,
            "text_len": 48,
            "text_offset": 5206,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "へ―、そうなんだ―。\\nみおちゃんと一緒だね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5272,
            "text_len": 24,
            "text_offset": 5278,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "ちょっ！？　ゆっこ！！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5324,
            "text_len": 74,
            "text_offset": 5332,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "あ―、でも、先輩って体育会系だし、\\nメガネかけた男はきっとダメだよね―。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5442,
            "text_len": 26,
            "text_offset": 5450,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "メガネ、いいじゃない！！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5506,
            "text_len": 6,
            "text_offset": 5512,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5546,
            "text_len": 82,
            "text_offset": 5554,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え―、だって今時メガネなんて……ねぇ？\\nイケてないってゆ―か、なんてゆ―か……。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5662,
            "text_len": 12,
            "text_offset": 5670,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5756,
            "text_len": 12,
            "text_offset": 5762,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5900,
            "text_len": 12,
            "text_offset": 5906,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "へやっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5938,
            "text_len": 10,
            "text_offset": 5946,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "がっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6092,
            "text_len": 10,
            "text_offset": 6100,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "面っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6130,
            "text_len": 12,
            "text_offset": 6138,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ぶへっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6266,
            "text_len": 52,
            "text_offset": 6274,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ちょ、ちょっと、なんで！？\\n二人してヒドイよ―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6368,
            "text_len": 24,
            "text_offset": 6376,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "メガネ、いいじゃない！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6426,
            "text_len": 22,
            "text_offset": 6432,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "メガネ、いいじゃん！\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6574,
            "text_len": 12,
            "text_offset": 6580,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "へやっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6612,
            "text_len": 10,
            "text_offset": 6620,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "がっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6760,
            "text_len": 10,
            "text_offset": 6768,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "面っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6944,
            "text_len": 30,
            "text_offset": 6952,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あなたとは、話が合いそうね。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7004,
            "text_len": 26,
            "text_offset": 7010,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はい、私もそう思います。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7140,
            "text_len": 18,
            "text_offset": 7148,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "う……うう……。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7234,
            "text_len": 24,
            "text_offset": 7240,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "メガネ、いいじゃん！！\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7296,
            "text_len": 6,
            "text_offset": 7304,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7336,
            "text_len": 36,
            "text_offset": 7344,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え、な、なんでみおちゃんが怒るの？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7402,
            "text_len": 74,
            "text_offset": 7410,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……あ、そっか。ごめんごめん。\\nみおちゃんは笹原先輩が好きなんだもんね。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7526,
            "text_len": 32,
            "text_offset": 7532,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "バ、バ、バ、バ、バカやろう！！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7600,
            "text_len": 28,
            "text_offset": 7608,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "〜〜〜〜〜〜〜〜〜〜〜！？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7658,
            "text_len": 60,
            "text_offset": 7666,
            "warnings": 1
        },
        "speaker": "みお姉",
        "text": "みおの好きな人って笹原って言うんだ？\\nえ〜どんな人だろ〜\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7752,
            "text_len": 16,
            "text_offset": 7760,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……演劇部の。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7798,
            "text_len": 4,
            "text_offset": 7806,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7832,
            "text_len": 64,
            "text_offset": 7840,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "演劇部の部長です、笹原っての。\\n同じクラスなんで、知ってます。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7926,
            "text_len": 46,
            "text_offset": 7934,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "へ〜そうなんだ〜。\\nねえねえ、どんな人なの？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8002,
            "text_len": 68,
            "text_offset": 8010,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ど、どんなって……メガネかけてて……\\n人の迷惑考えないヤツで……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8100,
            "text_len": 38,
            "text_offset": 8108,
            "warnings": 1
        },
        "speaker": "みさと",
        "text": "でも……時は、優しいヤツ……です。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8170,
            "text_len": 10,
            "text_offset": 8176,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8218,
            "text_len": 50,
            "text_offset": 8224,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（やっぱり……\\n　この人も笹原先輩のことを！？）\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8316,
            "text_len": 12,
            "text_offset": 8324,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8358,
            "text_len": 58,
            "text_offset": 8364,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "立花先輩……でしたっけ。\\n私に剣道、教えてくれませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8444,
            "text_len": 50,
            "text_offset": 8452,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……いいよ。\\nじゃあ、ちょっと試合してみようか。\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8524,
            "text_len": 26,
            "text_offset": 8530,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "はい。\\n望むところです。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8586,
            "text_len": 32,
            "text_offset": 8594,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "え〜！？　ちょっ、ちょっと！？\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8648,
            "text_len": 64,
            "text_offset": 8656,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "この勝負に勝った方と、\\nその人の仲を取り持ってあげるからね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 8764,
            "text_len": 6,
            "text_offset": 8778,
            "warnings": 0
        },
        "speaker": "みお＆みさと",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8824,
            "text_len": 66,
            "text_offset": 8830,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（負けたら、笹原先輩は\\n　この人と……ハッピ―ウェディング！？）\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8922,
            "text_len": 72,
            "text_offset": 8930,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（で、でも、勝ったら私は笹原と……\\n　イチャイチャラブラブ生活が……）\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9114,
            "text_len": 12,
            "text_offset": 9128,
            "warnings": 0
        },
        "speaker": "みお＆みさと",
        "text": "ぶほっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9186,
            "text_len": 26,
            "text_offset": 9194,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "みおちゃん！？　先輩！？\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 9248,
            "text_len": 20,
            "text_offset": 9262,
            "warnings": 0
        },
        "speaker": "みお＆みさと",
        "text": "きゅうぅ…………。\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9304,
            "text_len": 12,
            "text_offset": 9312,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "ん―……。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9428,
            "text_len": 22,
            "text_offset": 9436,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "この勝負、引き分け！\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9522,
            "text_len": 52,
            "text_offset": 9530,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "まあまあ、メガネでも演劇部でも\\nなんでもいいよ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9604,
            "text_len": 58,
            "text_offset": 9612,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "実は剣道部のみんなにと思って、\\nケ―キ作ってきたんだ〜。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9704,
            "text_len": 12,
            "text_offset": 9712,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ケ―キ！？\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9752,
            "text_len": 30,
            "text_offset": 9760,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "でも、事情が変わったからね。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9812,
            "text_len": 64,
            "text_offset": 9820,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "みさとに彼ができちゃった\\n記念ケ―キってことにしちゃおうね〜。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9912,
            "text_len": 46,
            "text_offset": 9920,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "だ、だ、だ、\\n誰ができちゃった婚ですかっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10004,
            "text_len": 28,
            "text_offset": 10010,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そこまで言ってないって！！\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10068,
            "text_len": 48,
            "text_offset": 10076,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "あははは。\\nんじゃ、ウェディングケ―キ登場〜！\\n",
        "type": "dialog"
    },
    {
        "id": 97,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10160,
            "text_len": 48,
            "text_offset": 10168,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（こ、これは……\\n　どう見てもマズそう……！）\\n",
        "type": "dialog"
    },
    {
        "id": 98,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10246,
            "text_len": 66,
            "text_offset": 10254,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（でも、あこがれの先輩のケ―キだし、\\n　私からは言いづらい……）\\n",
        "type": "dialog"
    },
    {
        "id": 99,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10340,
            "text_len": 72,
            "text_offset": 10346,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ちょっと、誰かはっきり言ってやってよ！\\n　こんなもん食えるかって！）\\n",
        "type": "dialog"
    },
    {
        "id": 100,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10440,
            "text_len": 34,
            "text_offset": 10448,
            "warnings": 0
        },
        "speaker": "みお姉",
        "text": "ささ、遠慮しないで食べて食べて！\\n",
        "type": "dialog"
    },
    {
        "id": 101,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10508,
            "text_len": 58,
            "text_offset": 10516,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "（誰も何も言わない……もしかしてこれ、\\n　美味しいの？）\\n",
        "type": "dialog"
    },
    {
        "id": 102,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10594,
            "text_len": 66,
            "text_offset": 10602,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（よし、覚悟を決めたわ。\\n　私、先輩のケ―キでなら……死ねる！）\\n",
        "type": "dialog"
    },
    {
        "id": 103,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10688,
            "text_len": 72,
            "text_offset": 10696,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "（たとえどんな味だろうと、先輩には\\n　美味しいって言ってみせるわ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 104,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10788,
            "text_len": 56,
            "text_offset": 10794,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（ちょ……二人ともなんで\\n　食べる気まんまんなの！？）\\n",
        "type": "dialog"
    },
    {
        "id": 105,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10870,
            "text_len": 56,
            "text_offset": 10876,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "（これじゃ私だけ食べないって\\n　言いづらいじゃない！）\\n",
        "type": "dialog"
    },
    {
        "id": 106,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 10956,
            "text_len": 16,
            "text_offset": 10964,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "それでは……。\\n",
        "type": "dialog"
    },
    {
        "id": 107,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11004,
            "text_len": 16,
            "text_offset": 11012,
            "warnings": 1
        },
        "speaker": "ゆっこ",
        "text": "えんりょなく\\n",
        "type": "dialog"
    },
    {
        "id": 108,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 11058,
            "text_len": 24,
            "text_offset": 11064,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "い、いただきます……。\\n",
        "type": "dialog"
    },
    {
        "id": 109,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11120,
            "text_len": 14,
            "text_offset": 11128,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "……苦っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 110,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11178,
            "text_len": 62,
            "text_offset": 11186,
            "warnings": 1
        },
        "speaker": "みお姉",
        "text": "え、ウソ〜！　ケ―キはあまいものだよ〜。\\n恋愛と同じで、ね\\n",
        "type": "dialog"
    },
    {
        "id": 111,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 11282,
            "text_len": 38,
            "text_offset": 11288,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "苦いよ！　なんか失恋のほろ苦さだよ！\\n",
        "type": "dialog"
    },
    {
        "id": 112,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11350,
            "text_len": 30,
            "text_offset": 11358,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……せ、先輩、美味しいです！\\n",
        "type": "dialog"
    },
    {
        "id": 113,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11404,
            "text_len": 18,
            "text_offset": 11412,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "ええっ、うそ！？\\n",
        "type": "dialog"
    },
    {
        "id": 114,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 11446,
            "text_len": 10,
            "text_offset": 11452,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "そんな！\\n",
        "type": "dialog"
    },
    {
        "id": 115,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 11488,
            "text_len": 78,
            "text_offset": 11496,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "美味しいから、まずくて苦くて吐きそうだ\\nなんてこと全然ありませんからねっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 116,
        "internal": {
            "speaker_len": 12,
            "speaker_offset": 11678,
            "text_len": 36,
            "text_offset": 11692,
            "warnings": 0
        },
        "speaker": "ゆっこ＆みお",
        "text": "……うん。\\nやっぱり、マズイよね。\\n",
        "type": "dialog"
    }
]