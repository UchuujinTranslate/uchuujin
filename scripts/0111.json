[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 50,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "喉渇いたな……\\n自販機、それか大工カフェで……。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2222,
            "text_len": 12,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2352,
            "text_len": 12,
            "text_offset": 2358,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2484,
            "text_len": 6,
            "text_offset": 2494,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2522,
            "text_len": 14,
            "text_offset": 2528,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……壊れた？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2564,
            "text_len": 12,
            "text_offset": 2570,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2692,
            "text_len": 6,
            "text_offset": 2702,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2742,
            "text_len": 26,
            "text_offset": 2748,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "桜井先生の後を追ってる？\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2796,
            "text_len": 28,
            "text_offset": 2802,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……なんだ、日常の風景だ。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2872,
            "text_len": 12,
            "text_offset": 2878,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "あっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2920,
            "text_len": 12,
            "text_offset": 2930,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2968,
            "text_len": 26,
            "text_offset": 2974,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……私を追いかけていた？\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3022,
            "text_len": 26,
            "text_offset": 3028,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "これは非日常の風景……。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3132,
            "text_len": 40,
            "text_offset": 3136,
            "warnings": 1
        },
        "speaker": "",
        "text": "げっ、バレちまった！\\nに、逃げないと！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3200,
            "text_len": 68,
            "text_offset": 3204,
            "warnings": 1
        },
        "speaker": "",
        "text": "待て！　逃げたら自分からつけ回してたと\\n言っているようなもんだぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3294,
            "text_len": 56,
            "text_offset": 3298,
            "warnings": 2
        },
        "speaker": "",
        "text": "あっ、そうか！\\nこういう時は逆に堂としておくんだな！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3376,
            "text_len": 70,
            "text_offset": 3380,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうだ、そして話しかけられても、\\n偶然、目が合っただけだと言うんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3472,
            "text_len": 68,
            "text_offset": 3476,
            "warnings": 1
        },
        "speaker": "",
        "text": "その判断力、さすがだな！\\nまるでスト―キングの経験があるみたいだ。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3580,
            "text_len": 6,
            "text_offset": 3584,
            "warnings": 1
        },
        "speaker": "",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3612,
            "text_len": 12,
            "text_offset": 3616,
            "warnings": 1
        },
        "speaker": "",
        "text": "……えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3658,
            "text_len": 78,
            "text_offset": 3662,
            "warnings": 1
        },
        "speaker": "",
        "text": "な、なななな、なんのことだ？　あれは偶然！\\n本当にいつも偶然だったんだよ！？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3762,
            "text_len": 16,
            "text_offset": 3766,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、お前……。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3828,
            "text_len": 18,
            "text_offset": 3832,
            "warnings": 1
        },
        "speaker": "",
        "text": "偶然なんだ――！\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3942,
            "text_len": 14,
            "text_offset": 3948,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "だけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3990,
            "text_len": 12,
            "text_offset": 4000,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4046,
            "text_len": 22,
            "text_offset": 4052,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……あれでは変質者。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4096,
            "text_len": 30,
            "text_offset": 4102,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "桜井先生もかわいそうに……。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4202,
            "text_len": 12,
            "text_offset": 4208,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……先生。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4248,
            "text_len": 16,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "うわあああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4296,
            "text_len": 20,
            "text_offset": 4302,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "何してるんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4344,
            "text_len": 72,
            "text_offset": 4354,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "み、水上！？　こ、これは、あの、\\nそう、今日の夕飯の材料を買いにだな！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4448,
            "text_len": 32,
            "text_offset": 4454,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "あ、あんなところに桜井先生が。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4508,
            "text_len": 68,
            "text_offset": 4518,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ドキッ！？　あ、ああ、本当だ、\\n桜井先生がいるな。気付かなかった！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4608,
            "text_len": 58,
            "text_offset": 4614,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "学校を出たら赤の他人……\\nだから気付かなかったんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4694,
            "text_len": 82,
            "text_offset": 4704,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "ち、ちち違う！　例え学校を出ても同僚だ！\\n教職者として強い仆で結ばれていてだな！\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4808,
            "text_len": 10,
            "text_offset": 4814,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "変質者？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4860,
            "text_len": 10,
            "text_offset": 4870,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "なっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4902,
            "text_len": 38,
            "text_offset": 4908,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "最近、スト―カ―が出るらしいですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4974,
            "text_len": 58,
            "text_offset": 4984,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "なっ！？　だだだ、誰がだ！\\n俺は偶然ここにいただけで弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5064,
            "text_len": 8,
            "text_offset": 5070,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "先生。\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5100,
            "text_len": 18,
            "text_offset": 5110,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "な、なんだ……？\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5150,
            "text_len": 30,
            "text_offset": 5156,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ス―パ―、通り過ぎましたよ。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5208,
            "text_len": 10,
            "text_offset": 5218,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "う……。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5250,
            "text_len": 54,
            "text_offset": 5260,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "で、では俺はここで！\\nスト―カ―に気をつけるんだぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5336,
            "text_len": 8,
            "text_offset": 5342,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5396,
            "text_len": 12,
            "text_offset": 5402,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5518,
            "text_len": 18,
            "text_offset": 5524,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "いつ捕まるかな？\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5604,
            "text_len": 12,
            "text_offset": 5610,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5648,
            "text_len": 40,
            "text_offset": 5658,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あら、水上さん。\\n水上さんもお買い物？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5720,
            "text_len": 16,
            "text_offset": 5726,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ボディガ―ド？\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5780,
            "text_len": 44,
            "text_offset": 5790,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ボディガ―ド……ですか？\\nへ―、誰のです？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5856,
            "text_len": 14,
            "text_offset": 5862,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "それは内緒。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5898,
            "text_len": 44,
            "text_offset": 5908,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、そうですね。\\nそれは言えませんよね。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5994,
            "text_len": 12,
            "text_offset": 6000,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6056,
            "text_len": 6,
            "text_offset": 6066,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6112,
            "text_len": 72,
            "text_offset": 6122,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そうだ、水上さん。今日の夕飯なんですけど、\\n何にするか迷ってるんです。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6216,
            "text_len": 10,
            "text_offset": 6222,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "カレ―。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6254,
            "text_len": 62,
            "text_offset": 6264,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "カレ―、ですか？　いいですね―。\\n私も弟も好きなんですよ―。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6348,
            "text_len": 56,
            "text_offset": 6358,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "今日はお肉が安い日ですから、\\nカレ―にしちゃいますね。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6436,
            "text_len": 24,
            "text_offset": 6442,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ついでに防犯ブザ―も。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6488,
            "text_len": 18,
            "text_offset": 6498,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ぼ、防犯ブザ―？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6538,
            "text_len": 34,
            "text_offset": 6544,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "この辺りはスト―カ―が出るから。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6600,
            "text_len": 58,
            "text_offset": 6610,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……そ、そうなんですか！？\\nじゃあ防犯ブザ―も買います！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6710,
            "text_len": 12,
            "text_offset": 6716,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6770,
            "text_len": 6,
            "text_offset": 6780,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
