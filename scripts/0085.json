[
    {
        "id": 0,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2152,
            "text_len": 42,
            "text_offset": 2162,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "それでは小テストを始めます。\\nどうぞっ。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2230,
            "text_len": 24,
            "text_offset": 2236,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（よ―し、頑張るぞ―）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2382,
            "text_len": 10,
            "text_offset": 2390,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん〜〜？\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2428,
            "text_len": 70,
            "text_offset": 2436,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "テレビでもエアコンでもないし……\\nこのボタン、何のボタンだったっけ？\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2528,
            "text_len": 16,
            "text_offset": 2536,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "ん〜〜〜〜？？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2672,
            "text_len": 62,
            "text_offset": 2678,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あれ？　この問題は……\\n　昨日勉強したはずなんだけど……）\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2768,
            "text_len": 6,
            "text_offset": 2774,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2828,
            "text_len": 12,
            "text_offset": 2836,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2874,
            "text_len": 48,
            "text_offset": 2884,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あれ？\\nどうして、ワンちゃんがこんなところに？\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2954,
            "text_len": 40,
            "text_offset": 2964,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ダメですよ―。\\nおうちに帰りなさ〜い。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3082,
            "text_len": 20,
            "text_offset": 3086,
            "warnings": 1
        },
        "speaker": "",
        "text": "いいな―、この人。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3130,
            "text_len": 60,
            "text_offset": 3134,
            "warnings": 1
        },
        "speaker": "",
        "text": "ああ、こういう優しそうな人は、\\n体罰とか絶対しないよな―。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3216,
            "text_len": 56,
            "text_offset": 3220,
            "warnings": 1
        },
        "speaker": "",
        "text": "ははは、幻想抱きすぎだろ？\\nでも、その通りだろうな―。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3320,
            "text_len": 6,
            "text_offset": 3328,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3366,
            "text_len": 48,
            "text_offset": 3370,
            "warnings": 1
        },
        "speaker": "",
        "text": "何でアンテナが！？\\nあ！　頭をなでられてる！？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3464,
            "text_len": 36,
            "text_offset": 3472,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "優しさが、まさかの体罰化―――！！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3686,
            "text_len": 6,
            "text_offset": 3692,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3728,
            "text_len": 72,
            "text_offset": 3734,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（な、なんで腕からロ―ルケ―キが！？\\n　どうしてこのタイミングで！？）\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3826,
            "text_len": 68,
            "text_offset": 3832,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（と言うか、またロ―ルケ―キが入ってる！\\n　いつの間に補充を！？）\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3920,
            "text_len": 50,
            "text_offset": 3926,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（と、とにかく、\\n　見られる前にしまわないと！）\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4084,
            "text_len": 24,
            "text_offset": 4090,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ふぅ……これで良し）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4146,
            "text_len": 14,
            "text_offset": 4156,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4200,
            "text_len": 22,
            "text_offset": 4206,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（良くなかった！！）\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4262,
            "text_len": 42,
            "text_offset": 4268,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ま、まさか、今のを先生に見られて……）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4344,
            "text_len": 14,
            "text_offset": 4352,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "…………！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4390,
            "text_len": 22,
            "text_offset": 4396,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ぎゃああああ！！）\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4448,
            "text_len": 52,
            "text_offset": 4454,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（まだいた！\\n　見てしまった人が、まだいた―！！）\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4526,
            "text_len": 66,
            "text_offset": 4532,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（どうしよう！　どうすれば？\\n　これはテストより難題では……！）\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4754,
            "text_len": 40,
            "text_offset": 4762,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（と、とんでもない物をみてしまった！）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4828,
            "text_len": 56,
            "text_offset": 4836,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（僕の目の錯覚だろうか？\\n　でも、さっきは確かに……）\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4918,
            "text_len": 34,
            "text_offset": 4924,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（どうすれば……どうすれば……）\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5086,
            "text_len": 18,
            "text_offset": 5094,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（………………）\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5138,
            "text_len": 10,
            "text_offset": 5146,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "先生！！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5190,
            "text_len": 46,
            "text_offset": 5200,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はっ、はい！！\\nどうしましたか、中之条くん。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5268,
            "text_len": 38,
            "text_offset": 5276,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "見てしまったんです！\\n東雲さんの弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5344,
            "text_len": 6,
            "text_offset": 5350,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5386,
            "text_len": 62,
            "text_offset": 5392,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ど……どうしよ―！\\n　わたしがロボットだってバレちゃう！）\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5486,
            "text_len": 24,
            "text_offset": 5496,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "それは……本当ですか？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5544,
            "text_len": 44,
            "text_offset": 5552,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はい、見てしまったんです。\\n東雲さんの弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5630,
            "text_len": 34,
            "text_offset": 5636,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ああああっ、もう終わりだ―！）\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5698,
            "text_len": 16,
            "text_offset": 5706,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "弌弌テストを！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5752,
            "text_len": 16,
            "text_offset": 5758,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "って、テスト？\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5798,
            "text_len": 50,
            "text_offset": 5806,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はい、見たのはテストです。\\n……カンニングです。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5880,
            "text_len": 42,
            "text_offset": 5886,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "そんな！\\n先生、中之条くんが見たのは弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5960,
            "text_len": 20,
            "text_offset": 5970,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……わかりました。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6010,
            "text_len": 72,
            "text_offset": 6016,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（……！？　もしかしたら二人は、\\n　見なかったことにしてくれようと？）\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6122,
            "text_len": 84,
            "text_offset": 6132,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "でも、中之条くん。今はテスト中ですから、\\nえっと……先生と廊下でお話ししましょう。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6240,
            "text_len": 8,
            "text_offset": 6248,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6282,
            "text_len": 34,
            "text_offset": 6290,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "（弌弌うん、これでよかったんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6350,
            "text_len": 34,
            "text_offset": 6360,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（これでよかったんですよね弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6548,
            "text_len": 24,
            "text_offset": 6556,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "んぎゃああああああ！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6600,
            "text_len": 24,
            "text_offset": 6610,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "きゃ―――――――！？\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6776,
            "text_len": 68,
            "text_offset": 6786,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "東雲なのが出てくると思ったら、\\nまさか、こんなことになるとは弌弌。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6880,
            "text_len": 16,
            "text_offset": 6890,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6940,
            "text_len": 12,
            "text_offset": 6950,
            "warnings": 0
        },
        "speaker": "中村先生",
        "text": "逃げろっ！\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7086,
            "text_len": 32,
            "text_offset": 7094,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "ここは？\\n僕は一体なにを……。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7150,
            "text_len": 24,
            "text_offset": 7160,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっと、私は何を……。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7208,
            "text_len": 60,
            "text_offset": 7218,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、中之条くんどうしたんですか？\\nテスト始まってますよ。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7300,
            "text_len": 20,
            "text_offset": 7308,
            "warnings": 0
        },
        "speaker": "中之条",
        "text": "はい、すみません！\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7454,
            "text_len": 12,
            "text_offset": 7460,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7576,
            "text_len": 24,
            "text_offset": 7582,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "テスト、受けようっと。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7662,
            "text_len": 64,
            "text_offset": 7668,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あ、そうだ、テスト！\\n　小テストだから急がないと時間が弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7752,
            "text_len": 70,
            "text_offset": 7758,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（でも、二人の視線が気になって、\\n　テストなんて受けられません！！）\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7852,
            "text_len": 28,
            "text_offset": 7858,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ああ……どうすれば……）\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7906,
            "text_len": 18,
            "text_offset": 7912,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あっ、そうだ）\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7970,
            "text_len": 36,
            "text_offset": 7976,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "桜井先生、ちょっと質問があります！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8046,
            "text_len": 16,
            "text_offset": 8056,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "はっ、はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8094,
            "text_len": 20,
            "text_offset": 8100,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "えっとですね……。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8142,
            "text_len": 34,
            "text_offset": 8148,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "先生、ロ―ルケ―キは好きですか？\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8210,
            "text_len": 30,
            "text_offset": 8220,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\nす、好きですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8272,
            "text_len": 56,
            "text_offset": 8278,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "良かった！\\nなら後で、中之条くんも呼んで食べませんか？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8356,
            "text_len": 36,
            "text_offset": 8366,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、えっと……\\nそれはつまり……。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8424,
            "text_len": 44,
            "text_offset": 8430,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "何も見ていませんよね？\\n見ていませんよね！\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8496,
            "text_len": 12,
            "text_offset": 8506,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……はい。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8540,
            "text_len": 44,
            "text_offset": 8546,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "ありがとうございます。\\nテスト頑張ります。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8612,
            "text_len": 24,
            "text_offset": 8622,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……頑張ってください。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8672,
            "text_len": 12,
            "text_offset": 8678,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8798,
            "text_len": 62,
            "text_offset": 8804,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（秘密を守る為とは言え、\\n　賄賂を渡すことになるなんて……）\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8886,
            "text_len": 52,
            "text_offset": 8892,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（ごめんなさい、はかせ……\\n　私は悪い子です……）\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9060,
            "text_len": 60,
            "text_offset": 9068,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あれ〜？　ロ―ルケ―キどこに置いたっけ？\\n阪本知ってる〜？\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9144,
            "text_len": 66,
            "text_offset": 9154,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "それなら、またおまえが悪さをして、\\n娘の腕の中にしまってただろ？\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 9236,
            "text_len": 46,
            "text_offset": 9244,
            "warnings": 0
        },
        "speaker": "はかせ",
        "text": "あれれ、そうだったっけ〜？\\nあははははは〜！\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 18,
            "speaker_offset": 9412,
            "text_len": 64,
            "text_offset": 9432,
            "warnings": 0
        },
        "speaker": "ナレ―タ―お姉さん",
        "text": "その後、事情が判明して、\\nお説教の時間が準備されたと言います。\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9648,
            "text_len": 56,
            "text_offset": 9654,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（あ、でも下手に騒ぐと\\n　みんなに広がる気がする……）\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9740,
            "text_len": 66,
            "text_offset": 9746,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（もうロ―ルケ―キはしまったんだから\\n　普通にしてればいいんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9838,
            "text_len": 66,
            "text_offset": 9844,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（だって腕からロ―ルケ―キが\\n　出てくるなんて誰も信じないし！）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 9936,
            "text_len": 44,
            "text_offset": 9942,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "（そうとわかればテストの続きをしようっと）\\n",
        "type": "dialog"
    },
    {
        "id": 87,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10110,
            "text_len": 30,
            "text_offset": 10120,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あ、あの、東雲さん……。\\n",
        "type": "dialog"
    },
    {
        "id": 88,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10174,
            "text_len": 26,
            "text_offset": 10180,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "はい？　なんでしょうか。\\n",
        "type": "dialog"
    },
    {
        "id": 89,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10228,
            "text_len": 76,
            "text_offset": 10238,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あの、その……ロ―ルケ―キは腕の中じゃ\\nなくて、ちゃんとした入れ物に……。\\n",
        "type": "dialog"
    },
    {
        "id": 90,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10336,
            "text_len": 62,
            "text_offset": 10342,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "またまた〜。腕にロ―ルケ―キが\\n入るわけないじゃないですか。\\n",
        "type": "dialog"
    },
    {
        "id": 91,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10432,
            "text_len": 30,
            "text_offset": 10442,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ええ？　あ、でも、あの……。\\n",
        "type": "dialog"
    },
    {
        "id": 92,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10494,
            "text_len": 56,
            "text_offset": 10500,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "それじゃあまるで私が\\nロボットみたいじゃないですか〜。\\n",
        "type": "dialog"
    },
    {
        "id": 93,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10586,
            "text_len": 20,
            "text_offset": 10596,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 94,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10648,
            "text_len": 12,
            "text_offset": 10654,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 95,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 10698,
            "text_len": 20,
            "text_offset": 10708,
            "warnings": 0
        },
        "speaker": "生徒たち",
        "text": "……………………。\\n",
        "type": "dialog"
    },
    {
        "id": 96,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 10842,
            "text_len": 22,
            "text_offset": 10848,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "あれれれれれれ〜っ？\\n",
        "type": "dialog"
    }
]