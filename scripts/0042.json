        "internal": {
            "speaker_len": 8,
            "speaker_offset": 9742,
            "text_len": 24,
            "text_offset": 9752,
            "warnings": 0
        },
        "speaker": "高崎先生",
        "text": "今日はいい天気ですね！\\n",
