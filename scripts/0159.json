[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2144,
            "text_len": 14,
            "text_offset": 2150,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……帰ろう。\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2186,
            "text_len": 6,
            "text_offset": 2192,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2302,
            "text_len": 72,
            "text_offset": 2308,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、このステ―キの天晴れな火加減。\\n料理長め、また腕を上げおったわ。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2400,
            "text_len": 12,
            "text_offset": 2406,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2438,
            "text_len": 28,
            "text_offset": 2444,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "（ああ、みおの好きな人か）\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2592,
            "text_len": 10,
            "text_offset": 2598,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "おっと。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2634,
            "text_len": 10,
            "text_offset": 2640,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "（あっ）\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2672,
            "text_len": 68,
            "text_offset": 2678,
            "warnings": 1
        },
        "speaker": "笹原",
        "text": "私としたことがフォ―クを落としてしまうとは、\\n少興奮しすぎたか。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2772,
            "text_len": 12,
            "text_offset": 2778,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2826,
            "text_len": 12,
            "text_offset": 2834,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2872,
            "text_len": 46,
            "text_offset": 2878,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "むむ、どうした？\\n肉の匂いにつられてきたか。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2950,
            "text_len": 12,
            "text_offset": 2958,
            "warnings": 0
        },
        "speaker": "バディ",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2992,
            "text_len": 56,
            "text_offset": 2998,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "フ、いいだろう。\\n貴様も料理長の腕に腰を抜かすといい。\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3132,
            "text_len": 44,
            "text_offset": 3136,
            "warnings": 1
        },
        "speaker": "",
        "text": "す、すげ―！\\nあんな高そうな肉をくれたぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3204,
            "text_len": 64,
            "text_offset": 3208,
            "warnings": 1
        },
        "speaker": "",
        "text": "お、俺、あんな肉が食えるなら\\n犬でいい！　つ―か犬になりたい！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3294,
            "text_len": 64,
            "text_offset": 3298,
            "warnings": 1
        },
        "speaker": "",
        "text": "俺も俺も！　弌弌あ―、くそ！\\nあの犬、うまそうに食いやがって！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3384,
            "text_len": 40,
            "text_offset": 3388,
            "warnings": 1
        },
        "speaker": "",
        "text": "いいな―……\\nうおっと、よだれが……。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3470,
            "text_len": 12,
            "text_offset": 3478,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3630,
            "text_len": 42,
            "text_offset": 3642,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "さっきから肉ばかり映しているが弌弌\\nん？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3714,
            "text_len": 8,
            "text_offset": 3718,
            "warnings": 1
        },
        "speaker": "",
        "text": "わん！\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3750,
            "text_len": 16,
            "text_offset": 3754,
            "warnings": 1
        },
        "speaker": "",
        "text": "わう―ん……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3808,
            "text_len": 12,
            "text_offset": 3820,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3878,
            "text_len": 6,
            "text_offset": 3886,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 3916,
            "text_len": 36,
            "text_offset": 3928,
            "warnings": 0
        },
        "speaker": "スタ―ラ姫",
        "text": "……焼き方は、ウェルダンで良いな？\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4010,
            "text_len": 18,
            "text_offset": 4018,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "きゃい―――ん！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4128,
            "text_len": 68,
            "text_offset": 4134,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "とは言えこのステ―キにはこの笹原幸次郎を\\n昂らせるだけの味がある。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4232,
            "text_len": 26,
            "text_offset": 4238,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふふ、腹もいななきおる。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4286,
            "text_len": 52,
            "text_offset": 4292,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "まあそう急かすでない。\\nすぐに腹を満たしてやろう。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4370,
            "text_len": 16,
            "text_offset": 4376,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "（変な人……）\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4414,
            "text_len": 42,
            "text_offset": 4420,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "弌弌っと、フォ―クを落としたのであった。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4488,
            "text_len": 54,
            "text_offset": 4494,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "そこの娘。\\nすまぬがそのフォ―クを取ってもらえぬか？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4600,
            "text_len": 8,
            "text_offset": 4606,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "これ？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4718,
            "text_len": 72,
            "text_offset": 4724,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "うむ、すまぬな。礼として一口やろう。\\n料理長の腕に、腰を抜かすと良い。\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4816,
            "text_len": 18,
            "text_offset": 4822,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "じゃ、遠慮なく。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4896,
            "text_len": 6,
            "text_offset": 4902,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5038,
            "text_len": 24,
            "text_offset": 5044,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "んっ、んっ、んっ……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5094,
            "text_len": 12,
            "text_offset": 5100,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5134,
            "text_len": 36,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "……ご馳走様。確かにおいしかった。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5198,
            "text_len": 42,
            "text_offset": 5204,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "でも、ウェルダンだったらもっと良かった。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5268,
            "text_len": 6,
            "text_offset": 5274,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5302,
            "text_len": 12,
            "text_offset": 5308,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "じゃ……。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5348,
            "text_len": 12,
            "text_offset": 5354,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5470,
            "text_len": 26,
            "text_offset": 5476,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふふ…………丼きおるわ。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5560,
            "text_len": 8,
            "text_offset": 5566,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "はい。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5596,
            "text_len": 24,
            "text_offset": 5602,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "すまんな。助かったぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5648,
            "text_len": 28,
            "text_offset": 5654,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "でもそれ、落ちたフォ―ク。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5704,
            "text_len": 54,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふっ、はははは。\\nもちろんこれで食べるわけではない。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5790,
            "text_len": 12,
            "text_offset": 5796,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5830,
            "text_len": 56,
            "text_offset": 5836,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "これとは別にフォ―クを持っているので\\n安心するが良い。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5918,
            "text_len": 26,
            "text_offset": 5924,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "そこを、あえて手づかみ。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5998,
            "text_len": 6,
            "text_offset": 6004,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6036,
            "text_len": 26,
            "text_offset": 6042,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "男らしいイメ―ジがつく。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6090,
            "text_len": 12,
            "text_offset": 6096,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふむ……。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6136,
            "text_len": 82,
            "text_offset": 6142,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "何を馬鹿なと、一蹴することはできんな。\\nよし、では手づかみで食べてみるとしよう。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6250,
            "text_len": 12,
            "text_offset": 6256,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6300,
            "text_len": 12,
            "text_offset": 6306,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6340,
            "text_len": 8,
            "text_offset": 6346,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "はっ！\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6386,
            "text_len": 12,
            "text_offset": 6392,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6426,
            "text_len": 14,
            "text_offset": 6432,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "熱いな……。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6564,
            "text_len": 30,
            "text_offset": 6570,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "（……私も今度やってみよう）\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6672,
            "text_len": 12,
            "text_offset": 6678,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6716,
            "text_len": 12,
            "text_offset": 6722,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6760,
            "text_len": 12,
            "text_offset": 6766,
            "warnings": 0
        },
        "speaker": "麻衣",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6810,
            "text_len": 12,
            "text_offset": 6816,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6864,
            "text_len": 12,
            "text_offset": 6870,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6986,
            "text_len": 22,
            "text_offset": 6992,
            "warnings": 0
        },
        "speaker": "笹原",
        "text": "ふっ……かぶきおる。\\n",
        "type": "dialog"
    }
]