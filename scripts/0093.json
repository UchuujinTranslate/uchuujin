[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2150,
            "text_len": 34,
            "text_offset": 2158,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ウェボシ―！　笹原見なかった！？\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2216,
            "text_len": 36,
            "text_offset": 2228,
            "warnings": 0
        },
        "speaker": "ウェボシ―",
        "text": "え、いや〜、ちょっとわかんないな。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2290,
            "text_len": 28,
            "text_offset": 2298,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そう。見かけたら教えてね！\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2480,
            "text_len": 72,
            "text_offset": 2488,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あいつ、一体どこ行ったのよ！？\\n今日という今日は絶対許さないんだから！\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2596,
            "text_len": 6,
            "text_offset": 2606,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2640,
            "text_len": 34,
            "text_offset": 2650,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ちょっ……あの、た、立花さんっ！\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2708,
            "text_len": 24,
            "text_offset": 2716,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "は、はい。なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2762,
            "text_len": 48,
            "text_offset": 2772,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの……その、\\n廊下は走っちゃダメですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2848,
            "text_len": 38,
            "text_offset": 2856,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、そ、そうですね。すみません……。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2916,
            "text_len": 18,
            "text_offset": 2926,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そ、それと……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2972,
            "text_len": 20,
            "text_offset": 2980,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "え、まだ何か……？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3022,
            "text_len": 76,
            "text_offset": 3032,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "それ！　と言うか、え―っとですね。\\nとても言いにくいことなんですけど……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3130,
            "text_len": 24,
            "text_offset": 3140,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "その立花さんの手の弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3186,
            "text_len": 12,
            "text_offset": 3194,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……手の？\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3228,
            "text_len": 36,
            "text_offset": 3238,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "弌弌手の爪、ちょっと長いですよね！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3306,
            "text_len": 48,
            "text_offset": 3314,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "えっ！？\\nあ、そう言えば切ってくるの忘れてた。\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3384,
            "text_len": 72,
            "text_offset": 3392,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "先生、教えてくれてありがとうございます。\\n部活までには切っときますね。\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3486,
            "text_len": 44,
            "text_offset": 3496,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "そ、そうですね。\\n割れたら危ないですから。\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3562,
            "text_len": 32,
            "text_offset": 3570,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "じゃあ先生、私、急いでるんで！\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3662,
            "text_len": 14,
            "text_offset": 3672,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "……おっと。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3718,
            "text_len": 44,
            "text_offset": 3726,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あっ……校長先生……\\nす、すみません……。\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3800,
            "text_len": 68,
            "text_offset": 3810,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ほら、立花さんったら。\\n廊下は走っちゃダメって言ったばかりなのに。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3910,
            "text_len": 18,
            "text_offset": 3918,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "す、すみません。\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3960,
            "text_len": 48,
            "text_offset": 3970,
            "warnings": 0
        },
        "speaker": "校長先生",
        "text": "はっはっは。\\nこれからは気をつけてくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4040,
            "text_len": 10,
            "text_offset": 4048,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "は―い。\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4136,
            "text_len": 12,
            "text_offset": 4140,
            "warnings": 1
        },
        "speaker": "",
        "text": "なあ……。\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4176,
            "text_len": 6,
            "text_offset": 4180,
            "warnings": 1
        },
        "speaker": "",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4208,
            "text_len": 26,
            "text_offset": 4212,
            "warnings": 1
        },
        "speaker": "",
        "text": "なにか変だと思わないか？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4260,
            "text_len": 12,
            "text_offset": 4264,
            "warnings": 1
        },
        "speaker": "",
        "text": "何がだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4298,
            "text_len": 58,
            "text_offset": 4302,
            "warnings": 1
        },
        "speaker": "",
        "text": "いや……何がって言われると困るが……\\nとにかく変だよな。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4382,
            "text_len": 64,
            "text_offset": 4386,
            "warnings": 1
        },
        "speaker": "",
        "text": "そうか？　オレにはさっぱりわからんな。\\nいや、わかりたくない！\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 4480,
            "text_len": 44,
            "text_offset": 4484,
            "warnings": 1
        },
        "speaker": "",
        "text": "……なあ二人とも、そろそろ現実をみようぜ？\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4574,
            "text_len": 34,
            "text_offset": 4582,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "現実なんて見たくな――――いっ！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4708,
            "text_len": 40,
            "text_offset": 4718,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、あの！　も、もうひとついいですか？\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 4780,
            "text_len": 18,
            "text_offset": 4788,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、はい。何か？\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4834,
            "text_len": 66,
            "text_offset": 4844,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "その手の……て、手の銃です！\\n学校への銃の持ち込みは禁止ですっ！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4960,
            "text_len": 66,
            "text_offset": 4970,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "これは先生が預かっておきますから、\\n放課後取りに来てくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5060,
            "text_len": 24,
            "text_offset": 5068,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "……はい、すみません。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5132,
            "text_len": 6,
            "text_offset": 5140,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5176,
            "text_len": 20,
            "text_offset": 5184,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原見つけたっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5260,
            "text_len": 60,
            "text_offset": 5268,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "待ちなさい笹原！\\n今日という今日は絶対許さないんだからね！\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5354,
            "text_len": 16,
            "text_offset": 5364,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "まだ持ってた！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5402,
            "text_len": 42,
            "text_offset": 5410,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あっ！\\nす、すみません、銃は禁止でした。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5474,
            "text_len": 40,
            "text_offset": 5484,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "いえ、わかってもらえればいいんですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 5546,
            "text_len": 46,
            "text_offset": 5554,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ、笹原が行っちゃう！\\nこら、待ちなさ―い！\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5642,
            "text_len": 74,
            "text_offset": 5652,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "よかった、銃はあぶないもんね。\\n……私、少しは生徒指導らしくできたかな？\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5748,
            "text_len": 68,
            "text_offset": 5758,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "それにしても立花さん、あんなに一生懸命に。\\n若いって良いですね―。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5848,
            "text_len": 56,
            "text_offset": 5858,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……あら？\\nでも立花さん、なんで笹原くんとは逆方向へ？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5992,
            "text_len": 10,
            "text_offset": 6002,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "……！？\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6042,
            "text_len": 14,
            "text_offset": 6050,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "笹原―っ！！\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6172,
            "text_len": 18,
            "text_offset": 6182,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "なんてこった！！\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6248,
            "text_len": 32,
            "text_offset": 6256,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ちょっと待ちなさい、笹原―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6310,
            "text_len": 60,
            "text_offset": 6320,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ……立花さん、廊下は走っちゃ……\\nあぁ……行っちゃった。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6402,
            "text_len": 68,
            "text_offset": 6412,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "仕方ないですね……とりあえず、これを\\n職員室へ持っていきましょう。\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6506,
            "text_len": 10,
            "text_offset": 6516,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ！？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6788,
            "text_len": 6,
            "text_offset": 6794,
            "warnings": 0
        },
        "speaker": "みお",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6822,
            "text_len": 6,
            "text_offset": 6830,
            "warnings": 0
        },
        "speaker": "ゆっこ",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6860,
            "text_len": 56,
            "text_offset": 6866,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "……な、なんでしょう？\\nなんだか、みんなの視線が……？\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 6960,
            "text_len": 64,
            "text_offset": 6968,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "はぁ……笹原を見失っちゃった。\\n武器もないし、どうしよう……。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7054,
            "text_len": 6,
            "text_offset": 7062,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "！！\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7090,
            "text_len": 46,
            "text_offset": 7098,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そこの１年生！　悪いけど武器になってくれる？\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7174,
            "text_len": 56,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "へっ！？　え？　あれ？\\nわ、私、なんでこんな姿に―！？\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7350,
            "text_len": 34,
            "text_offset": 7358,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "さあ、頑張って笹原を倒すわよ！！\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7408,
            "text_len": 48,
            "text_offset": 7414,
            "warnings": 0
        },
        "speaker": "なの",
        "text": "え―っ！？\\nなんで、どうしてこんなことに―！？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7516,
            "text_len": 46,
            "text_offset": 7524,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "先生、それ返してください！\\n今必要なんです！\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7592,
            "text_len": 36,
            "text_offset": 7602,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ダ、ダメですよ！\\nこれは没収です！\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7660,
            "text_len": 52,
            "text_offset": 7668,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "そんな―。\\nちょっとだけ、ちょっとだけですから―！\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7742,
            "text_len": 18,
            "text_offset": 7752,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ダメなんです―！\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 7842,
            "text_len": 8,
            "text_offset": 7850,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "あ！！\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7874,
            "text_len": 10,
            "text_offset": 7884,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "銃が！！\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8016,
            "text_len": 66,
            "text_offset": 8026,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "ふふふ……今日のわら人形は亟身の一作。\\n我ながら会心の出来だな。\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8120,
            "text_len": 62,
            "text_offset": 8130,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "さて、今日はこれを燃やして、\\n一段上の呪いを試してみようか。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8214,
            "text_len": 58,
            "text_offset": 8224,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "今までのやり方では効果が薄い……\\n校長の髪が薄いだけに。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8304,
            "text_len": 20,
            "text_offset": 8314,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "……面白くないな。\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8356,
            "text_len": 78,
            "text_offset": 8366,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "それはさておき、ここまで見事な出来だと、\\n燃やしてしまうのは惜しい気も……。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8508,
            "text_len": 68,
            "text_offset": 8518,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "おや？　何か、落ちてきたぞ。\\n焼却炉に入ってしまったようだが……？\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8608,
            "text_len": 36,
            "text_offset": 8618,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "立花さん、走っちゃダメですよ―っ！\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 8674,
            "text_len": 22,
            "text_offset": 8682,
            "warnings": 0
        },
        "speaker": "みさと",
        "text": "ああ、火の中にっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8726,
            "text_len": 36,
            "text_offset": 8736,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "それに何か騒がしい。\\nいったい弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 8930,
            "text_len": 28,
            "text_offset": 8940,
            "warnings": 0
        },
        "speaker": "教頭先生",
        "text": "弌弌何が――――――っ！？\\n",
        "type": "dialog"
    }
]