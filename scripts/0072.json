[
    {
        "id": 0,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2142,
            "text_len": 22,
            "text_offset": 2148,
            "warnings": 1
        },
        "speaker": "田中",
        "text": "（……時思うんだ）\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2278,
            "text_len": 60,
            "text_offset": 2284,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（オレのアフロヘア―の中は\\n　どうなってんだろうな―って）\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2364,
            "text_len": 72,
            "text_offset": 2370,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（みんな謎だ謎だっつ―けどさ、\\n　実はオレ自身、良く知らないんだよな）\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2462,
            "text_len": 36,
            "text_offset": 2468,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（ま、便利だからい―んだけどね！）\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2626,
            "text_len": 30,
            "text_offset": 2636,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、ス、スイマセン田中君。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2698,
            "text_len": 28,
            "text_offset": 2704,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ん―？　センセ、どしたの？\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2754,
            "text_len": 86,
            "text_offset": 2764,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "い、いえ、前も言ったと思うんですけど……\\nあの、学生でアフロはちょっとどうかなって。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2872,
            "text_len": 18,
            "text_offset": 2878,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あ―、その話ね。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2922,
            "text_len": 38,
            "text_offset": 2928,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（前みたいにどら焼きでごまかそっと）\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3018,
            "text_len": 12,
            "text_offset": 3024,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "センセ―。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3058,
            "text_len": 14,
            "text_offset": 3068,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "なんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3196,
            "text_len": 62,
            "text_offset": 3202,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "はいっ！　……ってあれ？\\n何かアフロの中で引っかかって……。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3284,
            "text_len": 16,
            "text_offset": 3290,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "よいしょお！！\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3412,
            "text_len": 26,
            "text_offset": 3422,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "わあ―、猫ちゃんですね！\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3464,
            "text_len": 68,
            "text_offset": 3470,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "……猫？　うわ、マジだ！\\nね、猫なんて入れた覚えないんだけど……？\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3614,
            "text_len": 62,
            "text_offset": 3618,
            "warnings": 2
        },
        "speaker": "",
        "text": "うぎゃああ、アンテナが折れた―っ！！\\n、何してたんだよっ！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3726,
            "text_len": 50,
            "text_offset": 3730,
            "warnings": 1
        },
        "speaker": "",
        "text": "くそ――っ！\\n絶好の隠れ場だと思ったのに――っ！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3884,
            "text_len": 10,
            "text_offset": 3890,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "はいっ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 3920,
            "text_len": 36,
            "text_offset": 3930,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え、えっと……コレ、何でしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3982,
            "text_len": 8,
            "text_offset": 3988,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4016,
            "text_len": 36,
            "text_offset": 4022,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（ほんとだ、何だコレ？　コケシ？）\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4082,
            "text_len": 58,
            "text_offset": 4088,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（おっかしいなぁ、\\n　確かどら焼き掴んだはずなんだけど）\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4188,
            "text_len": 28,
            "text_offset": 4198,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、あの？　えっと、え？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4252,
            "text_len": 60,
            "text_offset": 4258,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（うっわ―、この状況どうすっかな―。\\n　こうなったら弌弌）\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4378,
            "text_len": 24,
            "text_offset": 4384,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "センセ―、どうぞっ！！\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4432,
            "text_len": 8,
            "text_offset": 4442,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4472,
            "text_len": 12,
            "text_offset": 4478,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あげるよ！\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4512,
            "text_len": 14,
            "text_offset": 4522,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ、えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4558,
            "text_len": 32,
            "text_offset": 4568,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あのっ、これ何なんでしょうか？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4622,
            "text_len": 38,
            "text_offset": 4628,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "なんでもいいから！\\nはい、センセ―！\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4688,
            "text_len": 20,
            "text_offset": 4698,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっと、その……？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4740,
            "text_len": 46,
            "text_offset": 4750,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ス、スイマセン。\\n気持ちは嬉しいのですが弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4818,
            "text_len": 36,
            "text_offset": 4828,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "やっぱり、コレって何なんでしょう？\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4896,
            "text_len": 14,
            "text_offset": 4902,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "どうぞ！！！\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4944,
            "text_len": 12,
            "text_offset": 4954,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 4994,
            "text_len": 34,
            "text_offset": 5004,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、ありがとうゴザイマス……。\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5062,
            "text_len": 44,
            "text_offset": 5068,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あっ、授業はじまるじゃん！\\n先生、またね！\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5134,
            "text_len": 38,
            "text_offset": 5144,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、はい、勉強頑張ってくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5232,
            "text_len": 12,
            "text_offset": 5242,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5360,
            "text_len": 32,
            "text_offset": 5370,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ほんとにこれ、何なんでしょう？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5456,
            "text_len": 44,
            "text_offset": 5462,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あっれ―？\\n何だろコレ、間違えちゃったよ。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5548,
            "text_len": 28,
            "text_offset": 5554,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "え―っと……あ、コレコレ。\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5626,
            "text_len": 18,
            "text_offset": 5632,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "はいっ、ど―ぞ！\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5684,
            "text_len": 8,
            "text_offset": 5694,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "えっ？\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5726,
            "text_len": 8,
            "text_offset": 5732,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あれ？\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5782,
            "text_len": 50,
            "text_offset": 5788,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ちょ、ちょい待って、センセ―！\\nも、もう一回弌弌\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 5866,
            "text_len": 12,
            "text_offset": 5876,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あ、はい！\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5930,
            "text_len": 38,
            "text_offset": 5936,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "弌弌よっ！　はっ！\\nよし、今度こそ。\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6024,
            "text_len": 16,
            "text_offset": 6034,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "………………。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6088,
            "text_len": 58,
            "text_offset": 6094,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "（……ダメだ―っ！\\n　何回やってもコレしか出てこね―！）\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6174,
            "text_len": 20,
            "text_offset": 6180,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ごめん、センセ―。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6222,
            "text_len": 54,
            "text_offset": 6232,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、別にそんな、\\n気を遣って貰わなくていいですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6308,
            "text_len": 34,
            "text_offset": 6314,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ホント？　サンキュ―、センセ―！\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6370,
            "text_len": 72,
            "text_offset": 6376,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "つ―か、何でコレしか出てこないんだろ。\\nだいたいコレってなんなんだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6470,
            "text_len": 58,
            "text_offset": 6480,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "え？　あのっ、それって\\n田中君の持ち物じゃないんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6560,
            "text_len": 38,
            "text_offset": 6566,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "ん―。\\nアフロから勝手に出てきた物？\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6626,
            "text_len": 36,
            "text_offset": 6636,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "なるほど―、そうだったんですね―。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 6780,
            "text_len": 60,
            "text_offset": 6790,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "（ところで……\\n　結局、私はどうすればいいんでしょうか？）\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6914,
            "text_len": 38,
            "text_offset": 6920,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "あれ？\\nおっかしいな―、何だろコレ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6980,
            "text_len": 66,
            "text_offset": 6986,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "センセ―にあげようと思って\\nどら焼き出したつもりなんだけどな―。\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7074,
            "text_len": 64,
            "text_offset": 7084,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "あっ、ありがとうございます。\\nわざわざ気遣ってもらっちゃって。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7170,
            "text_len": 72,
            "text_offset": 7180,
            "warnings": 0
        },
        "speaker": "桜井先生",
        "text": "ところで……結局それは何なんでしょう？\\n見た目はコケシっぽいですけど。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7274,
            "text_len": 54,
            "text_offset": 7280,
            "warnings": 0
        },
        "speaker": "田中",
        "text": "でもコケシって、\\nこんなツノとかついてないっスよね？\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 7356,
            "text_len": 60,
            "text_offset": 7366,
            "warnings": 0
        },
        "speaker": "桜井先生",
