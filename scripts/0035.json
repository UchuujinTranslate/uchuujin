[
    {
        "id": 0,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 2144,
            "text_len": 62,
            "text_offset": 2152,
            "warnings": 0
        },
        "speaker": "桜井誠",
        "text": "それじゃ、オレはこっちなんで。\\nお疲れ様でした！　また明日！\\n",
        "type": "dialog"
    },
    {
        "id": 1,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2236,
            "text_len": 18,
            "text_offset": 2242,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、お疲れ様。\\n",
        "type": "dialog"
    },
    {
        "id": 2,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2282,
            "text_len": 16,
            "text_offset": 2288,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "お疲れ様です。\\n",
        "type": "dialog"
    },
    {
        "id": 3,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2328,
            "text_len": 40,
            "text_offset": 2334,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "さ―て、いつも通りここからは二人だな。\\n",
        "type": "dialog"
    },
    {
        "id": 4,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2398,
            "text_len": 18,
            "text_offset": 2404,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……そうですね。\\n",
        "type": "dialog"
    },
    {
        "id": 5,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2444,
            "text_len": 18,
            "text_offset": 2450,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……あ、そうだ。\\n",
        "type": "dialog"
    },
    {
        "id": 6,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2490,
            "text_len": 60,
            "text_offset": 2496,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口、大工カフェ寄ってかないか？\\nオレのど渇いちゃってさ。\\n",
        "type": "dialog"
    },
    {
        "id": 7,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2578,
            "text_len": 42,
            "text_offset": 2584,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "いいです。\\n私は別にのど渇いてませんし。\\n",
        "type": "dialog"
    },
    {
        "id": 8,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2648,
            "text_len": 48,
            "text_offset": 2654,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ははは、そう言うなよ。\\n乂るから寄ってけって。\\n",
        "type": "dialog"
    },
    {
        "id": 9,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2724,
            "text_len": 12,
            "text_offset": 2730,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "はぁ……。\\n",
        "type": "dialog"
    },
    {
        "id": 10,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2764,
            "text_len": 30,
            "text_offset": 2770,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ―し決まり。じゃあ行くか。\\n",
        "type": "dialog"
    },
    {
        "id": 11,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2844,
            "text_len": 30,
            "text_offset": 2854,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "おう。ケ―キもつけてくれよ。\\n",
        "type": "dialog"
    },
    {
        "id": 12,
        "internal": {
            "speaker_len": 10,
            "speaker_offset": 2900,
            "text_len": 6,
            "text_offset": 2912,
            "warnings": 0
        },
        "speaker": "大工＆関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 13,
        "internal": {
            "speaker_len": 8,
            "speaker_offset": 2944,
            "text_len": 18,
            "text_offset": 2954,
            "warnings": 0
        },
        "speaker": "阪本さん",
        "text": "あっ、やば……。\\n",
        "type": "dialog"
    },
    {
        "id": 14,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 2994,
            "text_len": 36,
            "text_offset": 3000,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おおお！\\n関口！　この猫喋るぞ！！\\n",
        "type": "dialog"
    },
    {
        "id": 15,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3114,
            "text_len": 30,
            "text_offset": 3118,
            "warnings": 1
        },
        "speaker": "",
        "text": "うわっ！　つかまっちまった！\\n",
        "type": "dialog"
    },
    {
        "id": 16,
        "internal": {
            "speaker_len": 2,
            "speaker_offset": 3172,
            "text_len": 40,
            "text_offset": 3176,
            "warnings": 1
        },
        "speaker": "",
        "text": "あああ！\\nそんなっ、乱暴に扱わないで！\\n",
        "type": "dialog"
    },
    {
        "id": 17,
        "internal": {
            "speaker_len": 6,
            "speaker_offset": 3262,
            "text_len": 26,
            "text_offset": 3270,
            "warnings": 2
        },
        "speaker": "＆",
        "text": "うわああああ――――っ！\\n",
        "type": "dialog"
    },
    {
        "id": 18,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3490,
            "text_len": 70,
            "text_offset": 3496,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いや―、それにしても今日は楽しかったな。\\n明日からも頑張らないとな。\\n",
        "type": "dialog"
    },
    {
        "id": 19,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3590,
            "text_len": 8,
            "text_offset": 3596,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長。\\n",
        "type": "dialog"
    },
    {
        "id": 20,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3626,
            "text_len": 6,
            "text_offset": 3632,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 21,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3660,
            "text_len": 42,
            "text_offset": 3666,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "囲碁サッカ―、\\n本気でやる気なんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 22,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3730,
            "text_len": 58,
            "text_offset": 3736,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、もちろんだ。\\nこんなに面白いもんほっとけないだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 23,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3826,
            "text_len": 32,
            "text_offset": 3832,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……試合も頑張ってくださいね。\\n",
        "type": "dialog"
    },
    {
        "id": 24,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3888,
            "text_len": 30,
            "text_offset": 3894,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "え？　オレは試合に出ないよ？\\n",
        "type": "dialog"
    },
    {
        "id": 25,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 3956,
            "text_len": 10,
            "text_offset": 3962,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……え？\\n",
        "type": "dialog"
    },
    {
        "id": 26,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4008,
            "text_len": 68,
            "text_offset": 4014,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "バカだな関口、今日はじめた初心者なのに\\n試合に出てどうするんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 27,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4108,
            "text_len": 12,
            "text_offset": 4114,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 28,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4148,
            "text_len": 48,
            "text_offset": 4154,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あの。\\nだったら何のために練習してたんですか？\\n",
        "type": "dialog"
    },
    {
        "id": 29,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4254,
            "text_len": 32,
            "text_offset": 4260,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "おいおい関口、何言ってんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 30,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4314,
            "text_len": 66,
            "text_offset": 4320,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "こんだけお膳立てしといて\\nオレが練習してなかったらおかしいだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 31,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4414,
            "text_len": 80,
            "text_offset": 4420,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それに、次の試合に出られないだけで\\nその次の試合くらいまでには形になってるさ。\\n",
        "type": "dialog"
    },
    {
        "id": 32,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4522,
            "text_len": 68,
            "text_offset": 4528,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "むしろそのくらいまでには\\n部長としてやれるようになっとかないとな！\\n",
        "type": "dialog"
    },
    {
        "id": 33,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4618,
            "text_len": 12,
            "text_offset": 4624,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "部長……。\\n",
        "type": "dialog"
    },
    {
        "id": 34,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4658,
            "text_len": 30,
            "text_offset": 4664,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "そんなに本気だったんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 35,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4716,
            "text_len": 52,
            "text_offset": 4722,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ああ、ボ―ドゲ―ムやってるぐらい\\n楽しかったしな！\\n",
        "type": "dialog"
    },
    {
        "id": 36,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4810,
            "text_len": 6,
            "text_offset": 4816,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 37,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4844,
            "text_len": 62,
            "text_offset": 4850,
            "warnings": 1
        },
        "speaker": "大工",
        "text": "元お遊びで作った部活なんだし、\\n楽しく遊べるならいいだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 38,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4934,
            "text_len": 18,
            "text_offset": 4940,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……そうですね。\\n",
        "type": "dialog"
    },
    {
        "id": 39,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 4980,
            "text_len": 46,
            "text_offset": 4986,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "よ―し！　明日もがんばるぞ！\\n関口も頼むな？\\n",
        "type": "dialog"
    },
    {
        "id": 40,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5054,
            "text_len": 34,
            "text_offset": 5060,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ……はい。\\n私もがんばります。\\n",
        "type": "dialog"
    },
    {
        "id": 41,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5116,
            "text_len": 26,
            "text_offset": 5122,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "それじゃ、また明日な！！\\n",
        "type": "dialog"
    },
    {
        "id": 42,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5170,
            "text_len": 12,
            "text_offset": 5176,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "あ、部長。\\n",
        "type": "dialog"
    },
    {
        "id": 43,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5292,
            "text_len": 30,
            "text_offset": 5298,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "コ―ヒ―、まだ来てないのに。\\n",
        "type": "dialog"
    },
    {
        "id": 44,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5386,
            "text_len": 78,
            "text_offset": 5392,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そりゃ付き合いだからに決まってるだろ？\\nぶっちゃけ練習とかめんどくさいしさ。\\n",
        "type": "dialog"
    },
    {
        "id": 45,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5492,
            "text_len": 72,
            "text_offset": 5498,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "でも、人集めといて何もしないのは\\n上に立つものとしてどうかと思うしな。\\n",
        "type": "dialog"
    },
    {
        "id": 46,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5598,
            "text_len": 42,
            "text_offset": 5604,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なるほど。\\nその、部長ってすごいですね。\\n",
        "type": "dialog"
    },
    {
        "id": 47,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5668,
            "text_len": 8,
            "text_offset": 5674,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん―？\\n",
        "type": "dialog"
    },
    {
        "id": 48,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5704,
            "text_len": 62,
            "text_offset": 5710,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "その……失礼ですけど、\\nもっと不真面目な人だと思ってました。\\n",
        "type": "dialog"
    },
    {
        "id": 49,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5794,
            "text_len": 30,
            "text_offset": 5800,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "はははっ、関口は正直だなぁ。\\n",
        "type": "dialog"
    },
    {
        "id": 50,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5852,
            "text_len": 18,
            "text_offset": 5858,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "ス、スミマセン。\\n",
        "type": "dialog"
    },
    {
        "id": 51,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5898,
            "text_len": 20,
            "text_offset": 5904,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いいっていいって。\\n",
        "type": "dialog"
    },
    {
        "id": 52,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 5946,
            "text_len": 74,
            "text_offset": 5952,
            "warnings": 1
        },
        "speaker": "大工",
        "text": "ま、それに、ちゃんと練習しておいた方が\\n後便利ってのもあるんだけどな。\\n",
        "type": "dialog"
    },
    {
        "id": 53,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6058,
            "text_len": 12,
            "text_offset": 6064,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……便利？\\n",
        "type": "dialog"
    },
    {
        "id": 54,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6112,
            "text_len": 86,
            "text_offset": 6118,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "本当に行われていた大会だと思わなかったから、\\nあの時は簡単にコレでどうにかしたけど。\\n",
        "type": "dialog"
    },
    {
        "id": 55,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6226,
            "text_len": 70,
            "text_offset": 6232,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "一生懸命やってる桜井くんも居るし、\\nいまは下手なコレもできないだろ。\\n",
        "type": "dialog"
    },
    {
        "id": 56,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6324,
            "text_len": 48,
            "text_offset": 6330,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "だから、前の県大会みたいな手は\\n使えないんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 57,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6400,
            "text_len": 74,
            "text_offset": 6406,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "なるほど……練習試合じゃなくて、\\nその後の大会のことを考えてるんですね。\\n",
        "type": "dialog"
    },
    {
        "id": 58,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6502,
            "text_len": 62,
            "text_offset": 6508,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "その通り。それに県大会に出たおかげで\\n次からシ―ドなんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 59,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6608,
            "text_len": 10,
            "text_offset": 6614,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "え……？\\n",
        "type": "dialog"
    },
    {
        "id": 60,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6646,
            "text_len": 22,
            "text_offset": 6652,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "うち、シ―ドらしい。\\n",
        "type": "dialog"
    },
    {
        "id": 61,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6696,
            "text_len": 76,
            "text_offset": 6702,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "だからこれも前と違って、普段から\\n本気でやらないとバレる可能性があるんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 62,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6804,
            "text_len": 12,
            "text_offset": 6810,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 63,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 6926,
            "text_len": 54,
            "text_offset": 6932,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口、もし何か聞かれても\\n知らないって言ってくれよ？\\n",
        "type": "dialog"
    },
    {
        "id": 64,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7002,
            "text_len": 16,
            "text_offset": 7008,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……は、はい。\\n",
        "type": "dialog"
    },
    {
        "id": 65,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7082,
            "text_len": 24,
            "text_offset": 7088,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "そりゃ決まってるだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 66,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7134,
            "text_len": 22,
            "text_offset": 7140,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "みんなのためですか？\\n",
        "type": "dialog"
    },
    {
        "id": 67,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7188,
            "text_len": 26,
            "text_offset": 7194,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "いいや、内申書のためだ。\\n",
        "type": "dialog"
    },
    {
        "id": 68,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7252,
            "text_len": 54,
            "text_offset": 7258,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "初心者なのに一から部活を立ち上げた\\n練習熱心な部長。\\n",
        "type": "dialog"
    },
    {
        "id": 69,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7344,
            "text_len": 60,
            "text_offset": 7350,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "しかも顧問は元高校ＬＵＯ、\\n後輩は中学ＬＵＯときたもんだ。\\n",
        "type": "dialog"
    },
    {
        "id": 70,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7432,
            "text_len": 22,
            "text_offset": 7438,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "これはおいしいだろ？\\n",
        "type": "dialog"
    },
    {
        "id": 71,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7486,
            "text_len": 12,
            "text_offset": 7492,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 72,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7536,
            "text_len": 60,
            "text_offset": 7542,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "県大会にも出たし、\\nあとは全国で勝ったりした日には凄いぞ。\\n",
        "type": "dialog"
    },
    {
        "id": 73,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7624,
            "text_len": 36,
            "text_offset": 7630,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "もしかしたら推薦とかイケるかもな？\\n",
        "type": "dialog"
    },
    {
        "id": 74,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7692,
            "text_len": 12,
            "text_offset": 7698,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 75,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7732,
            "text_len": 68,
            "text_offset": 7738,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "関口も副部長とかやらないか？\\nかなりおいしいポストだと思うんだよ。\\n",
        "type": "dialog"
    },
    {
        "id": 76,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7834,
            "text_len": 18,
            "text_offset": 7840,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "……あの、部長。\\n",
        "type": "dialog"
    },
    {
        "id": 77,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7880,
            "text_len": 6,
            "text_offset": 7886,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "ん？\\n",
        "type": "dialog"
    },
    {
        "id": 78,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7914,
            "text_len": 22,
            "text_offset": 7920,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "似合ってないですよ。\\n",
        "type": "dialog"
    },
    {
        "id": 79,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 7964,
            "text_len": 32,
            "text_offset": 7970,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "……なんだよ、バレバレかよ―！\\n",
        "type": "dialog"
    },
    {
        "id": 80,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8024,
            "text_len": 58,
            "text_offset": 8030,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "だって内申書のためだったら\\n普通の部に入ればいいですし。\\n",
        "type": "dialog"
    },
    {
        "id": 81,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8110,
            "text_len": 74,
            "text_offset": 8116,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "あ―、それもそっか。……よし！\\n弌弌じゃあ今から野球部に入部してくるわ！\\n",
        "type": "dialog"
    },
    {
        "id": 82,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8212,
            "text_len": 6,
            "text_offset": 8218,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "！？\\n",
        "type": "dialog"
    },
    {
        "id": 83,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8246,
            "text_len": 42,
            "text_offset": 8252,
            "warnings": 0
        },
        "speaker": "大工",
        "text": "な―んてな。\\n今度こそひっかかっただろ！\\n",
        "type": "dialog"
    },
    {
        "id": 84,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8320,
            "text_len": 12,
            "text_offset": 8326,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "…………。\\n",
        "type": "dialog"
    },
    {
        "id": 85,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8446,
            "text_len": 56,
            "text_offset": 8452,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（みんなのためなのを認めるのが\\n　恥ずかしいんだ……）\\n",
        "type": "dialog"
    },
    {
        "id": 86,
        "internal": {
            "speaker_len": 4,
            "speaker_offset": 8528,
            "text_len": 32,
            "text_offset": 8534,
            "warnings": 0
        },
        "speaker": "関口",
        "text": "（……気付いてないフリしよう）\\n",
        "type": "dialog"
    }
]